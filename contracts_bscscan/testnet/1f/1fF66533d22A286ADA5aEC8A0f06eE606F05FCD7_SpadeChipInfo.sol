pragma solidity ^0.8.4;

import "../ISpadeChipTypes.sol";
import "base64-sol/base64.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

library SpadeChipLib {
    enum ChipType {
        BRONZE,
        SILVER,
        GOLD,
        PLATINUM,
        DIAMOND
    }

    struct TokenInfo {
        uint256 usedCounter; // how often used for dividends
        uint256 totalClaimed; // amount claimed from dividends
        ChipType chipType; // chip type of this token
    }

    struct ChipInfo {
        uint256 ratio; // e.g. 30
        uint256 shares; // e.g. 50 = 5%
        uint256 price; // e.g. 1 / 10 ethers
        string displayText;
        string image;
        uint256 count; // e.g 10 minted
    }

    struct MintStatus {
        address issuer;
        uint256 tokenId;
        bool paid;
    }

    function getChipsPublicOfType(uint256 publicCount, uint256 ratio)
        public
        pure
        returns (uint256)
    {
        // e.g. 1000 * 30 = 30,000 / 100 = 300
        // e.g. 1000 * 15 = 15,000 / 100 = 150

        return
            SafeMathUpgradeable.div(
                SafeMathUpgradeable.mul(publicCount, ratio),
                100
            );
    }

    function getChipsTotalOfType(uint256 maxCount, uint256 ratio)
        public
        pure
        returns (uint256)
    {
        // e.g. 10,000 * 30 = 300,000 / 100 = 3,000
        // e.g. 10,000 * 15 = 150,000 / 100 = 1,500

        return
            SafeMathUpgradeable.div(
                SafeMathUpgradeable.mul(maxCount, ratio),
                100
            );
    }

    function getRandomChipType(
        uint256 randomNumber,
        ChipType[] memory listOfAvailableChipTypes
    ) public pure returns (ChipType) {
        return
            listOfAvailableChipTypes[
                randomNumber % listOfAvailableChipTypes.length
            ];
    }

    function getTokenURI(
        uint256 tokenID,
        string memory displayText,
        string memory imageIPFS
    ) public pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                strConcat(
                                    "SPADE - NFT Chip #",
                                    StringsUpgradeable.toString(tokenID)
                                ),
                                '", "description":"Owning such a chip gives you as SPADE holder benefits. Read more at spade.zone", "image":"',
                                imageIPFS,
                                '"external_url": "https://spade.zone"',
                                '", "attributes":',
                                '{ "chip_type": "',
                                displayText,
                                '" }',
                                "}"
                            )
                        )
                    )
                )
            );
    }

    // get chip share per 10,000
    function getChipShare(ChipInfo memory chipInfo, uint256 totalChipsOfType)
        public
        pure
        returns (uint256)
    {
        uint256 multiplier = 1000000000; // 100,000,000

        uint256 quotient = SafeMathUpgradeable.mul(chipInfo.shares, multiplier);
        uint256 dividend = totalChipsOfType;

        // e.g. 50 x 1,000,000 / 3,000,000 = 16 / 1,000,000 = 0,0016
        return
            SafeMathUpgradeable.div(
                SafeMathUpgradeable.div(quotient, dividend),
                1000000
            );
    }

    function getRandomMintPrice(uint256 currentCount)
        public
        pure
        returns (uint256 price)
    {
        uint256 basePrice = SafeMathUpgradeable.div(1 ether, 5);

        if (currentCount <= 249) {
            return basePrice;
        } else if (currentCount >= 250 && currentCount <= 499) {
            return SafeMathUpgradeable.mul(basePrice, 2);
        } else if (currentCount >= 500 && currentCount <= 749) {
            return SafeMathUpgradeable.mul(basePrice, 3);
        } else if (currentCount >= 750 && currentCount <= 999) {
            return SafeMathUpgradeable.mul(basePrice, 4);
        }

        return 1 ether;
    }

    function strConcat(string memory _a, string memory _b)
        public
        pure
        returns (string memory result)
    {
        result = string(abi.encodePacked(bytes(_a), bytes(_b)));
    }
}

