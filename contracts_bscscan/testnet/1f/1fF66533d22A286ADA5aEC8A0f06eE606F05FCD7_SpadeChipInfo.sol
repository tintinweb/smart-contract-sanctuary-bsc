pragma solidity ^0.8.4;
import "@openzeppelin/contracts/access/Ownable.sol";
import "./lib/SpadeChipLib.sol";
import "hardhat/console.sol";

contract SpadeChipInfo is Ownable {
    mapping(SpadeChipLib.ChipType => SpadeChipLib.ChipInfo) public chipInfo;

    // max count of this tokens
    uint256 public maxCount;

    // max available to public
    uint256 public publicCount;

    address spadeChipContractAddress;

    // Possible ChipTypes for random mint - length must equal 15 to finish a mint
    SpadeChipLib.ChipType[] listOfAvailableChipTypes;

    event InitPriceChanged(uint256 amount, SpadeChipLib.ChipType chipType);
    event RandomMintProbabilities(
        uint256 bronzeCount,
        uint256 silverCount,
        uint256 goldCount,
        uint256 platinumCount,
        uint256 diamondCount
    );

    modifier onlySpadeChip() {
        console.log("Spade Chip: %s", spadeChipContractAddress);
        console.log("Sender: %s", msg.sender);

        require(msg.sender == spadeChipContractAddress);
        _;
    }

    constructor() {
        // max count of tokens to 10,000
        maxCount = 1000;

        // max count of public mintable
        publicCount = 1000;
    }

    function setSpadeChipContract(address spadeChipContract) public onlyOwner {
        spadeChipContractAddress = spadeChipContract;
    }

    function setChipType(
        SpadeChipLib.ChipType chipType,
        string memory displayText,
        uint256 ratio,
        uint256 shares,
        uint256 price
    ) public onlyOwner {
        chipInfo[chipType].ratio = ratio;
        chipInfo[chipType].price = price;
        chipInfo[chipType].shares = shares;
        chipInfo[chipType].displayText = displayText;
    }

    function getChipInfo(SpadeChipLib.ChipType chipType)
        public
        view
        returns (SpadeChipLib.ChipInfo memory)
    {
        return chipInfo[chipType];
    }

    function setChipPrice(
        SpadeChipLib.ChipType chipType,
        uint256 price // in ethers
    ) public onlyOwner {
        require(price > 0, "E0");
        chipInfo[chipType].price = price;

        emit InitPriceChanged(price, chipType);
    }

    // get chip share per 10,000
    function getChipShare(SpadeChipLib.ChipType chipType)
        public
        view
        returns (uint256)
    {
        // return
        //     SpadeChipLib.getChipShare(
        //         chipInfo[tokenInfo[tokenId].chipType],
        //         SpadeChipLib.getChipsTotalOfType(
        //             maxCount,
        //             chipInfo[tokenInfo[tokenId].chipType].ratio
        //         )
        //     );

        // e.g. 3,000
        uint256 totalChipsOfType = SpadeChipLib.getChipsTotalOfType(
            maxCount,
            chipInfo[chipType].ratio
        );
        uint256 multiplier = 1000000000; // 100,000,000

        uint256 quotient = SafeMathUpgradeable.mul(
            chipInfo[chipType].shares,
            multiplier
        );
        uint256 dividend = totalChipsOfType;

        // e.g. 50 x 1,000,000 / 3,000,000 = 16 / 1,000,000 = 0,0016
        return
            SafeMathUpgradeable.div(
                SafeMathUpgradeable.div(quotient, dividend),
                1000000
            );
    }

    // sets the image for a chipType
    function setChipImage(
        SpadeChipLib.ChipType chipType,
        string memory imageIPFS
    ) public onlyOwner {
        chipInfo[chipType].image = imageIPFS;
    }

    // function to set maximum chips to be public minted
    function setPublicCount(uint256 publicCount_) public onlyOwner {
        // TODO:
        // require(publicCount_ > publicCount, "can only be higher");
        publicCount = publicCount_;
    }

    // function to set maximum chips to be minted
    function setMaxCount(uint256 maxCount_) public onlyOwner {
        require(maxCount_ > maxCount, "can only be higher");
        maxCount = maxCount_;
    }

    // TODO: ONLY CHIP CONTRACT
    function increaseCount(SpadeChipLib.ChipType chipType)
        public
        onlySpadeChip
    {
        chipInfo[chipType].count += 1;
    }

    // TODO: ONLY CHIP CONTRACT
    function addRandomChipType(uint256 count, SpadeChipLib.ChipType chipType)
        private
    {
        for (uint256 i = 0; i < count; i++) {
            listOfAvailableChipTypes.push(chipType);
            console.log("Added chip");
        }
    }

    // TODO: ONLY CHIP CONTRACT
    function setRandomChipTypesPrivate(
        uint256 bronzeCount,
        uint256 silverCount,
        uint256 goldCount,
        uint256 platinumCount,
        uint256 diamondCount
    ) private {
        console.log("Deleting entries");
        delete listOfAvailableChipTypes;

        addRandomChipType(bronzeCount, SpadeChipLib.ChipType.BRONZE);
        addRandomChipType(silverCount, SpadeChipLib.ChipType.SILVER);
        addRandomChipType(goldCount, SpadeChipLib.ChipType.GOLD);
        addRandomChipType(platinumCount, SpadeChipLib.ChipType.PLATINUM);
        addRandomChipType(diamondCount, SpadeChipLib.ChipType.DIAMOND);

        console.log("Has entries", listOfAvailableChipTypes.length > 0);
    }

    function setRandomChipTypes(
        uint256 bronzeCount,
        uint256 silverCount,
        uint256 goldCount,
        uint256 platinumCount,
        uint256 diamondCount
    ) public onlyOwner {
        setRandomChipTypesPrivate(
            bronzeCount,
            silverCount,
            goldCount,
            platinumCount,
            diamondCount
        );

        emit RandomMintProbabilities(
            bronzeCount,
            silverCount,
            goldCount,
            platinumCount,
            diamondCount
        );
    }

    function canRandomMint() public view returns (bool) {
        console.log("Count:", listOfAvailableChipTypes.length > 0);
        return listOfAvailableChipTypes.length > 0;
    }

    function getRandomChipType(uint256 randomness)
        public
        view
        returns (SpadeChipLib.ChipType)
    {
        return
            SpadeChipLib.getRandomChipType(
                randomness,
                listOfAvailableChipTypes
            );
    }

    // TODO: ONLY SPADECHIP
    function recalculateProbability() public onlySpadeChip {
        setRandomChipTypesPrivate(
            chipInfo[SpadeChipLib.ChipType.BRONZE].count ==
                SpadeChipLib.getChipsPublicOfType(
                    publicCount,
                    chipInfo[SpadeChipLib.ChipType.BRONZE].ratio
                )
                ? 0
                : 5,
            chipInfo[SpadeChipLib.ChipType.SILVER].count ==
                SpadeChipLib.getChipsPublicOfType(
                    publicCount,
                    chipInfo[SpadeChipLib.ChipType.SILVER].ratio
                )
                ? 0
                : 4,
            chipInfo[SpadeChipLib.ChipType.GOLD].count ==
                SpadeChipLib.getChipsPublicOfType(
                    publicCount,
                    chipInfo[SpadeChipLib.ChipType.GOLD].ratio
                )
                ? 0
                : 3,
            chipInfo[SpadeChipLib.ChipType.PLATINUM].count ==
                SpadeChipLib.getChipsPublicOfType(
                    publicCount,
                    chipInfo[SpadeChipLib.ChipType.PLATINUM].ratio
                )
                ? 0
                : 2,
            chipInfo[SpadeChipLib.ChipType.DIAMOND].count ==
                SpadeChipLib.getChipsPublicOfType(
                    publicCount,
                    chipInfo[SpadeChipLib.ChipType.DIAMOND].ratio
                )
                ? 0
                : 1
        );
    }
}

