contract AURCToken is Ownable, ERC20, Divestor {
    using IterableMapping for IterableMapping.Map;

    struct Meta {
        uint taxFee;
        uint claimWait;
        uint lastProcessedIndex;
        uint gasForProcessing;
        uint addLiquidityAmount;
        uint magnifiedDividendPerShare;  
        uint totalDividendsDistributed;
        uint swapTokensAtAmount;
        bool whiteOnly;
    }
    struct Addr {
        IPancakeRouter02 router;
        address pair;
        address market;
        address rewardToken;
        IPancakeHelper helper;
    }

    struct User {
        uint lastClaimTime;
        uint withdrawnDividend;
    }
    mapping(address => User) userInfo;

    Meta public meta;
    Addr public addr;
    
    bool private inSwapAndLiquify;
    uint constant public magnitude = 2 ** 128;  
    uint private _totalSupply;
    IterableMapping.Map private _balances;
    mapping(address => bool) public noDevidends;
    

    event DividendsDistributed(address indexed from, uint weiAmount);
    event Claim(address indexed account, uint amount, bool indexed automatic);
    event DividendWithdrawn(address indexed to, uint weiAmount);

    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    constructor() ERC20("AURC TOKEN", "AURC")  {
        _mint(_msgSender(), 10000000000000000 ether);

        meta.claimWait = 10;
        meta.gasForProcessing = 300000;

        meta.addLiquidityAmount = 1000 ether;

        addr.rewardToken = address(this);

        // addr.router = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        // addr.pair = IPancakeFactory(addr.router.factory()).createPair(address(this), addr.router.WETH());


        addr.helper = IPancakeHelper(0x8aCE41186aa19EC9C93E69Cc8CeE1Ccee9f46E8f);
        
        meta.taxFee = 8;

        // meta.whiteOnly = true;
        noDevidends[address(this)] = true;
        noDevidends[address(0)] = true;
        // noDevidends[address(addr.router)] = true;
        noDevidends[addr.pair] = true;
        noDevidends[address(addr.helper)] = true;

        noDevidends[_msgSender()] = true;
    }

    function setRewardToken(address rewardToken_) external onlyOwner {
        addr.rewardToken = rewardToken_;
    }

    function setAddress(address pair_, address helper_) external onlyOwner {
        addr.pair = pair_;
        addr.helper = IPancakeHelper(helper_);

        noDevidends[pair_] = true;
        noDevidends[helper_] = true;
    }

    function setClaimWait(uint wait_) public onlyOwner {
        meta.claimWait = wait_;
    }

    function setProcessAmount(uint gas_, uint liquidityAmount_) external onlyOwner {
        meta.gasForProcessing = gas_;
        meta.addLiquidityAmount = liquidityAmount_;
    }

    function setNoDividends(address[] calldata accounts_, bool isAdd_) external onlyOwner {
        for (uint i; i < accounts_.length; i++) {
            noDevidends[accounts_[i]] = isAdd_;
        }
    }

   


    function _processTransfer(address from_, address to_, uint amount_) private returns(uint) {
        uint taxFee = amount_ * meta.taxFee / 100;

        uint dividends = taxFee * 3 / 10;
        if (addr.pair == address(0)) {
           _transfer(from_, address(0), dividends); 
        } else {
            _transfer(from_, address(this), dividends);
            distributeDividends(dividends);
        }
        _transfer(from_, addr.market, taxFee  / 10);
        _transfer(from_, address(0), taxFee * 2 / 10);
        _transfer(from_, address(addr.helper), taxFee * 2 / 10);

        if (balanceOf(address(addr.helper)) >= meta.addLiquidityAmount) {
            addr.helper.addLiquidity();
        }

        uint realAmount = amount_ - taxFee;
        _transfer(from_, to_, realAmount);
        
        process(meta.gasForProcessing);
        return realAmount;
    }

    function _transfer(address sender, address recipient, uint amount) internal override {
        require(sender != address(0), "ERC20: transfer from the zero address");

        uint senderBalance = _balances.values[sender];
        uint recipientBalance = _balances.values[recipient];
        require(senderBalance >= amount, "ERC20: transfer amount exceeds balance");

        // IterableMapping.set(_balances, sender, senderBalance - amount);
        // IterableMapping.set(_balances, recipient, recipientBalance + amount);
        _balances.set(sender, senderBalance - amount);
        _balances.set(recipient, recipientBalance + amount);
        if (balanceOf(sender) == 0) {
            // IterableMapping.remove(_balances, sender);
            _balances.remove(sender);
        }
        uint tempDebt = userInfo[sender].withdrawnDividend * amount / senderBalance;
        userInfo[recipient].withdrawnDividend += tempDebt;
        userInfo[sender].withdrawnDividend -= tempDebt;
        emit Transfer(sender, recipient, amount);

    }

    function _mint(address account, uint amount) internal override {
        require(account != address(0), "ERC20: mint to the zero address");

        _beforeTokenTransfer(address(0), account, amount);
        uint balance = _balances.values[account];
        _totalSupply += amount;
        // IterableMapping.set(_balances, account, balance + amount);
        _balances.set(account, balance + amount);


        emit Transfer(address(0), account, amount);

        _afterTokenTransfer(address(0), account, amount);
    }

    function distributeDividends(uint amount_) private {
        uint total = totalSupply();
        uint balance0 = balanceOf(addr.pair);
        uint balance1 = balanceOf(address(0));
        require(total > 0 && total > (balance0 + balance1));
        total -= (balance0 + balance1);

        if (amount_ > 0) {
            meta.magnifiedDividendPerShare +=  (amount_ * magnitude / total);
            emit DividendsDistributed(_msgSender(), amount_);

            meta.totalDividendsDistributed +=  amount_;
        }
    }

    function totalSupply() public view virtual override returns (uint) {
        return _totalSupply;
    }

    function balanceOf(address account_) public view override returns (uint) {
        // return IterableMapping.get(_balances, account_);
        return _balances.get(account_);
    }
    
    function getNumberOfTokenHolders() external view returns(uint256) {
        return _balances.keys.length;
    }

    function withdrawableDividendOf(address account_) public view returns (uint) {
        if (accumulativeDividendOf(account_) <= userInfo[account_].withdrawnDividend) {
            return 0;
        }
        return accumulativeDividendOf(account_) - userInfo[account_].withdrawnDividend;
    }

    function accumulativeDividendOf(address account_) public view returns (uint) {
        return meta.magnifiedDividendPerShare * _balances.values[account_] / magnitude;
    }

    
    function transfer(address recipient_, uint amount_) public virtual override  returns (bool) {
        _processTransfer(_msgSender(), recipient_, amount_);
        return true;
    }


    function transferFrom(address sender_, address recipient_, uint amount_) public virtual override returns (bool) {
        _processTransfer(sender_, recipient_, amount_);
        uint currentAllowance = allowance(sender_, _msgSender());
        require(currentAllowance >= amount_, "ERC20: transfer amount exceeds allowance");
        unchecked {
            _approve(sender_, _msgSender(), currentAllowance - amount_);
        }
        return true;
    }

    function burn(uint amount) public {
        uint balance = balanceOf(_msgSender());
        require(balance >= amount,'out of balance');
        // IterableMapping.set(_balances, _msgSender(), balance - amount);
        _balances.set(_msgSender(), balance - amount);
        _totalSupply -= amount;
    }

    function process(uint gas) public returns (uint, uint, uint) {
        uint numberOfTokenHolders = _balances.keys.length;
        if (numberOfTokenHolders == 0) {
            return (0, 0, meta.lastProcessedIndex);
        }

        uint lastIndex = meta.lastProcessedIndex;
        uint gasUsed = 0;
        uint gasLeft = gasleft();
        uint iterations = 0;
        uint claims = 0;

        while (gasUsed < gas && iterations < numberOfTokenHolders) {
            lastIndex++;
            if (lastIndex >= _balances.keys.length) {
                lastIndex = 0;
            }

            address account = _balances.keys[lastIndex];
            if (canAutoClaim(userInfo[account].lastClaimTime)) {
                if (processAccount(account, true)) {
                    claims++;
                }
            }

            iterations++;
            uint newGasLeft = gasleft();

            if (gasLeft > newGasLeft) {
                gasUsed = gasUsed + (gasLeft - newGasLeft);
            }

            gasLeft = newGasLeft;
        }

        meta.lastProcessedIndex = lastIndex;

        return (iterations, claims, meta.lastProcessedIndex);
    }

    function canAutoClaim(uint lastClaimTime_) private view returns (bool) {
        if (lastClaimTime_ > block.timestamp) {
            return false;
        }

        return (block.timestamp - lastClaimTime_) >= meta.claimWait;
    }

 
    function processAccount(address account, bool automatic) private returns (bool) {
        uint amount = _withdrawDividendOfUser(account);

        if (amount > 0) {
            userInfo[account].lastClaimTime = block.timestamp;
            emit Claim(account, amount, automatic);
            return true;
        }

        return false;
    }



    function _withdrawDividendOfUser(address  account_) private returns (uint) {
        uint _withdrawableDividend = withdrawableDividendOf(account_);
        if (_withdrawableDividend > 0) {
            userInfo[account_].withdrawnDividend += _withdrawableDividend;
            emit DividendWithdrawn(account_, _withdrawableDividend);

            if (account_ != addr.pair && !noDevidends[account_]) {
                IERC20(addr.rewardToken).transfer(account_, _withdrawableDividend);
            }
            return _withdrawableDividend;
        }

        return 0;
    }
}

