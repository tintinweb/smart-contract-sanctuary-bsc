//
contract Task is Ownable {
    using SafeERC20 for IERC20;
    using Address for address;

    // event NewTicket(address user, uint256 price);
    // event Reward(address user, uint256 amount, bool isLucky);

    uint256 public lastPrice; // * 1e18 decimals
    uint256 public lastTaskTime;
    IPancakePair public pair;
    IPancakeRouter01 public router;
    IERC20 public stableCoin;
    uint256 public period;
    uint256 public downLimit; //200 = 20%
    uint256 public upLimit; //200 = 20%
    uint256 public upRate; //200 = 20%
    bool public open;

    constructor(
        address _router,
        address _pair,
        address _stableCoin,
        address _owner,
        uint256 _downLimit,
        uint256 _upLimit,
        uint256 _upRate
    ) {
        transferOwnership(_owner);
        stableCoin = IERC20(_stableCoin);
        router = IPancakeRouter01(_router);
        pair = IPancakePair(_pair);
        downLimit = _downLimit;
        upLimit = _upLimit;
        upRate = _upRate;
        approveToken();
        open = true;
        period = 1 days - 1 minutes;
        lastPrice = getPrice();
        lastTaskTime = block.timestamp;
    }

    function approveToken() public {
        IERC20(pair.token0()).approve(address(router), type(uint256).max);
        IERC20(pair.token1()).approve(address(router), type(uint256).max);
    }

    function canTask() public view returns (bool) {
        if (!open) {
            return false;
        }
        return canTaskByTime() || canTaskByLimit();
    }

    function canTaskByTime() public view returns (bool) {
        return block.timestamp > lastTaskTime + period;
    }

    function canTaskByLimit() public view returns (bool) {
        return canTaskByLimitPrice() && checkBalance();
    }

    function canTaskByLimitPrice() public view returns (bool) {
        uint256 curPrice = getPrice();
        return
            (curPrice > lastPrice &&
                checkUpRate(lastPrice, curPrice, upLimit)) ||
            (curPrice < lastPrice &&
                checkDownRate(lastPrice, curPrice, downLimit));
    }

    function checkBalance() public view returns (bool) {
        (
            uint256 amountIn,
            uint256 amountOut,
            address[] memory path
        ) = calcTaskByLimit();
        return IERC20(path[0]).balanceOf(address(this)) >= amountIn;
    }

    function task() public {
        if (!open) {
            return;
        }
        taskByTime();
        taskByLimit();
    }

    function taskByTime() public {
        if (!canTaskByTime()) {
            return;
        }
        uint256 curPrice = getPrice();
        if (
            lastPrice >= curPrice || !checkUpRate(lastPrice, curPrice, upRate)
        ) {
            uint256 x; //reserveStableCoinAmt
            uint256 y; //reserveTokenAmt
            (uint256 _reserve0, uint256 _reserve1, ) = pair.getReserves();
            address token0 = pair.token0();
            (x, y) = address(stableCoin) == token0
                ? (_reserve0, _reserve1)
                : (_reserve1, _reserve0);
            uint256 amountIn = getUpStableCoinInDayUp(lastPrice, x, y, upRate);
            uint256 amountOut = getAmountOut(amountIn, x, y);
            address[] memory path = new address[](2);
            path[0] = address(stableCoin) == token0 ? token0 : pair.token1();
            path[1] = address(stableCoin) == token0 ? pair.token1() : token0;
            router.swapExactTokensForTokens(
                amountIn,
                (amountOut * 90) / 100,
                path,
                address(this),
                block.timestamp + 10
            );
        }
        lastPrice = curPrice;
        lastTaskTime = block.timestamp;
    }

    function taskByLimit() public {
        if (!canTaskByLimit()) {
            return;
        }
        (
            uint256 amountIn,
            uint256 amountOut,
            address[] memory path
        ) = calcTaskByLimit();
        router.swapExactTokensForTokens(
            amountIn,
            (amountOut * 90) / 100,
            path,
            address(this),
            block.timestamp + 10
        );
    }

    function calcTaskByLimit()
        public
        view
        returns (
            uint256 amountIn,
            uint256 amountOut,
            address[] memory path
        )
    {
        uint256 curPrice = getPrice();
        uint256 x; //stable token amount
        uint256 y; //token amount
        path = new address[](2);
        address token0 = pair.token0();
        (uint256 _reserve0, uint256 _reserve1, ) = pair.getReserves();
        (x, y) = address(stableCoin) == token0
            ? (_reserve0, _reserve1)
            : (_reserve1, _reserve0);
        if (lastPrice < curPrice && checkUpRate(lastPrice, curPrice, upLimit)) {
            //sell
            amountIn = getDownTokenIn(lastPrice, x, y, upLimit);
            amountOut = getAmountOut(amountIn, y, x);
            path[0] = address(stableCoin) == token0 ? pair.token1() : token0;
            path[1] = address(stableCoin) == token0 ? token0 : pair.token1();
        } else if (
            lastPrice > curPrice &&
            checkDownRate(lastPrice, curPrice, downLimit)
        ) {
            //buy
            amountIn = getUpStableCoinIn(lastPrice, x, y, downLimit);
            amountOut = getAmountOut(amountIn, x, y);
            path[0] = address(stableCoin) == token0 ? token0 : pair.token1();
            path[1] = address(stableCoin) == token0 ? pair.token1() : token0;
        }
    }

    function getPrice() public view returns (uint256) {
        (uint256 _reserve0, uint256 _reserve1, ) = pair.getReserves();
        address token0 = pair.token0();
        if (address(stableCoin) == token0) {
            return (1e18 * _reserve0) / _reserve1;
        } else {
            return (1e18 * _reserve1) / _reserve0;
        }
    }

    function getUpStableCoinInDayUp(
        uint256 lp,
        uint256 x,
        uint256 y,
        uint256 rate
    ) public pure returns (uint256) {
        uint256 a = (1000 + rate) * lp;
        uint256 aDe = 1e21;
        uint256 b24ac = ((x * x) / (400 * 400)) +
            ((4 * 399 * a * x * y) / (400 * aDe));
        uint256 b24acSqrt = sqrt(b24ac);
        return ((b24acSqrt - x) * 400 - 399 * x) / (399 * 2);
    }

    function getUpStableCoinIn(
        uint256 lp,
        uint256 x,
        uint256 y,
        uint256 rate
    ) public pure returns (uint256) {
        uint256 a = (1000 - rate) * lp;
        uint256 aDe = 1e21;
        uint256 b24ac = ((x * x) / (400 * 400)) +
            ((4 * 399 * a * x * y) / (400 * aDe));
        uint256 b24acSqrt = sqrt(b24ac);
        return ((b24acSqrt - x) * 400 - 399 * x) / (399 * 2);
    }

    function getDownTokenIn(
        uint256 lp,
        uint256 x,
        uint256 y,
        uint256 rate
    ) public pure returns (uint256) {
        uint256 a = (1000 + rate) * lp;
        uint256 aDe = 1e21;
        //b^2-4ac,
        uint256 b24ac = ((y * y) / (400 * 400)) +
            ((4 * x * y * 399 * aDe) / (400 * a));
        uint256 b24acSqrt = sqrt(b24ac);
        return (((b24acSqrt - y) * 200) / 399) - (y / 2);
    }

    // given an input amount of an asset and pair reserves, returns the maximum output amount of the other asset
    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountOut) {
        require(amountIn > 0, "PancakeLibrary: INSUFFICIENT_INPUT_AMOUNT");
        require(
            reserveIn > 0 && reserveOut > 0,
            "PancakeLibrary: INSUFFICIENT_LIQUIDITY"
        );
        uint256 amountInWithFee = amountIn * 9975;
        uint256 numerator = amountInWithFee * reserveOut;
        uint256 denominator = reserveIn * 10000 + amountInWithFee;
        amountOut = numerator / denominator;
    }

    // babylonian method (https://en.wikipedia.org/wiki/Methods_of_computing_square_roots#Babylonian_method)
    function sqrt(uint256 y) public pure returns (uint256 z) {
        if (y > 3) {
            z = y;
            uint256 x = y / 2 + 1;
            while (x < z) {
                z = x;
                x = (y / x + x) / 2;
            }
        } else if (y != 0) {
            z = 1;
        }
    }

    function checkUpRate(
        uint256 prePrice,
        uint256 nowPrice,
        uint256 rate
    ) public pure returns (bool) {
        return ((nowPrice - prePrice) * 1000) > rate * prePrice;
    }

    function checkDownRate(
        uint256 prePrice,
        uint256 nowPrice,
        uint256 rate
    ) public pure returns (bool) {
        return ((prePrice - nowPrice) * 1000) > rate * prePrice;
    }

    function setOpen(bool _open) public onlyOwner {
        open = _open;
    }

    function withdraw(address token, uint256 amount) public onlyOwner {
        IERC20(token).transfer(msg.sender, amount);
    }
}