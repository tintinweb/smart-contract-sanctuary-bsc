pragma solidity ^0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import '@openzeppelin/contracts/utils/Strings.sol';
import "./TOPIA.sol";
import "./NewRNG.sol";

contract DogeWorld is IERC721Receiver{
   
    TOPIA public topia;

    Randomizer rng;

    address public Genaddr;

    address public Alphaddr;

    uint256 CatsDaily = (20 * (10**18));

    uint256 AlphasDaily = (35 * (10**18));

    uint256 MIN_EXIT_DAYS = 2 days;

    address owner;

    mapping (uint16 => uint256) toprewards;

    mapping (uint16 => uint256) atoprewards;

    mapping (uint16 => uint8) toktype;

    mapping (uint16 => uint256) sttime;

    mapping (uint16 => uint256) asttime;

    uint256 stolenrwds;

    mapping( address => st) sttok;

    struct st{
        
        uint16[] td;
        mapping(uint16 => uint256) lastclaim;
        mapping(uint16 => uint256) index;
        uint16[] atd;
        mapping(uint16 => uint256) alastclaim;
        mapping(uint16 => uint256) aindex;
    }

    uint16[] stolenCats;

    uint16[] stolenDogs;

    uint256 stkCats;

    uint16[] stkVet;

    uint256 stkAlphas;

    uint16[] stkDogs;

    mapping(uint16 => uint16[]) stCats;

    mapping(uint16 => uint16[]) stDogs;

    event CatStolen(uint16);

    event DogStolen(uint16);

    event StakeGen(uint16);

    event StakeAlpha(uint16);

    event UnstakeGen(uint16);

    event UnstakeAlpha(uint16);

    event ClaimRewards(uint16, uint256);

    event ClaimRewardsbyAddr(address, uint256);

    event BuyPye(uint16, uint256);

     constructor(
        address payable _topia,
        address _genaddr,
        address _apaddr,
        address payable _randomness
    ){
        topia = TOPIA(_topia);
        Genaddr = _genaddr;
        Alphaddr = _apaddr;
        rng = Randomizer(_randomness);
        owner = msg.sender;
    }

    function updateTopiaRewards(uint256 cats, uint256 Alphas) external onlyOwner {

        CatsDaily = cats;

        AlphasDaily = Alphas;

    }

    function setTopia(address payable _topia) external onlyOwner {
        topia = TOPIA(_topia);
    }

    function nftType(uint16 tokId)public view returns(uint256 n){
        if(toktype[tokId]==1){
            return 1;
        } else if(toktype[tokId]==2){
            return 2;
        } else if(toktype[tokId]==3){
            return 3;
        }
    }

    function setnfttype(uint16 tokId, uint8 typ)external onlyOwner(){
        toktype[tokId] = typ;
    }

    function stake(uint16 _tokId) public {
        require(IERC721(Genaddr).ownerOf(_tokId) == msg.sender, "Not your NFT");

        IERC721(Genaddr).approve(address(this), _tokId);
        
        IERC721(Genaddr).safeTransferFrom(msg.sender, address(this), _tokId);

        sttok[msg.sender].lastclaim[_tokId] = block.timestamp;

        sttok[msg.sender].td.push(_tokId);

        sttok[msg.sender].index[_tokId] = sttok[msg.sender].td.length;

        sttime[_tokId] = block.timestamp;

        if(nftType(_tokId) == 1){
            stkCats++;
        }
        else if(nftType(_tokId) == 2){
            stkDogs.push(_tokId);
        }
        else if(nftType(_tokId) == 3){
            stkVet.push(_tokId);
        }

        emit StakeGen(_tokId);
    }

    function stakeAlpha(uint16 _tokId)public {
        require(IERC721(Alphaddr).ownerOf(_tokId) == msg.sender, "Not your NFT");

        IERC721(Alphaddr).approve(address(this), _tokId);

        IERC721(Alphaddr).safeTransferFrom(msg.sender, address(this), _tokId);

        sttok[msg.sender].alastclaim[_tokId] = block.timestamp;

        sttok[msg.sender].atd.push(_tokId);

        sttok[msg.sender].aindex[_tokId] = sttok[msg.sender].atd.length;

        stkAlphas++;

        asttime[_tokId] = block.timestamp;

        emit StakeAlpha(_tokId);
    }

    function unstake(uint16 _tokId) public returns(bool){

        require(block.timestamp > (sttime[_tokId] + 10), "Haven't been staked for 2 days");
        bool success;
        uint256[] memory rand = rng.getRandomWords(1);

        if(nftType(_tokId) == 1){
            if(rand[0] % 10 == 1){

                uint256 x = rand[0] % stkDogs.length;

                uint16[] memory toks;

                stCats[stkDogs[x]].push(_tokId);

                toks[0] = (_tokId);

                claimbytoks(toks, msg.sender, false);

                stolenCats.push(_tokId);

                sttok[msg.sender].td[sttok[msg.sender].index[_tokId] - 1] = sttok[msg.sender].td[ sttok[msg.sender].td.length - 1 ];

                sttok[msg.sender].td.pop();
                
                emit CatStolen(_tokId);

                success = true;
            } else {
                uint16[] memory toks;

                toks[0] = _tokId;

                claimbytoks(toks, msg.sender, true);
            
                IERC721(Genaddr).safeTransferFrom( address(this), msg.sender, _tokId);

                sttok[msg.sender].td[sttok[msg.sender].index[_tokId] - 1] = sttok[msg.sender].td[ sttok[msg.sender].td.length - 1 ];

                sttok[msg.sender].td.pop();

                claimbytoks(toks, msg.sender, false);

                stkCats--;

                success = false;
            }

        } else if(nftType(_tokId) == 2){
             if(rand[0] % 10 == 1){

                uint256 x = rand[0] % stkVet.length;

                uint16[] memory toks;

                stDogs[stkVet[x]].push(_tokId);

                toks[0] = (_tokId);

                claimbytoks(toks, msg.sender, false);

                stolenCats.push(_tokId);

                sttok[msg.sender].td[sttok[msg.sender].index[_tokId] - 1] = sttok[msg.sender].td[ sttok[msg.sender].td.length - 1 ];

                sttok[msg.sender].td.pop();
                
                emit DogStolen(_tokId);

                success = true;
            } else {
                uint16[] memory toks;

                toks[0] = _tokId;

                claimbytoks(toks, msg.sender, true);
            
                IERC721(Genaddr).safeTransferFrom( address(this), msg.sender, _tokId);

                sttok[msg.sender].td[sttok[msg.sender].index[_tokId] - 1] = sttok[msg.sender].td[ sttok[msg.sender].td.length - 1 ];

                sttok[msg.sender].td.pop();

                claimbytoks(toks, msg.sender, false);

                success = false;

            }
        } else if(nftType(_tokId) == 3){
            IERC721(Genaddr).safeTransferFrom(address(this), msg.sender, _tokId);

            sttok[msg.sender].td[sttok[msg.sender].index[_tokId] - 1] = sttok[msg.sender].td[ sttok[msg.sender].td.length - 1 ];

            sttok[msg.sender].td.pop();

            emit UnstakeGen(_tokId);

            success = false;

        }

        return success;

    }

    function unstakeAlpha(uint16 _tokId) public {
        require(block.timestamp > (asttime[_tokId] + 2 days), "Haven't been staked for 2 days");

        sttok[msg.sender].atd[sttok[msg.sender].aindex[_tokId] - 1] = sttok[msg.sender].atd[ sttok[msg.sender].atd.length - 1 ];

        sttok[msg.sender].atd.pop();

        uint16[] memory toks;

        toks[0] = _tokId;

        stkAlphas--;

        claimbytoks(toks, msg.sender, true);

        IERC721(Alphaddr).safeTransferFrom(address(this), msg.sender, _tokId);

        emit UnstakeAlpha(_tokId);
    }

    function calcrewardbyAddr(address addr) public view returns(uint256){

        uint256 totalclaimable;

        for(uint16 i = 0 ; i < sttok[addr].td.length; i++){

            uint16 tid = sttok[addr].td[i];

            totalclaimable += estRewards(tid, addr, false);
        }
        for(uint16 i = 0 ; i < sttok[addr].atd.length; i++){
        
            uint16 tid = sttok[addr].atd[i];

            totalclaimable += estRewards(tid, addr, true);
        }

        return totalclaimable;
    }

    function claimbytoks(uint16[] memory toks, address user, bool isAlpha) public {
        uint256 rwds;

        for(uint256 i = 0 ; i < toks.length; i++){
            uint16 tok = toks[i];
            uint rew = estRewards(tok, user, isAlpha);
            require(rew > 0, "No Rewards to claim");
            rwds += rew;
            if(isAlpha){
                atoprewards[tok]=0;
                sttok[user].alastclaim[tok] = block.timestamp;
            }
            else {
                toprewards[tok]=0;
                sttok[user].lastclaim[tok] = block.timestamp;
            }
        }
        emit ClaimRewardsbyAddr(user , rwds);
        topia.transfer(user, rwds);
    }

    function estRewards(uint16 _tid, address user, bool isAlpha) public view returns(uint256){

        uint256 topiarewards;

        uint256 td = (( block.timestamp - sttok[user].lastclaim[_tid] )/86400 );
        if(!isAlpha){
            if(nftType(_tid)==1){
                topiarewards = toprewards[_tid] + (CatsDaily * td) - (toprewards[_tid]/10);
            } 
            else if(nftType(_tid)==2){
                topiarewards = toprewards[_tid] + ((stolenrwds)/(4*stkDogs.length));
            }
            else if(nftType(_tid)==3){
                topiarewards = toprewards[_tid] + ((stolenrwds * 3)/(4*stkVet.length));
            }
        }
        else{
            topiarewards = atoprewards[_tid] + (AlphasDaily * td);
        }

        return topiarewards;
    }

    function resetrewbyAddr(address addr) internal {
        for(uint16 i = 0 ; i < sttok[addr].td.length; i++){
            uint16 tid = sttok[addr].td[i];
            toprewards[tid] = 0;
            sttok[msg.sender].lastclaim[tid] = block.timestamp;

        }
        for(uint16 i = 0 ; i < sttok[addr].atd.length; i++){
            uint16 tid = sttok[addr].atd[i];
            atoprewards[tid] = 0;
            sttok[msg.sender].alastclaim[tid] = block.timestamp;
        }
    }

    function claimbyaddr(address user) public {
        uint256 rew = calcrewardbyAddr(user);
        require(rew > 0, "No Rewards to claim");
        emit ClaimRewardsbyAddr(user, rew);
        topia.transfer(user, rew);
        resetrewbyAddr(user);
    }

    function checkStolenCats(uint16 tokId)external view returns(uint256){
        return stCats[tokId].length;
    }

    function checkStolenDogs(uint16 tokId)external view returns(uint256){
        return stDogs[tokId].length;
    }

    function acquireStolen(address user, uint16 tokId)external{

        if(stCats[tokId].length > 0){

            for(uint16 i = 0; i < stCats[tokId].length; i++){

                sttok[user].td.push(stCats[tokId][i]);

            }

        }

        if(stDogs[tokId].length > 0){

            for(uint16 i = 0; i < stDogs[tokId].length; i++){

                sttok[user].td.push(stDogs[tokId][i]);

            }

        }

    }

    function getStakedGenbyOwner(address user) internal view returns(uint16[] memory){

        uint16[] memory toks = sttok[user].td;

        return toks;

    }

    function getStakedAlphabyOwner(address user) internal view returns(uint16[] memory){

        uint16[] memory toks = sttok[user].atd;

        return toks;

    }

    modifier onlyOwner() {
        require( msg.sender == owner , "Ownable: caller is not the owner");
        _;
    }

    function onERC721Received(
        address,
        address from,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        //require(from == address(0x0), "Cannot send to Armory directly");
        return IERC721Receiver.onERC721Received.selector;
    }
}

