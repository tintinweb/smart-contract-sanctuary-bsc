abstract contract EBSPayment is EBSAddress, ERC1155Holder {
    function _sendPayment(
        address paymentMethod,
        address sender,
        address receiver,
        uint256 amounts,
        uint8 useWrapper
    ) internal {
        if (useWrapper == 0){
            if (paymentMethod == address (0x1))
                if (sender == address(this)) payable(receiver).transfer(amounts);
                else {
                    require(msg.value == amounts, "EBSPayment: Insufficient funds");
                    if(receiver != address(this)){
                        payable(receiver).transfer(amounts);
                    }
                }
            else{
                if (sender == address(this)) SafeERC20.safeTransfer(IERC20(paymentMethod), receiver, amounts);
                else SafeERC20.safeTransferFrom(IERC20(paymentMethod), sender, receiver, amounts);
            }
        }

        else if (useWrapper == 1) {
            EBS_WRAPPER1.safeTransferFrom(sender, receiver, EBS_WRAPPER1.getTokenID(paymentMethod), amounts, abi.encodePacked(address(this)));
        }

        else if (useWrapper == 2) {
            EBS_WRAPPER2.safeTransferFrom(sender, receiver, EBS_WRAPPER2.getTokenID(paymentMethod), amounts, abi.encodePacked(address(this)));
        }

        else{
            revert("EBSPayment: Invalid wrapper");
        }
    }
}
