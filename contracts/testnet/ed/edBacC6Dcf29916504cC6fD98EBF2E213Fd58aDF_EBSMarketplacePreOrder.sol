contract NFToken is FreezeData, ERC721Preset {
    event NewURI(
        uint256 indexed tokenId, 
        string indexed oldURI,
        string indexed newURI
    );

    constructor(
        string memory name, 
        string memory symbol,
        string memory baseTokenURI
    ) ERC721Preset(name, symbol, baseTokenURI){

    }

    function mintToken(
        address to, 
        uint256 tokenId, 
        string memory uri, 
        bool isFreeze
    ) external onlyRole(MINTER_ROLE) {
        _mintToken(to, tokenId, uri, "0x", isFreeze);
    }

    function mintToken(
        address to, 
        uint256 tokenId, 
        string memory uri, 
        bytes memory data, 
        bool isFreeze
    ) external onlyRole(MINTER_ROLE) {
        _mintToken(to, tokenId, uri, data, isFreeze);
    }

    function mintBatchTokens(
        address[] memory to, 
        uint256[] memory  tokenIds, 
        string[] memory uri, 
        bytes[] memory data, 
        bool[] memory isFreeze
    ) external onlyRole(MINTER_ROLE) {
        for (uint8 i; i < tokenIds.length; i++){
            _mintToken(to[i], tokenIds[i], uri[i], data[i], isFreeze[i]);
        }
    }

    function _mintToken(
        address to, 
        uint256 tokenId, 
        string memory uri, 
        bytes memory data, 
        bool isFreeze
    ) private {
        _safeMint(to, tokenId, data);
        _setTokenURI(tokenId, uri);

        if (isFreeze) _freezeToken(tokenId);
    }

    function setTokenURIs(
        uint256[] memory tokenIds, 
        string[] memory newUris
    ) external {
        require(tokenIds.length == newUris.length, "ERC721: Length mismatch");

        for(uint8 i; i < tokenIds.length; i++){
            setTokenURI(tokenIds[i], newUris[i]);
        }
    }

    function setTokenURI(
        uint256 tokenId, 
        string memory newURI
    ) public {
        require(_exists(tokenId), "ERC721: operator query for nonexistent token");
        require(_msgSender() == ERC721.ownerOf(tokenId), "ERC721: caller is not owner");
        require(!freezeToken(tokenId), "ERC721FreezeData: Token is freeze");

        string memory oldURI = tokenURI(tokenId);

        _setTokenURI(tokenId, newURI);
        emit NewURI(tokenId, oldURI, newURI);
    }
}

