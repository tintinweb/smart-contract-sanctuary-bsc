    ) public view returns (
        uint256 registered,
        uint256 revoked,
        uint256 unrelease,
        uint256 released
    ){
        for (uint8 i; i < _registered[productId]; i++){
            if(_register[productId][i] == user){
                registered++;
                if (_order[productId][i] == OrderState.revoked) revoked++;
                if (_order[productId][i] == OrderState.unrelease) unrelease++;
                if (_order[productId][i] == OrderState.released || _order[productId][i] == OrderState.claimed) released++;
            }
        }
    }

    function productUserUnrelease(
        bytes32 productId,
        address user
    ) public view returns (
        uint256 unrelease
    ){
        for (uint8 i; i < _registered[productId]; i++){
            if(_register[productId][i] == user){
                if (_order[productId][i] == OrderState.unrelease) unrelease++;
            }
        }
    }

    function registeres(
        bytes32 productId
    ) public view returns (address[] memory){
        address[] memory registered = new address[](_registered[productId]);
        for (uint256 i; i < _registered[productId]; i++){
            registered[i] = _register[productId][i];
        }
        return registered;
    }

    function unreleaseOrder(
        bytes32 productId
    ) public view returns (uint256 total){
        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.unrelease) total++;
        }
    }

    function revokedOrder(
        bytes32 productId
    ) public view returns (uint256 total){
        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.revoked) total++;
        }
    }

    function releasedOrder(
        bytes32 productId
    ) public view returns (uint256 total){
        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.released || _order[productId][i] == OrderState.claimed) total++;
        }
    }

    function unclaimedToken(
        bytes32 productId
    ) public view returns (uint256 token){
        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.released){
                token += _price[productId];
            }
        }
    }

    function claimedToken(
        bytes32 productId
    ) public view returns (uint256 token){
        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.claimed){
                token += _price[productId];
            }
        }
    }

    function newProducts(
        bool[] memory nftTypes,
        uint8[] memory useWrappers,
        address[] memory paymentMethods,
        uint256[] memory openTimes,
        uint256[] memory closeTimes,
        uint256[] memory releaseTimes,
        uint256[] memory max,
        uint256[] memory prices,
        bytes32[] memory productIds
    ) external onlyWhitelistUser whenNotPaused {
        require(
            nftTypes.length == useWrappers.length &&
            nftTypes.length == paymentMethods.length &&
            nftTypes.length == openTimes.length &&
            nftTypes.length == closeTimes.length &&
            nftTypes.length == releaseTimes.length &&
            nftTypes.length == max.length &&
            nftTypes.length == prices.length &&
            nftTypes.length == productIds.length,
            "MarketplaceOrder: Length mismatch"
        );

        for (uint8 i; i < nftTypes.length ; i++){
            newProduct(nftTypes[i], useWrappers[i], paymentMethods[i], openTimes[i], closeTimes[i], releaseTimes[i],max[i], prices[i], productIds[i]);
        }
    }

    function newProduct(
        bool nftType,
        uint8 useWrapper,
        address paymentMethod,
        uint256 openTime,
        uint256 closeTime,
        uint256 releaseTime,
        uint256 max,
        uint256 price,
        bytes32 productId
    ) public onlyWhitelistUser whenNotPaused {
        require(_state[productId] == ProductState.inactive, "MarketplaceOrder: Product ID existed");
        require(closeTime > block.timestamp, "MarketplaceOrder: Invalid time config");
        require(closeTime > openTime, "MarketplaceOrder: Invalid time config");
        require(releaseTime >= closeTime, "MarketplaceOrder: Invalid time config");

        _type[productId] = nftType;
        _useWrapper[productId] = useWrapper;
        _paymentMethod[productId] = paymentMethod;
        _beneficiary[productId] = msg.sender;
        _openTime[productId] = openTime;
        _closeTime[productId] = closeTime;
        _releaseTime[productId] = releaseTime;
        _max[productId] = max;
        _price[productId] = price;
        _state[productId] = ProductState.unlocked;

        emit NewProduct(
            nftType,
            useWrapper,
            paymentMethod,
            msg.sender,
            openTime,
            closeTime,
            releaseTime,
            max,
            price,
            productId
        );
    }

    function editProducts(
        uint256[] memory openTimes,
        uint256[] memory closeTimes,
        uint256[] memory releaseTimes,
        uint256[] memory max,
        uint256 nonce,
        uint256 validTxTime,
        bytes32[] memory productIds,
        bytes[] calldata marketSigns
    ) external onlyWhitelistUser whenNotPaused onlyNonce(nonce){
        require(
            productIds.length == openTimes.length &&
            productIds.length == closeTimes.length &&
            productIds.length == releaseTimes.length &&
            productIds.length == max.length,
            "MarketplaceOrder: Length mismatch"
        );
        for (uint8 i; i < productIds.length ; i++){
            editProduct(openTimes[i], closeTimes[i], releaseTimes[i], max[i], nonce, validTxTime, productIds[i], marketSigns[i]);
        }
    }

    function editProduct(
        uint256 openTime,
        uint256 closeTime,
        uint256 releaseTime,
        uint256 max,
        uint256 nonce,
        uint256 validTxTime,
        bytes32 productId,
        bytes calldata marketSign
    ) public onlyWhitelistUser whenNotPaused onlyNonce(nonce){
        require(_state[productId] == ProductState.unlocked || _state[productId] == ProductState.locked, "MarketplaceOrder: Product not exist");
        require(msg.sender == _beneficiary[productId], "MarketplaceOrder: Invalid caller");
        require(block.timestamp <= validTxTime, "MarketplaceOrder: Invalid time");
    
        bytes32 marketMessage = keccak256(abi.encodePacked(openTime, closeTime, releaseTime, max, productId, nonce, msg.sender, validTxTime, "Edit Product"));
        require (EBS_CONTROL.checkSignerValidSignature(marketSign, marketMessage), "MarketplaceOrder: Invalid signature");

        bytes32 marketSignatureHash = keccak256(abi.encodePacked(marketSign));
        EBS_CONTROL.useSignature(marketSignatureHash);
        
        _increaseNonce(msg.sender);

        _openTime[productId] = openTime;
        _closeTime[productId] = closeTime;
        _releaseTime[productId] = releaseTime;
        _max[productId] = max;

        emit EditProduct(
            openTime,
            closeTime,
            releaseTime,
            max,
            productId
        );
    }

    function confirmReleased(bytes32 productId) external {
        require(EBS_CONTROL.isOperator(msg.sender),"MarketplaceOrder: Must have OPERATOR_ROLE");

        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.unrelease){
                _order[productId][i] = OrderState.released;

                emit ConfirmedReleased(productId, msg.sender, _register[productId][i]);
            }
        }
    }

    function confirmReleasedSpecify(
        bytes32 productId,
        address receiver,
        uint256 amounts
    ) external {
        require(EBS_CONTROL.isOperator(msg.sender),"MarketplaceOrder: Must have OPERATOR_ROLE");
        require(productUserUnrelease(productId, receiver) >= amounts, "MarketplaceOrder: insufficiency item");

        uint256 released;
        uint256 i;

        while (i != _registered[productId] && released != amounts){
            if(_register[productId][i] == receiver && _order[productId][i] == OrderState.unrelease){
                _order[productId][i] = OrderState.released;

                emit ConfirmedReleased(productId, msg.sender, receiver);
                released++;
            }
            i++;
        }
    }

    function changeBeneficiary(address newBeneficiary, bytes32 productId) external {
        require(_state[productId] == ProductState.unlocked || _state[productId] == ProductState.locked, "MarketplaceOrder: Product not exist");
        require(_beneficiary[productId] == msg.sender, "MarketplaceOrder: invalid caller");
        require(EBS_CONTROL.isWhitelistUser(newBeneficiary), "MarketplaceOrder: New beneficiary not Whitelist user");

        address curBeneficiary = _beneficiary[productId];
        _beneficiary[productId] = newBeneficiary;

        emit ChangeBeneficiary(curBeneficiary, newBeneficiary, productId);
    }

    function placeOrders(
        bytes32[] memory productIds,
        uint256[] memory amountsPerProduct
    ) external payable notBlacklistUser whenNotPaused {
        require(productIds.length == amountsPerProduct.length,"MarketplaceOrder: Length mismatch");
        for (uint8 i; i < productIds.length ; i++){
            placeOrder(productIds[i], amountsPerProduct[i]);
        }
    }

    function placeOrder(
        bytes32 productId,
        uint256 amounts
    ) public payable notBlacklistUser whenNotPaused notLockedProduct(productId){
        require(block.timestamp >= _openTime[productId], "MarketplaceOrder: Product not open yet");
        require(block.timestamp <= _closeTime[productId], "MarketplaceOrder: Products expired");
        require(_registered[productId] + amounts <= _max[productId], "MarketplaceOrder: Out of stock");

        uint256 total = _price[productId] * amounts;

        _sendPayment(_paymentMethod[productId], msg.sender, address(this), total, _useWrapper[productId]);

        for (uint256 i = _registered[productId]; i < _registered[productId] + amounts; i++){
            _register[productId][i] = msg.sender;
        }

        _registered[productId] += amounts;

        emit PlaceOrder(total, productId, amounts, msg.sender);
    }

    function revokeOrders(
        bytes32[] memory productIds,
        uint256[] memory amountsPerProduct
    ) public notBlacklistUser whenNotPaused {
        require(productIds.length == amountsPerProduct.length,"MarketplaceOrder: Length mismatch");
        for (uint8 i; i < productIds.length ; i++){
            revokeOrder(productIds[i], amountsPerProduct[i]);
        }
    }

    function revokeOrder(
        bytes32 productId,
        uint256 amounts
    ) public notBlacklistUser whenNotPaused {
        require (productUserUnrelease(productId, msg.sender) >= amounts, "MarketplaceOrder: insufficiency item");

        uint256 revoked;
        uint256 token;
        uint256 i;

        while (i != _registered[productId] && revoked != amounts){
            if(_register[productId][i] == msg.sender && _order[productId][i] == OrderState.unrelease){
                token += _price[productId];
                _order[productId][i] = OrderState.revoked;
                revoked++;
            }
            i++;
        }

        _max[productId] += amounts;
        _sendPayment(_paymentMethod[productId], address(this), msg.sender, token, _useWrapper[productId]);

        emit RevokedOrder(productId, amounts, token, _paymentMethod[productId], msg.sender, _useWrapper[productId]);
    }

    function releaseAndClaim(
        uint256[] memory tokenIds,
        string[] memory uris,
        bytes calldata marketSign,
        address nftAddress,
        uint256 nonce,
        bool isFreeze,
        bytes32 productId
    ) external onlyNonce(nonce) onlyWhitelistUser whenNotPaused notLockedProduct(productId){
        release(tokenIds, uris, marketSign, nftAddress, nonce, isFreeze, productId);

        claim(productId);
    }
    
    function release(
        uint256[] memory tokenIds,
        string[] memory uris,
        bytes calldata marketSign,
        address nftAddress,
        uint256 nonce,
        bool isFreeze,
        bytes32 productId
    ) public onlyNonce(nonce) onlyWhitelistUser whenNotPaused notLockedProduct(productId){
        require(block.timestamp >= _releaseTime[productId], "MarketplaceOrder: Invalid release time");
        require(msg.sender == _beneficiary[productId], "MarketplaceOrder: Invalid caller");

        bytes32[] memory signUris = new bytes32[](uris.length);
        for (uint256 i; i < uris.length; i++){
            signUris[i] = keccak256(abi.encodePacked(uris[i]));
        }

        bytes32 marketMessage = keccak256(abi.encodePacked(nftAddress, tokenIds, signUris, isFreeze, nonce, productId, msg.sender, "Release"));
        require (EBS_CONTROL.checkSignerValidSignature(marketSign, marketMessage), "MarketplaceOrder: Invalid signature");

        bytes32 marketSignatureHash = keccak256(abi.encodePacked(marketSign));
        EBS_CONTROL.useSignature(marketSignatureHash);

        _increaseNonce(msg.sender);

        if (_type[productId] == true){
            require (IERC721(nftAddress).supportsInterface(type(IERC721).interfaceId), "MarketplaceOrder: Invalid token type");
            require (tokenIds.length <= unreleaseOrder(productId), "MarketplaceOrder: Invalid token id length");

            _releaseERC721(tokenIds, uris, productId, nftAddress, isFreeze);
        }

        else{
            require(IERC1155(nftAddress).supportsInterface(type(IERC1155).interfaceId), "MarketplaceOrder: Invalid token type");
            require (tokenIds.length == 2, "MarketplaceOrder: Invalid Id");
            require (uris.length == 1, "MarketplaceOrder: Invalid uri");
            require(ERC1155MixedFungibleMintable(nftAddress).getCreator(tokenIds[0]) == msg.sender, "MarketplaceOrder: Invalid Id creator");

            _releaseERC1155(tokenIds[0], tokenIds[1], uris[0], productId, nftAddress, isFreeze);
        }
    }

    function claim(
        bytes32 productId
    ) public onlyWhitelistUser whenNotPaused {
        require(msg.sender == _beneficiary[productId], "MarketplaceOrder: Invalid caller");
        uint256 token;

        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.released){
                token += _price[productId];
                _order[productId][i] = OrderState.claimed;
            }
        }

        _sendPayment(_paymentMethod[productId], address(this), msg.sender, token, _useWrapper[productId]);

        emit ClaimedOrder(msg.sender, _paymentMethod[productId], token, productId, _useWrapper[productId]);
    }

    function returnOrder(
        bytes32 productId
    ) public whenNotPaused {
        require(_state[productId] == ProductState.unlocked || _state[productId] == ProductState.locked, "MarketplaceOrder: Product not exist");
        require(block.timestamp >= _releaseTime[productId], "MarketplaceOrder: Invalid time");
        require(EBS_CONTROL.isOperator(msg.sender),"Marketplace: Must have OPERATOR_ROLE");

        for (uint256 i; i < _registered[productId]; i++){
            if (_order[productId][i] == OrderState.unrelease){
                _sendPayment(_paymentMethod[productId], address(this), _register[productId][i], _price[productId], _useWrapper[productId]);
                _order[productId][i] = OrderState.claimed;

                emit PaymentReturned(productId, _price[productId], _register[productId][i], _paymentMethod[productId], _useWrapper[productId]);
            }
        }
    }

    function returnOrderSpecify(
        bytes32 productId,
        address receiver,
        uint256 amounts
    ) public whenNotPaused {
        require(_state[productId] == ProductState.unlocked || _state[productId] == ProductState.locked, "MarketplaceOrder: Product not exist");
        require(block.timestamp >= _releaseTime[productId], "MarketplaceOrder: Invalid time");
        require(EBS_CONTROL.isOperator(msg.sender),"Marketplace: Must have OPERATOR_ROLE");
        require(productUserUnrelease(productId, receiver) >= amounts, "MarketplaceOrder: insufficiency item");

        uint256 returned;
        uint256 i;

        while(i != _registered[productId] && returned != amounts){
            if (_register[productId][i] == receiver && _order[productId][i] == OrderState.unrelease){
                _sendPayment(_paymentMethod[productId], address(this), _register[productId][i], _price[productId], _useWrapper[productId]);
                _order[productId][i] = OrderState.claimed;
                emit PaymentReturned(productId, _price[productId], _register[productId][i], _paymentMethod[productId], _useWrapper[productId]);
                returned++;
            }
            i++;
        }
    }

    function _releaseERC721(
        uint256[] memory tokenIds, 
        string[] memory uris,
        bytes32 productId,
        address nftAddress, 
        bool isFreeze
    ) private {
        uint256 i;
        uint8 id;

        while (i != _registered[productId] && id != tokenIds.length){
            if (_order[productId][i] == OrderState.unrelease){
                NFToken(nftAddress).mintToken(_register[productId][i], tokenIds[id], uris[id], isFreeze);
                require(NFToken(nftAddress).ownerOf(tokenIds[id]) == _register[productId][i], "MarketplaceOrder: Invalid owner");

                _order[productId][i] = OrderState.released;
                emit ReleasedOrder(nftAddress, _register[productId][i], tokenIds[id], productId);
                id++;
            }
            i++;
        }
    }

    function _releaseERC1155(
        uint256 type_,
        uint256 amounts,
        string memory uri,
        bytes32 productId,
        address nftAddress,
        bool isFreeze
    ) private {
        uint256 i;
        uint8 released;

        while (i != _registered[productId] && released != amounts){
            if (_order[productId][i] == OrderState.unrelease){
                uint256 itemId = ERC1155MixedFungibleMintable(nftAddress).mintOrder(type_, _register[productId][i], uri, isFreeze);

                _order[productId][i] = OrderState.released;
                emit ReleasedOrder(nftAddress, _register[productId][i], itemId, productId);
                released++;
            }
            i++;
        }
    }
}