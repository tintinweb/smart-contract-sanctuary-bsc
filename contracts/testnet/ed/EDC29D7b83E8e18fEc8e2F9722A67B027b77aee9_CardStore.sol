// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "./common/WithdrawUpgradeable.sol";
import "./interfaces/ISavingCommissionGlobal.sol";
import "./interfaces/IUserCommission.sol";
import "./interfaces/ICardStoreStorageGlobal.sol";
import "./interfaces/ISavingCommission.sol";
import "./interfaces/ICardStoreStorage.sol";
import "./interfaces/IInvestorManager.sol";

contract CardStore is
    ICardStoreStorageGlobal,
    WithdrawUpgradeable,
    AccessControlEnumerableUpgradeable,
    ERC1155HolderUpgradeable,
    ERC721HolderUpgradeable
{
    using SafeMathUpgradeable for uint256;
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
    address public globalWallet;
    IERC1155 public itemToken;
    IERC20 public usdmToken;
    IERC20 public usdtToken;
    IERC20 public busdToken;
    ICardStoreStorage public cardStoreStorage;
    ISavingCommission public savingCommission;
    mapping(uint256 => Item) public storeItems;
    uint256 public TRADING_FEE;
    uint256 public limitQuantity;
    mapping(uint256 => CardPrivilege) public cardPrivileges;
    IUserCommission public userCommission;
    IInvestorManager public investorManager;

    function initialize(Config memory _config) public initializer {
        __AccessControl_init();
        __AccessControlEnumerable_init();
        __Ownable_init();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(ADMIN_ROLE, _msgSender());
        globalWallet = owner();

        setConfig(_config);
        TRADING_FEE = 5;
        limitQuantity = 1;
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControlEnumerableUpgradeable, ERC1155ReceiverUpgradeable)
        returns (bool)
    {
        return
            interfaceId == type(IERC1155ReceiverUpgradeable).interfaceId ||
            interfaceId ==
            type(IAccessControlEnumerableUpgradeable).interfaceId ||
            interfaceId == type(IERC721ReceiverUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    struct Config {
        IERC1155 itemToken;
        IERC20 usdmToken;
        IERC20 usdtToken;
        IERC20 busdToken;
        ISavingCommission savingCommission;
        ICardStoreStorage cardStoreStorage;
        IUserCommission userCommission;
        IInvestorManager investorManager;
    }

    struct Item {
        uint256 tokenId;
        uint256 ethPrice;
        uint256 usdmPrice;
        uint256 usdtPrice;
        uint256 busdPrice;
        uint256 quantity;
    }

    event BuyItemByEth(
        uint256 tokenId,
        address buyer,
        uint256 ethPrice,
        uint256 quantity
    );
    event BuyItem(
        uint256 tokenId,
        address buyer,
        uint256 ethPrice,
        uint256 quantity,
        PaymentEnum paymentEnum
    );

    function setConfig(Config memory _config) public onlyRole(ADMIN_ROLE) {
        itemToken = _config.itemToken;
        usdmToken = _config.usdmToken;
        usdtToken = _config.usdtToken;
        busdToken = _config.busdToken;
        savingCommission = _config.savingCommission;
        cardStoreStorage = _config.cardStoreStorage;
        userCommission = _config.userCommission;
        investorManager = _config.investorManager;
    }

    function setLimitQuantity(uint256 _limitQuantity)
        public
        onlyRole(ADMIN_ROLE)
    {
        limitQuantity = _limitQuantity;
    }

    function setTradingFee(uint256 newFee) public onlyRole(ADMIN_ROLE) {
        TRADING_FEE = newFee;
    }

    function setCardPrivileges(CardPrivilege[] memory _cardPrivileges)
        public
        onlyRole(ADMIN_ROLE)
    {
        uint256 length = _cardPrivileges.length;
        for (uint256 index = 0; index < length; index++) {
            CardPrivilege memory _cardPrivilege = _cardPrivileges[index];
            cardPrivileges[_cardPrivilege.tokenId] = _cardPrivilege;
        }
    }

    function setGlobalWallet(address _globalWallet) public onlyOwner {
        globalWallet = _globalWallet;
    }

    function setStoreItem(Item[] memory items) public onlyRole(ADMIN_ROLE) {
        uint256 length = items.length;
        for (uint8 index = 0; index < length; index++) {
            Item memory _item = items[index];
            storeItems[_item.tokenId] = _item;
        }
    }

    function getStoreItems(uint256[] memory tokenIds)
        public
        view
        returns (Item[] memory)
    {
        uint256 length = tokenIds.length;
        Item[] memory result = new Item[](length);
        for (uint8 index = 0; index < length; index++) {
            uint256 tokenId = tokenIds[index];
            result[index] = storeItems[tokenId];
        }
        return result;
    }

    function buyItemByEth(uint256 tokenId, uint256 quantity) public payable {
        Item memory item = storeItems[tokenId];
        require(item.ethPrice > 0 && item.quantity > 0, "Not allow to buy");
        require(quantity <= limitQuantity, "Quantity too large");
        require(item.quantity >= quantity, "Not enough item to buy");
        require(
            itemToken.balanceOf(address(this), item.tokenId) >= quantity,
            "Store not enough item to transfer"
        );
        uint256 price = item.ethPrice.mul(quantity);
        require(msg.value >= price, "insufficient fun");

        payCommission(PaymentEnum.ETH, _msgSender(), msg.value);
        savingCommission.updateWalletRevenues(_msgSender(), msg.value);
        itemToken.safeTransferFrom(
            address(this),
            _msgSender(),
            tokenId,
            quantity,
            ""
        );
        storeItems[tokenId].quantity -= quantity;
        addTransaction(tokenId, PaymentEnum.ETH);
        emit BuyItemByEth(tokenId, _msgSender(), price, quantity);
    }

    function isGetCardWallet(address wallet) public view returns (bool) {
        for (uint256 tokenId = 2; tokenId <= 6; tokenId++) {
            if (itemToken.balanceOf(wallet, tokenId) > 0) {
                return true;
            }
        }
        return false;
    }

    function buyItemByToken(
        uint256 tokenId,
        uint256 quantity,
        PaymentEnum paymentEnum
    ) public {
        Item memory item = storeItems[tokenId];
        require(
            paymentEnum != PaymentEnum.ETH,
            "Not support payment enum on this method"
        );
        require(quantity > 0, "Quantity too low");
        require(
            item.quantity > 0,
            "The Store not sell this items at the moment"
        );
        require(quantity <= limitQuantity, "Quantity too large");
        uint256 purchaseAmount = getPriceByPaymentEnum(
            item,
            quantity,
            paymentEnum
        );
        require(purchaseAmount > 0, "Amount is zero");
        uint256 userBalance = balanceOfByPaymentEnum(paymentEnum, _msgSender());
        require(
            userBalance >= purchaseAmount,
            "Buyer balance reached the limit"
        );

        transferFromByPaymentEnum(
            paymentEnum,
            _msgSender(),
            address(this),
            purchaseAmount
        );
        payCommission(paymentEnum, _msgSender(), purchaseAmount);
        savingCommission.updateWalletRevenues(_msgSender(), purchaseAmount);
        addTransaction(tokenId, paymentEnum);

        itemToken.safeTransferFrom(
            address(this),
            _msgSender(),
            tokenId,
            quantity,
            ""
        );
        storeItems[tokenId].quantity -= quantity;
        emit BuyItem(
            tokenId,
            _msgSender(),
            purchaseAmount,
            quantity,
            paymentEnum
        );
    }

    function transferFromByPaymentEnum(
        PaymentEnum paymentEnum,
        address from,
        address to,
        uint256 amount
    ) internal {
        if (paymentEnum == PaymentEnum.USDM)
            usdmToken.transferFrom(from, to, amount);
        if (paymentEnum == PaymentEnum.USDT)
            usdtToken.transferFrom(from, to, amount);
        if (paymentEnum == PaymentEnum.BUSD)
            busdToken.transferFrom(from, to, amount);
    }

    function balanceOfByPaymentEnum(PaymentEnum paymentEnum, address wallet)
        internal
        view
        returns (uint256)
    {
        if (paymentEnum == PaymentEnum.USDT) return usdtToken.balanceOf(wallet);
        if (paymentEnum == PaymentEnum.BUSD) return busdToken.balanceOf(wallet);
        if (paymentEnum == PaymentEnum.ETH) return address(wallet).balance;
        return usdmToken.balanceOf(wallet);
    }

    function getPriceByPaymentEnum(
        Item memory item,
        uint256 quantity,
        PaymentEnum paymentEnum
    ) internal pure returns (uint256) {
        if (paymentEnum == PaymentEnum.USDT) return item.usdtPrice * quantity;
        if (paymentEnum == PaymentEnum.BUSD) return item.busdPrice * quantity;
        if (paymentEnum == PaymentEnum.ETH) return item.ethPrice * quantity;
        return item.usdmPrice * quantity;
    }

    function payCommission(
        PaymentEnum paymentEnum,
        address buyer,
        uint256 amount
    ) internal {
        if (amount == 0) return;
        ISavingCommissionGlobal.Commission[]
            memory commissions = savingCommission.getCommissions(buyer, amount);
        for (uint256 index = 0; index < commissions.length; index++) {
            ISavingCommissionGlobal.Commission memory commission = commissions[
                index
            ];
            if (commission.receiver == address(0) || commission.amount == 0)
                continue;
            uint256 normalType = 0;
            if (commission.rank != ISavingCommissionGlobal.Rank.CITIZEN) continue;
            userCommission.addWalletCommission(
                commission.receiver,
                commission.amount,
                uint8(paymentEnum)
            );
            return;
        }
    }

    function transferByPaymentEnum(
        PaymentEnum paymentEnum,
        address to,
        uint256 amount
    ) internal {
        if (paymentEnum == PaymentEnum.USDM) usdmToken.transfer(to, amount);
        if (paymentEnum == PaymentEnum.USDT) usdtToken.transfer(to, amount);
        if (paymentEnum == PaymentEnum.BUSD) busdToken.transfer(to, amount);
        if (paymentEnum == PaymentEnum.ETH) payable(to).transfer(amount);
    }

    function getTransactionTotalLength() external view returns (uint256) {
        return cardStoreStorage.getTransactionLength();
    }

    function getTransactionsByIndex(uint256 fromIndex, uint256 toIndex)
        public
        view
        returns (Transaction[] memory)
    {
        uint256 length = cardStoreStorage.getTransactionLength();
        Transaction[] memory emptyResponse = new Transaction[](0);
        if (length == 0) return emptyResponse;
        if (fromIndex >= length) return emptyResponse;

        uint256 normalizedToIndex = toIndex < length ? toIndex : length - 1;
        if (fromIndex > normalizedToIndex) return emptyResponse;

        Transaction[] memory result = new Transaction[](
            normalizedToIndex - fromIndex + 1
        );
        for (uint256 index = fromIndex; index <= normalizedToIndex; index++) {
            result[index - fromIndex] = cardStoreStorage.getTransactionByIndex(
                index
            );
        }
        return result;
    }

    function addTransaction(uint256 tokenId, PaymentEnum paymentEnum) internal {
        Item memory item = storeItems[tokenId];
        uint256 id = cardStoreStorage.getId();
        Transaction memory _transaction = Transaction(
            id,
            _msgSender(),
            tokenId,
            item.ethPrice,
            item.usdmPrice,
            item.usdtPrice,
            item.busdPrice,
            item.quantity,
            paymentEnum,
            block.number
        );
        cardStoreStorage.incrementId();
        cardStoreStorage.pushTransaction(_transaction);
        cardStoreStorage.pushTransactionByWallet(_msgSender(), _transaction);
    }

    function getTransactionsWalletByIndex(
        address wallet,
        uint256 fromIndex,
        uint256 toIndex
    ) public view returns (Transaction[] memory) {
        uint256 length = cardStoreStorage.getTransactionByWalletLength(wallet);
        Transaction[] memory emptyResponse = new Transaction[](0);
        if (length == 0) return emptyResponse;
        if (fromIndex >= length) return emptyResponse;

        uint256 normalizedToIndex = toIndex < length ? toIndex : length - 1;
        if (fromIndex > normalizedToIndex) return emptyResponse;

        Transaction[] memory result = new Transaction[](
            normalizedToIndex - fromIndex + 1
        );
        for (uint256 index = fromIndex; index <= normalizedToIndex; index++) {
            result[index - fromIndex] = cardStoreStorage
                .getTransactionByWalletByIndex(wallet, index);
        }
        return result;
    }

    function getTransactionWalletTotalLength(address wallet)
        external
        view
        returns (uint256)
    {
        return cardStoreStorage.getTransactionByWalletLength(wallet);
    }

    function getPaymentCommissions(uint256 fromIndex, uint256 toIndex)
        public
        view
        returns (PaymentCommission[] memory)
    {
        uint256 length = cardStoreStorage.getPaymentCommissionsLength();
        PaymentCommission[] memory emptyResponse = new PaymentCommission[](0);
        if (length == 0) return emptyResponse;
        if (fromIndex >= length) return emptyResponse;

        uint256 normalizedToIndex = toIndex < length ? toIndex : length - 1;
        if (fromIndex > normalizedToIndex) return emptyResponse;

        PaymentCommission[] memory result = new PaymentCommission[](
            normalizedToIndex - fromIndex + 1
        );
        for (uint256 index = fromIndex; index <= normalizedToIndex; index++) {
            result[index - fromIndex] = cardStoreStorage
                .getPaymentCommissionsByIndex(index);
        }
        return result;
    }

    function getPaymentCommissionsTotal() public view returns (uint256) {
        return cardStoreStorage.getPaymentCommissionsLength();
    }
}

