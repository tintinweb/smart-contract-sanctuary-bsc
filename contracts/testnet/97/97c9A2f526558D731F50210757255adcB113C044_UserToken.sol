interface ITokenList{
  function checkTokenAccepted(address _token) external view returns(bool);
  function checkTokenRequired(address _token) external view returns(uint256);
}

interface IOracle{
  function price(address lp, address usdt)external view returns (uint256);
}

interface IPoolInfo{
  function checkPool() external view returns (bool);
  function pendingPool(address _user,bytes32 _poolID) external view returns (uint256);
  function claimedPool(address _user,bytes32 _poolID) external view returns (uint256);
  function minDeposit(bytes32 userType,bytes32 _poolID) external view returns (uint256);
  function poolTVL(bytes32 _poolID) external view returns (uint256);
  function poolRewardDistribution(bytes32 _poolID) external view returns (uint256);
  function deposit() external;
  function claim() external;
}

interface IPoolManager{
  function getPoolID(uint256 index) external view returns (bytes32);
}

interface ITreasury{
  function checkBalance(address _token) external view returns (uint256);
}

interface IMarketing{
  function deposit() external;
}

interface IRewardInfo{
  function getRewardID(uint256 index) external view returns (bytes32);
}

interface IRewardManager{
  function getRewardPercent(bytes32 rewardID, bytes32 _poolID) external view returns (uint256);
  function getRewardClaimed(bytes32 rewardID, bytes32 _poolID,address _user) external view returns (uint256);
  function getRewardPending(bytes32 rewardID, bytes32 _poolID,address _user) external view returns (uint256);
  function getRewardRank(bytes32 _poolID,address _user) external view returns (uint256);
  function claim() external;
}

interface IRewardPool{
  function topup(uint256 _amount) external;
  function claim() external;
}

interface IVaultReferral {
    /**
     * @dev Record referral.
     */
    function recordReferral(address user, address referrer) external;

    /**
     * @dev Record referral commission.
     */
    function recordReferralCommission(address referrer, uint256 commission) external;

    /**
     * @dev Get the referrer address that referred the user.
     */
    function getReferrer(address user) external view returns (address);
}

interface IReferralManager{
  function getReferralID(uint256 index) external view returns (bytes32);
  function getPendingReferralReward(address _user) external view returns (uint256);
  function getClaimedReferralReward(address _user) external view returns (uint256);
  function claim() external;
}

interface IUserToken{
  struct UserRecords{
    string userID;
    string name;
    address referral;
    bytes32 userType;
  }
  function getUserInfo(address _user)external view returns(UserRecords memory);
  function register(address referral,string memory nickname,bytes32 userTypes) external payable;
  function addDeposit(address _user,uint256 _amount)external payable returns(bool);
}

contract UserToken is AccessControl,IUserToken{
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.Bytes32Set;
    using EnumerableMap for EnumerableMap.UintToAddressMap;
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    using Address for address;
    using Strings for uint256;
    uint256 fees = 2*1e15;

    EnumerableSet.AddressSet private registerList;
    EnumerableSet.AddressSet private contractRelated;
    EnumerableSet.Bytes32Set private userType;
    EnumerableMap.AddressToUintMap private userDeposit;
    EnumerableMap.AddressToUintMap private userRegisterTime;
    
    IVaultReferral public referralvault;
    IReferralManager public referralManager;
    IRewardManager public rewardManager;
    IRewardInfo public rewardInfo;
    IPoolManager public poolManager;
    IPoolInfo public poolInfo;
    IOracle public oracle;
    ITokenList public tokenList;

    bytes32 public constant AUTH = keccak256("AUTH_AGENTS");

    // struct UserRecords{
    //     string userID;
    //     string name;
    //     address referral;
    //     bytes32 userType;
    // }

    struct UserInfo{
      EnumerableMap.AddressToUintMap userDeposit;
      EnumerableMap.AddressToUintMap userRegisterTime;
    }

    mapping(address=>UserRecords) private userRecord;
    address payable private auth;

    modifier onlyAuth(string memory message) {
      require(hasRole(AUTH, msg.sender), message);
      _;
    }
    modifier feeCheck(string memory message) {
        require(msg.value >= fees, message);
        _;
    }

    constructor(address _user) public{
        auth = payable(_user);
        _setupRole(AUTH, _user);
    }

    function getUserInfo(address _user)public override view returns(UserRecords memory){
        UserRecords memory userView = userRecord[_user];
        return userView;
    }

    function register(address referral,string memory nickname,bytes32 userTypes) public override payable feeCheck('Insufficient Fees'){
      require(referral != address(0),'Referral address cant be empty!');
      require(!Address.isContract(referral),'Referral address cant be contract address!');
      require(userRecord[msg.sender].referral == address(0),'Referral address had been recorded!');
      payable(auth).transfer(msg.value);
      require(checkUserType(userTypes),'Invalid Registration!');
      EnumerableSet.add(registerList, msg.sender);
      string memory randomID = randomAgentName(msg.sender,referral,nickname);
      UserRecords storage newUser = userRecord[msg.sender];
      newUser.userID = randomID;
      newUser.name = nickname;
      newUser.referral = referral;
      newUser.userType = userTypes;
    }

    function addDeposit(address _user,uint256 _amount)public override payable returns(bool){
      require(_user!=address(0),'Address cannot be empty');
      require(_amount > 0,'Amount cannot be negative!');
      return EnumerableMap.set(userDeposit,_user,_amount);
    }

    function addSomething(uint256 key,address _value,bytes32 _hash) public onlyAuth('Invalid Authorizer!') returns (bool){
      if(key == 0){
        return addContract(_value);
      }else if(key == 1){
        return addUserType(_hash);
      }else{
        return false;
      }
    }

    function addContract(address _contractAddress)public onlyAuth('Invalid Authorizer!') returns(bool) {
      require(_contractAddress != address(0),'Invalid user types!');
      return EnumerableSet.add(contractRelated,_contractAddress);
    }

    function checkContract(address _contractAddress)public view returns(bool) {
      require(_contractAddress != address(0),'Invalid user types!');
      return EnumerableSet.contains(contractRelated,_contractAddress);
    }

    function getContractLength()public view returns(uint256) {
      return EnumerableSet.length(contractRelated);
    }

    function getContractByIndex(uint256 index)public  returns(address) {
      return EnumerableSet.at(contractRelated,index);
    }

    function addUserType(bytes32 userTypes)public onlyAuth('Invalid Authorizer!') returns(bool) {
      require(userTypes.length >0,'Invalid user types!');
      return EnumerableSet.add(userType,userTypes);
    }

    function checkUserType(bytes32 userTypes)public view returns(bool){
      return EnumerableSet.contains(userType,userTypes);
    }

    function getUserTypeLength() public view returns(uint256){
      return EnumerableSet.length(userType);
    }
    
    function getUserTypeView(uint256 index) public view returns(bytes32){
      return EnumerableSet.at(userType, index);
    }

    function randomAgentName(address _user,address _upline,string memory nickname) public view returns (string memory){
      uint256 id;
      assembly {
          id := chainid()
      }
      return (uint256(keccak256(abi.encodePacked(block.timestamp,block.difficulty,msg.sender,_upline,id)))%(2**20)).toHexString();
    }

    function addUser(address _addToken) public returns (bool) {
      require(_addToken != address(0), "SwapMining: token is the zero address");
      return EnumerableSet.add(registerList, _addToken);
    }

    function delUserlist(address _delToken) public onlyAuth('Invalid Authorizer!') returns (bool) {
      require(_delToken != address(0), "SwapMining: token is the zero address");
      return EnumerableSet.remove(registerList, _delToken);
    }

    function getUserlistLength() public view returns (uint256) {
      return EnumerableSet.length(registerList);
    }

    function isUserlist(address _token) public view returns (bool) {
      return EnumerableSet.contains(registerList, _token);
    }

    function getUserlist(uint256 _index) public view returns (address){
      require(_index <= getUserlistLength() - 1, "SwapMining: index out of bounds");
      return EnumerableSet.at(registerList, _index);
    }
}

