    ) external onlyOwner {
		require(gameDevFee + marketingFee + investorFee + rewardFee + teamFee <= 100, "BuyFee exceed 100");	
		emit FeeAllocationUpdated(feeAllocation.gameDevFee, feeAllocation.marketingFee, feeAllocation.investorFee, feeAllocation.rewardFee, feeAllocation.teamFee, gameDevFee, marketingFee, investorFee, rewardFee, teamFee);	
