contract Bot is Ownable {
    using SafeMath for uint256;
    using Address for address;

    mapping(address => uint256) public buyerAmount;

    uint256 public dayPrice = 1 * (10**16);
    uint256 public weekPrice = 1 * (10**17);
    uint256 public monthPrice = 1 * (10**18);
    uint256 public idoSelledAmount;

    function buyBot(address recipient) public payable {
        require(msg.value > 0, "invalid amount");

        if (msg.value == dayPrice) {
            buyerAmount[recipient] = block.timestamp.add(24 * 60 * 60);
        } else if (msg.value == weekPrice) {
            buyerAmount[recipient] = block.timestamp.add(24 * 60 * 60 * 7);
        } else if (msg.value == monthPrice) {
            buyerAmount[recipient] = block.timestamp.add(24 * 60 * 60 * 30);
        } else {
            revert("wrong amount");
        }
    }

    function checkUsefull(address recipient)
        public
        view
        returns (bool, uint256)
    {
        uint256 time = buyerAmount[recipient];
        if (block.timestamp <= buyerAmount[recipient]) {
            return (true, time);
        } else {
            return (false, time);
        }
    }

    function checkPair(address tokenA, address tokenB)
        public
        view
        returns (address, uint256)
    {
        IUniswapV2Router _uniswapV2Router = IUniswapV2Router(
            0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
        );

        address pair = IUniswapV2Factory(_uniswapV2Router.factory()).getPair(
            tokenA,
            tokenB
        );

        return (pair, block.number);
    }

    function withdrawToken(address recipient, address token) public onlyOwner {
        IERC20(token).transfer(
            recipient,
            IERC20(token).balanceOf(address(this))
        );
    }

    function withdrawBNB(address recipient) public onlyOwner {
        payable(recipient).transfer(address(this).balance);
    }

    receive() external payable {}
}