interface interfaceTokoMarketplace {
    struct Order {
        string uniqId;
        uint256 nonce;
        address buyer;
        address seller;
        address contractNFT;
        uint256 tokenId;
        address contractToken;
        uint256 price;
        uint256 start;
        uint256 end;
    }

    function executeOrderMatch(uint8, bytes32, bytes32, Order memory) external payable;
    function increaseAllowance(address, uint256) external;
}

interface interfaceERC20Custom {
    function increaseAllowance(address, uint256) external;
}

contract TokoMarketplaceProxy is Context {
    address public addressTokoMarketplace;

    event TransactionProxy(address indexed who, address indexed paymentToken, uint256 indexed price, address sendTo);

    constructor(address _addressTokoMarketplace) {
        addressTokoMarketplace = _addressTokoMarketplace;
    }

    function onERC721Received(address, address, uint256, bytes memory) public virtual returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    function executeOrderProxy(uint8 _v, bytes32 _r, bytes32 _s, interfaceTokoMarketplace.Order memory _order, address _sendTo) external payable {
        require(_sendTo != address(0), "_sendTo not to be address null");
        address who = _msgSender();
        if (_order.contractToken != address(0)) {
            IERC20(_order.contractToken).transferFrom(who, address(this), _order.price);
            interfaceERC20Custom(_order.contractToken).increaseAllowance(addressTokoMarketplace, _order.price);
        }
        interfaceTokoMarketplace(addressTokoMarketplace).executeOrderMatch{ value: msg.value }(_v, _r, _s, _order);
        IERC721(_order.contractNFT).safeTransferFrom(address(this), _sendTo, _order.tokenId);
        emit TransactionProxy(who, _order.contractToken, _order.price, _sendTo);
    }
}