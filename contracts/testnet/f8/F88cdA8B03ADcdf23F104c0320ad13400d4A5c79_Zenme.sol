pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

import "./utilities/SignContract.sol";
import "./interfaces/ITefToken.sol";
import "./enums/Rank.sol";


/**
 * @dev {ERC721} token, including:
 *
 *  - ability for holders to burn (destroy) their tokens
 *  - a minter role that allows for token minting (creation)
 *  - a pauser role that allows to stop all token transfers
 *  - token ID and URI autogeneration
 *
 * This contract uses {AccessControl} to lock permissioned functions using the
 * different roles - head to its documentation for details.
 *
 * The account that deploys the contract will be granted the minter and pauser
 * roles, as well as the default admin role, which will let it grant both minter
 * and pauser roles to other accounts.
 */
contract Zenme is
    SignContract,
    Initializable,
    PausableUpgradeable,
    AccessControlEnumerableUpgradeable,
    ERC721HolderUpgradeable,
    UUPSUpgradeable
{
    using AddressUpgradeable for address;
    using StringsUpgradeable for uint256;
    using SafeMathUpgradeable for uint256;

    string public name;
    string public symbol;
    
    address public tefToken;
    address public znmToken;

    event OpenBox(address indexed from,address indexed nftAddress,uint256 indexed tokenId, string userId);
    event DepositZnm(address indexed from, uint256 amount, string userId);
    event DepositTef(address indexed from, uint256 amount, string userId);
    event WithdrawZnm(address indexed from, uint256 amount, uint256 nonce);
    event WithdrawTef(address indexed from, uint256 amount, uint256 nonce);

    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant WITHDRAW_ROLE = keccak256("WITHDRAW_ROLE");

    uint256 private _tokenIdTracker;
    uint256 private _blockingPeriod;

    mapping(address => uint256) internal nonceMaps;
    mapping(address => bool) internal boxMaps;



    

    /**
     * @dev Grants `DEFAULT_ADMIN_ROLE`, `MINTER_ROLE` and `PAUSER_ROLE` to the
     * account that deploys the contract.
     *
     * Token URIs will be autogenerated based on `baseURI` and their token IDs.
     * See {ERC721-tokenURI}.
     */
    function initialize(
        string memory _name,
        string memory _symbol
    ) public initializer {
        __Zenme_init();
        name = _name;
        symbol = _symbol;
    }

    /**
     * @dev Grants `DEFAULT_ADMIN_ROLE`, `MINTER_ROLE` and `PAUSER_ROLE` to the
     * account that deploys the contract.
     *
     * Token URIs will be autogenerated based on `baseURI` and their token IDs.
     * See {ERC721-tokenURI}.
     */
    function __Zenme_init(
    ) internal onlyInitializing {
        __Context_init_unchained();
        __ERC165_init_unchained();
        __AccessControl_init_unchained();
        __AccessControlEnumerable_init_unchained();
        __Pausable_init_unchained();
        __Zenme_init_unchained();
        __UUPSUpgradeable_init();
    }

    function __Zenme_init_unchained(
    ) internal onlyInitializing {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(WITHDRAW_ROLE, _msgSender());
        _setupRole(PAUSER_ROLE, _msgSender());
    }

    modifier adminGuard() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()),
            "Zenme: must have admin role"
        );
        _;
    }

    modifier withdrawGuard() {
        require(
            hasRole(WITHDRAW_ROLE, _msgSender()),
            "Zenme: must have withdraw role"
        );
        _;
    }

    modifier pauserGuard() {
        require(
            hasRole(PAUSER_ROLE, _msgSender()),
            "Zenme: must have pauser role"
        );
        _;   
    }

    function transferAdmin(address _to) public adminGuard {
        _setupRole(DEFAULT_ADMIN_ROLE, _to);
        _setupRole(WITHDRAW_ROLE, _to);
        _setupRole(PAUSER_ROLE, _to);
    }

    function addWithdrawer(address _admin) public {
        super.grantRole(WITHDRAW_ROLE, _admin);
    }

    /**
     * @dev Import NFT from other smartcontract.
     *
     * Requirements:
     *
     * - the nft address must be whitelisted.
     */
    function openBox(address boxAddress, uint256 _tokenId, string memory userId) external {
        require(boxMaps[boxAddress], "Wrong Box NFT");
        IERC721Upgradeable(boxAddress).safeTransferFrom(_msgSender(), address(this), _tokenId, "");
        emit OpenBox(_msgSender(), boxAddress, _tokenId, userId);
    }

    function getBlockingPeriod() external view adminGuard returns(uint256) {
        return _blockingPeriod;
    }

    function setBlockingPeriod(uint256 _seconds) external adminGuard {
        _blockingPeriod = _seconds;
    }

    //Set signer address
    function setSigner(address _signer) external adminGuard {
        signer = _signer;
    }

    //Set TEF address
    function setTefToken(address _tef) external adminGuard {
        tefToken = _tef;
    }
    //Set ZNM address
    function setZnmToken(address _znm) external adminGuard {
        znmToken = _znm;
    }

    //Set NFT box address Whitelist
    function setBoxWhitelist(address _nftAddress, bool _isWhitelist) external adminGuard {
        boxMaps[_nftAddress] = _isWhitelist;
    }
    /**
     * @dev Deposit TEFToken.
     *
     * Requirements:
     *
     * - the caller must have enough token.
     */
    function depositTef(uint256 amount, string memory userId) external {
        require(amount > 0, "TokenValueNE");
        IERC20Upgradeable(tefToken).transferFrom(
            msg.sender,
            address(this),
            amount
        );
        emit DepositTef(msg.sender, amount, userId);
    }

    function withdrawTefs(
        uint256[] calldata _tefAmounts,
        address[] calldata _toAddresses
    ) external withdrawGuard returns(bool){
        require(_tefAmounts.length == _toAddresses.length, "Invalid array");
        for (uint256 i = 0; i < _tefAmounts.length; i++) {
            withDrawTefToken(_tefAmounts[i], _toAddresses[i]);
        }

        return true;
    }

    function withDrawTefToken(uint256 _amount, address _to) internal{
        // check balance of TEF for zenme contract
        uint256 balance = IERC20Upgradeable(tefToken).balanceOf(address(this));
        // if withdraw amount > balance of
        if (_amount > balance) {
            uint256 mintAmount = _amount.sub(balance);
            if (balance > 0) {
                IERC20Upgradeable(tefToken).transfer(_to, balance);
            }
            ITefToken(tefToken).mint(_to, mintAmount);
        } else {
            IERC20Upgradeable(tefToken).transfer(_to, _amount);
        }
    }
    /**
     * @dev Withdraw TEFToken.
     *
     * Requirements:
     *
     * - the caller must have enough token.
     */
    function withdrawTef(
        uint256 _tefAmount,
        string memory _message,
        bytes memory _signature
    ) external {
        require(_tefAmount > 0, "TokenValueNE");
        uint256 nonce = nonceMaps[msg.sender];
        bytes32 messageHash = getWithdrawTefHash(
            msg.sender,
            _tefAmount,
            _message,
            nonce
        );
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);
        require(
            recoverSigner(ethSignedMessageHash, _signature) == signer,
            "Invalid signature"
        );
        //Update user nonce
        nonceMaps[msg.sender] += 1;
        // check balance of TEF for zenme contract
        uint256 balance = IERC20Upgradeable(tefToken).balanceOf(address(this));
        // if withdraw amount > balance of
        if (_tefAmount > balance) {
            uint256 mintAmount = _tefAmount.sub(balance);
            if (balance > 0) {
                IERC20Upgradeable(tefToken).transfer(msg.sender, balance);
            }
            ITefToken(tefToken).mint(msg.sender, mintAmount);
        } else {
            IERC20Upgradeable(tefToken).transfer(msg.sender, _tefAmount);
        }

        emit WithdrawTef(msg.sender, _tefAmount, nonce);
    }

    // function withdrawToken(
    //     address _token,
    //     address receiver,
    //     uint256 amount
    // ) external adminGuard {
    //     // it will throw if transfer fails
    //     IERC20Upgradeable(_token).transfer(receiver, amount);
    // }

    function totalTefBalance() public view adminGuard returns (uint256) {
        return IERC20Upgradeable(tefToken).balanceOf(address(this));
    }

    /**
     * @dev Pauses all token transfers.
     *
     * See {ERC721Pausable} and {Pausable-_pause}.
     *
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function pause() public virtual pauserGuard {
        _pause();
    }

    /**
     * @dev Unpauses all token transfers.
     *
     * See {ERC721Pausable} and {Pausable-_unpause}.
     *
     * Requirements:
     *
     * - the caller must have the `PAUSER_ROLE`.
     */
    function unpause() public virtual pauserGuard {
        _unpause();
    }
    
    function _authorizeUpgrade(address newImplementation)
        internal
        override
        adminGuard
    {
        // Do something here.
    }
    
    uint256[45] private __gap;
}

