            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }

        // implicitly return (r, s, v)
    }

    function getWithdrawTefHash(
        address _to,
        uint256 _tefAmount,
        string memory _message,
        uint256 _nonce
    ) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(_to, _tefAmount, _message, _nonce));
    }

    function getWithdrawZenHash(
        address _to,
        uint256 _zenAmount,
        string memory _message,
        uint256 _nonce
    ) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(_to, _zenAmount, _message, _nonce));
