contract RigelLaunchpadNFTs is ERC1155, ERC2981 {
    address private immutable TOKENA;
    address private immutable TOKENB;
    address public tokenA;
    address public tokenB;

    
    string public name;
    string public contractURI;
    bool private status;
    uint96 private feeNumerator = 250;
  
    struct cardDetails {
        uint256 tokenPrice;
        uint256 nextAvailableID;
    }
  
    mapping (uint256 => uint256[]) private idsSoldFromClass;
    mapping (uint256 => cardDetails) private buyWithToken;
    mapping (uint256 => uint256) private stop;

    event Purchase(address indexed recipient, uint256 price, uint256 id);

    constructor(string memory __name, string memory _contractURI, string memory _base, address _tokenA, address _tokenB) ERC1155(_base) {       
        name = __name;
        TOKENA = _tokenA;
        TOKENB = _tokenB;
        tokenA = TOKENA;
        tokenB = TOKENB;
        setContractURI(_contractURI);
        status = true;
    }

    error INVALIDTOKEN();
    error ITEMSOLD();
    error OUTOFSTOCK();
    error INVALIDCLASS();
    error VERSIONENDED();

    function setStop(uint256[] calldata tokenClass, uint256[] calldata stopMintingAt) external onlyOwner {
        uint256 lent = tokenClass.length;
        require(lent == stopMintingAt.length, "Invalid Length");

        for (uint256 i; i < lent;) {
            stop[tokenClass[i]] = stopMintingAt[i];
            unchecked {
                i++;
            }
        }
    }
  
    function setContractURI(string memory newURI) public onlyOwner {
        contractURI = newURI;
    }

    function updateCardDetails( uint256[] calldata tokenClass, uint256[] calldata _tokenPrice, uint256[] calldata startID) external onlyOwner {
        uint256 tokenLenght = tokenClass.length;
        require(tokenLenght == _tokenPrice.length && _tokenPrice.length == startID.length, "Invalid length of Array.");

        for (uint256 i; i < tokenLenght; )  {
            buyWithToken[tokenClass[i]].tokenPrice = _tokenPrice[i];
            buyWithToken[tokenClass[i]].nextAvailableID = startID[i];
            unchecked {
                i++;
            }
        }
    }

    function setGeneralRoyaltyFee(uint96 royaltyFee) public onlyOwner {
        feeNumerator = royaltyFee;
    }
    

    function mintBatch( address _to, uint256[] calldata _tokenId, uint256[] calldata _amount)  external onlyOwner returns (bool) {      
        _mintBatch(_to, _tokenId, _amount, " "); 
        _setTokenRoyalty( _tokenId, owner(), 250);
        return true;
    }


    function burnBatch(address _to, uint256[] memory _id, uint256[] memory _amount) external onlyOwner {
        _burnBatch(_to, _id, _amount);
    }

    function salesStatus(bool _status) external onlyOwner {
        status = _status;
    }

    function mint(uint256 tokenClass, address tokenAddress) external {
        if(!status) revert OUTOFSTOCK();

        if(buyWithToken[tokenClass].tokenPrice == 0) revert INVALIDCLASS();

        require(tokenAddress == TOKENA || tokenAddress == TOKENB,"INVALIDTOKEN!");
        
        address token = tokenAddress != TOKENA ? tokenAddress : TOKENB;

        IERC20(token).transferFrom(_msgSender(), address(this), buyWithToken[tokenClass].tokenPrice);

        _trade(tokenClass);

    }

    function _trade(uint256 tokenClass) internal {

        uint256 _IAmMintingID = buyWithToken[tokenClass].nextAvailableID;
        
        if(_IAmMintingID > stop[tokenClass]) revert VERSIONENDED();
        
        buyWithToken[tokenClass].nextAvailableID++;

        idsSoldFromClass[tokenClass].push(_IAmMintingID);

        _setRoyaltyForID(_IAmMintingID, owner(), 250);

        _mint(msg.sender, _IAmMintingID, 1, "");
        
        emit Purchase(_msgSender(), 1, _IAmMintingID);
    }

    function nftsPurchaseData(uint256 tokenClass) external view  returns (cardDetails memory card) {
        card =  buyWithToken[tokenClass];
    }

    function WithdrawERC20(address _token, address recipient, uint256 _amt) external onlyOwner {
        IERC20(_token).transfer(recipient, _amt);
    }

    function checkStatus() external view returns(bool) {
        return status;
    }

    function stopMintingSalesAt(uint256 tokenClass) external view returns(uint256) {
        return stop[tokenClass];
    }

    function getAllNFTsSoldFromClass(uint256 _class) external view returns(uint256[] memory _ids, uint256 all) {
        all = idsSoldFromClass[_class].length;
        _ids = new uint256[](all);
        for (uint256 j; j < all; ) {
            _ids[j] = idsSoldFromClass[_class][j];
            unchecked {
                j++;
            }
        }
    }

}