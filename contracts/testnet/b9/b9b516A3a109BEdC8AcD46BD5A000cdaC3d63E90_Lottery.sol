pragma solidity 0.8.15;

import "@chainlink/contracts/src/v0.8/AutomationCompatible.sol";
import "@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "hardhat/console.sol";
import "contracts/Banker.sol";

/**
 * @title Lottery contract
 * NOTE: Contract use Chainlink Oracle for generating random words
 * Needs to fund subscription and add contract address after deploying as a consumer
 * on https://vrf.chain.link in order to work with VRFv2
 * @dev Lottery ticket is ERC721 token standard and could be bought with AWARD ERC-20 tokens
 */
contract Lottery is VRFConsumerBaseV2, Ownable {

    enum ROUND_STATUS {
        STARTED,
        ENDED,
        PICKING_WINNER
    }

    uint256 private _currentBetId;
    uint256 private _currentRoundId;
    uint256 private _currentTicketNumber;
    ROUND_STATUS private _currentRoundStatus;
    uint256 private _currentRoundJackpot;
    uint256[] private _currentRoundTickets;
    address[] private _currentRoundPlayers;

    ERC20 private _token;
    address private _tokenAddress;
    uint256 private _ticketPrice;
    uint256 private _rtp;
    string private _cronExpression;

    Banker private _banker;
    address private _bankerAddress;

    uint16 private constant _REQUEST_CONFIRMATIONS = 3;
    uint32 private constant _CALLBACK_GAS_LIMIT = 140000;
    uint32 private constant _NUM_WORDS = 1;

    VRFCoordinatorV2Interface private _coordinator;
    uint64 private _subscriptionId;
    address private _vrfCoordinator;
    bytes32 private _keyHash;
    uint256 public requestId;

    // Mapping from ticket number to player.
    mapping(uint256 => address) private _playerTickets;

    // Mapping from ticket number to bet
    mapping(uint256 => uint256) private _ticketBets;

    // Mapping from round number to round winner.
    mapping(uint256 => address) private _roundWinners;

    /**
     * @dev Emitted when new request to VRF coordinator is sent
     * @param requestId id of generate randomness request
     */
    event RequestedRandomness(uint256 requestId);

    /**
     * @dev Emitted when VRF coordinator returns a new random word
     * @param requestId id of generate randomness request
     * @param number random number returned from VRF coordinator contract
     */
    event ReceivedRandomness(uint256 requestId, uint256 number);

    /**
     * @dev Emitted when new round started
     * @param roundId the id of the new round
     */
    event RoundStarted(uint256 roundId);

    /**
     * @dev Emitted when round ended
     * @param roundId id
     * @param winner of this round
     */
    event RoundEnded(uint roundId, address winner, uint winningBetId, uint winningTicket, uint winningAmount, address token);

    /**
    * @dev Emitted when a new bet is placed
     */
    event BetPlaced(uint betId, uint roundId, address indexed player, uint[] tickets, uint256 betAmount, address token);

    /**
     * @dev Emitted when new request to VRF coordinator is sent and we are waiting for game results
     * @param roundId id of the round that waits for the game results
     */
    event PickingWinnerStartedEvent(uint256 roundId);

    /**
     * @dev Emitted when new RTP is set
     * @param newRtp new RTP value
     */
    event RtpChanged(uint256 newRtp);

    /**
     * @dev Emitted when a new subscriptionId setted
     */
    event SubscriptionChanged(uint64 subscriptionId);

    /**
     * @dev Emitted when a new participation fee is established
     */
    event TicketPriceChanged(uint256 newPrice);

    /**
    * @dev Emitted when payment token address changed
     */
    event TokenChanged(address newToken);

    /**
     * @dev Emitted when new schedule for this lottery is [emailprotected]
     * It's not used in the smart-contracts, but is used by back-end to determine when to  trigger the round
     *
     * @param newCron new cron expression
     */
    event CronExpressionChanged(string newCron);

    /**
     * @dev emitted when banker address is changed
     *
     * @param newBankerAddress new banker address
     */
    event BankerAddressChanged(address newBankerAddress);

    /**
     * @dev vrfCoordinator_ and keyHash_ can be obtained
     * from here: https://docs.chain.link/docs/vrf-contracts/
     *
     * Requirements:
     *
     * - `subscriptionId_` cannot be zero.
     * - `keyHash_` cannot be address zero.
     * - `vrfCoordinator_` cannot be address zero.
     * - `token_` cannot be address zero.
     * - `rtp_` should be between 0 and 100
     *
     * @param subscriptionId_  can be obtained from here: https://vrf.chain.link
     * @param vrfCoordinator_  VRF coordinator contract address
     * @param keyHash_ The gas lane key hash value, which is the maximum
     * gas price you are willing to pay for a request in wei
     * @param token_ 'AWARD' tokem contract address
     * @param rtp_ RTP rate
     * @param ticketPrice_ price in 'AWARD' tokens for one ticket in order to participate
     */
    constructor(
        uint64 subscriptionId_,
        address vrfCoordinator_,
        bytes32 keyHash_,
        address token_,
        address bankerAddress_,
        uint256 rtp_,
        uint256 ticketPrice_,
        string memory cronExpression_
    ) VRFConsumerBaseV2(vrfCoordinator_) {
        require(subscriptionId_ != 0, "Wrong subscriptionId");
        require(vrfCoordinator_ != address(0), "Wrong vrfCoordinator address");
        require(keyHash_ != 0, "Wrong keyHash");
        require(token_ != address(0), "Wrong token address");
        require(bankerAddress_ != address(0), "Wrong banker address");
        require(rtp_ <= 100, "RTP can't be higher than 100%");
        require(bytes(cronExpression_).length != 0, "Cron expression must not be null or empty");
        _coordinator = VRFCoordinatorV2Interface(vrfCoordinator_);
        _subscriptionId = subscriptionId_;
        _vrfCoordinator = vrfCoordinator_;
        _keyHash = keyHash_;
        _token = ERC20(token_);
        _tokenAddress = token_;
        _banker = Banker(bankerAddress_);
        _bankerAddress = bankerAddress_;
        _currentRoundStatus = ROUND_STATUS.ENDED;
        _rtp = rtp_;
        _ticketPrice = ticketPrice_ * 10 ** _token.decimals();
        _currentTicketNumber = 100_000_000;
        _cronExpression = cronExpression_;

        emit RtpChanged(_rtp);
        emit TokenChanged(_tokenAddress);
        emit TicketPriceChanged(_ticketPrice);
        emit BankerAddressChanged(_bankerAddress);
        emit CronExpressionChanged(_cronExpression);
    }

    /**
     * @notice Start new round and allow players to buy tickets
     * Only owner could call this function
     * @dev The status of previous round is reset
     *
     * Requirements:
     *
     * - `_currentRoundStatus` must be in `ENDED` status.
     *
     * Emits a {RoundStarted} event.
     */
    function startRound() external onlyOwner {
        _startRound();
    }

    function _startRound() private {
        require(_currentRoundStatus == ROUND_STATUS.ENDED, "Can't start a new round");
        _currentRoundId++;
        _currentRoundJackpot = 0;
        _currentRoundStatus = ROUND_STATUS.STARTED;
        _currentRoundTickets = new uint256[](0);
        _currentRoundPlayers = new address[](0);
        emit RoundStarted(_currentRoundId);
    }

    /**
     * @notice ticket price is based on 'AWARD' ERC20 standard token
     * @dev Each ticket is ERC721 token
     *
     * Requirements:
     *
     * - It's required to approve the total tickets amount of tokens for this contract
     * in order to participate.
     * - `_currentRoundStatus` must be in `STARTED` status.
     *
     * Emits a {BetPlaced} event.
     */
    function placeBet(uint64 ticketsAmount_) external {
        require(ticketsAmount_ > 0, "Ticket amount should be at least 1");
        require(_currentRoundStatus == ROUND_STATUS.STARTED, "Wait until the next round");

        uint256 totalTicketsPrice = ticketsAmount_ * _ticketPrice;

        _banker.debitPlayer(msg.sender, _tokenAddress, totalTicketsPrice);

        _currentBetId++;

        uint256[] memory playerTickets = new uint256[](ticketsAmount_);

        for (uint i = 0; i < ticketsAmount_; i++) {
            _currentTicketNumber++;
            _playerTickets[_currentTicketNumber] = msg.sender;
            _currentRoundJackpot += _ticketPrice;
            _currentRoundTickets.push(_currentTicketNumber);

            playerTickets[i] = _currentTicketNumber;

            _ticketBets[_currentTicketNumber] = _currentBetId;
        }

        _currentRoundPlayers.push(msg.sender);

        emit BetPlaced(_currentBetId, _currentRoundId, msg.sender, playerTickets, totalTicketsPrice, _tokenAddress);
    }

    /**
     * @notice End round function, which calculates the winner and pay the prize
     * Only owner could call this function
     * @dev endRound() calls _pickWinner(), which in turn calls the
     * requestRandomWords function from VRFv2
     *
     * Requirements:
     *
     * - At least one player is required to allow owner call this function.
     * - `_currentRoundStatus` must be in `STARTED` status.
     * - `_currentRoundJackpot` must be equal to or greater than 100 tokens.
     *
     * Emits a {RequestedRandomness} event.
     * Emits a {PickingWinnerStartedEvent} event.
     * Emits a {RoundEnded} event.
     */
    function endRound() external onlyOwner {
        require(_currentRoundStatus == ROUND_STATUS.STARTED, "Can't end round yet");

        if (_currentRoundTickets.length > 0) {
            _currentRoundStatus = ROUND_STATUS.PICKING_WINNER;
            requestId = _coordinator.requestRandomWords(
                _keyHash,
                _subscriptionId,
                _REQUEST_CONFIRMATIONS,
                _CALLBACK_GAS_LIMIT,
                _NUM_WORDS
            );
            emit RequestedRandomness(requestId);
            emit PickingWinnerStartedEvent(_currentRoundId);
        } else {
            emit RoundEnded(_currentRoundId, address(0), uint(0), uint(0), uint(0), _tokenAddress);

            _currentRoundStatus = ROUND_STATUS.ENDED;
            _startRound();
        }
    }

    /**
     * @notice Get random number, pick winner and sent prize to winner
     * @dev Function can be fulfilled only from _vrfcoordinator
     * @param reqId_ requestId for generating random number
     * @param random_ received number from VRFv2
     *
     * Requirements:
     *
     * - The first random word must be higher than zero.
     *
     * Emits a {ReceivedRandomness} event.
     * Emits a {RoundEnded} event.
     */
    function fulfillRandomWords(
        uint256 reqId_, /* requestId */
        uint256[] memory random_
    ) internal override {

        require(random_[0] > 0, "Random number not found");
        emit ReceivedRandomness(reqId_, random_[0]);

        if (_currentRoundTickets.length > 0) {
            uint256 winnerTicketIndex = (random_[0] % _currentRoundTickets.length);
            uint256 winnerTicket = _currentRoundTickets[winnerTicketIndex];
            address winnerAddress = _playerTickets[winnerTicket];
            uint256 winnerAmount = (_currentRoundJackpot * _rtp) / 100;
            uint256 winnerBetId = _ticketBets[winnerTicket];

            _roundWinners[_currentRoundId] = winnerAddress;

            _banker.creditPlayer(winnerAddress, _tokenAddress, winnerAmount);

            emit RoundEnded(_currentRoundId, winnerAddress, winnerBetId, winnerTicket, winnerAmount, _tokenAddress);
        } else {
            emit RoundEnded(_currentRoundId, address(0), uint(0), uint(0), uint(0), _tokenAddress);
        }

        _currentRoundStatus = ROUND_STATUS.ENDED;
        _startRound();
    }

    /**
     * @notice Update the return to the player
     *
     * Requirements:
     *
     * - Must be between 0 and 100
     *
     * @param rtp_ the RTP param
     *
     * Emits a {RtpChanged} event.
     */
    function changeRtp(uint256 rtp_) external onlyOwner {
        require(rtp_ <= 100, "RTP can't be higher than 100%");
        _rtp = rtp_;
        emit RtpChanged(_rtp);
    }

    /**
     * @notice Update _subscriptionId
     * @param newSubscriptionId_ new _subscriptionId
     *
     * Emits a {SubscriptionChanged} event.
     */
    function changeSubscriptionId(uint64 newSubscriptionId_) external onlyOwner {
        _subscriptionId = newSubscriptionId_;
        emit SubscriptionChanged(_subscriptionId);
    }

    /**
     * @notice Update Participation Fee
     * @param newTicketPrice_ new price of ticket in 'AWARD' tokens
     *
     * Emits a {TicketPriceUpdated} event.
     */
    function changeTicketPrice(uint64 newTicketPrice_) external onlyOwner {
        _ticketPrice = newTicketPrice_ * 10 ** _token.decimals();
        emit TicketPriceChanged(_ticketPrice);
    }

    /**
     * @notice Change Payment Token
     * @param newToken_ address of the new payment token
     *
     * Emits a {TokenChanged} event.
     */
    function changeToken(address newToken_) external onlyOwner {
        _token = ERC20(newToken_);
        _tokenAddress = newToken_;
        emit TokenChanged(newToken_);
    }

    /**
     * @notice Update the schedule cron expression for this lottery
     *
     * Emits a {CronExpressionChanged} event.
     */
    function changeCronExpression(string memory cronExpression_) external onlyOwner {
        _cronExpression = cronExpression_;

        emit CronExpressionChanged(_cronExpression);
    }

    /**
     * @notice Change the banker address for this lottery. Don't forget to approve all banker funds for this contract usage!
     *
     * Emits a {BankerAddressChanged} event.
     */
    function changeBankerAddress(address bankerAddress_) external onlyOwner {
        _bankerAddress = bankerAddress_;

        emit BankerAddressChanged(_bankerAddress);
    }

    /**
     * @dev Get address of the banker
     */
    function getBankerAddress() external view returns (address) {
        return _bankerAddress;
    }

    /**
     * @dev Returns the RTP
     */
    function getRtp() external view returns (uint256) {
        return _rtp;
    }

    /**
     * @dev Returns lottery balance of current lottery
     */
    function getCurrentRoundJackpot() external view returns (uint256) {
        return _currentRoundJackpot * _rtp / 100;
    }

    /**
     * @dev Returns subscription Id
     */
    function getSubscriptionId() external view returns (uint64) {
        return _subscriptionId;
    }

    /**
     * @dev Returns current ticket price
     */
    function getTicketPrice() external view returns (uint256) {
        return _ticketPrice;
    }

    /**
     * @dev Returns number of tickets from current lottery
     */
    function getCurrentTicketNumber() external view returns (uint256) {
        return _currentTicketNumber;
    }

    /**
     * @dev Returns current lottery status
     */
    function getCurrentRoundStatus() external view returns (ROUND_STATUS) {
        return _currentRoundStatus;
    }

    /**
     * @dev Returns current lottery ID
     */
    function getCurrentRoundId() external view returns (uint256) {
        return _currentRoundId;
    }

    /**
     * @dev Returns current lottery ID
     */
    function getCurrentRoundTickets() external view returns (uint[] memory) {
        return _currentRoundTickets;
    }

    /**
     * @dev Returns payment token address
     */
    function getToken() external view returns (address) {
        return _tokenAddress;
    }

    /**
     * @dev Returns current cron expression for the game
     */
    function getCronExpression() public view returns (string memory) {
        return _cronExpression;
    }

    /**
     * @dev Returns address of the ticket owner
     * @param ticketNumber_ number of ticket in actual lottery
     */
    function getTicketOwner(uint256 ticketNumber_) external view returns (address) {
        return _playerTickets[ticketNumber_];
    }

    /**
     * @dev Returns the winner's address for a specific lottery
     * @param roundId_ number of lottery
     */
    function getRoundWinner(uint256 roundId_) external view returns (address) {
        return _roundWinners[roundId_];
    }

    /**
     * @dev Returns current round players
     */
    function getCurrentRoundPlayers() public view returns (address[] memory) {
        return _currentRoundPlayers;
    }
}

