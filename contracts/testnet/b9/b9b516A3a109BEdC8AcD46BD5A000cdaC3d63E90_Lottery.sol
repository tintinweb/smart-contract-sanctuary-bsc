pragma solidity 0.8.15;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract Banker is AccessControlEnumerable, Ownable {
    bytes32 public constant GAME_ROLE = keccak256("GAME_ROLE");

    address[] private _supportedTokensList;
    mapping(address => bool) private _supportedTokensMap;

    /**
     * @dev Emitted when new supported token added
     * @param token token address
     */
    event TokenAdded(address token);

    /**
     * @dev Emitted when existing supported token is removed
     * @param token token address
     */
    event TokenRemoved(address token);

    /**
     * @dev Emitted when new game contract added
     * @param gameContractAddress address of the contract
     */
    event GameContractAdded(address gameContractAddress);

    /**
     * @dev Emitted when the game contract removed
     * @param gameContractAddress address of the contract
     */
    event GameContractRemoved(address gameContractAddress);

    constructor(address[] memory gameContracts_, address[] memory supportedTokens_) {

        for (uint256 i = 0; i < gameContracts_.length; i++) {
            _setupRole(GAME_ROLE, gameContracts_[i]);
            emit GameContractAdded(gameContracts_[i]);
        }

        for (uint256 i = 0; i < supportedTokens_.length; i++) {
            address token = supportedTokens_[i];
            _supportedTokensList.push(token);
            _supportedTokensMap[token] = true;
            emit TokenAdded(token);
        }
    }

    function debitPlayer(address player_, address token_, uint amount_) public onlyRole(GAME_ROLE) {
        bool success = ERC20(token_).transferFrom(player_, address(this), amount_);
        require(success, "Could not debit player balance. Please check approved allowance amount.");
    }

    function creditPlayer(address player_, address token_, uint256 amount_) public onlyRole(GAME_ROLE) {
        bool success = ERC20(token_).transfer(player_, amount_);
        require(success, "Could not credit player balance. Please check the banker balance");
    }

    function addSupportedToken(address token_) public onlyOwner {
        bool isAlreadyAdded = _supportedTokensMap[token_];

        if (!isAlreadyAdded) {
            _supportedTokensMap[token_] = true;
            _supportedTokensList.push(token_);
            emit TokenAdded(token_);
        }
    }

    function removeSupportedToken(address token_) public onlyOwner {
        bool isPresent = _supportedTokensMap[token_];

        if (isPresent) {
            _supportedTokensMap[token_] = false;

            for (uint256 i = 0; i < _supportedTokensList.length; i++) {
                if (_supportedTokensList[i] == token_) {
                    _supportedTokensList[i] = address(0);
                }
            }

            emit TokenRemoved(token_);
        }

    }

    function withdrawTokens(address token_, uint256 amount_, address receiver_) public onlyOwner {
        ERC20(token_).transfer(receiver_, amount_);
    }

    function addGameContract(address gameContract_) public onlyOwner {
        _grantRole(GAME_ROLE, gameContract_);

        emit GameContractAdded(gameContract_);
    }

    function removeGameContract(address gameContract_) public onlyOwner {
        _revokeRole(GAME_ROLE, gameContract_);

        emit GameContractRemoved(gameContract_);
    }

    function getSupportedTokens() public view returns (address[] memory) {
        return _supportedTokensList;
    }

    function getSupportedGameContracts() public view returns (address[] memory) {
        uint256 amountOfGames = getRoleMemberCount(GAME_ROLE);
        address[] memory gameContractAddresses = new address[](amountOfGames);
        for (uint256 i = 0; i < amountOfGames; i++) {
            gameContractAddresses[i] = getRoleMember(GAME_ROLE, i);
        }
        return gameContractAddresses;
    }

}

