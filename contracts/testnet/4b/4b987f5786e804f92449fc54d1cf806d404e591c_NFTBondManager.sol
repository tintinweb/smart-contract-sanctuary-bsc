import "./ManagerParts/BondTreasury.sol";
import "./ManagerParts/BondAssetsManager.sol";
import "../libs/interfaces/INFTBond.sol";
import "../libs/interfaces/INFTBondManager.sol";

contract NFTBondManager is
    BondTreasury,
    BondAssetsManager,
    Initializable,
    OwnableUpgradeable,
    INFTBondManager
{
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap;

////////////////////////////////////////// events

    event CreateNftInvesting(address indexed from, address indexed to, uint256 indexed tokenId);
    event CurrencyWithdraw(address indexed owner, address indexed approved, uint256 indexed tokenId);
    event InvestingToObligation(address indexed owner, address indexed operator, bool approved);

////////////////////////////////////////// initialize

    function initialize(
        address NFTBond,
        address rbis,
        address uniswapV2Router
    ) public initializer {
        _NFTBond = NFTBond;
        __Unlocked_init();
        __Ownable_init();
        __Treasury_init(
            rbis,
            uniswapV2Router
        );
        __BondAssetsManager_init(
            rbis,
            uniswapV2Router
        );
        __AddAsset(_WETH, 100000e18); //ETHEREUM
    }

////////////////////////////////////////// fields definition

    address public override _NFTBond;

    /** see https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps */
    uint256[49] private __gap;

////////////////////////////////////////// modifiers

    modifier onlyNftOwner(uint256 heroId) {
        require(
            INFTBond(_NFTBond).ownerOf(heroId) == msg.sender,
            "NFTBondManager::onlyNftOwner: only nft owner allowed"
        );
        _;
    }

////////////////////////////////////////// owner methods for edit assets

    function addAsset(
        address currency,
        uint256 cap
    )
        public
        onlyOwner
    {
        __AddAsset(currency, cap);
    }

    function setAssetCap(
        address currency,
        uint256 cap

    ) public onlyOwner {
        __EditAssetCap(currency, cap);
    }

    function removeAsset(
        address currency
    )
        public
        onlyOwner
    {
        __RemoveAsset(currency);
    }

////////////////////////////////////////// methods view

    function currencyBalance(uint256 tokenId, address asset_address)
        public view
        assetIsAllowed(asset_address)
        returns (uint256 balance, uint256 balanceLp)
    {
        (balance,  balanceLp) = __getAssetBalance(asset_address, tokenId);
    }

    function currencyDepositList(uint256 tokenId, address asset_address)
        public view
        assetIsAllowed(asset_address)
        returns (DepositData[] memory deposits)
    {
        return _getCurrencyDepositList(asset_address, tokenId);
    }


    function currencyBalancesList(uint256 tokenId)
        public view
        returns (address[] memory currencyList, uint256[] memory balancesList)
    {
        uint256 count = 0;

        uint256 countAssets = asset_pools.length();
        for (uint256 i = 0; i < countAssets; ++i) {
            (address asset_address,) = asset_pools.at(i);
            (uint256 balance,) = __getAssetBalance(asset_address, tokenId);
            if (balance > 0 ){
                count++;
            }
        }

        currencyList = new address[](count);
        balancesList = new uint256[](count);

        uint256 j = 0 ;
        for (uint256 i = 0; i < countAssets; i++) {
            (address asset_address,) = asset_pools.at(i);
            (uint256 balance,) = __getAssetBalance(asset_address, tokenId);

            if (balance > 0){
                currencyList[j] = asset_address;
                balancesList[j] = balance;
                j++;
            }
        }

        return(currencyList, balancesList);
    }


////////////////////////////////////////// events

    // todo таки нужны event-ы, без них web3 из готовой транзы взять ничего не сможет,
    event MintNFTBond(
        address tokenOwner,
        uint256 tokenId
    );

////////////////////////////////////////// methods for Investing


    function createNftInvesting(address[] memory currencies, uint256[] memory amounts)
        public
        payable
        returns (uint256)
    {
        uint256 tokenId = INFTBond(_NFTBond).mint(msg.sender);
        
        require(currencies.length == amounts.length, "");
        
        for (uint256 i = 0; i < currencies.length; i++) {
            if (currencies[i] == _WETH) {
                 require(msg.value >= amounts[i]);
            }
            
            _investingToObligation(tokenId, currencies[i], amounts[i]);
        }

        emit MintNFTBond(msg.sender, tokenId);
        return tokenId;
    }


    function currenciesInvesting(uint256 tokenId, address[] memory currencies, uint256[] memory amounts)
        public
        payable
        onlyNftOwner(tokenId)
        returns (bool)
    {

        require(currencies.length == amounts.length, "");
        
        for (uint256 i = 0; i < currencies.length; i++) {
            if (currencies[i] == _WETH) {
                 require(msg.value >= amounts[i]);
            }

            _investingToObligation(tokenId, currencies[i], amounts[i]);
        }

        return true;
    }

    function _investingToObligation(uint256 tokenId, address currency, uint256 amount)
        internal
        assetIsAllowToDeposit(currency, amount)
    {
        _takeAssetFromSender(
            currency,
            tokenId,
            amount
        );

        INFTBond(_NFTBond).add(tokenId, currency, amount);
    }

////////////////////////////////////////// methods for Withdraw

    function currenciesWithdraw(uint256 tokenId, address[] memory currencies, uint256[] memory amounts)
        public
        onlyNftOwner(tokenId)
        returns (bool)
    {
        for (uint256 i = 0; i < currencies.length; i++) {
            _currencyWithdraw(tokenId, currencies[i], amounts[i]);
        }

        return true;
    }

    function currenciesWithdrawAll(uint256 tokenId)
        public
        onlyNftOwner(tokenId)
        returns (bool)
    {
        uint256 countAssets = asset_pools.length();
        for (uint256 i = 0; i < countAssets; ++i) {
            (address currency,) = asset_pools.at(i);
            (uint256 balance,) = __getAssetBalance(currency, tokenId);
            if (balance > 0 ){
                _currencyWithdraw(tokenId, currency, balance);
            }
        }

        return true;
    }

    function _currencyWithdraw(uint256 tokenId, address currency, uint256 amount)
        internal
        assetIsAllowed(currency)
    {
        uint256 asset_balance = INFTBond(_NFTBond).get(tokenId, currency);
        if (asset_balance > 0) {
            _withdrawAssetByObligationHolder(currency, tokenId, amount);
            INFTBond(_NFTBond).set(tokenId, currency, asset_balance > amount ?  asset_balance - amount : 0);
        }
    }


    function getAssestsAvalibleToBond(uint256 tokenId) public view returns(address[] memory alowed_assets){
        (address[] memory asset_address, ) = getAssetWithCaps();


        uint256 count = 0;
        for (uint256 i = 0; i < asset_address.length; i++) {
            DepositData[] memory deposits = _getCurrencyDepositList(asset_address[i], tokenId);

            if(deposits.length > 0){
                count ++;
            } 
        }

        alowed_assets = new address[](count);

        uint256 j = 0;
        for (uint256 i = 0; i < asset_address.length; i++) {
            DepositData[] memory deposits = _getCurrencyDepositList(asset_address[i], tokenId);
            if(deposits.length > 0){
                alowed_assets[j] = asset_address[i];
                j ++;
            } 
        }

    }


    function getTotalAvalibleToBond(uint256 tokenId, address currency) public view returns(uint256 total, uint256 frezed, uint256 frized_redem_percent ){
        DepositData[] memory deposits = _getCurrencyDepositList(currency, tokenId);

         for (uint256 i = 0; i < deposits.length; i++) {
                total += deposits[i].amount;
                if ((deposits[i].time + frized_days) > block.timestamp){
                       frezed += deposits[i].amount;
                       frized_redem_percent += deposits[i].amount - (deposits[i].amount / 100) * percent; 
                       
                }
         }
    }

}

