pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableMapUpgradeable.sol";
import "../utils/structs/AddressToUintMapAdditionalMethods.sol";

abstract contract AbstractAssetsManager
{
    using EnumerableMapUpgradeable for EnumerableMapUpgradeable.AddressToUintMap;
    using AddressToUintMapAdditionalMethods for EnumerableMapUpgradeable.AddressToUintMap;

////////////////////////////////////////// fields definition

    // asset address => asset cap
    EnumerableMapUpgradeable.AddressToUintMap internal asset_pools;

    /** see https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps */
    uint256[49] private __gap;

////////////////////////////////////////// modifiers

    modifier assetIsAllowed(address _asset) {
        __AssertAssetIsAllowed(_asset);
        _;
    }
    modifier assetIsAllowToDeposit(address _asset, uint256 amount) {
        __AssertAssetIsAllowed(_asset);
        __AssertAssetIsAllowToDeposit(_asset, amount);
        _;
    }
    modifier assetIsNotExist(address _asset) {
        __AssertAssetNotExist(_asset);
        _;
    }

    function __AssertAssetIsAllowed(address _asset) internal view {
        require(asset_pools.contains(_asset), 'asset Is Not Allowed');
    }
    function __AssertAssetNotExist(address _asset) internal view {
        require(!asset_pools.contains(_asset), 'asset Is Exist');
    }
    function __AssertAssetIsAllowToDeposit(address _asset, uint256 amount) internal view {
        uint256 selfBalance = __getSelfAssetBalance(_asset);
        require(asset_pools.get(_asset) >= (selfBalance + amount), 'overflow of asset pool');
    }

////////////////////////////////////////// base methods

    function __getSelfAssetBalance(address) internal view virtual returns (uint256) {
        require(false, '__getSelfAssetBalance not implemented yet!');
        return 0;
    }

////////////////////////////////////////// write methods

    function __AddAsset(
        address _asset,
        uint256 _assetCap
    )
        internal
        assetIsNotExist(_asset)
    {
        asset_pools.set(_asset, _assetCap);
    }

    function __EditAssetCap(
        address _asset,
        uint256 _assetCap
    )
        internal
        assetIsAllowed(_asset)
    {
        asset_pools.set(_asset, _assetCap);
    }

    function __RemoveAsset(
        address _asset
    )
        internal
        assetIsAllowed(_asset)
    {
        uint256 selfBalance = __getSelfAssetBalance(_asset);
        require(selfBalance == 0, 'only assets with empty balance allow to remove');
        asset_pools.remove(_asset);
    }
    
////////////////////////////////////////// methods view

    function getAssetWithCaps()
        public
        view
        returns (address[] memory, uint256[] memory)
    {
        return asset_pools.getKeyValueList();
    }

    function getAssetCap(address _asset)
        public
        view
        assetIsAllowed(_asset)
        returns (uint256)
    {
        return asset_pools.get(_asset);
    }

}

