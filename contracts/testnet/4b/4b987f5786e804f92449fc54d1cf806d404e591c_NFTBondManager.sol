pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "../../libs/v2libraries/interfaces/IUniswapV2Router02.sol";
import "../../libs/v2libraries/UniswapV2Library.sol";
import "../../libs/abstract/AbstractAssetsManager.sol";

abstract contract BondAssetsManager is AbstractAssetsManager
{

////////////////////////////////////////// initialize

    function __BondAssetsManager_init(
        address _rbis,
        address _uniswapV2Router
    ) internal {
        rbis = _rbis;
        uniswapV2Factory = IUniswapV2Router02(_uniswapV2Router).factory();
    }

////////////////////////////////////////// fields definition

    address private rbis;
    address private uniswapV2Factory;

    /** see https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps */
    uint256[48] private __gap;

////////////////////////////////////////// base methods

    function __getSelfAssetBalance(address _asset) internal view override returns (uint256) {
        address pair = UniswapV2Library.pairFor(uniswapV2Factory, _asset, rbis);

        (uint256 assetReserve,) = UniswapV2Library.getReserves(uniswapV2Factory, _asset, rbis);

        uint256 selfBalance = IERC20Upgradeable(pair).balanceOf(address(this));
        return (assetReserve * selfBalance * 2) / IERC20Upgradeable(pair).totalSupply();
    }

    function getAssetBalance(address _asset) external view  returns (uint256) {
       return  __getSelfAssetBalance(_asset);
    }

}

