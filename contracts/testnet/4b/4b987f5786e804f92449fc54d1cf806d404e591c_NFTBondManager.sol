pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/interfaces/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

import "../../libs/v2libraries/UniswapV2Library.sol";
import "../../libs/v2libraries/UniswapV2LiquidityMathLibrary.sol";
import "../../libs/v2libraries/interfaces/IUniswapV2Router02.sol";
import "../../libs/interfaces/IWEH.sol";

abstract contract BondTreasury {
    using SafeMathUpgradeable for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;

////////////////////////////////////////// initialize

    uint8 public constant percent = 80;
    //uint32 public constant frized_days = 365 days;
    uint32 public constant frized_days = 1 days;

    function __Treasury_init(
        address rbis,
        address uniswapV2Router
    ) internal {
        _rbis = rbis;
        _uniswapv2_router = uniswapV2Router;
        _WETH = IUniswapV2Router02(uniswapV2Router).WETH();
        _factory = IUniswapV2Router02(uniswapV2Router).factory();
    }

    function __Unlocked_init() internal {
        unlocked = 1;
    }

////////////////////////////////////////// fields definition

    uint8 private unlocked;

    address public _rbis;
    address public _uniswapv2_router;

    address public _WETH;
    address public _factory;

    struct DepositData {
        uint256 time;
        uint256 redem_time;
        uint256 init_amount;
        uint256 amount;
        uint256 lp_token;
        uint256 rate;
    }

    // ERC20 address => currencyId => DepositData
    mapping(address => mapping(uint256 => DepositData[])) public nft_asset_deposits;
    mapping(address => mapping(uint256 => uint256)) public deposit_count;

    /** see https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps */
    uint256[41] private __gap;

////////////////////////////////////////// modifiers

    modifier lock() {
        require(unlocked == 1, "LOCKED");
        unlocked = 0;
        _;
        unlocked = 1;
    }

////////////////////////////////////////// methods view

    function _getCurrencyDepositList(address erc20Address, uint256 tokenId)
        internal
        view
        returns (DepositData[] memory deposits)
    {

        return nft_asset_deposits[erc20Address][tokenId];
    }


    function __getAssetBalance(address erc20Address, uint256 tokenId)
        internal
        view
        returns (uint256 balance, uint256 balanceLp)
    {
        DepositData[] storage assetsAndTimeHold = nft_asset_deposits[erc20Address][tokenId];

        for (uint256 i = 0; i < assetsAndTimeHold.length; i++) {
            balance += assetsAndTimeHold[i].amount;
            balanceLp += assetsAndTimeHold[i].lp_token;
        }
    }

////////////////////////////////////////// methods for edit assets

    function __addAssetToBalance(
        address erc20Address,
        uint256 tokenId,
        uint256 amount,
        uint256 lp_token,
        uint256 rate
    )
        private
    {
        DepositData[] storage assetsAndTimeHold = nft_asset_deposits[
            erc20Address
        ][tokenId];

        assetsAndTimeHold.push(
            DepositData({
                time: block.timestamp,
                amount: amount,
                lp_token: lp_token,
                rate: rate,
                redem_time: 0,
                init_amount: amount
            })
        );
    }

    function __subAssetBalance(
        address erc20Address,
        uint256 tokenId,
        uint256 amount
    )
        private
        returns (uint256 withdrawable, uint256 withdrawableLp,  uint256 rate)
    {
        DepositData[] storage assetsAndTimeHold = nft_asset_deposits[erc20Address][tokenId];

        for (uint256 i = 0; i < assetsAndTimeHold.length; i++) {

            rate += assetsAndTimeHold[i].rate;
            if (assetsAndTimeHold[i].amount >= amount) {
                uint256 lp_token = (((assetsAndTimeHold[i].lp_token * 1e12) / assetsAndTimeHold[i].amount) * amount) / 1e12;
                withdrawable += (assetsAndTimeHold[i].time + frized_days) >= block.timestamp ? ((assetsAndTimeHold[i].amount / 100) * percent) : assetsAndTimeHold[i].amount;
                withdrawableLp += lp_token;
                
                assetsAndTimeHold[i].amount -= amount;
                assetsAndTimeHold[i].lp_token -= lp_token;
                return ( withdrawable, withdrawableLp, rate / (i+1));
            

            } else {
                amount -= assetsAndTimeHold[i].amount;
                withdrawable += (assetsAndTimeHold[i].time + frized_days) >= block.timestamp ? ((assetsAndTimeHold[i].amount / 100) * percent) : assetsAndTimeHold[i].amount; 
                withdrawableLp += assetsAndTimeHold[i].lp_token;
                assetsAndTimeHold[i].amount = 0;
                assetsAndTimeHold[i].lp_token = 0;
                assetsAndTimeHold[i].redem_time = block.timestamp;
            }
        }
        return (withdrawable, withdrawableLp, rate / (assetsAndTimeHold.length - 1) );
    }

////////////////////////////////////////// method like deposit

    function _takeAssetFromSender(
        address currency,
        uint256 tokenId,
        uint256 amount_currency
    )
        internal
        lock
    {
        (uint256 reserveA, uint256 reserveB) = UniswapV2Library.getReserves(
            _factory,
            currency,
            _rbis
        );
        uint256 amountB = UniswapV2Library.quote(
            amount_currency,
            reserveA,
            reserveB
        );
        address pair = UniswapV2Library.pairFor(_factory, currency, _rbis);
        if (currency == _WETH) {
            IWETH(_WETH).deposit{value: amount_currency}();
            assert(IWETH(_WETH).transfer(pair, amount_currency));
        } else {
            IERC20Upgradeable(currency).safeTransferFrom(
                msg.sender,
                pair,
                amount_currency
            );
        }

        IERC20Upgradeable(_rbis).safeTransfer(
            pair,
            amountB
        );
        uint256 liquidity = IUniswapV2Pair(pair).mint(address(this));
        (uint256 currencyReserve, uint256 _rbisReserve) = UniswapV2Library.getReserves(_factory, currency, _rbis);

        uint256 rate = _rbisReserve / currencyReserve;
        __addAssetToBalance(currency, tokenId, amount_currency, liquidity, rate);
    }

////////////////////////////////////////// methods for withdraw

    function _withdraw_an_asset_with_reinforcements(
        address currency,
        uint256 withdrawableLp,
        uint256 withdrawable
    )
        private
    {
        address pair = UniswapV2Library.pairFor(_factory, currency, _rbis);
        IUniswapV2Pair(pair).transfer(pair, withdrawableLp);
        (uint256 amount0, uint256 amount1) = IUniswapV2Pair(pair).burn(address(this) );
        (address token0, ) = UniswapV2Library.sortTokens(currency, _rbis);
        (, uint256 rbisAmount) = currency == token0 ? (amount0, amount1) : (amount1, amount0);

        if ( IERC20Upgradeable(currency).balanceOf(address(this)) < withdrawable ) {
            (uint256 currencyReserve, uint256 _rbisReserve) = UniswapV2Library.getReserves(_factory, currency, _rbis);
            uint256 amountOut = (withdrawable - IERC20Upgradeable(currency).balanceOf(address(this)));
            uint256 amountIn = UniswapV2Library.getAmountIn(
                amountOut,
                _rbisReserve,
                currencyReserve
            );

            if(rbisAmount < amountIn){
                IERC20Upgradeable(_rbis).safeTransfer(pair, (amountIn - rbisAmount));
            }

            IERC20Upgradeable(_rbis).transfer(pair, amountIn);

            (uint256 amount0Out, uint256 amount1Out) = _rbis == token0 ? (uint256(0), amountOut) : (amountOut, uint256(0));
            IUniswapV2Pair(pair).swap( amount0Out, amount1Out, address(this), new bytes(0));

        }
    }

    function _withdraw_an_asset(address currency, uint256 withdrawableLp)
        private
    {
        address pair = UniswapV2Library.pairFor(_factory, currency, _rbis);
        IUniswapV2Pair(pair).transfer(pair, withdrawableLp );
        IUniswapV2Pair(pair).burn(address(this)); 
    }

    function _withdraw_only_liquidity(address currency, uint256 withdrawableLp)
        private
    {
        address pair = UniswapV2Library.pairFor(_factory, currency, _rbis);
        IUniswapV2Pair(pair).transfer(msg.sender, withdrawableLp);
    }

    function _withdrawAssetByObligationHolder(
        address currency,
        uint256 tokenId,
        uint256 amount
    )
        internal
        lock
        returns(bool)
    {
        (uint256 withdrawable, uint256 withdrawableLp, uint256 withdrawable_rate) = __subAssetBalance(
            currency,
            tokenId,
            amount
        );

        //require(withdrawable <= amount, "Treasury::lack of balance");

        uint256 trasure_balanece = IERC20Upgradeable(currency).balanceOf(address(this));

        (uint256 currencyReserve, uint256 _rbisReserve) = UniswapV2Library.getReserves(_factory, currency, _rbis);

        uint256 rate = _rbisReserve / currencyReserve;

        if (withdrawable_rate * 3 <= (rate)) {
            _withdraw_only_liquidity(currency, withdrawableLp);
            return true;
         
        }

        if (withdrawable_rate < rate) {
            _withdraw_an_asset_with_reinforcements(currency, withdrawableLp,  withdrawable);

    
        } else {
            _withdraw_an_asset(currency, withdrawableLp);
        }

        withdrawable = IERC20Upgradeable(currency).balanceOf(address(this)) - trasure_balanece;
        if (currency == _WETH) {        
            IWETH(_WETH).withdraw(withdrawable);
            (bool success, ) = payable(address(msg.sender)).call{value: withdrawable}( new bytes(0) );
            require( success, "TransferHelper::safeTransferETH: ETH transfer failed" );
        } else {
            IERC20Upgradeable(currency).transfer(msg.sender, withdrawable);
        }

        //IERC20Upgradeable(_rbis).transfer(_NFTBondProtokolAssetFund,IERC20Upgradeable(_rbis).balanceOf(address(this)));

        return true;
    }

////////////////////////////////////////// receive

    receive() external payable {
        assert(msg.sender == _WETH); // only accept ETH via fallback from the WETH contract
    }
    
    fallback() external payable {
        assert(msg.sender == _WETH); // only accept ETH via fallback from the WETH contract
    }

}

