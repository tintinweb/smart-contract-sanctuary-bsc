contract node{
    using SafeMath for uint256;
    address owner;
    uint256 private lock = 0;
    IERC20 payToken1; 
    address[] public tokenPath1; //dui huan bfs jiao yi dui
    address tokenDestroy1; // dui huan de bfs zhuan dao na ge di zhi shang  
    uint256 tokenDestroyRate1 = 1000;  // dui huang hou zhuan zhang bi li bfs
    uint256 allocation = 80;
    address sendAddress;
    mapping (address => uint256) public ToReceive;  //待领取
    IERC20 public outputAddress; //chan chu bi
    IPancakeRouter02 uniswapRouter;
    event buyEvent(address indexed sender, uint256 tokenNum1 , uint256 timestamp); 
    event WithdrawLog(address indexed sender, uint256 num); 
    constructor(IPancakeRouter02 _uniswapRouter, IERC20 _payToken1) {
        owner = msg.sender;
        uniswapRouter = _uniswapRouter;
        payToken1 = _payToken1;
        changePayContractApprove(10**28);
    }
    modifier checkOwner() {
        require(msg.sender == owner);
        _;
    }
    modifier checkLock() {
        require(lock == 0);
        lock = 1;
        _;
        lock = 0;
    }

    modifier checkSend() {
        require(msg.sender == sendAddress);
        _;
    }
    function buy(uint256 num) checkLock public payable{
        require(num > 0);
        uint256 _payTokenNum1 = num;

        uint256 DestroyNum1 = 0;
    
        IERC20 _payToken1;
     
        if(_payTokenNum1 > 0){
            payToken1.transferFrom(msg.sender, address(this), _payTokenNum1);
            uint256 tokenNum1 = _payTokenNum1.mul(allocation).div(100);
      
            if(tokenPath1.length > 0 && tokenNum1 > 0){
                _payToken1 = IERC20(tokenPath1[tokenPath1.length-1]);
                uint256 tokenOldNum1 = _payToken1.balanceOf(address(this));
                uniswapRouter.swapExactTokensForTokens(tokenNum1, 0, tokenPath1, address(this), block.timestamp.add(5));
                DestroyNum1 = _payToken1.balanceOf(address(this)).sub(tokenOldNum1).mul(tokenDestroyRate1).div(1000);
                
            }
           
        }
        if(DestroyNum1 > 0){
            _payToken1.transfer(tokenDestroy1, DestroyNum1);
        }
      
        emit buyEvent(msg.sender,_payTokenNum1, block.timestamp);
    }

    function updateWaitReceive(address _u,uint _a, uint256 _t) checkSend public {
        if(_t == 1){
            ToReceive[_u] = ToReceive[_u].add(_a);
        }else if(_t == 2){
            ToReceive[_u] = ToReceive[_u].sub(_a);
        }
    }
    function receiveReward() public {
        uint256 num = ToReceive[msg.sender];
        require(num > 0 ,'0'); 
        outputAddress.transfer(msg.sender, num);
        ToReceive[msg.sender] = 0;
    }

    function setOutputAddress(IERC20 _a) checkOwner public {
        outputAddress = _a;
    }
    function setSendAddress(address _a) checkOwner public {
        sendAddress = _a;
    }

    function withdraw(IERC20 _a, uint256 num, address _to) checkOwner public {
        _a.transfer(_to, num);
    }

    function setOwner(address _a) checkOwner public {
        owner = _a;
    }
    function setToken1(IERC20 _t1) checkOwner public {
        payToken1 = _t1;
    }
   
    function setUniswapRouter(IPancakeRouter02 _a) checkOwner public {
        uniswapRouter = _a;
        changePayContractApprove(10**28);
    }
    function setPath(address[] memory _p1) checkOwner public {
        tokenPath1 = _p1;

    }
    function setTokenDestroy1(address _a1, uint256 _n1) checkOwner public {
        tokenDestroy1 = _a1;
        tokenDestroyRate1 = _n1;
     

    }
    function setAllocation(uint256 _n1) checkOwner public {
        allocation = _n1;
    }
    function changePayContractApprove(uint256 _n) internal  {
        payToken1.approve(address(uniswapRouter), _n);
    }
    
}