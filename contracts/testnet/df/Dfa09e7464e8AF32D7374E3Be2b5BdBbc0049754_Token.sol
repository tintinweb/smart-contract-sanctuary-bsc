        sellMarketingFee = _MarketingFee;
        sellStakingFee = _StakingFee;
        sellBurnFee = _BurnFee;
        sellTotalFees = sellMarketingFee + sellStakingFee + sellBurnFee;
        require(sellTotalFees <= 6, "Must keep fees at 6% or less");
