pragma solidity 0.8.9;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "./Taxable.sol";
import "./Vestable.sol";

contract OwnersUnityToken is Vestable, Taxable {
    using SafeMath for uint256;
    using Address for address;

    string private _Bname = "Owners Unity Token";
    string private _Bsymbol = "OUT";
    // 18 Decimals
    uint8 private _Bdecimals = 18;
    // 10B Supply
    uint256 private _BtotalSupply = 10_000_000_000 * 10**_Bdecimals;
    // 1% Max Wallet
    uint256 private _BmaxBalance = _BtotalSupply.mul(2).div(200);
    // 0.2% Max Transaction
    uint256 private _BmaxTx = _BtotalSupply.mul(5).div(2500);
    // 30% Max Fees
    uint8 private _BmaxFees = 30;
    // 5% Max AffordableHousing Fee
    uint8 private _BmaxAffordableHousingFee = 5;
    // Contract sell at 0.01% tokens
    uint256 private _BliquifyThreshhold = 1 * 10**6 * 10**_Bdecimals;
    TokenDistribution private _BtokenDistribution =
        TokenDistribution({
            totalSupply: _BtotalSupply,
            decimals: _Bdecimals,
            maxBalance: _BmaxBalance,
            maxTx: _BmaxTx
        });

    address payable _BAffordableHousingAddress =
        payable(address(0x19D0607781585de4e28ee808796DA7efdfa97554));
    address payable _BmarketingAddress =
        payable(address(0x90C0709435A4DCf8f30c6ba958A7F7B3A77E5565));
    address payable _BBuybackBurnAddress =
        payable(address(0x33Fa48F8A3f476831d50fE7114B3b561f997FB04));

    // Buy and sell fees will start at 99% to prevent bots/snipers at launch,
    // but will not be allowed to be set this high ever again.
    constructor(address newOwner)
        Taxable(
            _Bsymbol,
            _Bname,
            _BtokenDistribution,
            _BAffordableHousingAddress,
            _BmarketingAddress,
            _BBuybackBurnAddress,
            Taxes({
                AffordableHousingFee: 1,
                rewardsFee: 2,
                marketingFee: 32,
                BuybackBurnFee: 3,
                liqFee: 61
            }),
            Taxes({
                AffordableHousingFee: 1,
                rewardsFee: 2,
                marketingFee: 32,
                BuybackBurnFee: 3,
                liqFee: 61
            }),
            _BmaxFees,
            _BmaxAffordableHousingFee,
            _BliquifyThreshhold
        )
    {
        _balances[newOwner] = _totalSupply;
        transferOwnership(newOwner);
        emit Transfer(address(0), newOwner, _totalSupply);
    }

    function getTokenBalance() external view returns (uint256) {
        return balanceOf(address(this));
    }

    function withdrawTokens(uint8 _round) public {
        require(sales[_round].isClaimUnlocked, "Claim not unlocked yet");
        uint256 tokensToSend = getVestingTokensUnlocked(msg.sender, _round);
        if (tokensToSend > 0) {
            // Updating tokens sent counter
            users[_round][msg.sender].totalWithdrawn = users[_round][msg.sender]
                .totalWithdrawn
                .add(tokensToSend);
            // Sending allowed tokens amount
            _transfer(address(this), msg.sender, tokensToSend);

            users[_round][msg.sender].lastWithdrawTime = block.timestamp;
            // Raising event
            emit Withdraw(tokensToSend, block.timestamp);
        }
    }

    function startVesting(uint8 _round, uint256 _apy) external onlyOwner {
        require(!sales[_round].isActive, "Sale in progress. Close it first");
        require(
            !sales[_round].isClaimUnlocked,
            "Already set vesting for this round"
        );
        require(
            balanceOf(address(this)) >= sales[_round].tokenBalanceToDistribute,
            "not enough tokens in contract to dist"
        );

        sales[_round].isClaimUnlocked = true;
        sales[_round].apy = _apy;
        initVestingStages(_round);
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./Tradeable.sol";

abstract contract Taxable is Tradable {
    using SafeMath for uint256;

    struct Taxes {
        uint8 AffordableHousingFee;
        uint8 rewardsFee;
        uint8 marketingFee;
        uint8 BuybackBurnFee;
        uint8 liqFee;
    }

    uint8 constant BUYTX = 1;
    uint8 constant SELLTX = 2;
    //
    address payable public _AffordableHousingAddress;
    address payable public _marketingAddress;
    address payable public _BuybackBurnAddress;
    //
    uint256 public _liquifyThreshhold;
    bool inSwapAndLiquify;
    //
    uint8 public _maxFees;
    uint8 public _maxAffordableHousingFee;
    //
    Taxes public _buyTaxes;
    uint8 public _totalBuyTaxes;
    Taxes public _sellTaxes;
    uint8 public _totalSellTaxes;
    //
    uint256 private _AffordableHousingTokensCollected;
    uint256 private _rewardsTokensCollected;
    uint256 private _marketingTokensCollected;
    uint256 private _BuybackBurnTokensCollected;
    uint256 private _liqTokensCollected;
    //
    mapping(address => bool) private _isExcludedFromFees;

    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    constructor(
        string memory symbol,
        string memory name,
        TokenDistribution memory tokenDistribution,
        address payable AffordableHousingAddress,
        address payable marketingAddress,
        address payable BuybackBurnAddress,
        Taxes memory buyTaxes,
        Taxes memory sellTaxes,
        uint8 maxFees,
        uint8 maxAffordableHousingFee,
        uint256 liquifyThreshhold
    ) Tradable(symbol, name, tokenDistribution) {
        _AffordableHousingAddress = AffordableHousingAddress;
        _marketingAddress = marketingAddress;
        _BuybackBurnAddress = BuybackBurnAddress;
        _buyTaxes = buyTaxes;
        _sellTaxes = sellTaxes;
        _totalBuyTaxes =
            buyTaxes.AffordableHousingFee +
            buyTaxes.rewardsFee +
            buyTaxes.marketingFee +
            buyTaxes.BuybackBurnFee +
            buyTaxes.liqFee;
        _totalSellTaxes =
            sellTaxes.AffordableHousingFee +
            sellTaxes.rewardsFee +
            sellTaxes.marketingFee +
            sellTaxes.BuybackBurnFee +
            sellTaxes.liqFee;
        _maxFees = maxFees;
        _maxAffordableHousingFee = maxAffordableHousingFee;
        _liquifyThreshhold = liquifyThreshhold;

        _isExcludedFromFees[owner()] = true;
        _isExcludedFromFees[address(this)] = true;
        _isExcludedFromFees[marketingAddress] = true;
        _isExcludedFromFees[AffordableHousingAddress] = true;
        _isExcludedFromFees[BuybackBurnAddress] = true;
    }

    function setMarketingAddress(address payable newMarketingAddress)
        external
        onlyOwner
    {
        require(newMarketingAddress != _marketingAddress);
        _marketingAddress = newMarketingAddress;
    }

    function setAffordableHousingAddress(
        address payable newAffordableHousingAddress
    ) external onlyOwner {
        require(newAffordableHousingAddress != _AffordableHousingAddress);
        _AffordableHousingAddress = newAffordableHousingAddress;
    }

    function setBuybackBurnAddress(address payable newBuybackBurnAddress)
        external
        onlyOwner
    {
        require(newBuybackBurnAddress != _BuybackBurnAddress);
        _BuybackBurnAddress = newBuybackBurnAddress;
    }

    function includeInFees(address account) public onlyOwner {
        _isExcludedFromFees[account] = false;
    }

    function excludeFromFees(address account) public onlyOwner {
        _isExcludedFromFees[account] = true;
    }

    function setBuyFees(
        uint8 newAffordableHousingBuyFee,
        uint8 newRewardsBuyFee,
        uint8 newMarketingBuyFee,
        uint8 newBuybackBurnBuyFee,
        uint8 newLiqBuyFee
    ) external onlyOwner {
        uint8 newTotalBuyFees = newAffordableHousingBuyFee +
            newRewardsBuyFee +
            newMarketingBuyFee +
            newBuybackBurnBuyFee +
            newLiqBuyFee;
        require(!inSwapAndLiquify, "inSwapAndLiquify");
        require(
            newAffordableHousingBuyFee <= _maxAffordableHousingFee,
            "Cannot set AffordableHousing fee higher than max"
        );
        require(
            newTotalBuyFees <= _maxFees,
            "Cannot set total buy fees higher than max"
        );

        _buyTaxes = Taxes({
            AffordableHousingFee: newAffordableHousingBuyFee,
            rewardsFee: newRewardsBuyFee,
            marketingFee: newMarketingBuyFee,
            BuybackBurnFee: newBuybackBurnBuyFee,
            liqFee: newLiqBuyFee
        });
        _totalBuyTaxes = newTotalBuyFees;
    }

    function setSellFees(
        uint8 newAffordableHousingSellFee,
        uint8 newRewardsSellFee,
        uint8 newMarketingSellFee,
        uint8 newBuybackBurnSellFee,
        uint8 newLiqSellFee
    ) external onlyOwner {
        uint8 newTotalSellFees = newAffordableHousingSellFee +
            newRewardsSellFee +
            newMarketingSellFee +
            newBuybackBurnSellFee +
            newLiqSellFee;
        require(!inSwapAndLiquify, "inSwapAndLiquify");
        require(
            newAffordableHousingSellFee <= _maxAffordableHousingFee,
            "Cannot set AffordableHousing fee higher than max"
        );
        require(
            newTotalSellFees <= _maxFees,
            "Cannot set total sell fees higher than max"
        );

        _sellTaxes = Taxes({
            AffordableHousingFee: newAffordableHousingSellFee,
            rewardsFee: newRewardsSellFee,
            marketingFee: newMarketingSellFee,
            BuybackBurnFee: newBuybackBurnSellFee,
            liqFee: newLiqSellFee
        });
        _totalSellTaxes = newTotalSellFees;
    }

    function setLiquifyThreshhold(uint256 newLiquifyThreshhold)
        external
        onlyOwner
    {
        _liquifyThreshhold = newLiquifyThreshhold;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transferWithTaxes(msg.sender, recipient, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transferWithTaxes(sender, recipient, amount);
        approveFromOwner(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function _transferWithTaxes(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        if (
            from != owner() && // Not from Owner
            to != owner() && // Not to Owner
            !_isExcludedFromMaxBalance[to] // is excludedFromMaxBalance
        ) {
            require(
                balanceOf(to).add(amount) <= _maxBalance,
                "Tx would cause wallet to exceed max balance"
            );
        }

        // Sell tokens for funding
        if (
            !inSwapAndLiquify && // Swap is not locked
            balanceOf(address(this)) >= _liquifyThreshhold && // liquifyThreshhold is reached
            from != pair && // Not from liq pool (can't sell during a buy)
            from != address(this) &&
            from != owner()
        ) {
            swapCollectedFeesForFunding();
        }

        // Send fees to contract if necessary
        uint8 txType = 0;
        if (from == pair) txType = BUYTX;
        if (to == pair) txType = SELLTX;
        if (
            txType != 0 &&
            !(_isExcludedFromFees[from] || _isExcludedFromFees[to]) &&
            ((txType == BUYTX && _totalBuyTaxes > 0) ||
                (txType == SELLTX && _totalSellTaxes > 0))
        ) {
            uint256 feesToContract = calculateTotalFees(amount, txType);

            if (feesToContract > 0) {
                amount = amount.sub(feesToContract);
                _transfer(from, address(this), feesToContract);
            }
        }

        _transfer(from, to, amount);
    }

    function calculateTotalFees(uint256 amount, uint8 txType)
        private
        returns (uint256)
    {
        uint256 AffordableHousingTokens = (txType == BUYTX)
            ? amount.mul(_buyTaxes.AffordableHousingFee).div(100)
            : amount.mul(_sellTaxes.AffordableHousingFee).div(100);
        uint256 rewardsTokens = (txType == BUYTX)
            ? amount.mul(_buyTaxes.rewardsFee).div(100)
            : amount.mul(_sellTaxes.rewardsFee).div(100);
        uint256 marketingTokens = (txType == BUYTX)
            ? amount.mul(_buyTaxes.marketingFee).div(100)
            : amount.mul(_sellTaxes.marketingFee).div(100);
        uint256 BuybackBurnTokens = (txType == BUYTX)
            ? amount.mul(_buyTaxes.BuybackBurnFee).div(100)
            : amount.mul(_sellTaxes.BuybackBurnFee).div(100);
        uint256 liqTokens = (txType == BUYTX)
            ? amount.mul(_buyTaxes.liqFee).div(100)
            : amount.mul(_sellTaxes.liqFee).div(100);

        _AffordableHousingTokensCollected = _AffordableHousingTokensCollected
            .add(AffordableHousingTokens);
        _rewardsTokensCollected = _rewardsTokensCollected.add(rewardsTokens);
        _marketingTokensCollected = _marketingTokensCollected.add(
            marketingTokens
        );
        _BuybackBurnTokensCollected = _BuybackBurnTokensCollected.add(
            BuybackBurnTokens
        );
        _liqTokensCollected = _liqTokensCollected.add(liqTokens);

        return
            AffordableHousingTokens
                .add(rewardsTokens)
                .add(marketingTokens)
                .add(BuybackBurnTokens)
                .add(liqTokens);
    }

    function swapCollectedFeesForFunding() private lockTheSwap {
        uint256 totalCollected = _AffordableHousingTokensCollected
            .add(_marketingTokensCollected)
            .add(_liqTokensCollected)
            .add(_BuybackBurnTokensCollected)
            .add(_liqTokensCollected);
        require(totalCollected > 0, "No tokens available to swap");

        uint256 initialFunds = address(this).balance;

        uint256 halfLiq = _liqTokensCollected.div(2);
        uint256 otherHalfLiq = _liqTokensCollected.sub(halfLiq);

        uint256 totalAmountToSwap = _AffordableHousingTokensCollected
            .add(_rewardsTokensCollected)
            .add(_marketingTokensCollected)
            .add(_BuybackBurnTokensCollected)
            .add(halfLiq);

        swapTokensForNative(totalAmountToSwap);

        uint256 newFunds = address(this).balance.sub(initialFunds);

        uint256 liqFunds = newFunds.mul(halfLiq).div(totalAmountToSwap);
        uint256 marketingFunds = newFunds.mul(_marketingTokensCollected).div(
            totalAmountToSwap
        );
        uint256 rewardsFunds = newFunds.mul(_rewardsTokensCollected).div(
            totalAmountToSwap
        );
        uint256 BuybackBurnFunds = newFunds
            .mul(_BuybackBurnTokensCollected)
            .div(totalAmountToSwap);
        uint256 AffordableHousingFunds = newFunds
            .sub(liqFunds)
            .sub(marketingFunds)
            .sub(rewardsFunds)
            .sub(BuybackBurnFunds);

        addLiquidity(otherHalfLiq, liqFunds);
        (bool sent, bytes memory data) = _AffordableHousingAddress.call{
            value: AffordableHousingFunds
        }("");
        (bool sent1, bytes memory data1) = _marketingAddress.call{
            value: marketingFunds
        }("");
        (bool sent2, bytes memory data2) = _BuybackBurnAddress.call{
            value: BuybackBurnFunds
        }("");
        require(sent && sent1 && sent2, "Failed to send BNB");
        try distributor.deposit{value: rewardsFunds}() {} catch {}

        _AffordableHousingTokensCollected = 0;
        _marketingTokensCollected = 0;
        _liqTokensCollected = 0;
        _rewardsTokensCollected = 0;
        _BuybackBurnTokensCollected = 0;
    }

    function swapTokensForNative(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        approveFromOwner(address(this), address(router), tokenAmount);

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        approveFromOwner(address(this), address(router), tokenAmount);

        router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0,
            0,
            address(0),
            block.timestamp
        );
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity 0.8.9;
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

abstract contract Vestable is Context, ReentrancyGuard, Ownable {
    using SafeMath for uint256;

    bool private vestingInit = false;

    struct Stage {
        uint256 date;
        uint256 tokensUnlockedPercentage;
    }

    struct User {
        uint256 initialBalance;
        uint256 totalWithdrawn;
        uint256 contribution;
        uint256 lastWithdrawTime;
    }

    struct Sale {
        address payable wallet;
        uint256 rate;
        uint256 divider;
        uint256 weiRaised;
        uint256 hardCap;
        uint256 capPerWallet;
        uint256 apy;
        uint256 tokenBalanceToDistribute;
        bool isActive;
        bool isClaimUnlocked;
    }

    mapping(uint8 => mapping(address => User)) public users;
    mapping(address => bool) public isWhitelisted;
    mapping(uint8 => Stage[]) public stages;
    uint256 minCap = 0.1 ether;
    Sale[3] public sales;

    uint8 public currentRound;

    event TokensPurchased(
        address indexed purchaser,
        uint256 value,
        uint256 amount,
        uint8 round
    );

    event Withdraw(uint256 tokensToSend, uint256 now);

    constructor() {}

    function initVestingSchedule(
        uint256 _rate,
        address payable _wallet,
        uint256 _divider,
        uint256 _hardCap,
        uint256 _capPerWallet
    ) external onlyOwner {
        require(!vestingInit);
        sales[0].isActive = true;
        sales[0].rate = _rate;
        sales[0].divider = _divider;
        sales[0].wallet = _wallet;
        sales[0].hardCap = _hardCap;
        sales[0].capPerWallet = _capPerWallet;
        vestingInit = true;
    }

    function vestingWhiteListUsers(address[] calldata _whitelist)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < _whitelist.length; i++) {
            isWhitelisted[_whitelist[i]] = true;
        }
    }

    function removeFromVestingWhitelist(address[] calldata _removeUser)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < _removeUser.length; i++) {
            isWhitelisted[_removeUser[i]] = false;
        }
    }

    function buyTokens() public payable nonReentrant {
        require(sales[currentRound].isActive, "Sale not active");
        require(
            msg.value >= minCap,
            "amount should be greator than min contribution"
        );
        uint256 weiAmount = msg.value;
        _preValidatePurchase(msg.sender, weiAmount, currentRound);

        // calculate token amount to be created
        uint256 tokens = _getTokenAmount(weiAmount, currentRound);

        // update state
        sales[currentRound].weiRaised = sales[currentRound].weiRaised.add(
            weiAmount
        );
        users[currentRound][msg.sender].contribution = users[currentRound][
            msg.sender
        ].contribution.add(weiAmount);

        _processPurchase(msg.sender, tokens, currentRound);
        emit TokensPurchased(msg.sender, weiAmount, tokens, currentRound);
        _forwardFunds(currentRound);
    }

    function _preValidatePurchase(
        address beneficiary,
        uint256 weiAmount,
        uint8 _round
    ) internal view {
        require(
            beneficiary != address(0),
            "Crowdsale: beneficiary is the zero address"
        );
        require(weiAmount != 0, "Crowdsale: weiAmount is 0");
        if (_round < 2) {
            require(isWhitelisted[beneficiary], "You're not whitelisted");
        }
        require(
            users[_round][beneficiary].contribution.add(weiAmount) <=
                sales[_round].capPerWallet,
            "Amount exceeds user limit"
        );
        require(
            sales[_round].weiRaised.add(weiAmount) <= sales[_round].hardCap,
            "Amount exceeds HardCap of the Round"
        );
    }

    function setMinCap(uint256 _mincap) external onlyOwner {
        require(
            _mincap > 0,
            " Min contribution amount should be greator than 0"
        );
        minCap = _mincap;
    }

    function _deliverTokens(
        address beneficiary,
        uint256 tokenAmount,
        uint8 round
    ) internal {
        users[round][beneficiary].initialBalance = users[round][beneficiary]
            .initialBalance
            .add(tokenAmount);
        sales[round].tokenBalanceToDistribute = sales[round]
            .tokenBalanceToDistribute
            .add(tokenAmount);
    }

    function _processPurchase(
        address beneficiary,
        uint256 tokenAmount,
        uint8 _round
    ) internal {
        _deliverTokens(beneficiary, tokenAmount, _round);
    }

    function _getTokenAmount(uint256 weiAmount, uint8 _round)
        internal
        view
        returns (uint256)
    {
        return weiAmount.mul(sales[_round].rate).div(sales[_round].divider);
    }

    function _forwardFunds(uint8 _round) internal {
        sales[_round].wallet.transfer(msg.value);
    }

    // Change Round
    function switchVestingToNextRound(
        uint256 _rate,
        address payable _wallet,
        uint256 _divider,
        uint256 _hardCap,
        uint256 _capPerWallet
    ) external onlyOwner {
        require(currentRound < 3, "Sale schedules completed");
        sales[currentRound].isActive = false;
        currentRound++;

        if (currentRound < 3) {
            sales[currentRound].isActive = true;
            sales[currentRound].rate = _rate;
            sales[currentRound].wallet = _wallet;
            sales[currentRound].divider = _divider;
            sales[currentRound].hardCap = _hardCap;
            sales[currentRound].capPerWallet = _capPerWallet;
        }
    }

    /// Change presale Round details if needed
    function changeVestingRoundParams(
        uint256 _rate,
        address payable _wallet,
        uint256 _divider,
        uint256 _hardCap,
        uint256 _capPerWallet
    ) external onlyOwner {
        sales[currentRound].rate = _rate;
        sales[currentRound].wallet = _wallet;
        sales[currentRound].divider = _divider;
        sales[currentRound].hardCap = _hardCap;
        sales[currentRound].capPerWallet = _capPerWallet;
    }

    function initVestingStages(uint8 _round) internal {
        stages[_round].push(
            Stage({date: block.timestamp, tokensUnlockedPercentage: 100})
        );

        uint256 vestStart = block.timestamp + 2 * 30 days; //2 months till cliff

        if (_round == 0) {
            uint256 counter;
            for (uint16 i = 200; i <= 1000; i += 100) {
                stages[_round].push(
                    Stage({
                        date: vestStart + (counter * 30 days),
                        tokensUnlockedPercentage: i
                    })
                );
                counter++;
            }
        } else if (_round == 1) {
            uint256 counter;
            for (uint16 i = 250; i <= 1000; i += 150) {
                stages[_round].push(
                    Stage({
                        date: vestStart + (counter * 30 days),
                        tokensUnlockedPercentage: i
                    })
                );
                counter++;
            }
        } else if (_round == 2) {
            uint256 counter;
            for (uint16 i = 325; i <= 1000; i += 225) {
                stages[_round].push(
                    Stage({
                        date: vestStart + (counter * 30 days),
                        tokensUnlockedPercentage: i
                    })
                );
                counter++;
            }
        }
    }

    function getVestingTokensUnlocked(address addr, uint8 _round)
        public
        view
        returns (uint256 tokensToSend)
    {
        if (
            users[_round][addr].totalWithdrawn >=
            users[_round][addr].initialBalance
        ) {
            return 0;
        }
        uint256 allowedPercent;
        // Getting unlocked percentage
        for (uint8 i = 0; i < stages[_round].length; i++) {
            if (block.timestamp >= stages[_round][i].date) {
                allowedPercent = stages[_round][i].tokensUnlockedPercentage;
            }
        }
        if (allowedPercent >= 1000) {
            tokensToSend = users[_round][addr].initialBalance.sub(
                users[_round][addr].totalWithdrawn
            );
        } else {
            uint256 totalTokensAllowedToWithdraw = users[_round][addr]
                .initialBalance
                .mul(allowedPercent)
                .div(1000);
            tokensToSend = totalTokensAllowedToWithdraw.sub(
                users[_round][addr].totalWithdrawn
            );
        }

        uint256 from = users[_round][addr].lastWithdrawTime >
            stages[_round][0].date
            ? users[_round][addr].lastWithdrawTime
            : stages[_round][0].date;
        uint256 to = block.timestamp >
            stages[_round][stages[_round].length - 1].date
            ? stages[_round][stages[_round].length - 1].date
            : block.timestamp;

        if (from < to) {
            uint256 reward = to
                .sub(from)
                .mul(tokensToSend)
                .mul(sales[_round].apy)
                .div(365 days)
                .div(1000);
            tokensToSend = tokensToSend.add(reward);
        }

        return tokensToSend;
    }

    function vestableTokens(uint8 _round) external view returns (uint256) {
        return sales[_round].tokenBalanceToDistribute;
    }

    function manuallyAddUsersToVesting(
        uint8 saleRound,
        address[] calldata accounts,
        uint256[] calldata contributions,
        uint256[] calldata tokenAmount
    ) external onlyOwner {
        require(saleRound >= 0 && saleRound < sales.length);
        require(
            accounts.length > 0 &&
                accounts.length == contributions.length &&
                contributions.length == tokenAmount.length
        );
        for (uint8 index = 0; index < accounts.length; index++) {
            sales[saleRound].weiRaised = sales[saleRound].weiRaised.add(
                contributions[index]
            );

            users[saleRound][accounts[index]].contribution = users[saleRound][
                accounts[index]
            ].contribution.add(contributions[index]);

            _processPurchase(accounts[index], tokenAmount[index], saleRound);
        }
    }
}

