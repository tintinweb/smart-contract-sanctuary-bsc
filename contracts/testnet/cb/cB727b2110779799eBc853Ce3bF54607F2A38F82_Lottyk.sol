        uint256 allowance = BUSD.allowance(msg.sender, address(this));
        require(allowance >= _value, "Check the BUSD allowance");

        // Number of tries to get a valid value from Chainlink
        uint256 tries = 0;

        while (tries < MAX_TRIES) {
            requestRandomWords();

