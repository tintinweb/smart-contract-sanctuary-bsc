    // let an user claim their reward per ticket after draw close
    function claimTicketReward(uint256 ticketID) public {
        require(ticketID < _ticketIdCounter.current());
        require(ownerOf(ticketID) == msg.sender, "Sorry you're not the owner");
        require(
            !allTickets[ticketID].hasBeenPaid,
            "Sorry a Ticket can't be paid twice"
        );
        require(
            allTickets[ticketID].matchedNumbers > 0,
            "No price for 0 matches sorry"
        );
        /*revisar*/
        require(
            allTickets[ticketID].drawID < currentDraw,
            "Please wait to the end of next draw to Claim"
        );

        require(
            block.timestamp - accounts[msg.sender].lastClaimTime > 5 minutes,
            "You can only claim a ticket every 5 minutes"
        );

//        require(
//            accounts[msg.sender].claimedTickets < accounts[msg.sender].myTicketsHistory.length,
//            "You can only claim as many tickets as you have bought"
//        );

        Ticket storage ticket = allTickets[ticketID];

        ticket.hasBeenPaid = true;

        uint256 drawOfTheTicket = ticket.drawID;
        uint256 numberOfMatches = ticket.matchedNumbers;

        uint256 price = allDraws[drawOfTheTicket].reward[numberOfMatches];

        if (price > 0) {
            BUSD.safeTransfer(ownerOf(ticketID), price);
            rewardDelivered += price;
            accounts[msg.sender].reward += price;
            totalPrizesCollected+= price;
        }

        //update lastClaimTime and claimedTickets
        accounts[msg.sender].lastClaimTime = block.timestamp;
        accounts[msg.sender].claimedTickets += 1;

        _burn(ticketID);
    }



    /* REFERRAL SYSTEM */

    /**
     * @dev Add an address as referrer
     * @param referrer The address would set as referrer of msg.sender
     * @return whether success to add upline
     * Referee. A person who was referred by another person.
     */
    function addReferrer(address referrer)
        external
        nonReentrant
        notBlacklisted
        whenNotPaused
        returns (bool)
    {
        require(!Address.isContract(msg.sender), "only EOA allowed");
        require(
            address(referrer) != address(msg.sender),
            "You can't be your own referrer"
        );

        if (referrer == address(0)) {
            emit RegisteredRefererFailed(
                msg.sender,
                referrer,
                "Referrer cannot be 0x0 address"
            );
            return false;
        } else if (isCircularReference(referrer, msg.sender)) {
            emit RegisteredRefererFailed(
                msg.sender,
                referrer,
                "Referee cannot be one of referrer uplines"
            );
            return false;
        } else if (accounts[msg.sender].referrer != address(0)) {
            emit RegisteredRefererFailed(
                msg.sender,
                referrer,
                "Address have been registered upline"
            );
            return false;
        }

        Account storage userAccount = accounts[msg.sender];

        userAccount.referrer = referrer;
        accounts[referrer].sons.push(msg.sender);

        emit RegisteredReferer(msg.sender, referrer);
        return true;
    }

    function isCircularReference(address referrer, address referee)
        internal
        view
        returns (bool)
    {
        address parent = referrer;

        for (uint256 i = 0; i < 2; i++) {
            if (parent == address(0)) {
                break;
            }

            if (parent == referee) {
                return true;
            }

            parent = accounts[parent].referrer;
        }

        return false;
    }

    function payReferral(uint256 _value, address sender)
        internal
        whenNotPaused
        returns (uint256)
    {
        uint256 paid = 0;

        Account storage userAccount = accounts[sender];
        Account storage parentAccount = accounts[userAccount.referrer];

        uint256 grandpaReward = _value.mul(3).div(100);
        uint256 parentReward = _value.mul(7).div(100);

        if (userAccount.referrer != referredByEmpower) {
            if (
                parentAccount.referrer != address(0) &&
                userAccount.referrer != address(0)
            ) // has a L1 & a L2 Referral
            {
                paid = parentReward.add(grandpaReward);

                accounts[parentAccount.referrer].reward = accounts[
                    parentAccount.referrer
                ].reward.add(grandpaReward);
                accounts[userAccount.referrer].reward = accounts[
                    userAccount.referrer
                ].reward.add(parentReward);

                accounts[userAccount.referrer].myReferralRewards.push(
                    ReferralPayments(sender, parentReward)
                );

                accounts[parentAccount.referrer].myReferralRewards.push(
                    ReferralPayments(sender, parentReward)
                );

                BUSD.safeTransfer(parentAccount.referrer, grandpaReward);

                BUSD.safeTransfer(userAccount.referrer, parentReward);

                emit PaidReferral(
                    sender,
                    userAccount.referrer,
                    parentReward,
                    1
                );

                emit PaidReferral(
                    sender,
                    parentAccount.referrer,
                    grandpaReward,
                    2
                );

                return paid;
            } else if (userAccount.referrer != address(0)) // has a L1 Referral
            {
                paid = parentReward;

                accounts[userAccount.referrer].reward = accounts[
                    userAccount.referrer
                ].reward.add(parentReward);

                accounts[userAccount.referrer].myReferralRewards.push(
                    ReferralPayments(sender, parentReward)
                );

                BUSD.safeTransfer(userAccount.referrer, parentReward);

                emit PaidReferral(
                    sender,
                    userAccount.referrer,
                    parentReward,
                    1
                );

                return paid;
            }
        } else {
            paid = parentReward.add(grandpaReward);

            accounts[referredByEmpower].reward.add(paid);

            accounts[referredByEmpower].myReferralRewards.push(
                ReferralPayments(sender, grandpaReward)
            );

            BUSD.safeTransfer(w2, paid);

            emit PaidReferral(sender, w2, paid, 0);

            return paid;
        }

        return paid;
    }

