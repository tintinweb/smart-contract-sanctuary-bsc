pragma solidity ^0.8.5;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./TOESale.sol";
import "./WRKXToken.sol";

// TODO Remove
import "hardhat/console.sol";

contract TOETokenDistribution is Ownable {
    using SafeMath for uint256;

    WRKXToken public tokenAddress;
    TOESale public toeSale;
    address immutable private toeSaleAddress;

    uint8 public tokenDecimals;

    uint256 public startTime;

    struct PoolMultiplierPrice {
        uint256 priceChangePerStep;
        uint256[] roundPriceStart;
        uint256[] poolSteps;
        uint256[] vestingPeriod;
    }

    PoolMultiplierPrice private poolsMultiplierPrice;

    bool public tokensDistributed;

    uint256 public distributedIndex;
    uint256 public immutable distributeBatchSize;

    address[] private investors;
    uint256 public immutable normalizingDecimals;

    event LogString(string logMessage);
    event LogAddress(address logAddress);
    event LogArray(uint256[] logArray);
    event LogInt(uint256 logInt);

    constructor(address _tokenAddress, address _toeSale, uint256 _distributeBatchSize) {
        tokenAddress = WRKXToken(_tokenAddress);
        toeSaleAddress = _toeSale;
        toeSale = TOESale(_toeSale);
        tokenDecimals = tokenAddress.decimals();
        investors = toeSale.getInvestors();

        distributeBatchSize = _distributeBatchSize;
        
        normalizingDecimals = toeSale.NORMALIZING_DECIMALS();
        
        poolsMultiplierPrice.priceChangePerStep = toeSale.priceChangePerStep();

        for (uint256 i = 0; i < toeSale.roundsAmount(); i++) {
            (,uint256 priceStart,) = toeSale.rounds(i);
            poolsMultiplierPrice.roundPriceStart.push(priceStart);
        }

        for (uint256 i = 0; i < toeSale.poolStepsAmount(); i++) {
            poolsMultiplierPrice.poolSteps.push(toeSale.poolSteps(i));
        }
    }

    function totalTokens(address investor) public returns (uint256) {
        uint256[] memory totalAmount = _totalTokens(investor);

        uint256 total = 0;
        for (uint8 i = 0; i < totalAmount.length; i++) {
            total += totalAmount[i];
        }
        return total;
    }

    function distributeTokens() public onlyOwner{
        require(
            !toeSale.getAnyRoundActive(), 
            "Not all rounds are closed"
        );
        require(
            tokensDistributed == false,
            "The tokens already have been distributed"
        );
        console.log("passed require statements");
        uint256 amountToProcess;
        if(distributedIndex + distributeBatchSize > investors.length){
            amountToProcess = investors.length - distributedIndex;
            console.log("investors.length", investors.length);
            console.log("distributedIndex", distributedIndex);
            console.log("amountToProcess1", amountToProcess);
        } else {
            amountToProcess = distributeBatchSize;
            console.log("amountToProcess2", amountToProcess);
        }

        for(uint256 investor = distributedIndex; investor < (distributedIndex + amountToProcess); investor++) {
            console.log("access for loop");
            uint256 totalTokensPerInvestor = totalTokens(investors[investor]);
            tokenAddress.mint(investors[investor], totalTokensPerInvestor);     
        }
        distributedIndex += distributeBatchSize;
        if(distributedIndex >= investors.length) {tokensDistributed = true;}
    }

    function _totalTokens(address investor)
        private
        returns (uint256[] memory)
    { 
        require(investor != address(0), "the buyer address is the null address");

        (
            uint256[] memory investorAllocations,
            uint256[] memory poolAllocations
        ) = toeSale.getInvestorTotalAllocation(investor);

        uint256[] memory totalClaimable = new uint256[](
             investorAllocations.length
        );

        for (uint256 round = 0; round < investorAllocations.length; round++) {
            uint256 decimalsPower = 10**(normalizingDecimals - tokenDecimals);
            totalClaimable[round] = _calculateTokenValue(
                round,
                investorAllocations[round].div(
                    decimalsPower
                ),
                poolAllocations[round].div(
                    decimalsPower
                )
            );
        }
        return totalClaimable;
    }

    function _calculateTokenValue(
        uint256 round,
        uint256 invested,
        uint256 poolSize
    ) private returns (uint256) {
        require(
            round < poolsMultiplierPrice.roundPriceStart.length,
            "TokenDistribution: Invalid round"
        );
        uint256 initialPrice = poolsMultiplierPrice.roundPriceStart[round];

        uint256 index;
        for (
            index = 0;
            index < poolsMultiplierPrice.poolSteps.length;
            index++
        ) {
            if (poolsMultiplierPrice.poolSteps[index] > poolSize) {
                break;
            }
        }
        if (
            poolSize ==
            poolsMultiplierPrice.poolSteps[
                poolsMultiplierPrice.poolSteps.length - 1
            ]
        ) {
            index = poolsMultiplierPrice.poolSteps.length - 1;
        }
        emit LogInt(index);
        uint256 nextPrice = initialPrice.add(
            poolsMultiplierPrice.priceChangePerStep.mul(
                poolsMultiplierPrice.poolSteps.length - index
            )
        );

        uint256 deltaPoolSize = poolSize.sub(
            poolsMultiplierPrice.poolSteps[index - 1]
        );

        uint256 deltaMaxRange = poolsMultiplierPrice.poolSteps[index].sub(
            poolsMultiplierPrice.poolSteps[index - 1]
        );

        uint256 relativeDiff = deltaPoolSize
            .mul(10**4)
            .div(deltaMaxRange)
            .mul(poolsMultiplierPrice.priceChangePerStep)
            .div(10**4);

        uint256 price = nextPrice.sub(relativeDiff);

        uint256 auxTokens = invested.mul(10).div(price);

        return
            (auxTokens.mod(10) == 0 ? auxTokens.div(10) : auxTokens.div(10) + 1)
                .mul(10**tokenDecimals);
    }
}

