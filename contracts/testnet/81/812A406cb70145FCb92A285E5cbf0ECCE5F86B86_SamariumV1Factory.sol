pragma solidity ^0.8.12;

import './abstract/SamariumV1FactoryStorage.sol';
import './libraries/Constants.sol';

import 'hardhat/console.sol';

contract SamariumV1Factory {
    address public owner;

    mapping(uint8 => address) public battleFieldContracts;

    /// @dev pveLevelRecorder[msg.sender][lv] = true
    mapping(address => mapping(address => bool)) public pveLvRecorder;

    mapping(address => uint256) public balances;

    struct Command {
        address battleField;
        uint8 from;
        uint8 to;
        string cmdStr;
    }

    event BattleFieldContractUpdated(uint8 id, address indexed battleFieldContract);
    event SubmitPosted(address indexed sender, address indexed battleField, uint256 reward);

    modifier onlyOwner() {
        require(msg.sender == owner, "owner: wut?");
        _;
    }

    constructor() {
        owner = msg.sender;
    }

    function updateBattleFieldContract(uint8 _id, address _contract) external onlyOwner {
        battleFieldContracts[_id] = _contract;
        emit BattleFieldContractUpdated(_id, _contract);
    }

    function encodeCommand(address battleField, uint8 fromX, uint8 fromY, uint8 toX, uint8 toY, string calldata cmdStr) external view returns (bytes memory data, address scOut, uint8 fromOut, uint8 toOut, bytes32 cmdStrEncodedOut) {
        require(battleField != address(0));
        require(
            fromX >= Constants.MIN_X && fromX <= Constants.MAX_X &&
            toX >= Constants.MIN_X && toX <= Constants.MAX_X,
            'coordinate x out of range');
        require(
            fromY >= Constants.MIN_Y && fromY <= Constants.MAX_Y &&
            toY >= Constants.MIN_Y && toY <= Constants.MAX_Y,
            'coordinate y out of range');
        bytes32 cmdStrEncoded = keccak256(abi.encode(cmdStr));
        require(
            cmdStrEncoded == 0x427da4928556075fb703a9ae117f70f94f45bed115e984c56bd6456e3ffc62c8 ||
            cmdStrEncoded == 0xb0aab9643838bd33a5348b4aab08af1ae38d2ea508fe5c66adec0b50c53309c1 ||
            cmdStrEncoded == 0x7f2d065ef25e797049d0c8ce7f76fbf4882519b1d338133b6aac236556b48088,
            'cmdStr unknown');
        uint8 from = fromX + (fromY << 4);
        uint8 to = toX + (toY << 4);
        data = abi.encode(Command({
            battleField: battleField,
            from: from,
            to: to,
            cmdStr: cmdStr
        }));
    }

    function getBalanceOf(address userAddress) external view returns (uint256) {
        require(userAddress != address(0));
        uint256 balance = balances[userAddress];
        return balance;
    }

    function submit(bytes[] calldata data) external {
        Command memory command = abi.decode(data[0], (Command));
        address battleFieldContract = command.battleField;
        require(battleFieldContract == battleFieldContracts[0], 'battle field is non-exist');
        require(pveLvRecorder[msg.sender][battleFieldContract] == false, 'already record');
        pveLvRecorder[msg.sender][battleFieldContract] = true;
//        uint8 fromOut = command.from;
//        uint8 toOut = command.to;
//        bytes32 cmdStrEncodedOut = keccak256(abi.encode(command.cmdStr));

        balances[msg.sender] += 100;

        emit SubmitPosted(msg.sender, battleFieldContract, 100);
    }
}

