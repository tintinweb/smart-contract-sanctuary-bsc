pragma solidity ^0.8.4;

import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";

import "hardhat/console.sol";
 
contract NFTMO_MARKETPLACE is Ownable, IERC721Receiver {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;

    uint256 public _marketplaceFee;             
    uint256 public _creatorFee;
    uint256 public _divider;
    uint256 public _dividerRoyalti;
    address public _marketingAddress;   

    struct Product {
        uint256 id;
        address seller;
        address contractNFT;
        uint256 price;           
    }
    
    struct ownership {
        uint[] tokenid;
        uint[] price;
        address[] ownerNow;
        string[] status;
    }

    struct Royalti {
        address ownerToken;
        uint256 tokenId;
        uint256 price_normal;
        uint256 price_royalti;
        uint256 epoch_buy;
        uint256 epoch_last_payment;
        uint256 payment_count;
    }

    
    IERC20 private acceptedToken;

    mapping(address => bool) private _suspendBuyer;
    mapping(address => bool) private _suspendSeller;
    mapping(address => ownership) private OwnerToken;    
    mapping(address => mapping(uint256 => Product)) public assetMarketplace;
    mapping(address => mapping(uint256 => address)) private _firstSellingTokens;    
    mapping(address => mapping(uint256 => Royalti)) public royaltiMapping;

    event NFT_buy(
        address indexed contractNFT, 
        uint256 indexed tokenId,
        uint256 Id,
        address seller, 
        address buyer, 
        uint256 price
    );

    event NFT_cancel_sale(
        address indexed contractNFT, 
        uint256 indexed tokenId,
        uint256 Id,
        address seller, 
        uint256 price
    );

    event check_price(
        uint256 basePrice,
        uint256 marketingFee,
        uint256 creatorFee
    );
    
    event royalti_payment(
        address indexed contractNFT, 
        uint256 indexed tokenId,
        uint256 amountRoyalti,
        uint256 epoch_payment
    );

    constructor(address acceptedToken_, address marketingAddress_) {
        _marketplaceFee = 5;
        _creatorFee = 15;
        _divider = 100;
        _dividerRoyalti = 3;
        _marketingAddress = marketingAddress_;
        acceptedToken = IERC20(acceptedToken_);


    }

    function sellBatch(
        address contractNFT_, 
        uint256[] calldata tokenIds_, 
        uint256 price_) 
    external {
        address sender = msg.sender;
    
        require(sender != address(0), "sellNFTBatch: Selling from the zero address");
        require(_requireERC721SC(contractNFT_), "sellNFTBatch: Invalid NFT Smart Contract");
        require(price_ > 0, "sellNFTBatch: Invalid Price");
        require(!_suspendSeller[sender], "sellNFTBatch: Suspended Seller");

        for (uint256 i = 0; i < tokenIds_.length; i++) {
            address assetOwner = IERC721(contractNFT_).ownerOf(tokenIds_[i]);                           
            require(sender == assetOwner, "sellNFTBatch: sender is not asset owner");                  
            require(IERC721(contractNFT_).getApproved(tokenIds_[i]) == address(this) || IERC721(contractNFT_).isApprovedForAll(assetOwner, address(this)),  
                "sellNFTBatch: The contract is not authorized to manage the asset"                                                                                        
            );

            address firstSellingToken = _firstSellingTokens[contractNFT_][tokenIds_[i]];               
            if (firstSellingToken == address(0)) {                                                     
                _firstSellingTokens[contractNFT_][tokenIds_[i]] = sender;
                
            }

            uint256 idMarketplace = _tokenIdCounter.current();                                          
            IERC721(contractNFT_).safeTransferFrom(sender, address(this), tokenIds_[i]);                
            //trackOwnership
            OwnerToken[contractNFT_].tokenid.push(tokenIds_[i]);
            OwnerToken[contractNFT_].price.push(price_);
            OwnerToken[contractNFT_].ownerNow.push(sender);
            OwnerToken[contractNFT_].status.push("SELL");
            assetMarketplace[contractNFT_][tokenIds_[i]] = Product(idMarketplace, sender, contractNFT_, price_);  

            _tokenIdCounter.increment();
        }

    }

    function buyNFT(address contractNFT_, uint256 idToken_, uint256 price_) external {
        address sender = msg.sender;
        uint256 epochNow = block.timestamp;
     
        require(sender != address(0), "buyNFT: buying from the zero address");
        require(_requireERC721SC(contractNFT_), "buyNFT: Invalid NFT Smart Contract");
        require(price_ > 0, "buyNFT: Invalid Price");
        require(!_suspendBuyer[sender], "buyNFT: Suspended Buyer");

        Product memory product = assetMarketplace[contractNFT_][idToken_];

        require(product.seller != sender, "buyNFT: Sender is seller");
        require(product.price == price_, "buyNFT: Price mismatch");

        uint256 marketplaceFeeAmount;
        uint256 creatorFeeAmount;
        uint256 baseAmount;
        address firstSellingTokenAddr = _firstSellingTokens[contractNFT_][idToken_];  


        //CALCULATE FEE Marketplace
        if(_marketplaceFee > 0) {                                                       
            marketplaceFeeAmount = (_marketplaceFee * price_) / _divider;
        }
        
        //CALCULATE FEE RoyaltiMaker
        if(_creatorFee > 0) {                                                      
            creatorFeeAmount = (_creatorFee * price_) / _divider;
        }
        unchecked { 
            baseAmount = price_ - (marketplaceFeeAmount + creatorFeeAmount);
        }
       
        acceptedToken.transferFrom(sender, _marketingAddress, marketplaceFeeAmount);    
        acceptedToken.transferFrom(                                                    
            sender,
            firstSellingTokenAddr, 
            creatorFeeAmount
        );

        acceptedToken.transferFrom(sender, product.seller, baseAmount);                 

        IERC721(product.contractNFT).safeTransferFrom(address(this), sender, idToken_); 
        
        OwnerToken[product.contractNFT].tokenid.push(idToken_);
        OwnerToken[product.contractNFT].price.push(price_);
        OwnerToken[product.contractNFT].ownerNow.push(sender);
        OwnerToken[product.contractNFT].status.push("BUY");
        
        royaltiMapping[product.contractNFT][idToken_].ownerToken = sender;
        royaltiMapping[product.contractNFT][idToken_].tokenId = idToken_;
        royaltiMapping[product.contractNFT][idToken_].price_normal = price_;
        royaltiMapping[product.contractNFT][idToken_].price_royalti = marketplaceFeeAmount/_dividerRoyalti;
        royaltiMapping[product.contractNFT][idToken_].epoch_buy = epochNow;
        royaltiMapping[product.contractNFT][idToken_].epoch_last_payment = epochNow;
        royaltiMapping[product.contractNFT][idToken_].payment_count = 0;
        

        delete assetMarketplace[contractNFT_][idToken_];

        emit check_price(baseAmount, marketplaceFeeAmount, _creatorFee);
        emit NFT_buy(contractNFT_, idToken_, product.id, product.seller, sender, price_);
    }

    function cancelSell(address contractNFT_, uint256[] calldata tokenIds_ ) external {
        address sender = msg.sender;
        require(!_suspendSeller[sender], "cancelNFT : Suspended Seller");
        
        for (uint256 i = 0; i < tokenIds_.length; i++) {
            Product memory product = assetMarketplace[contractNFT_][tokenIds_[i]];
            
            require(product.seller == sender, "cancelNFT: Sender is not owner");
            IERC721(contractNFT_).safeTransferFrom(address(this), product.seller, tokenIds_[i]);
            
            OwnerToken[contractNFT_].tokenid.push(tokenIds_[i]);
            OwnerToken[contractNFT_].price.push(0);
            OwnerToken[contractNFT_].ownerNow.push(product.seller);
            OwnerToken[contractNFT_].status.push("CANCEL");
                
            delete assetMarketplace[contractNFT_][tokenIds_[i]];
            emit NFT_cancel_sale(contractNFT_, tokenIds_[i], product.id, product.seller, product.price);
        }
    }


    function royaltiPayment(address contractNFT_, uint256 tokenId_) public onlyOwner returns(bool) {
        address sender = msg.sender;
        uint256 epochNow = block.timestamp;
        uint256 amonth = 2592000;

        Royalti memory royalti = royaltiMapping[contractNFT_][tokenId_];
        uint256 amountRoyalti = royalti.price_royalti;
        uint256 epoch_last_payment = royalti.epoch_last_payment;
        address owner = royalti.ownerToken;
        
        require(epochNow >= (epoch_last_payment + amonth), "royaltiPayment: invalid time to pay royalti, please check last payment");
        acceptedToken.approve(address(this), amountRoyalti);
        acceptedToken.transferFrom(sender, owner, amountRoyalti);  
        royaltiMapping[contractNFT_][tokenId_].payment_count = royalti.payment_count + 1; 
        royaltiMapping[contractNFT_][tokenId_].epoch_last_payment = epochNow; 
        
        emit royalti_payment(contractNFT_, tokenId_, amountRoyalti, epochNow);
        return true;
        
    }

    function getOwnerToken(address contractNFT_) public view returns(ownership memory) {
        return OwnerToken[contractNFT_];
    }

    function _requireERC721SC(address nftAddress) internal view returns (bool)  {
        return Address.isContract(nftAddress);
    }

    function changeMarketPlaceFee(uint256 marketplaceFee_) public onlyOwner {
        _marketplaceFee = marketplaceFee_;
    }

    function changeDividerRoyalti(uint256 dividerRoyalti_) public onlyOwner {
        _dividerRoyalti = dividerRoyalti_;
    }

    function changeCreatorFee(uint256 creatorFee_) public onlyOwner {
        _creatorFee = creatorFee_;
    }

    function changeSuspendSellerStatus(address user_, bool status_) public onlyOwner{
        _suspendSeller[user_]=status_;
    }

    function changeSuspendBuyerStatus(address user_, bool status_) public onlyOwner{
        _suspendBuyer[user_]=status_;
    }
    
    function checkStatusSuspendSeller(address address_) public view returns(bool) {
        return _suspendSeller[address_];
    } 

    function checkStatusSuspendBuyer(address address_) public view returns(bool) {
        return _suspendBuyer[address_];
    } 

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }



}

