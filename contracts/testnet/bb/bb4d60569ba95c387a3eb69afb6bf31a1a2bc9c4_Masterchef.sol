pragma solidity 0.6.12;

contract Masterchef is ContextUpgradeable, OwnableUpgradeable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;
    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
        uint256 lastRewardTimestamp;
    }

    // Info of each pool.
    struct PoolInfo {
        IBEP20 lpToken; // Address of LP token contract.
        uint16 depositFeeBP; // Deposit fee in basis points
        bool istoken;
    }

    // The ARYASWAP TOKEN!
    ARYASWAP public arya;
    // MEDAX tokens created per block.
    uint256 public aryaApy;
    // Bonus muliplier for early arya makers.
    uint256 public constant BONUS_MULTIPLIER = 1;
    // Deposit Fee address
    address public feeAddress;

    uint256 public perDaysecond;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // The block number when MEDAX mining starts.
    uint256 public startBlock;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(
        address indexed user,
        uint256 indexed pid,
        uint256 amount
    );

    constructor() public {}

    function initialize(
        ARYASWAP _arya,
        address _feeAddress,
        uint256 _aryaApy,
        uint256 _perDaysecond,
        uint256 _startBlock
    ) public initializer {
        arya = _arya;
        feeAddress = _feeAddress;
        aryaApy = _aryaApy;
        perDaysecond = _perDaysecond;
        startBlock = _startBlock;
        _owner = msg.sender;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    function add(
        IBEP20 _lpToken,
        uint16 _depositFeeBP,
        bool _istoken
    ) public onlyOwner {
        require(
            _depositFeeBP <= 10000,
            "add: invalid deposit fee basis points"
        );
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                depositFeeBP: _depositFeeBP,
                istoken: _istoken
            })
        );
    }

    function set(
        uint256 _pid,
        uint16 _depositFeeBP,
        bool _istoken
    ) public onlyOwner {
        require(
            _depositFeeBP <= 10000,
            "set: invalid deposit fee basis points"
        );
        poolInfo[_pid].depositFeeBP = _depositFeeBP;
        poolInfo[_pid].istoken = _istoken;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to)
        internal
        pure
        returns (uint256)
    {
        return _to.sub(_from).mul(BONUS_MULTIPLIER);
    }

    function pendingArya(uint256 _pid, address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 aryaReward = 0;
        if (block.timestamp > user.lastRewardTimestamp && user.amount > 0) {
            uint256 multiplier = getMultiplier(
                user.lastRewardTimestamp,
                block.timestamp
            );
            uint256 perYear = perDaysecond.mul(365);
            uint256 mpadPerSecond = aryaApy.mul(1e14).div(perYear);
            aryaReward = multiplier.mul(mpadPerSecond);
        } else {
            aryaReward = 0;
        }
        return user.amount.mul(aryaReward).div(1e18);
    }

    // Safe arya transfer function, just in case if rounding error causes pool to not have enough MEDAXs.
    function _harvest(uint256 _pid) internal {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        if (block.timestamp > user.lastRewardTimestamp && user.amount > 0) {
            uint256 multiplier = getMultiplier(
                user.lastRewardTimestamp,
                block.timestamp
            );
            uint256 perYear = perDaysecond.mul(365);
            uint256 mpadPerSecond = aryaApy.mul(1e14).div(perYear);
            uint256 aryaReward = multiplier.mul(mpadPerSecond);
            uint256 pending = user.amount.mul(aryaReward).div(1e18);
            if (pending > 0) {
                safeAryaTransfer(msg.sender, pending);
            }
        }
        user.lastRewardTimestamp = block.timestamp;
    }

    function deposit(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        _harvest(_pid);
        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(
                address(msg.sender),
                address(this),
                _amount
            );
            if (pool.depositFeeBP > 0) {
                uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
                pool.lpToken.safeTransfer(feeAddress, depositFee);
                user.amount = user.amount.add(_amount).sub(depositFee);
            } else {
                user.amount = user.amount.add(_amount);
            }
        }
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        _harvest(_pid);
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
        }
        emit Withdraw(msg.sender, _pid, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        uint256 amount = user.amount;
        user.amount = 0;
        user.rewardDebt = 0;
        pool.lpToken.safeTransfer(address(msg.sender), amount);
        emit EmergencyWithdraw(msg.sender, _pid, amount);
    }

    // Safe arya transfer function, just in case if rounding error causes pool to not have enough MEDAXs.
    function safeAryaTransfer(address _to, uint256 _amount) internal {
        uint256 aryaBal = arya.balanceOf(address(this));
        if (_amount > aryaBal) {
            arya.transfer(_to, aryaBal);
        } else {
            arya.transfer(_to, _amount);
        }
    }

    function safeAryaWithdraw(address _addr, uint256 _amount) public onlyOwner {
        uint256 aryaBal = arya.balanceOf(address(this));
        if (_amount > aryaBal) {
            arya.transfer(_addr, aryaBal);
        } else {
            arya.transfer(_addr, _amount);
        }
    }

    function setFeeAddress(address _feeAddress) public {
        require(msg.sender == feeAddress, "setFeeAddress: FORBIDDEN");
        feeAddress = _feeAddress;
    }

    function updateEmissionRate(uint256 _aryaApy) public onlyOwner {
        aryaApy = _aryaApy;
    }
}