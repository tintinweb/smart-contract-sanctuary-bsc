contract ProxyStake is Initializable, OwnableUpgradeable, UUPSUpgradeable {
    address public seed ;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    uint256 public poolCount ;
     IERC20 public Lfi;
    mapping (uint256 => poolDetails) public poolInfo ;
    mapping(address => bool) public adminList;
    mapping(address => uint256[]) private stakeHolderInfo; 
    event MultiSend(address indexed sender, address token, uint256 total);
 
    struct poolDetails{
        uint256 amountLimit ;
        uint256 lfiRequire;
        uint256 validity ;
        uint256 stakedAmount ;
    }

    constructor() {
        _disableInitializers();
    }

    function initialize() initializer public {
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function setToken(address _lfi) public onlyOwner{
        Lfi    = IERC20(_lfi);
    }

    function stake(address  account ,uint256[] calldata _nftId , uint256 _amount , uint256 poolid ) public{

     uint i ;
     require(owner() == msg.sender || adminList[msg.sender] == true,"address cannot mint" );
      require( poolInfo[poolid].amountLimit > (poolInfo[poolid].stakedAmount + _amount) , "  pool is full try other pool") ;
     Lfi.transferFrom(msg.sender, address(this), _amount);
         uint[] storage array = stakeHolderInfo[account];
    for (i = 0; i < array.length; i++) {
        require(array[i] != _nftId[i], "this nft id  already staked");
    }
     stakeHolderInfo[account].push(_nftId[i]);
     poolInfo[poolid].stakedAmount += _amount;
    }

    function createPool( uint256 _amountLimit, uint256 _minToken , uint256 _validity ) public onlyOwner {
     poolInfo[poolCount].amountLimit = _amountLimit;
     poolInfo[poolCount].lfiRequire =  _minToken ;
     poolInfo[poolCount].validity =  _validity ;
     poolCount ++;
    }

        function viewStakeNFT(address addr) public view returns (uint256[] memory) {
        return stakeHolderInfo[addr];
    }

 function multiSendTokenDifferentAmount(
        address token,
        address[] memory _recipients,
        uint256[] memory _amounts
    ) public  {
            require(
            owner() == msg.sender || adminList[msg.sender] == true,
            "address cannot mint"
        );
        require(_recipients.length == _amounts.length, 'error size');
        uint256 total = 0;
        for (uint256 i = 0; i < _recipients.length; i++) {
            IERC20(token).safeTransferFrom(msg.sender, _recipients[i], _amounts[i]);
            total = total.add(_amounts[i]);
        }
        emit MultiSend(msg.sender, token, total);
    }

        function multiSendTokenFixedAmount(
        address token,
        address[] memory _recipients,
        uint256 _amounts
    ) public  {

                require(
            owner() == msg.sender || adminList[msg.sender] == true,
            "address cannot mint"
        );
     uint256 sendAmount = _recipients.length.sub(1).mul(_amounts);
        for (uint256 i = 0; i < _recipients.length; i++) {
            IERC20(token).safeTransferFrom(msg.sender, _recipients[i], _amounts);

        }
        emit MultiSend(msg.sender, token, sendAmount);
    }

   
    function emergencyWithdrawEther() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function emergencyWithdrawErc20(address tokenAddress) public onlyOwner {
        IERC20 token = IERC20(tokenAddress);
        token.safeTransfer(msg.sender, token.balanceOf(address(this)));
    }

        function addNRemoveAdmin(address[] memory _addr, bool flag)
        public
        onlyOwner
    {
        for (uint256 i; i < _addr.length; i++) {
            adminList[_addr[i]] = flag;
        }
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
