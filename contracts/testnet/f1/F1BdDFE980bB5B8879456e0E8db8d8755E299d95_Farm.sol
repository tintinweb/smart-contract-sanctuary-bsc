pragma solidity ^0.8.4;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./Seed.sol";


contract SeedFactory is Ownable{

    address public shop;

    address public farm;

    mapping(string => mapping(string => mapping(uint256 => mapping(uint256 => address)))) public getSeed;

    address[] public allSeeds;

    event CreateSeed(address indexed seed);


    constructor (address _shop,address _farm) {
        shop = _shop;
        farm = _farm;
    }

    function createSeed(string memory name,
        string memory symbol,
        uint256 yield,
        uint256 matureTime) external onlyOwner returns(address seed){

        bytes memory bytecode = type(Seed).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(name, symbol,yield,matureTime));
        assembly {
            seed := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        ISeed(seed).initialize(name, symbol,yield,matureTime,shop,farm);

        getSeed[name][symbol][yield][matureTime] = seed;
        allSeeds.push(seed);
        emit CreateSeed(seed);
    }

}

