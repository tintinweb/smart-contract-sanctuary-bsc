    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation.
     */
    function toHexString(uint256 value) internal pure returns (string memory) {
        if (value == 0) {
            return "0x00";
        }
        uint256 temp = value;
        uint256 length = 0;
        while (temp != 0) {
            length++;
            temp >>= 8;
        }
        return toHexString(value, length);
    }

    /**
     * @dev Converts a `uint256` to its ASCII `string` hexadecimal representation with fixed length.
     */
    function toHexString(uint256 value, uint256 length) internal pure returns (string memory) {
        bytes memory buffer = new bytes(2 * length + 2);
        buffer[0] = "0";
        buffer[1] = "x";
        for (uint256 i = 2 * length + 1; i > 1; --i) {
            buffer[i] = _HEX_SYMBOLS[value & 0xf];
            value >>= 4;
        }
        require(value == 0, "Strings: hex length insufficient");
        return string(buffer);
    }

    /**
     * @dev Converts an `address` with fixed length of 20 bytes to its not checksummed ASCII `string` hexadecimal representation.
     */
    function toHexString(address addr) internal pure returns (string memory) {
        return toHexString(uint256(uint160(addr)), _ADDRESS_LENGTH);
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.9;

// Import this file to use console.log
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

contract Wallet {
	using SafeMath for uint256;

	mapping(uint256 => address) private tx_tokens;
	mapping(address => uint256) private balances;
	mapping(address => bool) private account_available;
	mapping(address => string) private services;

	address[] accounts;
	address public owner;

	// Events
	event NewAccount(address indexed account);
	event DepositMade(address indexed account, uint256 amount);
	event WithdrawMade(address indexed account, uint256 amount);
	event ServiceDepositMade(address indexed service, address indexed account, uint256 amount);
	event ServiceWithdrawMade(address indexed service, address indexed account, uint256 amount);

	modifier serviceOnly() {
		require(bytes(services[msg.sender]).length > 0, "service not found");
		_;
	}

	modifier accountAvailable(address account) {
		require(account_available[account], "account not found");
		_;
	}

	modifier validDepositAmount() {
		require(msg.value > 0, "invalid amount");
		_;
	}

	modifier validWithdrawAmount(uint256 amount) {
		require(amount > 0, "invalid amount");
		_;
	}

	modifier validBalance(address account, uint256 amount) {
		require(balances[account] >= amount, "balance is not enough");
		_;
	}

	// run only once when deploy the contract
	constructor() {
		owner = msg.sender;
	}

	// public deposit allow deposit from anywhere
	function deposit() external payable validDepositAmount returns (uint256) {
		// Record account into array
		if (!account_available[msg.sender]) {
			accounts.push(msg.sender);
			account_available[msg.sender] = true;
			emit NewAccount(msg.sender);
		}

		balances[msg.sender] = balances[msg.sender].add(msg.value);

		emit DepositMade(msg.sender, msg.value);
		return balances[msg.sender];
	}

	// public withdraw allow deposit from anywhere
	function withdraw(uint256 amount)
		external
		payable
		validWithdrawAmount(amount)
		accountAvailable(msg.sender)
		validBalance(msg.sender, amount)
		returns (uint256)
	{
		balances[msg.sender] = balances[msg.sender].sub(amount);

		// Send money back to user
		payable(msg.sender).transfer(amount);

		// emit event
		emit WithdrawMade(msg.sender, amount);

		return balances[msg.sender];
	}

	function accountBalance()
		external
		view
		accountAvailable(msg.sender)
		returns (uint256)
	{
		return balances[msg.sender];
	}

	function externalAccountBalance(address account)
		external
		view
		serviceOnly
		accountAvailable(account)
		returns (uint256)
	{
		return balances[account];
	}

	// external createToken allow only registered service
	// to create one time token to do withdraw or deposit
	// the sender of to call this function must be a service
	function createToken() external serviceOnly returns (uint256) {
		uint256 token = generateToken(msg.sender);
		tx_tokens[token] = msg.sender;
		return token;
	}

	// external serviceDeposit allow only registered service to transfer from service to specificed account
	function serviceDeposit(address account)
		external
		payable
		validDepositAmount
		serviceOnly
		returns (bool)
	{
		balances[account] = balances[account].add(msg.value);

		emit ServiceDepositMade(msg.sender, account, msg.value);
		return true;
	}

	// external serviceDeposit allow only registered service to withdraw from Wallet to service and deduct balance from specificed account
	function serviceWithdraw(address account, uint256 amount)
		external
		serviceOnly
		accountAvailable(account)
		validBalance(account, amount)
		returns (bool)
	{
		balances[account] = balances[account].sub(amount);

		// Send to service's address since we allow only service to call this function
		payable(msg.sender).transfer(amount);

		// emit event
		emit ServiceWithdrawMade(msg.sender, account, amount);
		return true;
	}

	function balance() public view returns (uint256) {
		return address(this).balance;
	}

	//--------------------------------------
	// ADMIN
	//--------------------------------------

	// external registerService for admin to register service to allow registered services to do
	// 1. create withdraw or deposit token
	// 2. withdraw from registered service
	// 3. deposit from registered service
	function registerService(address _address, string calldata service_name)
		external
		returns (bool)
	{
		require(bytes(service_name).length > 0, "not authorized");
		require(owner == msg.sender, "not authorized");
		string memory service = services[_address];
		require(bytes(service).length == 0, "service already registered");
		services[_address] = service_name;
		return true;
	}

	//--------------------------------------
	//private
	//--------------------------------------
	function generateToken(address _address) private view returns (uint256) {
		return
			uint256(
				keccak256(abi.encodePacked(block.difficulty, block.timestamp, _address))
			);
	}

	function destroyToken(uint256 token) private returns (bool) {
		delete tx_tokens[token];
		return true;
	}
}