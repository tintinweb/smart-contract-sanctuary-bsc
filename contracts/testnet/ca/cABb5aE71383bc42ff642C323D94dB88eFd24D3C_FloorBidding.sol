import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./Wallet.sol";

enum GameType {
	Game_0005,
	Game_0010,
	Game_0100,
	Game_0200,
	Game_0500,
	Game_1000
}

contract FloorBidding {

	using SafeMath for uint256;

	event NewGame(
		GameType indexed gameType,
		uint16 indexed gameId
	);

	event AnnounceWinner(
		GameType indexed gameType,
		uint16 indexed gameId,
		address indexed winner
	);

	event AnnounceBet(
		GameType indexed gameType,
		uint16 indexed gameId,
		address indexed bettor,
		uint32 number
	);

	event AnnounceWinnerChange(
		GameType indexed gameType,
		uint16 indexed gameId,
		address indexed bettor
	);

	event AnnounceParticipant(
		GameType indexed gameType,
		uint16 indexed gameId,
		address indexed bettor
	);

	// TODO: Let's consider remove this event
	event AcknowledgeBet(
		GameType indexed gameType,
		uint16 indexed gameId,
		address indexed bettor
	);

	event GameStart(GameType indexed gameType, uint16 indexed gameId);

	event GameEnd(GameType indexed gameType, uint16 indexed gameId);

	event Received(address indexed sender, uint256 indexed amount);

	enum GameStatus {
		Waiting,
		Start,
		Stop,
		Pause
	}

	struct Game {
		GameType gameType;
		uint16 gameId;
		uint24 duration;
		uint256 price;
		uint256 startedAt;
		uint256 prize;
		GameStatus status;
	}

	struct WinnerNUmber {
		address winner;
		uint32 number;
	}

	address payable public owner;
	address private wallet;
	// mapping for emum of game type to game name
	mapping(GameType => string) private gamesTypeName;
	// collection of game type to created game
	// since each game type will have only one game data at all time
	mapping(GameType => Game) private gamesPool;
	// collection of game key to nested mapping of betted number to array of bettor's address
	// such as {"0005_1": {1: [address1, address2], 2: [address1]}, "0010": {1: [address100, address200]}}
	mapping(string => mapping(uint32 => address[])) private gamesNumberAddresses;
	// collection of game key to unique betted number
	// such ass {"0005_1": [1, 2, 3, 5, ...], "0010_1": [1, ...]}
	mapping(string => uint32[]) private gamesUniqueNumbers;
	mapping(string => WinnerNUmber) private gamesWinner;

	error TransferError(GameType gameType, uint16 gameId);

	constructor(address _wallet) payable {
		owner = payable(msg.sender);
		wallet = _wallet;
		gamesPool[GameType.Game_0005] = createGame(GameType.Game_0005, 0, 0.00000000005 ether, 0);
		gamesPool[GameType.Game_0010] = createGame(GameType.Game_0010, 0, 0.0000000001 ether, 0);
		gamesPool[GameType.Game_0100] = createGame(GameType.Game_0100, 0, 0.000000001 ether, 0);
		gamesPool[GameType.Game_0200] = createGame(GameType.Game_0200, 0, 0.000000002 ether, 0);
		gamesPool[GameType.Game_0500] = createGame(GameType.Game_0500, 0, 0.000000005 ether, 0);
		gamesPool[GameType.Game_1000] = createGame(GameType.Game_1000, 0, 0.00000001 ether, 0);

		gamesTypeName[GameType.Game_0005] = "0005";
		gamesTypeName[GameType.Game_0010] = "0010";
		gamesTypeName[GameType.Game_0100] = "0100";
		gamesTypeName[GameType.Game_0200] = "0200";
		gamesTypeName[GameType.Game_0500] = "0500";
		gamesTypeName[GameType.Game_1000] = "1000";
	}

	receive() external payable {
		emit Received(msg.sender, msg.value);
  	}

	function startGame(GameType gameType) public {
		Game memory game = gamesPool[gameType];
		// TODO: use openzepplin Ownable
		require(msg.sender == owner, "only owner can start the game");
		require(
			game.status == GameStatus.Waiting || game.status == GameStatus.Stop,
			"invalid game status"
		);

		game.status = GameStatus.Start;
		game.startedAt = block.timestamp;
		gamesPool[gameType] = game;

		emit GameStart(gameType, game.gameId);
	}

	function endGame(GameType gameType) public {
		// TODO: use openzepplin Ownable
		require(msg.sender == owner, "only owner can end the game");
		Game memory game = gamesPool[gameType];
		require(
			game.status == GameStatus.Start &&
				game.startedAt + game.duration <= block.timestamp,
			"game can't be stop at the moment"
		);
		string memory gameKey = getKey(gameType, game);
		revealBet(gameType, gameKey, game.gameId);
		emit GameEnd(gameType, game.gameId);
		revealWinner(gameType, gameKey, game.gameId);

		bool success = transferAndResetGamePrize(game, gameKey);

		if (!success) revert TransferError(gameType, game.gameId);

		gamesPool[gameType] = createGame(
			gameType,
			uncheckedIncrease16(game.gameId),
			game.price,
			game.prize
		);
	}

	function bet(GameType gameType, uint32 number) public {
		require(msg.sender != owner, "won't allow owner to bet");
		Game memory game = gamesPool[gameType];
		require(
			game.status == GameStatus.Start &&
				game.startedAt + game.duration >= block.timestamp,
			"game is not available"
		);
		require(getBalance() > game.price, "can't deduct from wallet");

		string memory gameKey = getKey(gameType, game);
		WinnerNUmber memory winnerNumber = gamesWinner[gameKey];
		manageGameUniqueNumber(gameKey, number);

		// add bettor address to list of bettor who bet on the number in given game's key
		gamesNumberAddresses[gameKey][number].push(msg.sender);
		bool success = deductFromBettor(game);
		require(success, "can't deduct");

		// accumulate bet price to game prize
		gamesPool[gameType].prize = game.prize.add(game.price);

		emit AcknowledgeBet(gameType, game.gameId, msg.sender);

		(uint256 currentWinNumber, address currentWinner) = getSetWinner(gameKey);
		if (currentWinner == msg.sender && currentWinNumber == number) {
			revealWinner(gameType, gameKey, game.gameId);
		} else {
			emit AnnounceParticipant(gameType, game.gameId, msg.sender);
		}

		if (winnerNumber.winner != currentWinner) {
			emit AnnounceWinnerChange(gameType, game.gameId, winnerNumber.winner);
		}
	}

	function getGame(GameType gameType) external view returns (Game memory) {
		return gamesPool[gameType];
	}

	// ---------------------------- private ----------------------------

	function createGame(GameType gameType, uint16 id, uint256 _price, uint256 _prize)
		private
		returns (Game memory)
	{
		emit NewGame(gameType, id);
		return
			Game({
				gameType: gameType,
				gameId: id,
				price: _price,
				startedAt: 0,
				duration: 1 days,
				status: GameStatus.Waiting,
				prize: _prize
			});
	}

	function getKey(GameType gameType, Game memory game) private view returns (string memory) {
		uint16 id = game.gameId;
		return string(abi.encodePacked(gamesTypeName[gameType], "-", Strings.toString(id)));
	}

	// findUpperBound is a forked function base on openzepplin Arrays.findUpperBound.
	// This functon purpose is to search for the index of existing value which has equal or higher value
	// compare to the given value.
	function findUpperBound(uint32[] memory array, uint32 element) internal pure returns (uint256) {
		if (array.length == 0) {
			return 0;
		}

		uint256 low = 0;
		uint256 high = array.length;

		while (low < high) {
			uint256 mid = Math.average(low, high);

			// Note that mid will always be strictly less than high (i.e. it will be a valid array index)
			// because Math.average rounds down (it does integer division with truncation).
			if (array[mid] > element) {
				high = mid;
			} else {
				low = uncheckedIncrease256(mid);
			}
		}

		// At this point `low` is the exclusive upper bound. We will return the inclusive upper bound.
		if (low > 0 && array[uncheckedDecrease256(low)] == element) {
			return uncheckedDecrease256(low);
		} else {
			return low;
		}
	}

	// manageGameUniqueNumber will push number to gamesUniqueNumbers of the given gameKey
	// and make sure that list in gamesUniqueNumbers are sorted
	function manageGameUniqueNumber(string memory gameKey, uint32 number) private {
		address[] memory bettorsOnNumber = gamesNumberAddresses[gameKey][
			number
		];
		// To keep gamesUniqueNumbers[gameKey] be a unique array needed this line
		// to check if the given number exists then do nothing
		if (bettorsOnNumber.length > 0) return;

		uint32[] memory tempGameNumber = gamesUniqueNumbers[gameKey];

		uint256 index = findUpperBound(tempGameNumber, number);

		uint32 store;
		for (uint256 i = index; i < tempGameNumber.length; i = uncheckedIncrease256(i)) {
			store = tempGameNumber[i];
			tempGameNumber[i] = number;
			number = store;
		}
		gamesUniqueNumbers[gameKey] = tempGameNumber;
		gamesUniqueNumbers[gameKey].push(number);

		// console.log("check");
		// for (uint256 i = 0; i < gamesUniqueNumbers[gameKey].length; i = uncheckedIncrease256(i)) {
		// 	console.log(gamesUniqueNumbers[gameKey][i]);
		// }
	}

	function getSetWinner(string memory gameKey)
		private
		returns (uint256, address)
	{
		// expect gameNumber to always sort before this function call
		uint32[] memory gameNum = gamesUniqueNumbers[gameKey];
		for (uint256 i = 0; i < gameNum.length; i = uncheckedIncrease256(i)) {
			uint32 num = gameNum[i];
			address[] memory gameNumAddresses = gamesNumberAddresses[gameKey][num];
			if (gameNumAddresses.length == 1) {
				address winner = gameNumAddresses[0];
				gamesWinner[gameKey] = WinnerNUmber(winner, num);
				return (num, winner);
			}
		}
		return (0, address(0));
	}

	function revealWinner(GameType gameType, string memory gameKey, uint16 gameId) private {
		WinnerNUmber memory winnerNumber = gamesWinner[gameKey];
		if (winnerNumber.winner != address(0)) {
			emit AnnounceWinner(gameType, gameId, winnerNumber.winner);
		}
	}

	function revealBet(GameType gameType, string memory gameKey, uint16 gameId) private {
		uint32[] memory gameNum = gamesUniqueNumbers[gameKey];
		for (uint256 i = 0; i < gameNum.length; i = uncheckedIncrease256(i)) {
			uint32 number = gameNum[i];
			address[] memory gameNumAddresses = gamesNumberAddresses[gameKey][number];
			for (uint256 j = 0; j < gameNumAddresses.length; j = uncheckedIncrease256(j)) {
				address bettor = gameNumAddresses[j];
				emit AnnounceBet(gameType, gameId, bettor, number);
			}
		}
	}

	function getBalance() private view returns (uint256) {
		uint256 balance = Wallet(wallet).externalAccountBalance(msg.sender);
		return balance;
	}

	function getGamePrize(Game memory game)  private pure returns (uint256, uint256) {
		uint256 all = game.prize;
		uint256 prize = all.mul(80).div(100);
		uint256 fee = all.sub(prize);
		return (prize, fee);
	}

	function deductFromBettor(Game memory game) private returns (bool) {
		bool success = Wallet(wallet).serviceWithdraw(msg.sender, game.price);
		return success;
	}

	function transferAndResetGamePrize(Game memory game, string memory gameKey) private returns (bool) {
		WinnerNUmber memory winnerNumber = gamesWinner[gameKey];

		// if no winner then do nothing
		if (winnerNumber.winner ==  address(0)) return true;

		(uint256 prize, uint256 fee) = getGamePrize(game);
		// let's wallet transfer balance to winner on behave of FloorBidding's wallet
		bool success = Wallet(wallet).serviceDeposit{value: prize}(winnerNumber.winner);

		// TODO: recheck prize and fee
		// NOTE: Reset game prize to 0 once transfer then fee will go nowhere but this account

		game.prize = 0;

		// NOTE: Not yet transfer game's fee to other wallet then game fee will belong to FloorBidding's wallet
		emit Received(address(this), fee);

		return success;
	}

	// TODO: move below utilities functions to library
	function uncheckedIncrease16(uint16 value) private pure returns (uint16) {
		unchecked { return value + 1; }
	}

	function uncheckedIncrease32(uint16 value) private pure returns (uint32) {
		unchecked { return value + 1; }
	}

	function uncheckedIncrease256(uint256 value) private pure returns (uint256) {
		unchecked { return value + 1; }
	}

	function uncheckedDecrease256(uint256 value) private pure returns (uint256) {
		unchecked { return value - 1; }
	}
	// -----------------------------------------------
}

