pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "./MetaproMetaAsset.sol";
import "./libraries/Royalty.sol";
import "./libraries/Referral.sol";
import "hardhat/console.sol";

contract MetaproBuy is Ownable, ReentrancyGuard, ERC1155Holder {
    using SafeMath for uint256;

    struct BuyAuction {
        uint256 tokenId;
        uint256 tokenPrice;
        uint256 tokenQuantity;
        uint256 startBlock;
        uint256 endBlock;
        bool multipleDeposits;
        uint256 tokensSold;
        address operator;
        bool valid;
        uint256 auctionId;
    }

    struct BuyNowDeposit {
        // @dev: principal wallet address
        address wallet;
        // @dev: deposit amount in busd
        uint256 amount;
    }

    struct TokenBuyAuction {
        uint256 tokenId;
        address tokenContractAddress;
    }

    // Contracts
    IERC20 public busd;
    MetaproReferral public metaproReferral;
    MetaproRoyalty public metaproRoyalty;

    //Contract addresses
    address private busdAddress;
    address public tressuryAddress;
    address private referralAddress;
    address private royaltyAddress;

    mapping(uint256 => BuyAuction) public buyAuction;

    // @dev: Buy auction token contract address auctionId => contractAddress
    mapping(uint256 => address) private buyAuctionTokenContractAddress;

    // @dev: auctionID => Referral.ReferralFees
    mapping(uint256 => Referral.ReferralFees) public buyReferralFees;

    // @dev: auctionId => Royalty.RoyaltyTeamMember[]
    mapping(uint256 => Royalty.RoyaltyTeamMember[]) private buyRoyaltyFees;

    // @dev: Tressury fee - interger value - example: 500 -> 5%
    uint256 public treasuryFee = 500; // 500 = 5%

    // @dev: auctionId => BuyNowDeposit[]
    mapping(uint256 => BuyNowDeposit[]) private buyAuctionDeposits;

    uint256[] private createdBuyAuctionIds;

    TokenBuyAuction[] private activeBuyAuctionTokenIds;

    TokenBuyAuction[] private finishedBuyAuctionTokenIds;

    uint256 private auctionCount = 1;

    event BuyAuctionCreated(
        address indexed tokenContractAddress,
        uint256 indexed tokenId,
        uint256 tokenPrice,
        uint256 tokenQuantity,
        uint256 startBlock,
        uint256 endBlock,
        bool multipleDeposits,
        address operator,
        uint256 indexed auctionId
    );
    event Buy(
        address indexed tokenContractAddress,
        uint256 indexed tokenId,
        uint256 indexed auctionId,
        uint256 tokenQuantity,
        address referrer
    );
    event CloseAuction(uint256 tokenId, uint256 auctionId);

    event TreasuryFeeUpdated(uint256 _fee);
    event BUSDAddressUpdated(address _address);
    event TressuryAddressUpdated(address _address);
    event ReferralAddressUpdated(address _address);
    event RoyaltyAddressUpdated(address _address);

    constructor(
        address _busdAddress,
        address _tressuryAddress,
        address _referralAddress,
        address _royaltyAddress
    ) {
        busd = IERC20(_busdAddress);
        tressuryAddress = _tressuryAddress;
        metaproReferral = MetaproReferral(_referralAddress);
        referralAddress = _referralAddress;
        metaproRoyalty = MetaproRoyalty(_royaltyAddress);
        royaltyAddress = _royaltyAddress;
    }

    function createAuction(
        address _tokenContractAddress,
        uint256 _tokenId,
        uint256 _tokenPrice,
        uint256 _tokenQuantity,
        uint256 _startBlock,
        uint256 _endBlock,
        uint256 _level1ReferralFee,
        uint256 _level2ReferralFee,
        uint256 _level3ReferralFee,
        bool _multipleDeposits,
        bytes memory _data
    ) public nonReentrant returns (uint256) {
        require(
            Address.isContract(_tokenContractAddress),
            "Provide valid contract address"
        );
        require(
            IERC1155(_tokenContractAddress).balanceOf(msg.sender, _tokenId) >=
                _tokenQuantity,
            "Insufficient ERC1155 balance"
        );
        require(_tokenId > 0, "INS: tokenId must be greater than 0");
        require(_tokenPrice > 0, "INS: pricePerToken must ge greater than 0");
        require(
            _level1ReferralFee.add(_level2ReferralFee).add(
                _level3ReferralFee
            ) <= 1500,
            "The sum of referral fees can not be greater than 15%"
        );
        require(
            _startBlock < _endBlock,
            "INS: startBlock must be less than endBlock"
        );

        IERC1155(_tokenContractAddress).safeTransferFrom(
            msg.sender,
            address(this),
            _tokenId,
            _tokenQuantity,
            _data
        );

        BuyAuction memory auctionConfiguration = BuyAuction(
            _tokenId,
            _tokenPrice,
            _tokenQuantity,
            _startBlock,
            _endBlock,
            _multipleDeposits,
            0,
            msg.sender,
            true,
            auctionCount
        );

        buyAuctionTokenContractAddress[auctionCount] = _tokenContractAddress;

        saveBuyReferralFees(
            _level1ReferralFee,
            _level2ReferralFee,
            _level3ReferralFee
        );

        saveBuyRoyaltyFees(auctionCount, _tokenId);

        buyAuction[auctionCount] = auctionConfiguration;

        activateTokenId(_tokenId, _tokenContractAddress);

        emit BuyAuctionCreated(
            _tokenContractAddress,
            _tokenId,
            _tokenPrice,
            _tokenQuantity,
            _startBlock,
            _endBlock,
            _multipleDeposits,
            msg.sender,
            auctionCount - 1
        );

        auctionCount++;

        return auctionCount - 1;
    }

    function buy(
        uint256 _auctionId,
        uint256 _tokenQuantity,
        address _referrer,
        bytes memory _data
    ) public nonReentrant {
        BuyAuction storage currentAuction = buyAuction[_auctionId];

        Referral.ReferralFees storage refFees = buyReferralFees[_auctionId];

        require(_tokenQuantity > 0, "Token quantity must be positive value");

        require(
            currentAuction.startBlock <= block.number,
            "Auction has not started yet."
        );
        require(
            currentAuction.endBlock >= block.number,
            "Auction has already ended."
        );

        uint256 tokenAmount = currentAuction.tokenQuantity -
            currentAuction.tokensSold;

        require(
            tokenAmount >= _tokenQuantity,
            "Offered quantity is greater than available tokens."
        );
        require(
            currentAuction.tokenQuantity >=
                _tokenQuantity + currentAuction.tokensSold,
            "Token quantity exceeded for this auction."
        );

        require(
            currentAuction.valid == true,
            "Can not proccess on disabled auction"
        );

        if (
            currentAuction.tokensSold + _tokenQuantity ==
            currentAuction.tokenQuantity
        ) {
            currentAuction.valid = false;
            finalizeTokenId(
                currentAuction.tokenId,
                buyAuctionTokenContractAddress[_auctionId],
                _auctionId
            );
        }

        uint256 transactionAmount = _tokenQuantity * currentAuction.tokenPrice;

        busd.transferFrom(msg.sender, address(this), transactionAmount);

        handleMultipleDepositsCheck(currentAuction, transactionAmount);

        metaproReferral.setReferral(msg.sender, _referrer);

        uint256 referralFeeAmount = sendFeesToReferrers(
            currentAuction,
            refFees,
            transactionAmount,
            msg.sender
        );

        uint256 royaltyFeeAmount = sendFeesToRoyaltyTeamMembers(
            currentAuction,
            transactionAmount
        );

        uint256 treasuryFeeAmount = (transactionAmount.mul(treasuryFee)).div(
            10000
        );

        // Send fee to trasury address
        busd.transfer(tressuryAddress, treasuryFeeAmount);

        busd.transfer(
            currentAuction.operator,
            transactionAmount -
                referralFeeAmount -
                royaltyFeeAmount -
                treasuryFeeAmount
        );

        // Send tokens to depositer
        IERC1155(buyAuctionTokenContractAddress[_auctionId]).safeTransferFrom(
            address(this),
            msg.sender,
            currentAuction.tokenId,
            _tokenQuantity,
            _data
        );

        buyAuction[_auctionId].tokensSold += _tokenQuantity;

        emit Buy(
            buyAuctionTokenContractAddress[_auctionId],
            currentAuction.tokenId,
            _auctionId,
            _tokenQuantity,
            _referrer
        );
    }

    function activateTokenId(uint256 _tokenId, address _tokenContractAddress)
        private
    {
        // To avoid duplication we need to check if active _tokenId already exists
        createdBuyAuctionIds.push(auctionCount);
        bool enableToActivate = true;
        for (uint256 i = 0; i < activeBuyAuctionTokenIds.length; i++) {
            if (
                activeBuyAuctionTokenIds[i].tokenId == _tokenId &&
                activeBuyAuctionTokenIds[i].tokenContractAddress ==
                _tokenContractAddress
            ) {
                enableToActivate = false;
            }
        }

        TokenBuyAuction memory tokenAuction = TokenBuyAuction(
            _tokenId,
            _tokenContractAddress
        );

        if (enableToActivate) {
            activeBuyAuctionTokenIds.push(tokenAuction);
        }

        // We need to back remove _tokenId from finished
        for (uint256 i = 0; i < finishedBuyAuctionTokenIds.length; i++) {
            if (
                finishedBuyAuctionTokenIds[i].tokenId == _tokenId &&
                finishedBuyAuctionTokenIds[i].tokenContractAddress ==
                _tokenContractAddress
            ) {
                delete finishedBuyAuctionTokenIds[i];
            }
        }
    }

    function finalizeTokenId(
        uint256 _tokenId,
        address _tokenContractAddress,
        uint256 _auctionId
    ) private {
        BuyAuction[] memory tokenAuctionConfigurations = getTokenAuctions(
            _tokenId
        );
        TokenBuyAuction memory tokenAuction = TokenBuyAuction(
            _tokenId,
            _tokenContractAddress
        );

        bool allInsForTokenIdFinalized = true;

        for (uint256 i = 0; i < tokenAuctionConfigurations.length; i++) {
            if (
                tokenAuctionConfigurations[i].valid &&
                tokenAuctionConfigurations[i].auctionId != _auctionId
            ) {
                allInsForTokenIdFinalized = false;
            }
        }

        if (allInsForTokenIdFinalized) {
            for (uint256 i = 0; i < activeBuyAuctionTokenIds.length; i++) {
                if (
                    activeBuyAuctionTokenIds[i].tokenId == _tokenId &&
                    activeBuyAuctionTokenIds[i].tokenContractAddress ==
                    _tokenContractAddress
                ) {
                    delete activeBuyAuctionTokenIds[i];
                    finishedBuyAuctionTokenIds.push(tokenAuction);
                }
            }
        }
    }

    function getTokenAuctions(uint256 _tokenId)
        public
        view
        returns (BuyAuction[] memory)
    {
        uint256 correctArraySize = 0;

        for (uint256 i = 0; i < createdBuyAuctionIds.length; i++) {
            if (buyAuction[createdBuyAuctionIds[i]].tokenId == _tokenId) {
                correctArraySize += 1;
            }
        }

        BuyAuction[] memory tokenAuctions = new BuyAuction[](correctArraySize);

        uint256 correctIndex = 0;
        for (uint256 i = 0; i < createdBuyAuctionIds.length; i++) {
            if (buyAuction[createdBuyAuctionIds[i]].tokenId == _tokenId) {
                tokenAuctions[correctIndex] = buyAuction[
                    createdBuyAuctionIds[i]
                ];
                correctIndex++;
            }
        }

        return tokenAuctions;
    }

    function handleMultipleDepositsCheck(
        BuyAuction memory _buyConfiguration,
        uint256 _transactionAmount
    ) private {
        BuyNowDeposit memory walletDeposit = BuyNowDeposit(
            msg.sender,
            _transactionAmount
        );

        if (_buyConfiguration.multipleDeposits) {
            buyAuctionDeposits[_buyConfiguration.auctionId].push(walletDeposit);
        } else {
            BuyNowDeposit[] memory insDeposits = buyAuctionDeposits[
                _buyConfiguration.auctionId
            ];
            bool deposited = false;
            for (uint256 i = 0; i < insDeposits.length; ++i) {
                if (insDeposits[i].wallet == msg.sender) {
                    deposited = true;
                }
            }

            if (deposited) {
                revert("MetaproBuy: doesn't support multiple deposits");
            } else {
                buyAuctionDeposits[_buyConfiguration.auctionId].push(
                    walletDeposit
                );
            }
        }
    }

    function closeAuction(uint256 _auctionId, bytes memory _data) public {
        BuyAuction storage currentAuction = buyAuction[_auctionId];
        require(
            currentAuction.operator == msg.sender,
            "Wallet must be auction operator"
        );
        require(currentAuction.valid == true, "Auction is already closed");
        currentAuction.valid = false;
        finalizeTokenId(
            currentAuction.tokenId,
            buyAuctionTokenContractAddress[_auctionId],
            _auctionId
        );
        (currentAuction);
        IERC1155(buyAuctionTokenContractAddress[_auctionId]).safeTransferFrom(
            address(this),
            msg.sender,
            currentAuction.tokenId,
            currentAuction.tokenQuantity - currentAuction.tokensSold,
            _data
        );

        emit CloseAuction(currentAuction.tokenId, _auctionId);
    }

    function getAuctionDeposits(uint256 _auctionId)
        public
        view
        returns (BuyNowDeposit[] memory)
    {
        return buyAuctionDeposits[_auctionId];
    }

    function getAuctionRoyaltyFees(uint256 _auctionId)
        public
        view
        returns (Royalty.RoyaltyTeamMember[] memory)
    {
        return buyRoyaltyFees[_auctionId];
    }

    function getActiveAuctionTokenIds()
        public
        view
        returns (TokenBuyAuction[] memory)
    {
        uint256 correctArraySize = 0;

        for (uint256 i = 0; i < activeBuyAuctionTokenIds.length; i++) {
            if (activeBuyAuctionTokenIds[i].tokenId != 0) {
                correctArraySize += 1;
            }
        }

        TokenBuyAuction[] memory activeAuctions = new TokenBuyAuction[](
            correctArraySize
        );

        uint256 correctIndex = 0;
        for (uint256 i = 0; i < activeBuyAuctionTokenIds.length; i++) {
            if (activeBuyAuctionTokenIds[i].tokenId != 0) {
                activeAuctions[correctIndex] = activeBuyAuctionTokenIds[i];
                correctIndex++;
            }
        }

        return activeAuctions;
    }

    function getFinishedAuctionTokenIds()
        public
        view
        returns (TokenBuyAuction[] memory)
    {
        uint256 correctArraySize = 0;

        for (uint256 i = 0; i < finishedBuyAuctionTokenIds.length; i++) {
            if (finishedBuyAuctionTokenIds[i].tokenId != 0) {
                correctArraySize += 1;
            }
        }

        TokenBuyAuction[] memory finishedAuctions = new TokenBuyAuction[](
            correctArraySize
        );

        uint256 correctIndex = 0;
        for (uint256 i = 0; i < finishedBuyAuctionTokenIds.length; i++) {
            if (finishedBuyAuctionTokenIds[i].tokenId != 0) {
                finishedAuctions[correctIndex] = finishedBuyAuctionTokenIds[i];
                correctIndex++;
            }
        }

        return finishedAuctions;
    }

    function getBatchAuctions(uint256[] memory _auctionIds)
        public
        view
        returns (BuyAuction[] memory)
    {
        BuyAuction[] memory batchAuctions = new BuyAuction[](
            _auctionIds.length
        );

        for (uint256 i = 0; i < _auctionIds.length; i++) {
            if (buyAuction[_auctionIds[i]].tokenId == 0) {
                revert("One of given auctionIds does not exist");
            } else {
                batchAuctions[i] = buyAuction[_auctionIds[i]];
            }
        }

        return batchAuctions;
    }

    function saveBuyReferralFees(
        uint256 _level1ReferralFee,
        uint256 _level2ReferralFee,
        uint256 _level3ReferralFee
    ) private {
        Referral.ReferralFees memory feesConfig = Referral.ReferralFees(
            _level1ReferralFee,
            _level2ReferralFee,
            _level3ReferralFee
        );
        buyReferralFees[auctionCount] = feesConfig;
    }

    function saveBuyRoyaltyFees(uint256 _auctionId, uint256 _tokenId) private {
        Royalty.RoyaltyTeamMember[] memory teamMembers = metaproRoyalty
            .getTeamMembers(
                _tokenId,
                buyAuctionTokenContractAddress[_auctionId]
            );

        Royalty.RoyaltyTeamMember[] storage royaltyBuyNowFees = buyRoyaltyFees[
            _auctionId
        ];

        for (uint256 i = 0; i < teamMembers.length; ++i) {
            royaltyBuyNowFees.push(teamMembers[i]);
        }
    }

    function depositOnReferrer(
        uint256 _auctionId,
        address _referrer,
        address _depositer,
        uint256 _amount,
        uint256 _referralFee,
        uint256 _tokenId,
        uint256 _level
    ) private returns (uint256) {
        uint256 referralFeeAmount = _amount.mul(_referralFee).div(10000);

        busd.transfer(_referrer, referralFeeAmount);

        metaproReferral.saveReferralDeposit(
            _referrer,
            address(this),
            _auctionId,
            _tokenId,
            _depositer,
            _level,
            referralFeeAmount
        );
        return referralFeeAmount;
    }

    function sendFeesToReferrers(
        BuyAuction memory _buyConfiguration,
        Referral.ReferralFees memory _buyNowFeesConfiguration,
        uint256 _amount,
        address _depositer
    ) private returns (uint256) {
        uint256 fee = 0;
        address spacePadReferrer = metaproReferral.getReferral(_depositer);
        if (spacePadReferrer != address(0)) {
            // Level 1
            if (_buyNowFeesConfiguration.level1ReferrerFee > 0) {
                uint256 level1Fee = depositOnReferrer(
                    _buyConfiguration.auctionId,
                    spacePadReferrer,
                    _depositer,
                    _amount,
                    _buyNowFeesConfiguration.level1ReferrerFee,
                    _buyConfiguration.tokenId,
                    1
                );

                fee += level1Fee;
            }
            // Level 2
            address level2Referrer = metaproReferral.getReferral(
                spacePadReferrer
            );
            if (level2Referrer != address(0)) {
                if (_buyNowFeesConfiguration.level2ReferrerFee > 0) {
                    uint256 level2Fee = depositOnReferrer(
                        _buyConfiguration.auctionId,
                        level2Referrer,
                        _depositer,
                        _amount,
                        _buyNowFeesConfiguration.level2ReferrerFee,
                        _buyConfiguration.tokenId,
                        2
                    );

                    fee += level2Fee;
                }

                // Level 3
                address level3Referrer = metaproReferral.getReferral(
                    level2Referrer
                );
                if (level3Referrer != address(0)) {
                    if (_buyNowFeesConfiguration.level3ReferrerFee > 0) {
                        uint256 level3Fee = depositOnReferrer(
                            _buyConfiguration.auctionId,
                            level3Referrer,
                            _depositer,
                            _amount,
                            _buyNowFeesConfiguration.level3ReferrerFee,
                            _buyConfiguration.tokenId,
                            3
                        );
                        fee += level3Fee;
                    }
                }
            }
        }
        return fee;
    }

    function sendFeesToRoyaltyTeamMembers(
        BuyAuction memory _buyConfiguration,
        uint256 _amount
    ) private returns (uint256) {
        uint256 fee = 0;
        Royalty.RoyaltyTeamMember[] storage royaltyMembers = buyRoyaltyFees[
            _buyConfiguration.auctionId
        ];

        for (uint256 i = 0; i < royaltyMembers.length; i++) {
            Royalty.RoyaltyTeamMember memory member = royaltyMembers[i];
            uint256 royaltyFee = _amount.mul(member.royaltyFee).div(10000);
            busd.transfer(member.member, royaltyFee);
            fee += royaltyFee;
        }

        return fee;
    }

    function setTreasuryFee(uint256 _fee) external onlyOwner {
        require(_fee < 2500, "INS: Fee can't be greater than 2,5%; 2500");
        treasuryFee = _fee;
        emit TreasuryFeeUpdated(_fee);
    }

    function setBusdAddress(address _newAddress) external onlyOwner {
        busdAddress = _newAddress;
        busd = IERC20(_newAddress);
        emit BUSDAddressUpdated(_newAddress);
    }

    function setTressuryAddress(address _newAddress) external onlyOwner {
        tressuryAddress = _newAddress;
        emit TressuryAddressUpdated(_newAddress);
    }

    function setReferralAddress(address _newAddress) external onlyOwner {
        referralAddress = _newAddress;
        metaproReferral = MetaproReferral(_newAddress);
        emit ReferralAddressUpdated(_newAddress);
    }

    function setRoyaltyAddress(address _newAddress) external onlyOwner {
        royaltyAddress = _newAddress;
        metaproRoyalty = MetaproRoyalty(_newAddress);
        emit RoyaltyAddressUpdated(_newAddress);
    }
}

