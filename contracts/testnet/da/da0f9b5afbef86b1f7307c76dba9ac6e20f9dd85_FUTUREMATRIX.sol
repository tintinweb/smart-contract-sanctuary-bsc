    using SafeMath for uint256;
    address payable admin;
  

  event SendToAllAsset(uint256 value , address indexed sender);


  constructor() public {
    admin = msg.sender;
  }


  /**
   * @dev Returns the bep token owner.
   */
  function getOwner() external view returns (address) {
    return owner();
  }



 
