contract BUSDdistributor is AccessControl {

    bytes32 constant public ADMIN_ROLE = keccak256("Admin Role");

    uint256 public DURATION = 2 days;

    uint256 public periodFinish = 0;
    uint256 public rewardRate = 0;
    uint256 public lastUpdateTime;
    uint256 public rewardPerTokenStored;

    mapping(address => uint256) public userRewardPerTokenPaid;
    mapping(address => uint256) public rewards;

    address public callerContract;

    using SafeMath for uint256;
    uint256 public _totalSupply;
    //address deployer = tx.origin;
    address deployer;

    mapping(address => uint256) public _balances;

    mapping(address => uint256) public _endDates;


    // package => [date]
    mapping(uint256 => uint256[]) public packageDates;
    // package => [address]
    mapping(uint256 => address[]) public packageAddresses;


    //package => lastIndex
    mapping(uint256 => uint256) packageIndexes;

    uint256 public maxExpireCount = 10;

    event Expired(address indexed user, uint256 time);

    modifier onlyAdmin {
        require(hasRole(ADMIN_ROLE, msg.sender), "!admin");
        _;
    }

    constructor () {
        deployer = msg.sender;
        _setupRole(DEFAULT_ADMIN_ROLE, deployer);
        _setupRole(ADMIN_ROLE, deployer);
    }

    function totalSupply() public view returns(uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view returns(uint256) {
        return _balances[account];
    }

    modifier updateReward(address account) {
        rewardPerTokenStored = rewardPerToken();
        lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            rewards[account] = earned(account);
            userRewardPerTokenPaid[account] = rewardPerTokenStored;
        }
        _;
    }

    modifier checkAccess(){
        require(msg.sender == callerContract ||
            msg.sender == deployer
        , "!checkAccess");
        _;
    }

    function lastTimeRewardApplicable() public view returns(uint256) {
        return Math.min(block.timestamp, periodFinish);
    }

    function rewardPerToken() public view returns(uint256) {
        if (totalSupply() == 0) {
            return rewardPerTokenStored;
        }
        return rewardPerTokenStored.add(
            lastTimeRewardApplicable().sub(lastUpdateTime).mul(rewardRate).mul(1e18).div(totalSupply())
        );
    }

    function rewardPerTokenForUser(address user) public view returns(uint256) {
        if (totalSupply() == 0) {
            return rewardPerTokenStored;
        }
        uint256 d = Math.min(
            lastTimeRewardApplicable(),
            _endDates[user]
        );
        if(d < lastUpdateTime){
            return rewardPerTokenStored;
        }
        return rewardPerTokenStored.add(
            d.sub(lastUpdateTime).mul(rewardRate).mul(1e18).div(totalSupply())
        );
    }

    function earned(address account) public view returns(uint256) {
        return balanceOf(account).mul(
            rewardPerTokenForUser(account).sub(userRewardPerTokenPaid[account])
        ).div(1e18).add(rewards[account]);
    }

    function stake(uint256 amount, address forAddress/*,
        uint256 package*/
    ) public checkAccess updateReward(forAddress) {
        _totalSupply = _totalSupply.add(amount);
        uint endDate = block.timestamp + DURATION;
        _balances[forAddress] = _balances[forAddress].add(amount);

        if(endDate != _endDates[forAddress]){
            _endDates[forAddress] = endDate;
            packageAddresses[30].push(forAddress);
            packageDates[30].push(endDate); 
        }
    }

    function reStake(uint256 amount, address forAddress
    ) public checkAccess{
        if(_balances[forAddress] > 0){
            withdraw(_balances[forAddress], forAddress);
        }
        stake(amount, forAddress);
    }

    function withdraw(uint256 amount, address forAddress) public checkAccess updateReward(forAddress) {
         _totalSupply = _totalSupply.sub(amount);
        _balances[forAddress] = _balances[forAddress].sub(amount);
    }

    function exitFor(address forAddress) private  updateReward(forAddress) {
        _totalSupply -= _balances[forAddress];
        _balances[forAddress] = 0;
    }

    function getReward(address sender) public checkAccess updateReward(sender) {
        uint256 reward = earned(sender);
        if (reward > 0) {
            rewards[sender] = 0;
        }
    }

    function expireOldDatesExternal(uint256 max) public {
        expireOldDates(max);
    }

    function expireOldDates(uint256 max) private{
        uint[4] memory packages = [uint(30), uint(90), uint(180), uint(365)];
        uint processed = 0;
        for(uint i=0; i< packages.length;i++){
            uint256 package = packages[i];
            for(uint j=packageIndexes[package]; j < packageDates[package].length; j++){
                if(packageDates[package][j] > block.timestamp){
                    packageIndexes[package] = j;
                    break;
                }else{
                    if(_endDates[packageAddresses[package][j]] < block.timestamp){
                        exitFor(packageAddresses[package][j]);
                        emit Expired(packageAddresses[package][j], block.timestamp);    
                    }
                    processed ++;
                }
                if(max != 0 && processed > max){
                    return;
                }
            }
        }
    }

    function notifyRewardAmount(uint256 reward) public checkAccess updateReward(address(0)) {
        expireOldDates(maxExpireCount);
        if (block.timestamp >= periodFinish) {
            rewardRate = reward.div(DURATION);
        } else {
            uint256 remaining = periodFinish.sub(block.timestamp);
            uint256 leftover = remaining.mul(rewardRate);
            rewardRate = reward.add(leftover).div(DURATION);
        }
        lastUpdateTime = block.timestamp;
        periodFinish = block.timestamp.add(DURATION);
    }

    function setCallerContract(address addr) public checkAccess{
        callerContract = addr;
    }

  //  function setDuration(uint256 duration) public onlyAdmin {
    //    DURATION = duration;
    //}

    function setMaxExpireCount(uint256 _maxExpireCount) public onlyAdmin {
        maxExpireCount = _maxExpireCount;
    }
}


