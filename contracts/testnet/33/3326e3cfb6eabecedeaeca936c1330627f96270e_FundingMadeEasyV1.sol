    function _lastDepthIndex(uint index, uint last_index) pure internal returns(uint){
        while(true){
            if(last_index <= index*3+3){
                return index*3+3;
            }
            index = index*3 + 3;
        }
        return index;
    }

    function _firstDepthIndex(uint index, uint last_index) pure internal returns(uint){
        while(true){
            if(index*3+1 > last_index){
                return index;
            }
            index = index*3+1;
        }
        return index;
    }

    function _findFreeNodeByIndex(uint index,
            uint last_index, bool first) view public returns(uint256){
        
        uint last_depth_index = _lastDepthIndex(index, last_index);
        uint first_depth_index = first ? last_index : _firstDepthIndex(index, last_index);

        for(uint k=first_depth_index; k<=last_depth_index ; k++){
            if(matrix[k] == address(0)){
                return k;
            }
        }
        return _findFreeNodeByIndex(index, last_index*3+1, false);
    }

