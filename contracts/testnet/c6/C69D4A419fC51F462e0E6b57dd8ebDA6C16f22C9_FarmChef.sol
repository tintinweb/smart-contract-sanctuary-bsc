// 
// MasterChef is the master of Voucher. He can make Voucher and he is a fair guy.
//
// Note that it's ownable and the owner wields tremendous power. The ownership
// will be transferred to a governance smart contract once Voucher is sufficiently
// distributed and the community can show to govern itself.
//
// Have fun reading it. Hopefully it's bug-free. God bless.
// For any questions contact @vinceheng on Telegram
contract FarmChef is Ownable, ReentrancyGuard {
    using SafeMath
    for uint256;
    using SafeERC20
    for IERC20;
    bytes4 private constant INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 private constant INTERFACE_ID_ERC1155 = 0xd9b67a26;

    struct UserNftStake {
        IERC165 nft;
        uint256 quantity;
        uint256 tokenId;
        uint256 unlockTime;
    }
    // Info of each user.
    struct UserInfo {

        uint256 lastVoucherPerShare; // Voucher per share on last update
        uint256 unclaimed; // Unclaimed reward in Voucher.
        // pending reward = user.unclaimed + (user.amount * (pool.accVoucherPerShare - user.lastVoucherPerShare)
        //
        // Whenever a user deposits or withdraws Staking tokens to a pool. Here's what happens:
        //   1. The pool's `accVoucherPerShare` (and `lastVoucherBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `lastVoucherPerShare` gets updated.
        //   4. User's `amount` gets updated.
    }

    // Info of each pool.
    struct PoolInfo {
        IERC165 lpToken; // Address of LP token contract.
        uint256 allocPoint; // How many allocation points assigned to this pool. Voucher to distribute per block.
        uint256 totalDeposited; // The total deposited by users
        uint256 lastRewardBlock; // Last block number that Voucher distribution occurs.
        uint256 accVoucherPerShare; // Accumulated Voucher per share, times 1e18. See below.
        uint256 poolLimit;
        uint256 unlockDate;
        address nftAddress;
    }

    // The Voucher TOKEN!
    IERC20 public immutable voucher;
    address public pendingVoucherOwner;
    address public voucherTransferOwner;
    address public devAddress;


    // Voucher tokens created per block.
    uint256 public voucherPerBlock = 8 ether;
    uint256 public constant MAX_EMISSION_RATE = 1000 ether; // Safety check

    // Info of each pool.
    PoolInfo[] public poolInfo;

    mapping(uint256 => mapping(address => UserNftStake[])) public userStakeInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 0;
    uint256 public constant MAX_ALLOC_POINT = 100000; // Safety check
    // The block number when Voucher mining starts.
    uint256 public immutable startBlock;

    event Add(address indexed user, uint256 allocPoint, IERC165 indexed token, bool massUpdatePools);
    event Set(address indexed user, uint256 pid, uint256 allocPoint);
    event Deposit(address indexed user, uint256 indexed pid, uint256 amount, bool harvest);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount, bool harvest);
    event Harvest(address indexed user, uint256 indexed pid, uint256 amount);
    event HarvestMultiple(address indexed user, uint256[] _pids, uint256 amount);
    event HarvestAll(address indexed user, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event SetDevAddress(address indexed user, address indexed newAddress);
    event UpdateEmissionRate(address indexed user, uint256 voucherPerBlock);
    event SetVoucherTransferOwner(address indexed user, address indexed voucherTransferOwner);
    event AcceptVoucherOwnership(address indexed user, address indexed newOwner);
    event NewPendingVoucherOwner(address indexed user, address indexed newOwner);

    constructor(
        IERC20 _voucher,
        uint256 _startBlock,
        address _devAddress,
        address _voucherTransferOwner
    ) public {
        require(_devAddress != address(0), "!nonzero");
        voucher = _voucher;
        startBlock = _startBlock;

        devAddress = _devAddress;
        voucherTransferOwner = _voucherTransferOwner;

    }

    function GetUserStakeInfo(uint256 poolId, address addr) public view returns(UserNftStake[] memory) {
        return userStakeInfo[poolId][addr];
    }

    function poolLength() external view returns(uint256) {
        return poolInfo.length;
    }

    mapping(IERC165 => bool) public poolExistence;
    modifier nonDuplicated(address _lpToken) {
        require(poolExistence[IERC165(_lpToken)] == false, "nonDuplicated: duplicated");
        _;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function add(uint256 _allocPoint, address _lpToken, bool _massUpdatePools, uint256 _poolLimit, uint256 _lockForTimestamp) external onlyOwner nonDuplicated(_lpToken) {
        require(_allocPoint <= MAX_ALLOC_POINT, "!overmax");
        if (_massUpdatePools) {
            massUpdatePools(); // This ensures that massUpdatePools will not exceed gas limit
        }


        //  _lpToken.balanceOf(address(this)); // Check to make sure it's a token
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolExistence[IERC165(_lpToken)] = true;
        poolInfo.push(PoolInfo({
            lpToken: IERC165(_lpToken),
            totalDeposited: 0,
            allocPoint: _allocPoint,
            lastRewardBlock: lastRewardBlock,
            accVoucherPerShare: 0,
            poolLimit: _poolLimit,
            unlockDate: _lockForTimestamp,
            nftAddress: _lpToken
        }));
        emit Add(msg.sender, _allocPoint, IERC165(_lpToken), _massUpdatePools);
    }

    // Update the given pool's Voucher allocation point. Can only be called by the owner.
    function set(uint256 _pid, uint256 _allocPoint) external onlyOwner {
        require(_allocPoint <= MAX_ALLOC_POINT, "!overmax");
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
        emit Set(msg.sender, _pid, _allocPoint);
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public pure returns(uint256) {
        return _to.sub(_from);
    }

    // View function to see pending Voucher on frontend.
    function pendingVoucher(uint256 _pid, address _user) external view returns(uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accVoucherPerShare = pool.accVoucherPerShare;
        if (block.number > pool.lastRewardBlock && pool.totalDeposited != 0 && totalAllocPoint != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
            uint256 voucherReward = multiplier.mul(voucherPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
            accVoucherPerShare = accVoucherPerShare.add(voucherReward.mul(1e18).div(pool.totalDeposited));
        }
        return getNftStakedInPool(_pid, _user).mul(accVoucherPerShare.sub(user.lastVoucherPerShare)).div(1e18).add(user.unclaimed);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        if (pool.totalDeposited == 0 || pool.allocPoint == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 voucherReward = multiplier.mul(voucherPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
        //  voucher.mint(devAddress, voucherReward.div(50)); // 2%
        //  voucher.mint(address(this), voucherReward);
        pool.accVoucherPerShare = pool.accVoucherPerShare.add(voucherReward.mul(1e18).div(pool.totalDeposited));
        pool.lastRewardBlock = block.number;
    }

    // Deposit LP tokens to MasterChef for Voucher allocation.
    function deposit(uint256 _pid, uint256 _tokenId, bool _shouldHarvest) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        _updateUserReward(_pid, _shouldHarvest);


        if (_supportERC721(pool.nftAddress)) {
            IERC721 nft = IERC721(pool.nftAddress);
            require(
                nft.ownerOf(_tokenId) == _msgSender(),
                "Must be owner of NFT."
            );
            require(
                nft.isApprovedForAll(_msgSender(), address(this)),
                "Must be approved before list."
            );

            IERC721(pool.nftAddress).transferFrom(_msgSender(), address(this), _tokenId);
        } else if (_supportERC1155(pool.nftAddress)) {
            IERC1155 nft = IERC1155(pool.nftAddress);
            require(
                nft.balanceOf(_msgSender(), _tokenId) >= 1,
                "Must hold enough NFTs."
            );
            require(
                nft.isApprovedForAll(_msgSender(), address(this)),
                "Must be approved before list."
            );

            IERC1155(pool.nftAddress).safeTransferFrom(_msgSender(), address(this), _tokenId, 1, '');
        } else {
            revert("Invalid NFT address.");
        }

        pool.totalDeposited = pool.totalDeposited.add(1);
        userStakeInfo[_pid][msg.sender].push(UserNftStake(pool.lpToken, 1, _tokenId, block.timestamp + pool.unlockDate));

        require(pool.poolLimit > 0 && pool.totalDeposited <= pool.poolLimit, "Exceeded pool limit");

        emit Deposit(msg.sender, _pid, _tokenId, _shouldHarvest);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _tokenId, bool _shouldHarvest) external nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        //  UserNftStake userNftStake =  userStakeInfo[_pid][msg.sender];

        //   require(block.timestamp > userNftStake.unlockTime, "unlock date not reached");

        _updateUserReward(_pid, _shouldHarvest);

        UserNftStake[] storage userStakes = userStakeInfo[_pid][msg.sender];

        for (uint256 i = 0; i < userStakes.length; i++) {
            uint256 tokenId = userStakes[i].tokenId;
            UserNftStake memory userNftStake = userStakes[i];

            if (tokenId == _tokenId) {
                require(userNftStake.unlockTime < block.timestamp, "unlock date not reached");
                pool.totalDeposited = pool.totalDeposited.sub(1);
                // pool.lpToken.transferFrom(address(this), msg.sender, _tokenId);

                if (_supportERC721(pool.nftAddress)) {
                    IERC721(pool.nftAddress).transferFrom(address(this), _msgSender(), _tokenId);
                } else if (_supportERC1155(pool.nftAddress)) {
                    IERC1155(pool.nftAddress).safeTransferFrom(address(this), _msgSender(), _tokenId, 1, '');
                }

                emit Withdraw(msg.sender, _pid, _tokenId, _shouldHarvest);
                delete userStakes[i];
                return;
            }
        }
    }

    function getNftStakedInPool(uint256 pid, address user) internal view returns(uint256) {
        UserNftStake[] storage userStakes = userStakeInfo[pid][user];

        return userStakes.length;
    }



    // Update the rewards of caller, and harvests if needed
    function _updateUserReward(uint256 _pid, bool _shouldHarvest) internal {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);

        uint256 userAmount = getNftStakedInPool(_pid, msg.sender);

        if (userAmount == 0) {
            user.lastVoucherPerShare = pool.accVoucherPerShare;
        }
        uint256 pending = userAmount.mul(pool.accVoucherPerShare.sub(user.lastVoucherPerShare)).div(1e18).add(user.unclaimed);
        user.unclaimed = _shouldHarvest ? 0 : pending;

        user.lastVoucherPerShare = pool.accVoucherPerShare;
    }

    // Harvest one pool
    function harvest(uint256 _pid) external nonReentrant {
        _updateUserReward(_pid, true);
    }

    // Harvest specific pools into one vest
    function harvestMultiple(uint256[] calldata _pids) external nonReentrant {
        uint256 pending = 0;
        for (uint256 i = 0; i < _pids.length; i++) {
            updatePool(_pids[i]);
            PoolInfo storage pool = poolInfo[_pids[i]];
            UserInfo storage user = userInfo[_pids[i]][msg.sender];

            uint256 userAmount = getNftStakedInPool(_pids[i], msg.sender);
            if (userAmount == 0) {
                user.lastVoucherPerShare = pool.accVoucherPerShare;
            }
            pending = pending.add(userAmount.mul(pool.accVoucherPerShare.sub(user.lastVoucherPerShare)).div(1e18).add(user.unclaimed));
            voucher.transfer(msg.sender, pending);
            user.unclaimed = 0;
            user.lastVoucherPerShare = pool.accVoucherPerShare;
        }

        emit HarvestMultiple(msg.sender, _pids, pending);
    }

    // Harvest all into one vest. Will probably not be used
    // Can fail if pool length is too big due to massUpdatePools()
    function harvestAll() external nonReentrant {
        massUpdatePools();
        uint256 pending = 0;
        for (uint256 i = 0; i < poolInfo.length; i++) {
            PoolInfo storage pool = poolInfo[i];
            UserInfo storage user = userInfo[i][msg.sender];

            uint256 userAmount = getNftStakedInPool(i, msg.sender);
            if (userAmount == 0) {
                user.lastVoucherPerShare = pool.accVoucherPerShare;
            }
            pending = pending.add(userAmount.mul(pool.accVoucherPerShare.sub(user.lastVoucherPerShare)).div(1e18).add(user.unclaimed));

            voucher.transfer(msg.sender, pending);
            user.unclaimed = 0;
            user.lastVoucherPerShare = pool.accVoucherPerShare;
        }

        emit HarvestAll(msg.sender, pending);
    }


    // Update dev address by the previous dev.
    function setDevAddress(address _devAddress) external onlyOwner {
        require(_devAddress != address(0), "!nonzero");
        devAddress = _devAddress;
        emit SetDevAddress(msg.sender, _devAddress);
    }

    // Should never fail as long as massUpdatePools is called during add
    function updateEmissionRate(uint256 _voucherPerBlock) external onlyOwner {
        require(_voucherPerBlock <= MAX_EMISSION_RATE, "!overmax");
        massUpdatePools();
        voucherPerBlock = _voucherPerBlock;
        emit UpdateEmissionRate(msg.sender, _voucherPerBlock);
    }


    function _supportERC721(address _nftAddress) private view returns(bool) {
        return IERC165(_nftAddress).supportsInterface(INTERFACE_ID_ERC721);
    }

    function _supportERC1155(address _nftAddress) private view returns(bool) {
        return IERC165(_nftAddress).supportsInterface(INTERFACE_ID_ERC1155);
    }

}