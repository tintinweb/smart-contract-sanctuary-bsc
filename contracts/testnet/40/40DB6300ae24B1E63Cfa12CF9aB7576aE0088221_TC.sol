contract TC is ERC20 {
    using SafeMath for uint256;
    uint256 total = 2986 * 10 ** 18;
    uint8 _decimals = 18;
    uint256 public marketFees = 5;
    uint256 public burnFees = 1;
    uint256 public burnStopAmount = 168 * 10 ** _decimals;
    IUniswapV2Router02 public immutable uniswapV2Router;
    address public uniswapV2Pair;
	address[] marketAddress;
    bool public swap = false;
    mapping(address => bool) public isExcludedFromFees;
    mapping(address => bool) public lockAddressMap;

    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    constructor(address _tokenOwner) ERC20("TC", "TC",_decimals) {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), address(0x33c45f5bd585259a6C6acb0227F6895DbF7C0bA6));
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        marketAddress.push(0x8B15E81D0E1EcA38327C663c93340b4d06d2D5FE);
        marketAddress.push(0x995AA52fC1Dfd087E3936155392535fd6Ad2dA85);
        marketAddress.push(0x91304782622792b5cD7E81c93f9778974Cd38c8f);
        excludeFromFees(_tokenOwner, true);
        excludeFromFees(address(this), true);
        _mint(_tokenOwner, total);   
    }

    receive() external payable {

    }

    function excludeFromFees(address _account, bool _select) public onlyOwner {
        isExcludedFromFees[_account] = _select;
        emit ExcludeFromFees(_account, _select);
    }

    function batchExcludeFromFees(address[] calldata _accounts, bool _select) public onlyOwner {
        for (uint256 i = 0; i < _accounts.length; i++) {
            isExcludedFromFees[_accounts[i]] = _select;
        }
        emit ExcludeMultipleAccountsFromFees(_accounts, _select);
    }

    function lockAddress(address _account, bool _select) public onlyOwner {
        lockAddressMap[_account] = _select;
    }
    function setSwap(bool _enable) public onlyOwner {
        swap = _enable;
    }

    function setBurnFee(uint256 _burnFees) public onlyOwner {
        burnFees = _burnFees;
    }

    function batchLockAddress(address[] memory _accounts,bool _select) public onlyOwner {
        for (uint256 i = 0; i < _accounts.length; i++) {
            lockAddressMap[_accounts[i]] = _select;
        }
    }
	
    function _transfer(
        address _from,
        address _to,
        uint256 _amount
    ) internal override {
        require(_from != address(0), "ERC20: transfer from the zero address");
        require(_to != address(0), "ERC20: transfer to the zero address");
        require(!lockAddressMap[_msgSender()],"account is lock");
        require(_amount > 0,"not transfer zero amount");
        uint256 _initAmount = _amount;
        if(swap && !isExcludedFromFees[_from] && !isExcludedFromFees[_to] && (_from == uniswapV2Pair || _to == uniswapV2Pair)){
            uint256 _burnMaxAmount = totalSupply().sub(burnStopAmount);
            if(balanceOf(burnAddress) < _burnMaxAmount) {
                uint256 _burnAmount = calculateFee(_amount, burnFees);
                if(balanceOf(burnAddress).add(_burnAmount) >= _burnMaxAmount) _burnAmount = balanceOf(burnAddress).add(_burnAmount).sub(_burnMaxAmount); 
                _burn(_from, _burnAmount);
                _initAmount = _amount.sub(_burnAmount); 
            }
            uint256 _marketCountFee = calculateFee(_amount, marketFees);
            uint256 _marketFee = calculateMarketFee(_marketCountFee);
            for(uint i = 0;i < marketAddress.length;i++){ 
                _initAmount = _initAmount.sub(_marketFee);
                super._transfer(_from, marketAddress[i], _marketFee);
            }
            super._transfer(_from, _to,_initAmount);
        } else {
            super._transfer(_from, _to,_amount);
        }
    }

    function calculateMarketFee(uint256 _amount) internal view returns(uint256){
        return _amount.div(marketAddress.length);
    }
    function calculateFee(uint256 _amount,uint256 _fee) internal pure returns(uint256){
        return _amount.mul(_fee).div(10**2);
    }    

}