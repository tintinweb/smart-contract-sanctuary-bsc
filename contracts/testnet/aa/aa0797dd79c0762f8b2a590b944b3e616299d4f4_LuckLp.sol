// File: LuckLp.sol


pragma experimental ABIEncoderV2;
pragma solidity 0.6.12;





contract LuckLp is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    IBEP20 public IncomeAddress; //收益地址
    uint256 public lockTime = 60; //锁仓时间
    IBEP20 public IncomeToken =
        IBEP20(0xeD24FC36d5Ee211Ea25A80239Fb8C4Cfd80f12Ee); // 设置收益代币 默认busd
    uint256 public totalIncome = 0; //总矿池收益统计

    //用户质押信息
    struct UserInfo {
        uint256 amount; //用户有多少LP TOKEN
        uint256 received; //已经领取
        uint256 endTime; //用户最后一次质押时间
    }

    //矿池信息
    struct PoolInfo {
        IBEP20 lpToken; //LP地址
        uint256 total; //LP数量
        string lpName; //矿池名称
        uint256 stakingLimit; //锁仓位额度限制
    }

    // //排行榜
    struct Rankings {
        address userAddress;
        uint256 amount; //用户有多少LP TOKEN
    }

    PoolInfo[] public poolInfo; //每个池子信息

    mapping(uint256 => Rankings[]) public rankings; //排行榜

    mapping(uint256 => mapping(address => UserInfo)) public userInfo; // Info of each user that stakes LP tokens.

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);

    constructor(IBEP20 _IncomeAddress, uint256 _lockTime) public {
        IncomeAddress = _IncomeAddress;
        lockTime = _lockTime;
    }

    //设置收益池子地址以便计算收益
    function SetIncometAddress(IBEP20 adr) public onlyOwner {
        IncomeAddress = adr;
    }

    //设置发放的合约代币
    function SetIncomeToken(IBEP20 adr) public onlyOwner {
        IncomeToken = IBEP20(adr);
    }

    //添加Lp矿池
    function Add(
        IBEP20 _lpToken,
        uint256 _total,
        string memory _lpName,
        uint256 _stakingLimit
    ) public onlyOwner {
        //字符串怎么判断空值？
        require(_stakingLimit > 0, "Invalid lp _stakingLimit setting");
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                total: _total,
                lpName: _lpName,
                stakingLimit: _stakingLimit
            })
        );
    }

    //存入矿池
    function deposit(uint256 _pid, uint256 _amount) public {
        require(_amount >= 0, "Invalid lp quantity setting");
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        if ((pool.stakingLimit - pool.total) > _amount) {
            pool.lpToken.safeTransferFrom(
                address(msg.sender),
                address(this),
                (_amount)
            );
            user.amount = user.amount.add(_amount);
            user.endTime = block.timestamp;
            pool.total = pool.total.add(_amount);

            //排行榜更新
            UpdateRankingsForDeposit(_pid, _amount, msg.sender);
        }

        emit Deposit(msg.sender, _pid, _amount);
    }

    function UpdateRankingsForDeposit(
        uint _pid,
        uint256 _amount,
        address _userAddress
    ) public {
        //排行榜更新
        if (rankings[_pid].length > 10) {
            for (uint i = 0; i < rankings[_pid].length; i++) {
                //如果前9有自己的话更新金额从新排序
                if (_userAddress == rankings[_pid][i].userAddress) {
                    rankings[_pid][i].amount = _amount;
                    RankingsSort(_pid);
                    break;
                } else {
                    //前9没有自己，是从大到小比下来然后直接替换
                    if (_amount > rankings[_pid][i].amount) {
                        rankings[_pid][i].userAddress = _userAddress;
                        rankings[_pid][i].amount = _amount;
                        break;
                    }
                }
            }
        } else {
            for (uint i = 0; i < rankings[_pid].length; i++) {
                if (_userAddress == rankings[_pid][i].userAddress) {
                    rankings[_pid][i].amount = _amount;
                    RankingsSort(_pid);
                    break;
                }
            }

            rankings[_pid].push(
                Rankings({userAddress: msg.sender, amount: _amount})
            );
            //进行反冒泡排序
            RankingsSort(_pid);
        }
    }

    function RankingsSort(uint _pid) internal {
        //进行反冒泡排序
        for (uint i = 1; i < rankings[_pid].length; i++) {
            for (uint j = 1; j < (rankings[_pid].length.sub(i)); j++) {
                if (
                    rankings[_pid][j].amount < rankings[_pid][j.add(1)].amount
                ) {
                    Rankings memory temp = rankings[_pid][j];
                    rankings[_pid][j] = rankings[_pid][j.add(1)];
                    rankings[_pid][j.add(1)] = temp;
                }
            }
        }
    }

    //分配分红 测试阶用公开修饰符，接入开奖模块在改为内部修饰符
    function DistributionDividends(uint256 _amount) public {
        require(_amount > 0, "Amount error");
        totalIncome = totalIncome.add(_amount);
    }

    //查看收益
    function PendingIncome(uint256 _pid) public view returns (uint256) {
        if (totalIncome <= 0) {
            return 0;
        }
        return
            totalIncome
                .div(poolInfo[_pid].total)
                .mul(userInfo[_pid][msg.sender].amount)
                .add(userInfo[_pid][msg.sender].received);
    }

    //领取收益
    function ClaimIncome(uint256 _pid, uint256 _amount) public {
        require(_amount >= 0, "Amount error");
        uint256 Income = totalIncome
            .div(poolInfo[_pid].total)
            .mul(userInfo[_pid][msg.sender].amount)
            .add(userInfo[_pid][msg.sender].received);
        if (Income > 0) {
            userInfo[_pid][msg.sender].received += Income;
            //可能有decimal问题 先放着
            require(
                IncomeToken.balanceOf(address(IncomeAddress)) > Income,
                "e.."
            );
            IncomeToken.safeTransferFrom(
                address(IncomeAddress),
                msg.sender,
                Income
            );
        }
    }

    //解除质押
    function withdraw(uint256 _pid, uint256 _amount) public {
        require(_pid >= 0, "Invalid staking");
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");

        //判断满不满足解压时间
        require(
            user.endTime.add(lockTime) > block.timestamp,
            "It's not time to decompress"
        );
        require(_amount > 0, "Amount error");
        user.amount = user.amount.sub(_amount);
        pool.total = pool.total.sub(_amount);
        pool.lpToken.safeTransferFrom(
            address(this),
            address(msg.sender),
            _amount
        );
        RankingsSort(_pid);
        emit Withdraw(msg.sender, _pid, _amount);
    }

    //设置锁仓时间
    function SetLockTime(uint256 time) public onlyOwner {
        require(time > 0, "param error");
        lockTime = time;
    }

    //获取剩余质押额度
    function GetStakNum(uint256 _pid) public view returns (uint256) {
        return poolInfo[_pid].stakingLimit.sub(poolInfo[_pid].total);
    }

    function InRankings(uint256 _pid, address _userAddress)
        public
        view
        returns (bool)
    {
        for (uint i = 1; i < rankings[_pid].length; i++) {
            if (rankings[_pid][i].userAddress == _userAddress) {
                return true;
            }
        }
        return false;
    }

    function getRankings(uint _pid) public view returns(Rankings[] memory data) {
         data = new Rankings[](rankings[_pid].length);
         for(uint i=0;i<rankings[_pid].length;i++){
             data[i] = rankings[_pid][i];
         }
    }
}