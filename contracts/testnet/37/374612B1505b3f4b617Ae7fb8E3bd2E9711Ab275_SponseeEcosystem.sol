      0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3 //pancakeswap v2 mainnet router address
    );
    // Create a uniswap pair for this new token
    uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(
      address(this),
      _uniswapV2Router.WETH()
    );

    // set the rest of the contract variables
    uniswapV2Router = _uniswapV2Router;

    //exclude owner and this contract from fee
    _isExcludedFromFee[owner()] = true;
    _isExcludedFromFee[address(this)] = true;

    emit Transfer(address(0), _msgSender(), _totalSupply);
  }

  function migrate() public {
    address user = _msgSender();

    IERC20 oldSpon = IERC20(0x8a9424745056Eb399FD19a0EC26A14316684e274); //address of the old token
    uint256 migrationAmount = oldSpon.balanceOf(user);

    require(block.timestamp <= migrationStart + 5 days);
