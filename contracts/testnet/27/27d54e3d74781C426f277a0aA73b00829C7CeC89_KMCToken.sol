        internal
    {
        console.log(routerAddress);
        console.log(tokenAmount);

        IUniswapV2Router02 router = IUniswapV2Router02(routerAddress);

        // generate the Dex pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
