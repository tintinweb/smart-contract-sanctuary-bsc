    function finalizePresale() external virtual hasEnded onlyRole(OWNER_ROLE) {
        require(!canceled, "Presale was canceled");
        require(!finalized, "Already finalized");
        assertTransfers();
        uint256 supply = token.balanceOf(address(this));
        uint256 expectedSupply = requiredFunds();
        require(supply >= expectedSupply, "Insufficient supply");
        finalized = true;
        takeServiceFee();
        witdrawCapital(msg.sender);
    }

    function takeServiceFee() internal virtual;
    function witdrawCapital(address _destination) internal virtual;

    function _canClaim() internal virtual override returns (bool) {
        require(finalized, "Not finalized");
        return super._canClaim();
    }

    uint256[49] private __gap;
}

