import "../../lib/PresaleUtils.sol";
import "../../utils/TransferTester.sol";
import "./FundableFairLaunch.sol";

contract ClaimableFairLaunch is FundableFairLaunch {
    event Claim(address owner, uint256 amount);
    event Refund(address owner, uint256 amount);

    mapping(address => uint256) public _claimed;

    modifier canRefund() {
        require(_canRefund(), "Cannot refund");
        _;
    }

    modifier canClaim() {
        require(_canClaim(), "Cannot Claim");
        _;
    }

    function claim() external canClaim {
        uint256 amount = getRate() * investments[_msgSender()];
        require(_claimed[_msgSender()] == 0, "Nothing to claim");
        _claimed[_msgSender()] = amount;
        token.transfer(_msgSender(), amount);
        emit Claim(_msgSender(), amount);
    }

    function refund() external canRefund {
        uint256 amount = investments[_msgSender()];
        require(amount > 0, "Nothing to refund");
        investments[_msgSender()] = 0;
        _refund(amount);
        emit Refund(_msgSender(), amount);
    }

    function _refund(uint256 amount) internal virtual {
        payable(_msgSender()).transfer(amount);
    }

    function _canClaim() internal virtual returns (bool) {
        return !canceled && (block.timestamp > launchConfiguration.endTime);
    }

    function _canRefund() internal virtual returns (bool) {
        return canceled || (block.timestamp > launchConfiguration.endTime);
    }

    uint256[48] private __gap;
}

