pragma solidity ^0.8.0;

import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@openzeppelin/contracts/token/ERC20/utils/TokenTimelock.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

library PresaleUtils {
    struct PresaleConfiguration {
        uint256 rate;
        uint256 minBuy;
        uint256 maxBuy;
        uint256 hardCap;
        uint256 softCap;
        uint256 startTime;
        uint256 endTime;
        uint256 emergencyTax;
        uint256 serviceFee;
        address serviceFeeAddress;
    }

    struct FairLaunchConfiguration {
        uint256 amountForSale;
        uint256 softCap;
        uint256 tokenSupply;
        uint256 minBuy;
        uint256 maxBuy;
        uint256 startTime;
        uint256 endTime;
        uint256 emergencyTax;
        uint256 serviceFee;
        address serviceFeeAddress;
    }

    struct WhitelistStakeConfig {
        uint256 whitelistEndTime;
        uint256 stakingRoundStartTime;
        uint256 whitelistStakeMinAmount;
        address stakingContract;
    }

    struct InvestorInfo {
        uint256 balance; // tokens
        uint256 invested; // amount in bnb
        address addr;
    }

    function checkValid(
        PresaleConfiguration memory config,
        uint256 amount,
        uint256 balance,
        uint256 capitalRaised
    ) external view {
        require(block.timestamp >= config.startTime && block.timestamp <= config.endTime, "Presale-not-running");
        require(amount > 0, "zero-value");
        require(balance + amount <= config.maxBuy, "max-buy");
        require(balance + amount >= config.minBuy, "min-buy");
        require(capitalRaised + amount <= config.hardCap, "hardcap");
    }

    function getRemaining(
        PresaleConfiguration memory config,
        uint256 amount,
        uint256 investment,
        uint256 capitalRaised
    ) public pure returns (uint256) {
        uint256 userRemaning = config.maxBuy - investment;
        uint256 hardCapRemaining = config.hardCap - capitalRaised;
        uint256 remaning = userRemaning > hardCapRemaining ? hardCapRemaining : userRemaning;
        if (amount > remaning) {
            amount = remaning;
        }
        return amount;
    }
}

