pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "./lib/SwapUtils.sol";
import "./lib/PresaleUtils.sol";
import "./ERC20BasicFairLaunch.sol";

contract LiquidityRaise is ERC20BasicFairLaunch {
    function requiredFunds() public view virtual override returns (uint256) {
        return
            launchConfiguration.amountForSale +
            (launchConfiguration.amountForSale * liquidityLockConfiguration.liquidityAmount) /
            100;
    }

    function postUpgrade(PresaleUtils.FairLaunchConfiguration calldata presaleConfig) public {
        launchConfiguration = presaleConfig;
    }
}

