pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "./lib/SwapUtils.sol";
import "./lib/PresaleUtils.sol";
import "./lib/LiquidityLockUtils.sol";

import "./fairlaunch/claimable/LiquidityLockingFairLaunch.sol";
import "./fairlaunch/swap/ERC20SwapFairLaunch.sol";

contract ERC20BasicFairLaunch is BaseLiquidityLock, ERC20SwapFairLaunch {
    function initialize(
        PresaleUtils.FairLaunchConfiguration calldata presaleConfig,
        LiquidityLockUtils.FairLaunchLiquidityLockConfiguration memory _config,
        address _token,
        address _testerAddr,
        address _routerAddress,
        address _baseToken
    ) public initializer {
        __BaseLiquidityLock__init_(presaleConfig, _config, _token, _testerAddr);
        __ERC20SwapFairLaunch_init_unchained(_baseToken,_routerAddress);
    }

    // Returns the amount of tokens received per BNB
    function getRate() public view virtual override(BaseFairLaunch, FundableFairLaunch) returns (uint256) {
        return FundableFairLaunch.getRate();
    }

    function buy() public payable virtual override(BaseFairLaunch, ERC20SwapFairLaunch) {
        ERC20SwapFairLaunch.buy();
    }

    function emergencyWithdrawl() public virtual override(BaseFairLaunch, ERC20SwapFairLaunch) {
        ERC20SwapFairLaunch.emergencyWithdrawl();
    }

    function witdrawCapital(address _destination) internal override {
        lockLiquidity(token, IERC20(address(0)), msg.sender);
        uint256 weiBalance = address(this).balance;
        if (weiBalance > 0) {
            payable(_destination).transfer(weiBalance);
        }
    }

    function takeServiceFee() internal override {
        uint256 fee = (capitalRaised * launchConfiguration.serviceFee) / 100;
        payable(launchConfiguration.serviceFeeAddress).transfer(fee);
    }

    function lockLiquidity(
        IERC20 tokenA,
        IERC20 tokenB,
        address owner
    ) internal virtual override {
        super.lockLiquidity(tokenA, tokenB, owner);
        uint256 tokenLiquidityAmount = (launchConfiguration.amountForSale *
            liquidityLockConfiguration.liquidityAmount) / 100;
        uint256 spareAmount = launchConfiguration.amountForSale - tokenLiquidityAmount;
        token.transfer(0x000000000000000000000000000000000000dEaD, spareAmount);
    }
}

