        uint256 initialBalance = token.balanceOf(address(this));
        token.safeTransfer(address(transferTester), 1000);
        transferTester.recoverTokens(token);
        uint256 newBalance = token.balanceOf(address(this));
        require(newBalance == initialBalance, "Transfers tax mechanic detected");
    }

