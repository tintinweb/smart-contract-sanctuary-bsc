import "../base/BaseFairLaunch.sol";
import "./NativeSwapFairLaunch.sol";

contract ERC20SwapFairLaunch is BaseFairLaunch {
    using SafeERC20 for IERC20;

    IUniswapV2Router02 public router;
    IERC20 public baseToken;

    function __ERC20SwapFairLaunch_init_unchained(address _router, address _baseToken) public onlyInitializing {
        baseToken = IERC20(_baseToken);
        router = IUniswapV2Router02(_router);
    }

    function buy() public payable virtual override {
        revert("Not allowed");
    }

    function swapToBaseAndBuy(
        IERC20 token,
        uint256 tokenAmount,
        uint256 minAmount
    ) public payable virtual {
        if (address(token) == address(baseToken)) {
            buyWithBase(tokenAmount);
            return;
        }

        uint256 weiAmount = msg.value;
        if (weiAmount > 0) {
            tokenAmount = SwapUtils.swapExactETHForTokens(router, baseToken, weiAmount, minAmount);
        } else {
            token.safeTransferFrom(msg.sender, address(this), tokenAmount);
            tokenAmount += SwapUtils.swapTokensForTokens(router, token, baseToken, tokenAmount, minAmount);
        }

        handleBuy(tokenAmount);
    }

    function buyWithBase(uint256 amount) public virtual {
        baseToken.safeTransferFrom(msg.sender, address(this), amount);
        handleBuy(amount);
    }

    function handleBuy(uint256 amount) internal virtual {
        uint256 change = _buy(amount);
        if (change > 0) {
            baseToken.safeTransfer(msg.sender, change);
        }
    }

    function emergencyWithdrawl() public virtual override {
        require(!_hasEnded(), "Presale ended");
        uint256 amount = investments[_msgSender()];
        uint256 tax = (amount * launchConfiguration.emergencyTax) / 100;
        amount = amount - tax;
        capitalRaised -= investments[_msgSender()];
        investments[_msgSender()] = 0;

        baseToken.safeTransfer(_msgSender(), amount);
        baseToken.safeTransfer(launchConfiguration.serviceFeeAddress, tax);

        emit EmergencyWithdraw(_msgSender(), amount);
    }

    uint256[48] private __gap;
}

