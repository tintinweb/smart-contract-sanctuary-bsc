    function getRate() public view virtual override returns (uint256) {
        if (capitalRaised == 0) {
            return launchConfiguration.amountForSale / (10 ** token.decimals());
        }
        return (launchConfiguration.amountForSale * (10 ** (18 - token.decimals()))) / capitalRaised ;
    }

    function fund() external virtual {
        uint256 amount = requiredFunds();
        uint256 currentBalance = token.balanceOf(address(this));
        require(currentBalance == 0, "funding/already-funded");
