    TransferTester public transferTester;

    function __FundableFairLaunch_init_(
        PresaleUtils.FairLaunchConfiguration calldata presaleConfig,
        address _testerAddr,
        address _token
    ) public onlyInitializing {
        __BaseFairLaunch_init_unchained(presaleConfig);
        __FundableFairLaunch_init_unchained(_testerAddr, _token);
    }

