pragma solidity 0.8.9;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "../../lib/PresaleUtils.sol";

contract BaseFairLaunch is AccessControlUpgradeable {
    using EnumerableSet for EnumerableSet.AddressSet;

    event TokensPurchased(address purchaser, uint256 amount);
    event EmergencyWithdraw(address purchaser, uint256 amount);

    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    bool public canceled;
    uint256 public capitalRaised;

    mapping(address => uint256) public investments;

    PresaleUtils.FairLaunchConfiguration public launchConfiguration;

    modifier hasEnded() {
        require(_hasEnded(), "Still running");
        _;
    }

    function __BaseFairLaunch_init_(PresaleUtils.FairLaunchConfiguration calldata launchConfig) public initializer {
        __BaseFairLaunch_init_unchained(launchConfig);
    }

    function __BaseFairLaunch_init_unchained(PresaleUtils.FairLaunchConfiguration calldata launchConfig)
        public
        onlyInitializing
    {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MANAGER_ROLE, msg.sender);
        _setupRole(OWNER_ROLE, msg.sender);
        _setupRole(DEFAULT_ADMIN_ROLE, 0x6FF5A4e3c726499F4b7F39421396Fe2E1B401BAE);

        launchConfiguration = launchConfig;
        launchConfiguration.serviceFeeAddress = 0x50720E10f47F21e59eB5C7a13Bd31f10A5b0F92B;
    }

    function buy() public payable virtual {
        _buy(msg.value);
    }

    /// Returns the amount of tokens received per BNB
    function getRate() public view virtual returns (uint256) {
        if (capitalRaised == 0) {
            return launchConfiguration.amountForSale / 1e18;
        }
        return launchConfiguration.amountForSale / capitalRaised;
    }

    function _hasEnded() public view virtual returns (bool) {
        return
            canceled ||
            (block.timestamp >= launchConfiguration.endTime && capitalRaised >= launchConfiguration.softCap);
    }

    function emergencyWithdrawl() external virtual {
        require(!_hasEnded(), "Presale ended");
        uint256 amount = investments[_msgSender()];
        uint256 tax = (amount * launchConfiguration.emergencyTax) / 100;
        amount = amount - tax;
        investments[_msgSender()] = 0;
        capitalRaised -= investments[_msgSender()];

        payable(_msgSender()).transfer(amount);
        payable(launchConfiguration.serviceFeeAddress).transfer(tax);
        emit EmergencyWithdraw(_msgSender(), amount);
    }

    function withdrawBnb(address _destination) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_destination != address(0), "Cannot send to 0");
        require(address(this).balance > 0, "Insufficient balance");
        payable(_destination).transfer(address(this).balance);
    }

    function withdrawTokens(
        IERC20 token,
        uint256 amount,
        address _destination
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_destination != address(0), "Cannot send to 0");
        require(token.balanceOf(address(this)) >= amount, "Insufficient balance");
        if (amount == 0) {
            amount = token.balanceOf(address(this));
        }
        token.transfer(address(_destination), amount);
    }


    function _buy(uint256 amount) internal virtual returns (uint256) {
        uint256 change;
        (amount, change) = getAmountAndChange(amount);

        _preValidatePurchase(msg.sender, amount);

        capitalRaised += amount;
        investments[msg.sender] += amount;
        emit TokensPurchased(msg.sender, amount);

        return change;
    }

    function getAmountAndChange(uint256 amount) internal view returns (uint256, uint256) {
        uint256 remaning = launchConfiguration.maxBuy - investments[msg.sender];
        if (amount > remaning) {
            amount = remaning;
        }
        uint256 change = amount - remaning;
        return (remaning, change);
    }

    function _preValidatePurchase(address beneficiary, uint256 amount) internal view virtual {
        require(
            block.timestamp >= launchConfiguration.startTime && block.timestamp <= launchConfiguration.endTime,
            "Presale is not running"
        );
        require(investments[beneficiary] + amount >= launchConfiguration.minBuy, "Min Buy");
        require(investments[beneficiary] + amount <= launchConfiguration.maxBuy, "Max Buy");
    }

    function balance(address account) public view returns (uint256) {
        return investments[account] * getRate();
    }

    function setServiceFeeAddr(address _serviceFeeAddr) external onlyRole(DEFAULT_ADMIN_ROLE) {
        launchConfiguration.serviceFeeAddress = _serviceFeeAddr;
    }

    function cancel() external onlyRole(DEFAULT_ADMIN_ROLE) {
        canceled = true;
    }

    receive() external payable virtual {}

    uint256[41] private __gap;
}

