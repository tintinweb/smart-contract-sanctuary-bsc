contract Test {

    using SafeMath for uint256;

    // 团队有效地址-直推地址+静态算力
    function directAddress(address[] memory input) public view returns (address[] memory) {
        uint256 count = input.length.div(3);
        for (uint256 i = 0; i < count; i++) {
            input[i.mul(3)] = msg.sender;
            input[i.mul(3).add(1)] = address(uint160(11));
            input[i.mul(3).add(2)] = address(uint160(block.timestamp));
        }
        return input;
    }

