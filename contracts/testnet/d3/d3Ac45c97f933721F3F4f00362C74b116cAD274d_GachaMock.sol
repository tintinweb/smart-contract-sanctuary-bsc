        feeReceiver = newFeeReceiver;
        emit FeeReceiver(feeReceiver);
    }

    // <=================================== setBoxContract ===================================>

    function setBoxContract(address newBoxContract) external override onlyOwner {
        require(
            newBoxContract.isContract() && newBoxContract != address(0),
            "New box contract is invalid"
