    }

// <=================================== INTERNAL FUNCTION ===================================>

    function _isWhitelistCurrency(address _tokenCurrency)
        internal
        view
        returns (bool)
    {
        return _whitelistCurrency[_tokenCurrency];
    }


    function _updateBoxDataInEvent(
        uint256 eventId,
        uint256 totalBoxesAdding,
        BoxData[] memory boxData,
        uint256 totalChildBoxes
    ) internal {

        GachaEvent storage gachaEvent = _gachaEvents[eventId];
        gachaEvent.totalBoxes += totalBoxesAdding;
        gachaEvent.childBoxData.totalAmount += totalChildBoxes;
        // init variable totalBoxesAddingByType for checking data with totalBoxesAdding
        uint256 totalBoxesAddingByType = 0;
        uint256 totalChildBoxesAddingByType = 0;

        for (uint256 i = 0; i < boxData.length; i++) {
            BoxData memory _boxData = boxData[i];
            require(
                _boxData.totalBoxesByRarity.length == 10,
                "Total boxes by rarity length != 10"
                //Total boxes by rarity list must have ten elements
            );

            require(
                _isWhitelistCurrency(_boxData.tokenCurrency),
                "Token currency is not whitelist"
            );

            uint256 typeBox = _boxData.typeBox;
            (bool isExist, ) = ArrayLib.checkExists(
                gachaEvent.typeBoxes,
                typeBox
            );
            if (!isExist) {
                gachaEvent.typeBoxes.push(typeBox);
            }

            if (_boxData.boxesByType > 0 ) {

                //update total box adding by type in event
                gachaEvent.boxesByType[typeBox] += _boxData.boxesByType;
                gachaEvent.childBoxData.childBoxesByType[typeBox] += _boxData.totalChildBoxesByType;
                //update total box adding by type for variable checking
                totalBoxesAddingByType += _boxData.boxesByType;
                totalChildBoxesAddingByType += _boxData.totalChildBoxesByType;
                //check sum of totalBoxesByRarity list equal _boxData.boxesByType
                {
                    // init variable total box by rarity for checking
                    uint256 totalBoxesByRarity = 0;
                    for (
                        uint256 j = 0;
                        j < _boxData.totalBoxesByRarity.length;
                        j++
                    ) {
                        if(_boxData.totalBoxesByRarity[j] > 0 && ((_boxData.typeBox == uint8(GachaBoxLib.Type.GRAND_BOX)) || (_boxData.typeBox == uint8(GachaBoxLib.Type.META_BOX)))){
                            require(_boxData.totalChildBoxesByType > 0, "total child boxes = 0");
                            //total child boxes must greater than 0
                        }
                        totalBoxesByRarity += _boxData.totalBoxesByRarity[j];
                    }
                    require(
                        totalBoxesByRarity == _boxData.boxesByType,
                        "Total boxes by rarity != boxes by type"
                        //Total boxes by rarity must be equal boxes by type
                    );
                }
            }

            if (gachaEvent.boxesByType[typeBox] > 0) {
                require(
                    _boxData.price > 0,
                    "Box price < 0"
                );
            }

            _setGachaBoxPrice(
                eventId,
                _boxData.tokenCurrency,
                typeBox,
                _boxData.price
            );
            
            emit UpdateEventData(
                eventId,
                typeBox,
                _boxData.boxesByType,
                _boxData.totalChildBoxesByType,
                _boxData.totalBoxesByRarity,
                _boxData.tokenCurrency,
                _boxData.price
            );
        }
        require(
            totalBoxesAddingByType == totalBoxesAdding,
            "Total boxes by type != total boxes"
            //Total boxes by type must be equal total boxes
        );
        require(
            gachaEvent.typeBoxes.length == boxData.length,
            "Total type boxes != box data lists "
            //Total type boxes and box data lists must have same length
        );
    }

    function _payment(address _tokenCurrency, uint256 _boxPrice) internal {
        if (_tokenCurrency == address(0)) {
            // Handle BNB
            require(_msgSender().balance >= _boxPrice, "Not enough balance");
            require(msg.value == _boxPrice, "Payment amount is invalid");
            // payable(feeReceiver).transfer(boxPrice);
            (bool success, ) = feeReceiver.call{value: _boxPrice}("");
            require(success, "Failed transfer");
        } else {
            //Handle ERC20
            require(
                IERC20Upgradeable(_tokenCurrency).balanceOf(_msgSender()) >=
                    _boxPrice,
                "Not enough balance"
            );
            require(
                IERC20Upgradeable(_tokenCurrency).allowance(
                    _msgSender(),
                    address(this)
                ) >= _boxPrice,
                "Not allowance"
            );
            IERC20Upgradeable(_tokenCurrency).safeTransferFrom(
                _msgSender(),
                feeReceiver,
                _boxPrice
            );
        }
    }

    function _setGachaBoxPrice(
        uint256 eventId,
        address tokenCurrency,
        uint256 typeBox,
        uint256 price
    ) internal {
        _gachaEvents[eventId].price[tokenCurrency][typeBox] = price;
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function _unboxDetail(
        address wallet,
        uint256 boxId,
        ItemType giftType,
        BoxNft[] memory boxNft,
        EquipmentNft memory equipmentNft,
        DogNft memory dogNft,
        DKToken memory dkToken
    ) internal returns(uint256){
        uint256 tokenId = 0;
        bytes32 hashData;
        if(giftType == ItemType.Box) {
            hashData = keccak256(abi.encodePacked(boxNft[0].typeBox, boxNft[0].hashData));
            (, bytes32 boxData) = boxContract.boxInformation(boxId);
            require(hashData == boxData, "Data is invalid");
            for(uint256 i = 1; i < boxNft.length; i++) {

                // create box
                tokenId = boxContract.mint(
                    wallet,
                    boxNft[i].typeBox,
                    boxNft[i].hashData,
                    boxNft[i].cid
                );
            } 
        } else if (giftType == ItemType.DogNFT) {
            hashData = keccak256(abi.encodePacked(
                    dogNft.strength,
                    dogNft.speed,
                    dogNft.stamina,
                    dogNft.weight
                ));
            (, bytes32 boxData) = boxContract.boxInformation(boxId);
            require(hashData == boxData, "Data is invalid");

            // create dog
            tokenId = dogContract.mint(
                wallet,
                dogNft.strength,
                dogNft.speed,
                dogNft.stamina,
                dogNft.weight,
                dogNft.isPermanentLoyalty,
                dogNft.cid
            );
        } else if (giftType == ItemType.EquipmentNFT) {
            hashData = keccak256(abi.encodePacked(
                    equipmentNft.speed,
                    equipmentNft.stamina,
                    equipmentNft.strength
                ));
            (, bytes32 boxData) = boxContract.boxInformation(boxId);
            require(hashData == boxData, "Data is invalid");

            // create equipment
            tokenId = equipmentContract.mint(
                wallet,
                equipmentNft.speed,
                equipmentNft.stamina,
                equipmentNft.strength,
                equipmentNft.cid
            );
        } else {
            hashData = keccak256(abi.encodePacked(
                    dkToken.tokenCurrency,
                    dkToken.amount
                ));
            (, bytes32 boxData) = boxContract.boxInformation(boxId);
            require(hashData == boxData, "Data is invalid");

            // transfer token
            IERC20Upgradeable(dkToken.tokenCurrency).transferFrom(msg.sender, wallet, dkToken.amount);
        }
        return tokenId;
    }

    function _validateGachaEvent(
        string memory name,
        uint256 startTime,
        uint256 endTime,
        uint256 totalTypeBox,
        uint256 totalBoxes,
        BoxData[] memory boxData
    ) internal pure {
        require(bytes(name).length > 0, "Name must not be empty"); //Name must not be empty
        require(startTime < endTime, "Start time >= end time"); //Start time must be less then end time
        require(totalBoxes > 0, "Total boxes == 0"); //Total boxes must be than greater than zero
        require(
            totalTypeBox > 0,
            "Total type box == 0"
        ); //Total type box must be than greater than zero
        require(
            boxData.length == totalTypeBox,
            "Box data lists != total type box"
        ); //Box data lists must be equal total type box
    }

    function _handleBuyChildBox(
        uint256 eventId,
        uint256 typeBox,
        uint256[] memory childBoxByType,
        uint256 randomSeed
    ) internal {
        if(typeBox == uint8(GachaBoxLib.Type.GRAND_BOX) || typeBox == uint8(GachaBoxLib.Type.META_BOX)) {
            GachaEvent storage gachaEvent = _gachaEvents[eventId];
            uint256 totalAmount = 0;
            for(uint256 i = 0; i < childBoxByType.length; i++) {
                totalAmount += childBoxByType[i];
            }
            
            require(gachaEvent.childBoxData.childBoxesByType[typeBox] >= totalAmount, "amount of child box invalid");
            
            for(uint256 i = 0; i < childBoxByType.length; i++) {
                uint256 indexChildBoxData = GachaBoxLib._random(
                    gachaEvent.childBoxData.childBoxesByType[typeBox],
                    randomSeed,
                    _msgSender()
                );
                GachaBoxLib._remove(gachaEvent.childBoxData.childBoxDataPushedByType[typeBox], indexChildBoxData);
                gachaEvent.childBoxData.childBoxesByType[typeBox] -= childBoxByType[i];

            }
            gachaEvent.childBoxData.totalAmount -= totalAmount;
            gachaEvent.childBoxData.totalAmountPushed -= totalAmount;
        }
    }

}

