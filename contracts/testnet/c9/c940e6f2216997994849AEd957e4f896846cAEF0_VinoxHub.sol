import "../lib/types.sol";
import "../../../tokens/ERC1155Base.sol";

contract Utility is ERC1155Base, VinoxHubStorage, IUtility {
    string public name;
    string public symbol;

    mapping(uint256 => uint256) coolingdownSettings;

    constructor(string memory _name, string memory _symbol, string memory _uri) ERC1155(_uri) {
        name = _name;
        symbol = _symbol;
    }

    /// @dev the newuri should be the base uri of the metadata uris,
    ///         e.g, https://app.mercurylab.xyz/metadata/ instead of
    function setURI(string memory newuri) external onlyOwner {
        _setURI(newuri);
    }

    /// @dev override the default implementation for the NFT to display correctly on OpenSea
    function uri(uint256 tokenId) public view override returns (string memory) {
        return string(abi.encodePacked(super.uri(tokenId), Strings.toString(tokenId), ".json"));
    }

    function ifOnboardingCharacter(uint256[] memory ids) private pure returns(bool) {
        for (uint i = 0; i < ids.length; i++) {
            if (ids[i] == 1) {
                return true;
            }
        }
        return false;
    }

    function mint(address to, uint256 id, uint256 amount, bytes memory data) public override onlyRole(GOVERNOR_ROLE) {
        require(id != 1 || !_playerMinted[to], "Player already claimed onboarding pass");
        _mint(to, id, amount, data);
        _playerMinted[to] = true;
    }

    function burn(
        address account,
        uint256 id,
        uint256 value
    ) public override(IUtility, ERC1155Burnable) onlyRole(GOVERNOR_ROLE) {
        super.burn(account, id, value);
    }

    function burnBatch(
        address account,
        uint256[] memory ids,
        uint256[] memory values
    ) public override(IUtility, ERC1155Burnable) onlyRole(GOVERNOR_ROLE) {
        super.burnBatch(account, ids, values);
    }

    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) external payable onlyRole(GOVERNOR_ROLE) {
        bool onboardingCharacter = ifOnboardingCharacter(ids);
        require(!onboardingCharacter || !_playerMinted[to], "Player already claimed onboarding pass");
        _mintBatch(to, ids, amounts, data);
        if (onboardingCharacter) {
            _playerMinted[to] = true;
        }
    }

    // todo, move this to an extension
    function _beforeTokenTransfer(
        address operator,
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) internal virtual override {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
        int numOfBasic = 0;
        for (uint i = 0; i < ids.length; i++) {
            if (ids[i] == 1) {
                ++numOfBasic;
            }
        }
        require(numOfBasic == 0 || from == address(0), "Cannot transfer onboarding character NFT");
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC1155Base) returns (bool) {
        return interfaceId == type(IUtility).interfaceId || super.supportsInterface(interfaceId);
    }
}

