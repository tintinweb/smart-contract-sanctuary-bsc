pragma solidity ^0.8.16;

import "./fixins/FixinDrawer.sol";
import "../interfaces/IMysteryBox.sol";

contract Mix4BoxDrawer is FixinDrawer {

    address private mysteryBoxAddress;

    constructor(
        address mysteryBoxAddress_,
        uint64 subscriptionId_,
        address vrfCoordinatorAddress_,
        bytes32 keyHash_
    ) FixinRandomGenerator(subscriptionId_, vrfCoordinatorAddress_, keyHash_) {
        mysteryBoxAddress = mysteryBoxAddress_;
        utilityTotals[Types.UtilityType.Variety] = VARIETY_TOTAL;
        utilityTotals[Types.UtilityType.Terroir] = TERROIR_TOTAL;
        utilityTotals[Types.UtilityType.Tool] = TOOL_TOTAL;
        utilityTotals[Types.UtilityType.Character] = CHAR_TOTAL;
    }
    
    function fillBoxes(Types.DrawRequest memory request, uint256[] memory words) internal override {
        bool isFreeBox = IMysteryBox(mysteryBoxAddress).freeRewardToken(request.id);        
        uint256[] memory items = new uint256[](4);
        // select the misc token (4th word)    
        Types.UtilityType _slot4 = selectSlot4(words[3]);
        
        // select variety (1st word)
        uint256 variety = selectUtility(Types.UtilityType.Variety, words[0], isFreeBox);
        // select terroir (2nd word)
        uint256 terroir = selectUtility(Types.UtilityType.Terroir, words[1], isFreeBox);
        // select tool (3rd word)
        uint256 tool = selectUtility(Types.UtilityType.Tool, words[2], isFreeBox);
        uint256 misc = selectUtility(_slot4, words[3], isFreeBox);
        if (misc == variety || misc == terroir || misc == tool) {
            misc = selectUtility(_slot4, words[3], isFreeBox);
        }
        items[0] = variety;
        items[1] = terroir;
        items[2] = tool;
        items[3] = misc;
        mysteryBoxes[request.id] = Types.MysteryBox(request.id, items, true);
        pendingRequests[request.id] = 0;
        openedBoxes[request.id] = true;
    }    
}

