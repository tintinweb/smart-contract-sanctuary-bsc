        mysteryBoxes[request.id] = Types.MysteryBox(request.id, items, true);
        pendingRequests[request.id] = 0;
        openedBoxes[request.id] = true;
    }    
}

pragma solidity ^0.8.16;

import "../../lib/types.sol";
import "./FixinRandomGenerator.sol";
import "../../interfaces/IMysteryBoxDrawer.sol";

abstract contract FixinDrawer is FixinRandomGenerator, IMysteryBoxDrawer {

    uint16 private constant REQUEST_CONFIRMATIONS = 3;
    uint16 private constant VARIETY_START_IDX = 10001;
    uint16 private constant TERROIR_START_IDX = 1001;
    uint16 private constant TOOL_START_IDX = 5001;
    uint16 private constant CHAR_START_IDX = 1;

    // Total utility nfts might be updated from time to time
    uint16 internal constant VARIETY_TOTAL = 50;
    uint16 internal constant TERROIR_TOTAL = 15;
    uint16 internal constant TOOL_TOTAL = 25;
    uint16 internal constant CHAR_TOTAL = 10;

    uint16 private constant COMMON_LEVEL = 0;
    uint16 private constant UNCOMMON_LEVEL = 1;
    uint16 private constant RARE_LEVEL = 2;
    uint16 private constant EPIC_LEVEL = 3; 
    uint16 private constant LEGENDARY_LEVEL = 4;

    // total number of each utility NFT types
    mapping(Types.UtilityType => uint256) internal utilityTotals;
    // opened boxes 
    mapping(uint256 => bool) internal openedBoxes;
      // pending request to be filled boxId -> request id
    mapping(uint256 => uint256) internal pendingRequests;
    mapping(uint256 => Types.MysteryBox) internal mysteryBoxes;
    
    event MysteryBoxOpened(uint256 indexed boxId, uint256 variety, uint256 terrior, uint256 tool, uint256 misc, Types.UtilityType miscType);
    event RandomNumberRequested(uint256 indexed requestId, uint numOfItems, uint256 gasLimit);

    function requestRandomWords(Types.DrawRequest[] memory requests) external onlyRole(GOVERNOR_ROLE) {
        uint totalNumOfWords = 0;
        for (uint i = 0; i < requests.length; ++i) {
            Types.DrawRequest memory req = requests[i];
            require(readyToOpen(req.id), "box must not be opened or being opened previously!");
            totalNumOfWords += req.numOfItems;    
        }
        uint256 requestId = super.requestRandomWords_(totalNumOfWords);
        for (uint i = 0; i < requests.length; ++i) {
            drawRequests[requestId].push(requests[i]);
            pendingRequests[requests[i].id] = requestId; 
        }
        emit RandomNumberRequested(requestId, totalNumOfWords, gasLimit);
    }

    function setUtilityTotal(Types.UtilityType ut, uint256 totals) external onlyRole(GOVERNOR_ROLE) {
        utilityTotals[ut] = totals;
    }

    function startIdx(Types.UtilityType _type) internal pure returns(uint256 idx) {
        if (_type == Types.UtilityType.Variety) {
            return VARIETY_START_IDX;
        } else if (_type == Types.UtilityType.Terroir) {
            return TERROIR_START_IDX;
        } else if (_type == Types.UtilityType.Tool) {
            return TOOL_START_IDX;
        } else if (_type == Types.UtilityType.Character) {
            return CHAR_START_IDX;
        }
    }

    // we have 5 levels per token, so we reserve 5 token IDs for each type of the token
    function selectUtility(Types.UtilityType _type, uint256 _seed, bool isFree) view internal returns (uint256){
        uint256 _total = utilityTotals[_type];
        uint256 _startIdx = startIdx(_type);
        uint256 _random = _seed % _total;
        uint256 _level = selectLevel(_seed, isFree);
        return _startIdx + (_random - 1) * 5 + _level;
    }

    function selectLevel(uint256 _seed, bool isFree) internal pure returns (uint256) {
        uint256 position = _seed % 100;
        if (position == 1) {
            if (isFree) {
                return UNCOMMON_LEVEL;
            }
            return LEGENDARY_LEVEL;
        } else if (position <= 5) {
            if (isFree) {
                return COMMON_LEVEL;
            }
            return EPIC_LEVEL;
        } else if (position <= 20) {
            return RARE_LEVEL;
        } else if (position <= 50) {
            return UNCOMMON_LEVEL;
        } 
        return COMMON_LEVEL;
    }

    function selectSlot4(uint _seed4) pure internal returns (Types.UtilityType) {
        uint256 position = _seed4 % 100;
        if (position <= 32) {
            return Types.UtilityType.Variety;
        } else if (position <= 64) {
            return Types.UtilityType.Terroir;
        } else if (position <= 96) {
            return Types.UtilityType.Tool;
        } else {
            return Types.UtilityType.Character;
        }
    }

    function setRequestId(uint256 id, uint256 requestId) external {
        pendingRequests[id] = requestId;
    }

    function setOpenStatus(uint256 id, bool opened) external {
        openedBoxes[id] = opened;
    }

    function readyToOpen(uint256 id) public view returns (bool) {
        return !openedBoxes[id] && pendingRequests[id] == 0;
    }

    function fetchUtilities(uint256[] memory boxes) public view returns(uint256[] memory) {
        uint256 totalItems = 0;
        for (uint i = 0; i < boxes.length; ++i) {
            Types.MysteryBox memory box = mysteryBoxes[boxes[i]];
            totalItems += box.items.length;
        }
        uint256[] memory utilities = new uint256[](totalItems);
        uint _startidx = 0;
        for (uint i = 0; i < boxes.length; ++i) { 
            Types.MysteryBox memory box = mysteryBoxes[boxes[i]];
            for (uint j = 0; j < box.items.length; ++j) {
                utilities[_startidx+j] = box.items[j];
            }
            _startidx += box.items.length;
        }
        return utilities;
    }

    function mysteryBox(uint256 id) external view returns(Types.MysteryBox memory) {
        return mysteryBoxes[id];
    }
}

