pragma solidity ^0.8.9;

import "lib/ERC721A/contracts/ERC721A.sol";
import "lib/ERC721A/contracts/IERC721A.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC721/ERC721.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC1155/IERC1155.sol";
import "lib/openzeppelin-contracts/contracts/token/ERC721/IERC721.sol";
import "lib/openzeppelin-contracts/contracts/access/Ownable.sol";
import "lib/openzeppelin-contracts/contracts/token/common/ERC2981.sol";

error NotWhiteListed();
error InvalidPrice();
error InvalidQuantity();
error InvalidPassType();
error FreeMintUsed();
error WithdrawFailed();
error MintingNotStarted();
error MintingFinished();
error OnlyOneFreeMintAllowed();

contract VinoxGatingPass is ERC721A, ERC2981, Ownable {

    uint256 public constant MINT_START = 1666530000;
    event NFTWhiteListed(address indexed player);
    event NFTPassPurchased(address indexed buyer, uint256 indexed quantity, uint256 price);
    event OwnerWithdrawn(uint256 amount);
    event OwnerMinting(uint256 quantity);

    enum NFTPassType { ERC721, ERC1155, ERC721A }
    // baseURI for the token metadata
    string public metadataURI;
    // whitelist for early adopter passes
    mapping(address => bool) public eapWhitelist ;

    // for any whitelist wallet address, max number of passes they can mint is 1
    mapping(address => bool) public whitelistMinted;

    constructor(string memory name_, string memory symbol_, string memory baseUrl_) ERC721A(name_, symbol_) {
        metadataURI = baseUrl_;
    }

    function contractURI() public pure returns (string memory) {
        return "https://metadata.vitidiary.com/gating-metadata";
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default, it can be overridden in child contracts.
     */
    function _baseURI() internal view virtual override returns (string memory) {
        return metadataURI;
    }

    function _startTokenId() internal view virtual override returns (uint256) {
        return 1;
    }

    function checkUserMinted(address wallet) public view returns (bool) {
        return whitelistMinted[wallet];
    }

    function _mintingStarted() private view returns (bool) {
        return block.timestamp >= MINT_START;
    }

    function checkWhitelist(address player) public view returns (bool) {
        return eapWhitelist[player];
    }

    function batchCheckWhitelist(address[] memory players) public view returns (bool[] memory) {
        bool[] memory ret = new bool[](players.length);
        for (uint i = 0; i < players.length; ++i) {
            ret[i] = eapWhitelist[players[i]];
        }
        return ret;
    }

    function batchWhitelist(address[] memory players) external onlyOwner { 
        for (uint i = 0; i < players.length; ++i) {
            whitelist(players[i]);
        }
    }

    function whitelist(address player) public onlyOwner {
       eapWhitelist[player] = true;
    }

    function withdraw() external {
        uint256 balance = address(this).balance;
        address payable owner = payable(owner());
        (bool success, ) = owner.call{value: balance}("");
        if (!success) revert WithdrawFailed();
        emit OwnerWithdrawn(balance);
    }

    function setDefaultRoyaltyInfo(address receiver, uint96 feeBasisPoints) external onlyOwner {
        _setDefaultRoyalty(receiver, feeBasisPoints);
    }

    function setTokenRoyaltyInfo(uint256 tokenId, address receiver, uint96 feeBasisPoints) external onlyOwner {
        _setTokenRoyalty(tokenId, receiver, feeBasisPoints);
    }

    /**
     * @dev Mints Gating Pass for the given wallet address and transfer to the sender
     *
     * Requirements:
     *
     * - `quantity is required if it mints multiple tokens`.
     *
     * Emits a {Transfer} event.
     */
    function mint(uint256 quantity) external payable {
        // we only issue 1855 gating passes.
        if (totalSupply() + quantity > 1855) {
            revert MintingFinished();
        }
        
        // `_mint`'s second argument now takes in a `quantity`, not a `tokenId`.
        // if (!_mintingStarted()) revert MintingNotStarted();
        bool isWhitelisted;
        if (msg.sender == owner()) {
            emit OwnerMinting(quantity);
        } else {
            // // every whitelisted user can only do one free mint
            if (quantity > 1) {
                revert InvalidQuantity();
            }
            // check if the user has already minted a pass for free
            if (checkUserMinted(msg.sender)) {
                revert OnlyOneFreeMintAllowed();
            }
            // check the user is on the whitelist
            if (!checkWhitelist(msg.sender)) {
                revert NotWhiteListed();
            }
            isWhitelisted = true;
        }
        _mint(msg.sender, quantity);
        if (isWhitelisted) {
            whitelistMinted[msg.sender] = true;
        }
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC2981, ERC721A) returns (bool) {
        return ERC721A.supportsInterface(interfaceId) || ERC2981.supportsInterface(interfaceId);
    }
}

