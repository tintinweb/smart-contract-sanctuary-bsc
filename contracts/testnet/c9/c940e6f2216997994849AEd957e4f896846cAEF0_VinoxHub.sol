import "lib/ERC721A/contracts/IERC721A.sol";
import "lib/chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol";
import "lib/chainlink/contracts/src/v0.8/VRFConsumerBaseV2.sol";

abstract contract FixinRandomGenerator is VRFConsumerBaseV2, FixinAccessControl { 
     uint16 private constant REQUEST_CONFIRMATIONS = 3;
     /// @dev Chainlink VRFv2 callback gas limit
     uint32 private constant CALLBACK_GAS_LIMIT = 600_000;

     VRFCoordinatorV2Interface private vrfCoordinator;
     uint32 gasLimit;
     /// @dev Chainlink VRF subscription id
     uint64 private subscriptionId;
     /// @dev Chainlink VRF coordinator address
     address private vrfCoordinatorAddress;
     /// @dev Chainlink VRF keyHash
     bytes32 private keyHash;

     event VRFCoordinatorUpdated(address indexed vrfCoordinatorAddress);

     // the mapping between box type and number of items per boxes
     mapping(uint256 => Types.DrawRequest[]) internal drawRequests;

     constructor(
        uint64 subscriptionId_,
        address vrfCoordinatorAddress_,
        bytes32 keyHash_
     ) VRFConsumerBaseV2(vrfCoordinatorAddress_) {
        vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorAddress_);
        subscriptionId = subscriptionId_;
        keyHash = keyHash_;
        gasLimit = CALLBACK_GAS_LIMIT;  
     }

     function requestRandomWords_(uint totalNumOfWords) internal returns(uint256) {
          return vrfCoordinator.requestRandomWords(
            keyHash,
            subscriptionId,
            REQUEST_CONFIRMATIONS,
            gasLimit,
            uint32(totalNumOfWords)
          );
     }

     function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords) internal override {
          Types.DrawRequest[] memory requests = drawRequests[requestId];
          uint startIdx = 0;
          for (uint i = 0; i < requests.length; ++i) {
               uint _numOfItems = requests[i].numOfItems;
               uint256[] memory words = new uint256[](_numOfItems);  
               for (uint j = 0; j < _numOfItems; ++j) {
                    words[j] = randomWords[startIdx+j];
               }
               startIdx += _numOfItems;
               fillBoxes(requests[i], words); 
          }
     }

     function updateVrfCoordinator(address vrfCoordinatorAddress_) external onlyRole(GOVERNOR_ROLE) {
        require(vrfCoordinatorAddress_ != address(0), "VRF coordinator address cannot be 0");
        require(vrfCoordinatorAddress_ != vrfCoordinatorAddress, "Cannot be the same VRF coordinator address");
        vrfCoordinatorAddress = vrfCoordinatorAddress;
        vrfCoordinator = VRFCoordinatorV2Interface(vrfCoordinatorAddress_);
        emit VRFCoordinatorUpdated(vrfCoordinatorAddress_);
     }

     function updateGasFeeLimit(uint32 gaslimit_) external {
          require (isGovernor(), "only governor or owner can update the gas limit");
          gasLimit = gaslimit_;
     }

     function fillBoxes(Types.DrawRequest memory request, uint256[] memory words) internal virtual;  
}

