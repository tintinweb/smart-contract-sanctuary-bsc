import "lib/ERC721A/contracts/extensions/IERC721AQueryable.sol";
import "lib/ERC721A/contracts/extensions/ERC721AQueryable.sol";
import "lib/openzeppelin-contracts/contracts/access/IAccessControl.sol";

contract MysteryBox is ERC721ABase, IMysteryBox {

    uint256 public price = 0;
    string public boxType;
    // the mystery box data with drawn utility ids
    mapping(uint256 => bool) public freeRewardClaimed;
    mapping(uint256 => string) private tokenURIs;
    
    constructor(string memory _name, string memory _symbol, string memory boxType_, string memory _metadataUrl, string memory _contractUrl) ERC721ABase(_name, _symbol, _metadataUrl, _contractUrl){
        boxType = boxType_;
    }

    function contractURI() public view returns (string memory) {
        return _contractURI;
    }

    function setPrice(uint256 _price) external {
        require(isGovernor(), "User needs to have Governor role to change the price");
        price = _price;
    }

    function setTokenUri(uint256 _id, string memory _tokenURI) external  {
        require(isGovernor(), "User needs to have Governor role to change the token URI");
        tokenURIs[_id] = _tokenURI;
    }

    function tokenURI(uint256 tokenId) public view override(IERC721A, ERC721A) returns (string memory) {   
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        if (bytes(tokenURIs[tokenId]).length > 0) {
            return tokenURIs[tokenId];
        }
        return _baseURI();
    }

    function balanceOf(address owner) public view override(IERC721A, ERC721A, IMysteryBox) returns (uint256 balance) {
        return super.balanceOf(owner);
    }

    function tokenOwner(uint256 tokenId) external view  returns (address) {
        return super.ownerOf(tokenId);
    }

    function mint(address to, uint256 amount, bool freeReward) public payable {
        bool isGovenor = isGovernor();
        if (!isGovenor && !freeReward && msg.value < price * amount) {
            revert("Insufficient Payment Value");
        }
        if (freeReward && !isGovenor) {
            revert("Only governance contract or owner can mint free rewards");
        }
        uint256 nextId = _nextTokenId();
        _mint(to,amount);
        
        if (freeReward) {
            freeRewardClaimed[nextId] = true;
        }   
    }

    function freeRewardToken(uint256 id) public view returns(bool) {
        return freeRewardClaimed[id];
    }

    // mintBatch doesn't support free rewards
    function mintBatch(
        address to,
        uint quantity
    ) external payable {
        if (!isGovernor() && msg.value < price * quantity) {
            revert("Insufficient Payment Value, or not governance contract or owner");
        }
        _mint(to, quantity);
    }

    function burn(uint256 tokenId) external onlyRole(GOVERNOR_ROLE) {
        require(isBurner(), "User needs to have Burner role to burn Mystery Box");
        _burn(tokenId);
    }

    function burnBatch(uint256[] memory tokens) external onlyRole(GOVERNOR_ROLE) {
        require(isBurner(), "User needs to have Burner role to burn Mystery Box");
        for (uint256 i = 0; i < tokens.length; ++i) {
            _burn(tokens[i]);
        }        
    }

    function mysteryBoxType() external view returns (string memory) {
        return boxType;
    }

    function checkOwnership(address owner, uint256[] memory tokens) external view override returns (bool) {
        return _checkOwnership(owner, tokens);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721ABase) returns (bool) {
        return interfaceId == type(IMysteryBox).interfaceId ||
               super.supportsInterface(interfaceId);
    }
}

