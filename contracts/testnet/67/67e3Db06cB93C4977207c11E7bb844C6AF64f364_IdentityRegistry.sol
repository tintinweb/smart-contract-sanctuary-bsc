pragma solidity ^0.8.7;

import "@openzeppelin/contracts/utils/Create2.sol";
import "../tokens/erc721_1155/Asset.sol";
import "../identities/Identity.sol";
import "../identities/ClaimVerifier.sol";
import "hardhat/console.sol";

contract IdentityRegistry is ClaimVerifier {
    Asset asset;

    event RegistedIdentity(
        address indexed identity,
        address indexed asset,
        uint256 indexed tokenId,
        address owner
    );

    constructor(address _trustedClaimHolder, address _asset)
        ClaimVerifier(_trustedClaimHolder)
    {
        asset = Asset(_asset);
    }

    function registIdentity(
        uint256[] memory _claimType,
        uint256[] memory _scheme,
        address[] memory _issuer,
        bytes memory _signature,
        bytes memory _data,
        string memory _uri,
        uint256[] memory _sigSizes,
        uint256[] memory dataSizes,
        uint256[] memory uriSizes,
        bytes32 salt,
        uint256 tokenId,
        string memory metadataURI
    ) external {
        // Deploy a new identity
        bytes memory bytecode = type(Identity).creationCode;
        bytecode = abi.encodePacked(bytecode, abi.encode(_msgSender()));
        address identity = Create2.deploy(0, salt, bytecode);

        // Add multiple claim
        Identity(identity).addMultiple(
            _claimType,
            _scheme,
            _issuer,
            _signature,
            _data,
            _uri,
            _sigSizes,
            dataSizes,
            uriSizes
        );

        // Verify and minting a NFT
        // First claim type must be dedicated to validate
        require(
            _claimType.length > 0 && checkClaim(identity, _claimType[0]),
            "Claim is invalid"
        );
        asset.mint(_msgSender(), tokenId, metadataURI);

        emit RegistedIdentity(identity, address(asset), tokenId, _msgSender());
    }

    function computeAddress(address owner, bytes32 salt)
        external
        view
        returns (address precomputedAddr)
    {
        bytes memory bytecode = type(Identity).creationCode;
        bytecode = abi.encodePacked(bytecode, abi.encode(owner));

        precomputedAddr = Create2.computeAddress(salt, keccak256(bytecode));
    }
}

