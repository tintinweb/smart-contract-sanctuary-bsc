pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";

import "./ChainlinkClientUpgradeable.sol";

interface IERC20 {
    function totalSupply() external view returns (uint256);

    function balanceOf(address account) external view returns (uint256);

    function transfer(address recipient, uint256 amount) external returns (bool);

    function allowance(address owner, address spender) external view returns (uint256);

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);

    function mint(address to, uint256 amount) external;
}

contract EconUpgradeable is
    ChainlinkClientUpgradeable,
    AccessControlEnumerableUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable
{
    using Chainlink for Chainlink.Request;

    bytes32 public constant ORACLE_ROLE = keccak256("ORACLE_ROLE");

    address private _oracle;
    bytes32 private _jobId;
    uint256 private _fee;

    address private _pulv;
    address private _psb;

    struct RequestInfo {
        bytes32 requestId;
        uint256 amount;
        bool isProcessing;
        uint256 requestTime;
    }

    // user => token => requestInfo
    mapping(address => mapping(address => RequestInfo)) private _requests;

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "Error: ADMIN role required");
        _;
    }

    modifier onlyWhitelistToken(address token) {
        require(token == _pulv || token == _psb, "Error: Token not allowed");
        _;
    }

    event Deposited(address user, address token, uint256 amount);
    event RequestWithdraw(address user, bytes32 requestId, address token, uint256 amount);
    event Withdrawn(address user, bytes32 requestId, string lockId, address token, uint256 amount);
    event WithdrawFailed(address user, bytes32 requestId, string lockId, address token, uint256 amount);

    function initialize(
        address link,
        address oracle,
        string memory jobId
    ) public virtual initializer {
        setChainlinkToken(link);
        _oracle = oracle;
        _jobId = stringToBytes32(jobId);
        _fee = 0.1 * 1 ether;
        _setupRole(ORACLE_ROLE, oracle);
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        __ChainlinkClient_init();
    }

    function setPULV(address pulv) public nonReentrant onlyAdmin {
        _pulv = pulv;
    }

    function setPSB(address psb) public nonReentrant onlyAdmin {
        _psb = psb;
    }

    function deposit(address token, uint256 amount) external nonReentrant whenNotPaused onlyWhitelistToken(token) {
        require(IERC20(token).allowance(payable(_msgSender()), address(this)) >= amount, "Error: Token is not allowed");

        IERC20(token).transferFrom(_msgSender(), address(this), amount);

        emit Deposited(_msgSender(), token, amount);
    }

    function withdraw(address token, uint256 amount)
        external
        nonReentrant
        whenNotPaused
        onlyWhitelistToken(token)
        returns (bytes32 requestId)
    {
        require(!_requests[_msgSender()][token].isProcessing, "Error: Duplicate request");
        require(
            _requests[_msgSender()][token].requestTime + 1 hours < block.timestamp,
            "Error: Limit 1 withdraw per hour"
        );

        Chainlink.Request memory request = buildChainlinkRequest(_jobId, address(this), this.fulfill.selector);

        request.add("address", addressToString(_msgSender()));
        request.add("token", addressToString(token));
        request.addUint("amount", amount);

        requestId = sendChainlinkRequestTo(_oracle, request, _fee);

        _requests[_msgSender()][token] = RequestInfo(requestId, amount, true, block.timestamp);

        emit RequestWithdraw(_msgSender(), requestId, token, amount);
    }

    function fulfill(
        bytes32 requestId,
        string memory lockId,
        address beneficiary,
        address token,
        uint256 amount
    ) public recordChainlinkFulfillment(requestId) {
        require(hasRole(ORACLE_ROLE, _msgSender()), "Error: ORACLE role required");
        require(_requests[beneficiary][token].isProcessing, "Error: Request handled");

        IERC20 tokenContract = IERC20(token);

        if (tokenContract.balanceOf(address(this)) >= amount) {
            if (tokenContract.transfer(beneficiary, amount)) {
                _requests[beneficiary][token].isProcessing = false;
                emit Withdrawn(beneficiary, requestId, lockId, token, amount);
            } else {
                delete _requests[beneficiary][token];
                emit WithdrawFailed(beneficiary, requestId, lockId, token, amount);
            }
        }

        if (tokenContract.balanceOf(address(this)) < amount) {
            if (token == _pulv) {
                tokenContract.mint(beneficiary, amount);
                _requests[beneficiary][token].isProcessing = false;
                emit Withdrawn(beneficiary, requestId, lockId, token, amount);
            } else {
                delete _requests[beneficiary][token];
                emit WithdrawFailed(beneficiary, requestId, lockId, token, amount);
            }
        }
    }

    function stringToBytes32(string memory source) private pure returns (bytes32 result) {
        bytes memory tempEmptyStringTest = bytes(source);
        if (tempEmptyStringTest.length == 0) {
            return 0x0;
        }

        assembly {
            result := mload(add(source, 32))
        }
    }

    function addressToString(address addr) private pure returns (string memory) {
        return string(abi.encodePacked(addr));
    }
}

