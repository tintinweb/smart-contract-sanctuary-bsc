pragma solidity ^0.8.0;

import "./interfaces/IWiredPair.sol";
import "./interfaces/IWiredMaker.sol";
import "./interfaces/IWiredTaker.sol";

abstract contract WiredPair is IWiredPair {

    address public override maker;
    address public override taker;

    string private constant sForbidden = "WiredPair: Forbidded";
    string private constant sZeroAddress = "WiredPair: Zero address";

    modifier canWire() virtual;
    function getOwner() external view override virtual returns (address);

    function _isExternalCall() internal view virtual returns (bool) {
        return ! (msg.sender == maker || msg.sender == taker);
    }

    function setMaker(address _maker) public override virtual canWire {
        require(_maker != address(0), sZeroAddress);
        maker = _maker;
        emit SetMaker(_maker);
    }

    function setTaker(address _taker) public override virtual canWire {
        require(_taker != address(0), sZeroAddress);
        taker = _taker;
        emit SetTaker(_taker);
    }
}

