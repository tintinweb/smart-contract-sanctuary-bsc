pragma solidity ^0.8.0;

import "../session/WiredFactory.sol";
import "./interfaces/ICrossFactory.sol";
import "../periphery/interfaces/ICrossRouter.sol";
import "./CrossPair.sol";

contract CrossFactory is WiredFactory, ICrossFactory {
    bytes32 public constant INIT_CODE_PAIR_HASH = keccak256(abi.encodePacked(type(CrossPair).creationCode));

    address public override feeTo;
    address public override owner;
    
    mapping(address => mapping(address => address)) public override getPair;
    address[] public override allPairs;

    constructor(address _owner) {
        owner = _owner;
    }

    function getOwner() external view override returns (address) {
        return owner;
    }

    modifier canWire() override virtual {
        require(msg.sender == owner, "CrossFactory: FORBIDDEN");
        _;
    }

    function setMaker(address _maker) public override(WiredFactory) virtual {
        super.setMaker(_maker);

        for (uint256 i = 0; i < allPairs.length; i++) {
            IWiredPair(allPairs[i]).setMaker(_maker);
        }
    }

    function setTaker(address _taker) public override(WiredFactory) virtual {
        super.setTaker(_taker);

        for (uint256 i = 0; i < allPairs.length; i++) {
            IWiredPair(allPairs[i]).setTaker(_taker);
        }
    }


    function allPairsLength() external view override returns (uint256) {
        return allPairs.length;
    }

    function createPair(address tokenA, address tokenB) external override returns (address pair) {
        require(maker != address(0), "Cross: NO_MAKER");
        require(taker != address(0), "Cross: NO_TAKER");
        require(tokenA != tokenB, "Cross: IDENTICAL_ADDRESSES");
        (address token0, address token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), "Cross: ZERO_ADDRESS");
        require(getPair[token0][token1] == address(0), "Cross: PAIR_EXISTS"); // single check is sufficient
        bytes memory bytecode = type(CrossPair).creationCode;
        bytes32 salt = keccak256(abi.encodePacked(token0, token1));
        assembly {
            pair := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        ICrossPair(pair).initialize(token0, token1);
        IWiredPair(pair).setMaker(maker);
        IWiredPair(pair).setTaker(taker);
        getPair[token0][token1] = pair;
        getPair[token1][token0] = pair; // populate mapping in the reverse direction
        allPairs.push(pair);
        IWiredMaker(maker).informOfPair(pair, token0, token1);
        IWiredTaker(taker).informOfPair(pair, token0, token1);

        emit PairCreated(token0, token1, pair, allPairs.length);
    }

    function setFeeTo(address _feeTo) external override {
        require(msg.sender == owner, "Cross: FORBIDDEN");
        feeTo = _feeTo;
    }

    function setOwner(address _owner) external override {
        require(msg.sender == owner, "Cross: FORBIDDEN");
        owner = _owner;
    }
}

// SPDX-License-Identifier: BUSL-1.1
pragma solidity ^0.8.0;

import "./interfaces/IWiredFactory.sol";
import "./interfaces/IWiredMaker.sol";
import "./interfaces/IWiredTaker.sol";

abstract contract WiredFactory is IWiredFactory {

    address public override maker;
    address public override taker;

    modifier canWire() virtual;

    function getOwner() external view override virtual returns (address);

    function _isExternalCall() internal view virtual returns (bool) {
        return ! (msg.sender == maker || msg.sender == taker);
    }

    function setMaker(address _maker) public override virtual canWire {
        require(msg.sender == IWiredMaker(_maker).getOwner(), "WiredFactory: Forbidden");
        require(_maker != address(0), "WiredFactory: Zero address");
        maker = _maker;
        emit SetMaker(_maker);
    }

    function setTaker(address _taker) public override virtual canWire {
        require(msg.sender == IWiredTaker(_taker).getOwner(), "WiredFactory: Forbidden");
        require(_taker != address(0), "WiredFactory: Zero address");
        taker = _taker;
        emit SetTaker(_taker);
    }
}

