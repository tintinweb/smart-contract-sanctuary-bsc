pragma solidity >=0.7.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "../core/SafeOwnable.sol";
import "../interfaces/IEMERouter.sol";
import "../interfaces/IEMEFactory.sol";

interface ISafeUser {
    function getUserLevel(address account) external view returns (uint8);

    function getReferrer(address account) external view returns (address);

    function getUserNode(address account) external view returns (address);

    function setLevel(address account) external;

    function getInviters(address account)
        external
        view
        returns (address[] memory);

    function setTokenFromReferrer(address account, address _referrer) external;
}

contract MOSAToken is IERC20, SafeOwnable {
    using SafeMath for uint256;

    string public name;
    string public symbol;
    uint8 public decimals;
    uint256 public override totalSupply;

    ISafeUser public user;
    address public pairAddr;
    address public reflowAddr;
    mapping(address => bool) public whitelist;
    address public constant hole = 0x000000000000000000000000000000000000dEaD;

    mapping(address => uint256) public override balanceOf;
    mapping(address => mapping(address => uint256)) public override allowance;

    constructor(address _reflowAddr, address usdt, address factory) {
        name = "Gamosica";
        symbol = "MOSA";
        decimals = 18;
        totalSupply = 100000000000 * (10**18);
        reflowAddr = _reflowAddr;

        balanceOf[msg.sender] = totalSupply;

        pairAddr = IEMEFactory(factory).createPair(address(this), usdt);
        emit Transfer(address(0), msg.sender, totalSupply);
    }

    function setSafeUser(address _userContract) external onlyOwner {
        user = ISafeUser(_userContract);
    }

    function setPair(address _pair) external onlyOwner {
        pairAddr = _pair;
    }

    function setrReflowAddr(address _reflowAddr) external onlyOwner {
        reflowAddr = _reflowAddr;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(to != address(0), "zero address");
        uint256 balance = balanceOf[from].mul(99).div(100);
        require(amount <= balance, "balance not enough");

        balanceOf[from] = balanceOf[from].sub(amount);
        amount = hanleFee(from, to, amount);
        balanceOf[to] = balanceOf[to].add(amount);

        user.setTokenFromReferrer(to, from);

        emit Transfer(from, to, amount);
    }

    function hanleFee(
        address from,
        address to,
        uint256 amount
    ) internal returns (uint256) {
        uint256 referrerFee;
        uint256 holeFee;
        uint256 reflowFee;
        if (from == pairAddr && !whitelist[to]) {
            address referrerAddr = user.getReferrer(to);
            if (referrerAddr == address(0)) {
                referrerAddr = reflowAddr;
            }
            referrerFee = amount.mul(3).div(100);
            balanceOf[referrerAddr] += referrerFee;
            emit Transfer(from, referrerAddr, referrerFee);

            holeFee = amount.div(100);
            balanceOf[hole] += holeFee;
            emit Transfer(from, hole, holeFee);

            reflowFee = amount.mul(2).div(100);
            balanceOf[reflowAddr] += reflowFee;
            emit Transfer(from, reflowAddr, reflowFee);
        }
        if (to == pairAddr && !whitelist[from]) {
            address referrerAddr = user.getReferrer(to);
            if (referrerAddr == address(0)) {
                referrerAddr = reflowAddr;
            }
            referrerFee = amount.mul(3).div(100);
            balanceOf[referrerAddr] += referrerFee;
            emit Transfer(from, referrerAddr, referrerFee);

            holeFee = amount.div(100);
            balanceOf[hole] += holeFee;
            emit Transfer(from, hole, holeFee);

            reflowFee = amount.mul(8).div(100);
            balanceOf[reflowAddr] += reflowFee;
            emit Transfer(from, reflowAddr, reflowFee);
        }
        return amount.sub(referrerFee.add(holeFee).add(reflowFee));
    }

    function transfer(address to, uint256 amount)
        external
        override
        returns (bool)
    {
        _transfer(msg.sender, to, amount);
        return true;
    }

    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external override returns (bool) {
        require(allowance[from][msg.sender] >= amount, "allowance not enough");

        allowance[from][msg.sender] -= amount;
        _transfer(from, to, amount);

        return true;
    }

    function approve(address spender, uint256 amount)
        external
        override
        returns (bool)
    {
        require(spender != address(0), "zero address");

        allowance[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);

        return true;
    }
}