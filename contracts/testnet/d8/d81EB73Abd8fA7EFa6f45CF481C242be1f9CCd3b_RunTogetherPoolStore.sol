    );

    constructor (address _runTogetherPoolAddress, address _tokenStakeAddress, address _withdrawTokenAddress, address _devWallet) {
        runTogetherPoolAddress = _runTogetherPoolAddress;
        tokenStakeAddress = _tokenStakeAddress;
        withdrawTokenAddress = _withdrawTokenAddress;
        runTogetherPoolInfo = IRunTogetherPool(_runTogetherPoolAddress);
        devWallet = _devWallet;
    }

    //modifier
    modifier valConfigTimePool (uint256 _startTimePool, uint256 _endTimePool) {
      require(_startTimePool < _endTimePool, "Time of pool invalid");
      _;
    }

    modifier valTimePool(){
      require(block.timestamp <= endTimePool, "Pool is end");
      require(block.timestamp >= startTimePool, "Pool is not started yet");
      _;
    }

    function setDevWallet(address _devWallet) public onlyOwner {
      devWallet = _devWallet;
    }
    /*
     * @notice: set Total Profit 
     */
    function setTotalProfit(uint256 _totalProfitAmount) public onlyOwner {
        totalProfitAmount = _totalProfitAmount;
    }

    /*
     * @notice: set Base Rate
     */
    function setBaseRate(uint256 _baseRate) public onlyOwner {
      baseRate = _baseRate;
    }

    /*
     * @notice: set Run Together Address
     */
    function setRunTogetherPoolInfo(address _runTogetherPoolAddress) public onlyOwner {
        runTogetherPoolAddress = _runTogetherPoolAddress;
        runTogetherPoolInfo = IRunTogetherPool(_runTogetherPoolAddress);
    }

    /*
     * @notice: set Token Stake Address
     */
    function setTokenStakeAddress(address _tokenStakeAddress) public onlyOwner {
        tokenStakeAddress = _tokenStakeAddress;
    }

    /*
     * @notice: set withdraw token Address
     */
    function setWithdrawTokenAddress(address _withdrawTokenAddress) public onlyOwner {
        withdrawTokenAddress = _withdrawTokenAddress;
    }

    /*
     * @notice: set time of pool
     * @params: _startTimePool: start the pool, _endTimePool: the time to close pool.
     */
    function setTimePool(uint256 _startTimePool, uint256 _endTimePool) public valConfigTimePool(_startTimePool, _endTimePool) onlyOwner {
      startTimePool = _startTimePool;
      endTimePool = _endTimePool;
    }

    /*
     * @notice: profit snapshot by month
     */
    function snapshotStakedAmount() public onlyOwner {
      uint256 userInfoArrayLength = runTogetherPoolInfo.getUserInfoArrayLength();
      uint256 currentBlockNumber = block.number;
      for(uint256 index = 0; index < userInfoArrayLength; index++)
      {
        address userAddress = runTogetherPoolInfo.getUserInfoArray(index);
        uint256 stakedAmount = runTogetherPoolInfo.getUserInfoAmount(userAddress);
        snapshotStakedAmountMap[currentBlockNumber][userAddress] = stakedAmount;
        emit snapshotStakedAmountComplete(currentBlockNumber, userAddress);
      }
      blockNumber.push(currentBlockNumber);
    }

    //return lastest block.number snapshot
    function getLastestBlock() public view returns(uint256) {
      return blockNumber[blockNumber.length - 1];
    }

    //stake amount info of user at lastest block
    function getStakeAmountByBlock(address _userAddress, uint256 _blockNumber) public view returns(uint256) {
      return snapshotStakedAmountMap[_blockNumber][_userAddress];
    }
    
    /*
     * @notice: get info of user staked into run together pool
     */
    function percentageStakedByBlock(address _userAddress, uint256 _blockNumber) public view returns(uint256) {
      uint256 userStakeAmount = getStakeAmountByBlock(_userAddress, _blockNumber);
      uint256 runTogetherPoolBalance = ERC20(tokenStakeAddress).balanceOf(runTogetherPoolAddress);
      uint256 userStakePercent = ABDKMath64x644.mulu(ABDKMath64x644.divu(userStakeAmount,runTogetherPoolBalance), baseRate);
      return userStakePercent;
    }

    /*
     * @notice: the amount of interest the user can receive
     */
    function pendingReward(address _userAddress) public view returns(uint256) {
      uint256 userProfitPercent = percentageStakedByBlock(_userAddress, getLastestBlock());
      uint256 userProfitAmount;
      if(withdrawStatusByBlockMap[_userAddress][getLastestBlock()] == false)
      {
        userProfitAmount = userProfitPercent.mul(totalProfitAmount).div(baseRate);
      }
      else userProfitAmount = 0;
      return userProfitAmount;
    }

    function withdrawStatusByBlock(address _userAddr, uint256 _blockNumber) public view returns(bool) {
      return withdrawStatusByBlockMap[_userAddr][_blockNumber];
    }

    /*
     * @notice: user withdraw profit
     * @param: _userAddress: profit transfer to address,
               _amount: transfer with amount profit
     */
    function withdraw(address _user) public nonReentrant valTimePool() {
      require(_user == msg.sender, "Address invalid");
      
      uint256 userProfitPercent = percentageStakedByBlock(_user, getLastestBlock());
      uint256 userProfitAmount = userProfitPercent.mul(totalProfitAmount).div(baseRate);

      require(ERC20(withdrawTokenAddress).balanceOf(address(this)) >= userProfitAmount, "Not enough balance");

      if(userWithdrawCounter == runTogetherPoolInfo.getUserInfoArrayLength() - 1)
      {
        userProfitAmount = totalProfitAmount - userStakedAmountCounter;
      }
      else {
        userProfitAmount = userProfitPercent.mul(totalProfitAmount).div(baseRate);
      }
      
      if(withdrawStatusByBlockMap[_user][getLastestBlock()] == false)
      {
        runTogetherPoolInfo.withdraw( 0,  _user);
        ERC20(withdrawTokenAddress).transfer(_user, userProfitAmount);
        userStakedAmountCounter += userProfitAmount;
        userWithdrawCounter ++;
        withdrawStatusByBlockMap[_user][getLastestBlock()] = true;
      }
      else {
        runTogetherPoolInfo.withdraw( 0,  _user);
      }

      emit withdrawComplete(_user, userProfitAmount);
    }

    function emergencyProfitWithdraw() external onlyOwner {
        uint256 withdrawAmount = ERC20(withdrawTokenAddress).balanceOf(address(this));
        ERC20(withdrawTokenAddress).safeTransfer(devWallet, withdrawAmount);
    }
}

