    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    bytes32 public constant PRICE_UPDATER = keccak256("PRICE_UPDATER");

    AggregatorV3Interface internal priceFeed;
    IPancakePair internal catgirlPair;

    uint private currentBNBPerUSD;
    uint private currentCatgirlPerUSD;

    uint256 public chainlink_bnb_usd_decimals;

    /**
     * Network: BSC
     * Aggregator: BNB/USD
     * Address: 0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE
     * Network: BSC Testnet
     * Aggregator: BNB/USD
     * Address: 0x2514895c72f50D8bd4B4F9b1110F0D6bD2c97526
     */
    function initialize() public initializer {
        __PriceOracle_init_unchained();

        __AccessControl_init();
        __UUPSUpgradeable_init();
    }

    function __PriceOracle_init_unchained() internal initializer {
        priceFeed = AggregatorV3Interface(0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE);
        chainlink_bnb_usd_decimals = priceFeed.decimals();
        catgirlPair = IPancakePair(0x252C6e5a2b8eCa987b2678618546c388b9b4B933);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(UPGRADER_ROLE, msg.sender);
        _setupRole(PRICE_UPDATER, msg.sender);
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    onlyRole(UPGRADER_ROLE)
    override
    {}

    /**
     * Returns the latest price
     */
    function usdPerBnb() public view returns (int) {
        (
        uint80 roundID,
        int price,
        uint startedAt,
        uint timeStamp,
        uint80 answeredInRound
        ) = priceFeed.latestRoundData();
        return price;
    }
    //-------------------------------------------------------------------------
    // SETTERS
    //------------------------------------------------------------------------
    function processPriceUpdate() external onlyRole(PRICE_UPDATER) {
        int128 bnbRatio = ABDKMath64x64.divi(int256(1 * 10 ** chainlink_bnb_usd_decimals), usdPerBnb());
        currentBNBPerUSD = ABDKMath64x64.mulu(bnbRatio, (10 ** chainlink_bnb_usd_decimals)) * 10 ** (18 - chainlink_bnb_usd_decimals);
        // Process Catgirl price update
        // @dev testnet: Res0: WBNB, Res1: Catgirl
        // @dev mainnet: Res1: WBNB, Res0: Catgirl
        (uint Res1, uint Res0,) = catgirlPair.getReserves();

        currentCatgirlPerUSD = (currentBNBPerUSD * Res1)/Res0;
    }

    function setPriceFeedAddress(address _priceFeed) external onlyRole(PRICE_UPDATER) {
        priceFeed = AggregatorV3Interface(_priceFeed);
    }

    function setPairAddress(address _pair) external onlyRole(PRICE_UPDATER) {
        catgirlPair = IPancakePair(_pair);
    }

    //-------------------------------------------------------------------------
    // Views
    //------------------------------------------------------------------------
    function getRealTimeCatgirlPerUSD() external view returns (uint) {
        int128 bnbRatio = ABDKMath64x64.divi(int256(1 * 10 ** chainlink_bnb_usd_decimals), usdPerBnb());
        uint bnbPerUsd = ABDKMath64x64.mulu(bnbRatio, (10 ** chainlink_bnb_usd_decimals)) * 10 ** (18 - chainlink_bnb_usd_decimals);
        (uint Res1, uint Res0,) = catgirlPair.getReserves();
        return (bnbPerUsd * Res1)/Res0;
    }

    function getBNBPerUSD() external view returns (uint) {
        return currentBNBPerUSD;
    }

    function getCatgirlPerUSD() external view returns (uint) {
        return currentCatgirlPerUSD;
    }
}

