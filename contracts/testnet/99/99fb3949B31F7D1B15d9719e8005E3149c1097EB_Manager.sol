import '@openzeppelin/contracts/proxy/Proxy.sol';

/**
 * @title IQProxy
 * @notice A simple non-upgradeable proxy contract for saving on gas if you need to deploy multiple instances of the same contract.
 * @author gotbit
 */

contract IQProxy is Proxy {
    address public immutable impl;

    constructor(address impl_, bytes memory data) {
        impl = impl_;
        if (data.length > 0) {
            (bool success, ) = impl_.delegatecall(data);
            if (!success) {
                assembly {
                    // This assembly ensure the revert contains the exact string data
                    let returnDataSize := returndatasize()
                    returndatacopy(0, 0, returnDataSize)
                    revert(0, returnDataSize)
                }
            }
        }
    }

    function _implementation() internal view override returns (address) {
        return impl;
    }
}

