pragma solidity ^0.8.0;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../router.sol";

contract CK_Control is Ownable{
    IPancakeRouter02 public router = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
    address public U;
    address public CK;
    mapping(address => bool) public admin;

    function setAddress(address U_, address CK_) public onlyOwner {
        U = (U_);
        CK = (CK_);
        admin[CK_] = true;

        SwapApprove();

    }
    function setAdmin(address addr_,bool com_)public onlyOwner{
        admin[addr_] = com_;
    }

    function setRouter(address addr) external onlyOwner{
        router = IPancakeRouter02(addr);
    }

    function SwapApprove() public onlyOwner {
        IERC20(U).approve(address(router), 1e38);
        IERC20(CK).approve(address(router), 1e38);
    }
    modifier onlyAdmin(){
        require(admin[msg.sender],'not admin');
        _;
    }

    function addLiquidityAuto() public  onlyAdmin{
        require(admin[msg.sender], 'not admin');
        uint amount_ = IERC20(CK).balanceOf(address(this));
        address[] memory path = new address[](2);
        path[0] = CK;
        path[1] = U;
        uint half = amount_ / 2;
        uint last = IERC20(U).balanceOf(address(this));
        router.swapExactTokensForTokens(half, 0, path, address(this), block.timestamp + 720);
        uint now_ = IERC20(U).balanceOf(address(this));
        uint Uamount = now_ - last;
        router.addLiquidity(U, CK, Uamount, half, 0, 0, address(this), block.timestamp + 720);
    }

    function safePull(address token_, address wallet, uint amount_) public onlyOwner {
        IERC20(token_).transfer(wallet, amount_);
    }


}

