pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "../structs/InvestData.sol";

abstract contract IInvest is AccessControlEnumerable, Pausable,InvestData{
    bytes public constant USDT_ASSET_NAME = "usdt";

    bytes public constant CREDIT_ASSET_NAME = "credit";

    bytes public constant SIGN_ASSET_NAME = "sign";
    
    mapping(address => uint256) public userDirect;
    mapping(address => mapping(uint256=>uint256)) public userTeamInfo;
    mapping(address => address[]) public userTeam;
    mapping(address => uint256) public userLevel;

    SysConfigStruct public SysConfig;
    UserLevelStruct[] public _userLevelStruct;
    TeamLevelStruct[] public _teamLevelStruct;
  
     /**
     * @dev Get config
     */
    function getConfig() public view virtual returns (SysConfigStruct memory);
    /**
     * @dev Decrease Balance
     */
    function subAsset(address owner, bytes memory name, uint256 amount) external virtual;
    /**
     * @dev Decrease Balance
     */
    function _subAsset(address owner, bytes memory name, uint256 amount) internal virtual;
    /**
     * @dev Decrease Balance
     */
    function addAsset(address owner, bytes memory name, uint256 amount) external virtual;
    /**
     * @dev Decrease Balance
     */
    function _addAsset(address owner, bytes memory name, uint256 amount) internal virtual;
    /**
     * @dev Decrease Balance
     */
    function getUserBalance(address owner, uint256 types) external virtual returns(uint256);
     /**
     * @dev Decrease Balance
     */
    function _getUserBalance(address owner, uint256 types) internal virtual returns(uint256);
    /**
    * 返佣日志
     */
    function addRebate(address owner,bytes memory credittype,uint256 price,uint256 types,uint256 orderId) external virtual;
    /**
    * 返佣日志
     */
    function _addRebate(address owner,bytes memory credittype,uint256 price,uint256 types,uint256 orderId) internal virtual;
    /**
    * 获取用户比例
     */
    function getUserPercent(address owner) public view virtual returns(uint256);
    /**
     * @dev Equal String
     */
    function isEqual(string memory from, string memory to) internal pure returns (bool) {
        return keccak256(bytes(from)) == keccak256(bytes(to));
    }
}

