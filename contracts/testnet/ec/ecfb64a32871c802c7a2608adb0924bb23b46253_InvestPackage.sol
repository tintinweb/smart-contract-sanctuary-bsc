pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";
import "./interfaces/IInvest.sol";


contract InvestPackage is AccessControlEnumerable {
    struct PackageStruct{
        uint256 level;
        uint256 types;
        uint256 percent;
        uint256 percentStatus;
        uint256 day;
        uint256 price;
        uint256 total;
        uint256 sales;
        uint256 payNum;
        uint256 userLevel;
        bool enable;
    }
    struct UserPackageStruct{
        uint256 orderId;
        uint256 level;
        uint256 types;
        uint256 percent;
        uint256 day;
        uint256 price;
        uint256 rebatePrice;
        uint256 rebateDay;
        uint256 createtime;
        uint256 endtime;
        uint256 status;
        PackageStruct package;
    }

    bytes public constant USDT_ASSET_NAME = "usdt";
    bytes public constant CREDIT_ASSET_NAME = "credit";

    mapping(address => UserPackageStruct[]) public _userPackage;
    mapping(address => mapping(uint256=>uint256) ) public userPackageNum;
    PackageStruct[] public _packageStruct;

    address public gameAddress;
    uint256 public dayTime = 86400;
    uint256 public crontabTime = 3600;

    event BuyPackage(address indexed to, uint256 indexed amount);
    
    constructor() {
        dayTime = 20;
        crontabTime = 30;
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function setGameAddress(address addr) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "Must have admin role to do");

        gameAddress = addr;
    }

    function getGame() public view returns (IInvest) {
        require(gameAddress != address(0), "Game address invalid");
        
        return IInvest(gameAddress);
    }
    /**
    * 设置套餐信息
     */
    function setPackageInfo(PackageStruct memory packageInfo, bool isRemove) public{
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "error");
        bool isFound = false;
        for (uint256 i = 0; i < _packageStruct.length; i ++) {
            if (_packageStruct[i].level == packageInfo.level) {
                isFound = true;
                if (isRemove) {
                    delete _packageStruct[i];
                } else {
                    _packageStruct[i] = packageInfo;
                }
                break;
            }
        }
        if (!isFound && !isRemove) {
            _packageStruct.push(packageInfo);
        }
    }
    /**
    * 获取套餐信息
    */
    function getPackageIndex(uint256 level) public view returns(uint256){
        for (uint256 i = 0; i < _packageStruct.length; i ++) {
            if (_packageStruct[i].level == level) {
                return i;
            }
        }
        return 0;
    }
    /**
    * 购买套餐
     */
    function buyPackage(uint256 level) public{
        address owner = _msgSender();
        uint256 index = getPackageIndex(level);
        PackageStruct memory packageInfo = _packageStruct[index];
        require(packageInfo.enable,'Package not enabled');
        //库存验证
        require(packageInfo.total>packageInfo.sales,'Insufficient inventory');
        //购买量验证
        require(packageInfo.payNum>userPackageNum[owner][level],"Purchase times exceed the limit");
        //等级验证
        require(packageInfo.userLevel<getGame().userLevel(owner),'Insufficient level');
        //金额验证
        require(getGame().getUserBalance(owner,1)>=packageInfo.price,'Insufficient amount');
        //金额扣除
        getGame().subAsset(owner,USDT_ASSET_NAME, packageInfo.price);
        //套餐记录
        UserPackageStruct[] storage userPackages = _userPackage[owner];
        uint256 orderId = userPackages.length + 1;
        getGame().addRebate(owner,USDT_ASSET_NAME,packageInfo.price,3,orderId);
        uint256 endtime = block.timestamp + dayTime*packageInfo.day;
        uint256 percent = getGame().getUserPercent(owner) + packageInfo.percent;
         //uint256 percent = 0;
        userPackages.push(
            UserPackageStruct(
                orderId,
                level,
                packageInfo.types,
                percent,
                packageInfo.day,
                packageInfo.price,0,0,
                block.timestamp,
                endtime,1,
                packageInfo
            )
        );
        userPackageNum[owner][level] += 1;
        packageInfo.sales += 1;
        _packageStruct[index] = packageInfo;
        //团队奖
    }
    /**
    * 获取套餐记录
     */
    function getPackageData() public view returns(PackageStruct[] memory){
        return _packageStruct;
    }
    /**
    * 获取购买记录
     */
    function getPackageList(address owner) public view returns(UserPackageStruct[] memory){
        return _userPackage[owner];
    }
    /**
    * 用户定时返佣
     */
    function userCrontab(address owner) public{
        for(uint256 i =0;i<_userPackage[owner].length;i++){
            UserPackageStruct storage info = _userPackage[owner][i];
            if(info.status != 1){
                continue;
            }
            if(info.types == 1){
                //先利息后本金
                uint256 day  = (block.timestamp - info.createtime)/dayTime - info.rebateDay;
                if(info.rebateDay + day> info.day){
                    day = info.day - info.rebateDay; 
                }
                if(day>0){
                    info.rebateDay += day;
                    //返DAY天得钱
                    uint256 price = day * info.price * info.percent/10000;
                    info.rebatePrice += price;
                    _userPackage[owner][i] = info;
                    getGame().addAsset(owner,CREDIT_ASSET_NAME, price);
                    getGame().addRebate(owner,CREDIT_ASSET_NAME,price,5,info.orderId);
                }
            }else if(info.types == 2 && info.endtime<=block.timestamp){
                //到期一次返
                info.rebatePrice = info.day * info.price * info.percent/10000;
                //返佣加钱
                //返佣日志
                info.rebateDay = info.day;
                getGame().addAsset(owner,CREDIT_ASSET_NAME, info.rebatePrice);
                getGame().addRebate(owner,CREDIT_ASSET_NAME,info.rebatePrice,5,info.orderId);
            }
            if(info.endtime<=block.timestamp){
                //本金返回
                info.status = 2;
                _userPackage[owner][i] = info;
                getGame().addAsset(owner,CREDIT_ASSET_NAME, info.price);
                getGame().addRebate(owner,CREDIT_ASSET_NAME,info.price,4,info.orderId);
            }
        }
    }
    /**
    * 获取是否走定时
     */
     function getUserCrontabTime(address owner) public view returns(bool){
        bool flag = false;
        for(uint256 i =0;i<_userPackage[owner].length;i++){
            UserPackageStruct storage info = _userPackage[owner][i];
            if(info.status != 1){
                continue;
            }
            if(info.types == 1){
                uint256 day  = (block.timestamp - info.createtime)/dayTime - info.rebateDay;
                if(day>0){
                    flag = true;
                    break;
                }
            }
            if(info.endtime<=block.timestamp){
                flag = true;
                break;
            }
        }
        return flag;
     }
}

