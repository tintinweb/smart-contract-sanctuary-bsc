contract NFTCToken is ERC20, Ownable {
    bool public feesEnabled;
    address public supportAddress;
    uint256 public buyFee;
    uint256 public sellFee;
    mapping(address => address) NFTS;
    mapping(address => bool) permissionNFTAddresses;
    mapping(address => uint256) lockAddresses;
    bool private _inSwap;
    mapping(address => bool) private _isExcludedFromFee;
    
    modifier lockSwap {
        _inSwap = true;
        _;
        _inSwap = false;
    }

    constructor() ERC20("NFTC Token", "NFTC") {
        _isExcludedFromFee[address(0xdead)] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[_msgSender()] = true;
        uint256 _totalTokens = 810000000 * 1 ether;
        supportAddress = 0x2a6C5b0752E19C53a06c4772Ce03524693DBBA64;
        _mint(address(this), _totalTokens);
        super._transfer(address(this), msg.sender, 210000000 * 1 ether);
        super._transfer(address(this), 0xfb72aa49BA6ED646Aa41ee9fdaeb2CA57c58587b, 100000000 * 1 ether);
        super._transfer(address(this), 0x002cE820b2f7F96571CD5609B1C34d5753809672, 100000000 * 1 ether);
        super._transfer(address(this), 0x4fF0d748E7760640ba32dF3Ee4F708da65c41Ffb, 100000000 * 1 ether);
        super._transfer(address(this), 0x77e97bA289e94599416EE17E30aC7Af526805401, 100000000 * 1 ether);
        super._transfer(address(this), 0xDaC49a823c6f9069C0F0A74e853378d960d46C82, 100000000 * 1 ether);
        super._transfer(address(this), 0xb66e12DB6e9d36705Fbc434f0D85e8315394F9b5, 100000000 * 1 ether);
        lockAddresses[0xfb72aa49BA6ED646Aa41ee9fdaeb2CA57c58587b] = block.timestamp + 365 days;
        lockAddresses[0x002cE820b2f7F96571CD5609B1C34d5753809672] = block.timestamp + 730 days;
        lockAddresses[0x4fF0d748E7760640ba32dF3Ee4F708da65c41Ffb] = block.timestamp + 1095 days;
        lockAddresses[0x77e97bA289e94599416EE17E30aC7Af526805401] = block.timestamp + 1460 days;
        lockAddresses[0xDaC49a823c6f9069C0F0A74e853378d960d46C82] = block.timestamp + 1826 days;
        lockAddresses[0xb66e12DB6e9d36705Fbc434f0D85e8315394F9b5] = block.timestamp + 2191 days;
    }

    function addNFT(address nftaddress, address owneraccount) external {
        require(permissionNFTAddresses[msg.sender]);
        NFTS[nftaddress] = owneraccount;
    }

    function getNFT(address nftaddress) external view returns(address){
        return NFTS[nftaddress];
    }

    function addNftPermission(address permissionaddress) external onlyOwner {
        permissionNFTAddresses[permissionaddress] = true;
    }

    function setFees(bool _feesEnabled, uint256 _buyFee, uint256 _sellFee) external onlyOwner {
        require(_buyFee + _sellFee <= 20_000, "Total fees can't be higher than 20%.");

        feesEnabled = _feesEnabled;
        buyFee = _buyFee;
        sellFee = _sellFee;
    }

    function setSupportAddress(address account) external onlyOwner {
        supportAddress = account;
    }

    function setAccountFeeStatus(address account, bool isExcluded) external onlyOwner {
        _isExcludedFromFee[account] = isExcluded;
    }

    function isExcludedFromFee(address account) external view returns(bool) {
        return _isExcludedFromFee[account];
    }

    function recoverERC20(address tokenAddress, uint256 tokenAmount, address to) external onlyOwner {
        IERC20(tokenAddress).transfer(to, tokenAmount);
    }

    function _transfer(address sender, address recipient, uint256 amount) internal virtual override {
        uint256 feeAmount = amount * 5 / 100_000;
        super._transfer(sender, supportAddress, feeAmount);
        amount -= feeAmount;

        super._transfer(sender, recipient, amount);
    }
}