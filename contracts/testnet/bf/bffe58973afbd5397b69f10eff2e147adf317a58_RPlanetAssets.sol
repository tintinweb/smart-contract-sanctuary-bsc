// SPDX-License-Identifier: MIT
pragma solidity ^0.8.12;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";

// https://github.com/Arachnid/solidity-stringutils
import "./lib/strings.sol";

contract RPlanetAssets is ERC1155, Ownable, Pausable, ERC1155Burnable {

    using strings for *;

    string private _uri;

    constructor(string memory uri) ERC1155(uri) {
        _uri = uri;
    }

    function setURI(string memory newuri) public onlyOwner {
        _uri = newuri;
    }

    function uri(uint256 id) public view override returns (string memory) {

        strings.slice memory uri_slice = strings.toSlice(_uri);
        strings.slice memory uri_delim = strings.toSlice("{0}");
        strings.slice memory uri_rest = strings.rsplit(uri_slice, uri_delim);

        uint256 tmpl_id = id;

        bytes memory alphabet = "0123456789abcdef";
        bytes memory tmpl_str = new bytes(6);
        for (int i = 5; i >= 0; i--) {
            tmpl_str[uint(i)] = alphabet[uint(tmpl_id & 0xf)];
            tmpl_id = tmpl_id >> 4;
        }

        return string(abi.encodePacked(
                uri_slice.toString(),
                string(tmpl_str),
                uri_rest.toString()
            ));
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function mint(address account, uint256 id, uint256 amount, bytes memory data)
    public
    onlyOwner
    {
        _mint(account, id, amount, data);
    }

    function mintBatch(address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
    public
    onlyOwner
    {
        _mintBatch(to, ids, amounts, data);
    }

    function _beforeTokenTransfer(address operator, address from, address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
    internal
    whenNotPaused
    override
    {
        super._beforeTokenTransfer(operator, from, to, ids, amounts, data);
    }
}