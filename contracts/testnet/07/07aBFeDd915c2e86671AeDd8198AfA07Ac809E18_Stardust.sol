pragma solidity ^0.8.13;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";

import "./StardustShip.sol";
import "./Dust.sol";
import "./Gems.sol";
import "hardhat/console.sol";

contract Stardust is Initializable, AccessControlUpgradeable {
    
    event DustSwap(address swaper, uint dust);

    bytes32 public constant STARDUST_CONTRACTS_ROLE = keccak256("STARDUST_CONTRACTS_ROLE");
    address public STARDUSTSHIP_CONTRACT;
    address public DUST_CONTRACT;
    address public GEMS_CONTRACT;

    uint256 public GemsPrice; // 10 Dust
    uint256 public baseUpgradePrice; // 1
    uint256 public baseDustByShipPerDay;
    uint256 public maxShipLevel; //10

      // Number of dust that the wallet can spend to buy GEMS
    mapping(address => uint256) private gemsBuyAllowed;

    mapping(uint => bool) public shipStaked;
    // ship staked from timestamp
    mapping(uint => uint) public shipStakedFromTime;
    mapping(uint => uint) private shipLastClaim;

    function initialize(
        address _stardustShipContract,
        address _dustContract,
        address _gemsContract,
        uint256 _gemsPrice, // base gems price in DUST
        uint256 _baseDustByShipPerDay,
        uint256 _baseUpgradePrice, // 1
        uint256 _maxShipLevel
    ) initializer public {
         __AccessControl_init();
         _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);

        STARDUSTSHIP_CONTRACT = _stardustShipContract;
        DUST_CONTRACT = _dustContract;
        GEMS_CONTRACT = _gemsContract;
        GemsPrice = _gemsPrice;
        baseDustByShipPerDay =_baseDustByShipPerDay;
        baseUpgradePrice = _baseUpgradePrice;
        maxShipLevel = _maxShipLevel;
    }

    function swapDustForGems(uint _amount) external {
        require(_amount > 0, "Amount must be greater than 0");
        Dust d = Dust(DUST_CONTRACT);
        uint256 maxGemAllowedToBuyed = gemsBuyAllowed[msg.sender];

        require(maxGemAllowedToBuyed >= _amount, "maxGemAllowedToBuyed must be greater or equal to the amount");

        Gems g = Gems(GEMS_CONTRACT);

        d.burn(msg.sender, _amount);
        g.mint(msg.sender, _amount / GemsPrice);
        gemsBuyAllowed[msg.sender] = (maxGemAllowedToBuyed - _amount);
        emit DustSwap(msg.sender, _amount);
    }

    function upgradeShip(uint _tokenId) external {
        StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
        require(s.ownerOf(_tokenId) == msg.sender, "Not ur ship");
        Gems g = Gems(GEMS_CONTRACT);
        uint shipLvl = s.getShipLevel(_tokenId);
        require(g.balanceOf(msg.sender) >= ((shipLvl+1)*baseUpgradePrice), "You dont have enought Gems for upgrade");
        require(shipLvl < maxShipLevel, "Your ship is already lvl max");
        g.burn(msg.sender, ((shipLvl+1)*baseUpgradePrice));
        s.UpgradeShip(_tokenId, shipLvl);
    }

    function claimDustForAllShips() external {
        StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
        uint ships = s.balanceOf(msg.sender);
        require(ships > 0, "You dont have any ship");
        uint[] memory tokenIds = new uint[](ships);

        for (uint i = 0; i < ships; i++) {
            tokenIds[i] = s.tokenOfOwnerByIndex(msg.sender, i);
        }
        claimDust(tokenIds);
    }

    function unstakeShipWithoutClaim(uint _tokenId) external {
        StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
        address ownerOfShip = s.ownerOf(_tokenId);
        require(ownerOfShip == msg.sender || hasRole(STARDUST_CONTRACTS_ROLE, msg.sender), "Not ur ship");

        shipStaked[_tokenId] = false;
    }

    function stakeShip(uint _tokenId) public {
        StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
        require(s.ownerOf(_tokenId) == msg.sender, "Not ur ship");
        require(shipStaked[_tokenId] == false, "Already staked");
        shipStaked[_tokenId] = true;
        shipStakedFromTime[_tokenId] = block.timestamp;
        shipLastClaim[_tokenId] = block.timestamp;
    }

    function stakeShips(uint[] calldata _tokenIds) external {
        for (uint i = 0; i < _tokenIds.length; i++) {
            stakeShip(_tokenIds[i]);
        }
    }

    function claimableView(uint256 _tokenId) public view returns (uint) {
        if (shipStaked[_tokenId] == false) {
            return 0;
        } else {
            uint dustPerDay = baseDustByShipPerDay;
            uint deltaSeconds = block.timestamp - shipLastClaim[_tokenId];
            StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
            uint shipLvl = s.getShipLevel(_tokenId);

            return (dustPerDay * (deltaSeconds / 86400))/100 * (100 + (shipLvl* 60));
        }
    }

    function myClaimableView() external view returns (uint) {
        StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
        uint ships = s.balanceOf(msg.sender);
        if (ships == 0) return 0;
        uint totalClaimable = 0;
        for (uint i = 0; i < ships; i++) {
            uint tokenId = s.tokenOfOwnerByIndex(msg.sender, i);
            totalClaimable += claimableView(tokenId);
        }
        return totalClaimable;
    }

    function claimDust(uint[] memory _tokenIds) internal {
        StardustShip s = StardustShip(STARDUSTSHIP_CONTRACT);
        Dust d = Dust(DUST_CONTRACT);        

        uint totalDustToMint = 0;
        address ownerOfShip = s.ownerOf(_tokenIds[0]);
        for (uint i = 0; i < _tokenIds.length; i++) {
            require(s.ownerOf(_tokenIds[i]) == msg.sender || hasRole(STARDUST_CONTRACTS_ROLE, msg.sender), "Not ur ship");
            uint claimableDust = claimableView(_tokenIds[i]);
            if (claimableDust > 0) {
                totalDustToMint += claimableDust;
                gemsBuyAllowed[msg.sender] += claimableDust;
                shipLastClaim[_tokenIds[i]] = uint(block.timestamp);
            }
        }
        if (totalDustToMint > 0) {
            d.mint(ownerOfShip, totalDustToMint);
        }
    }

    function setStarDustContractAddress(address _contractAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _grantRole(STARDUST_CONTRACTS_ROLE, _contractAddress);
    }

    function changeMaxShipLevel(uint _maxShipLevel) external onlyRole(DEFAULT_ADMIN_ROLE) {
        maxShipLevel = _maxShipLevel;
    }
    // price is the price in dust
    function changeGemsPrice(uint _price) external onlyRole(DEFAULT_ADMIN_ROLE) {
        GemsPrice = _price;
    }
}

