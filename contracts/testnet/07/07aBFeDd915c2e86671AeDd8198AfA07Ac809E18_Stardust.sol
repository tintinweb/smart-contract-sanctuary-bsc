pragma solidity ^0.8.13;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

contract StardustShip is ERC721EnumerableUpgradeable, AccessControlUpgradeable {
    bytes4 private constant _INTERFACE_ID_ERC2981 = 0x2a55205a;
    bytes32 public constant STARDUST_CONTRACTS_ROLE = keccak256("STARDUST_CONTRACTS_ROLE");

    uint public constant MAX_SUPPLY = 10000;
    uint public constant MAX_SUPPLY_PRESALE = 4000;

    mapping(address => uint) public boughtByWL;
    uint public quantityMintedByWLs;

    string public baseURI;

    address public royaltiesAddr;
    uint public royaltyFees;

    bool public openForPresale;
    bool public openForPublic;
    uint public mintFeeAmountWL;
    uint public mintFeeAmount;

    struct Ship {
        uint tokenId;
        address pickedBy;
        uint lvl;
    }

    mapping(uint => Ship) public allShips;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    event ShipUpgrade(uint shipId, uint lvl);

    function initialize(
        address _royaltiesAddr,
        string memory _baseURIMetadata,
        uint _royaltyFees, 
        address _boxContract,
        address _whitelistContract 
    ) public initializer {
        __ERC721_init("StardustShip", "SHIP");
        __ERC721Enumerable_init();


        __AccessControl_init();

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);        
        _grantRole(STARDUST_CONTRACTS_ROLE, _boxContract);
        _grantRole(STARDUST_CONTRACTS_ROLE, _whitelistContract);
        
        openForPresale = false;
        openForPublic = false;
        mintFeeAmountWL = 0.9 ether;
        mintFeeAmount = 1.2 ether;
        royaltiesAddr = _royaltiesAddr;
        baseURI = _baseURIMetadata;
        royaltyFees = _royaltyFees;
    }

    function _mint(address _to) private {
        uint nextTokenId = totalSupply();
        Ship memory newShip = Ship(nextTokenId, _to, 0);
        allShips[nextTokenId] = newShip;
        _safeMint(_to, nextTokenId);
    }

    function UpgradeShip(uint _tokenId, uint _shiplvl) external onlyRole(STARDUST_CONTRACTS_ROLE) {
        allShips[_tokenId].lvl += 1;
        emit ShipUpgrade(_tokenId, _shiplvl);
    }

    function getShipLevel(uint256 _tokenId) external view returns(uint256) {
        return allShips[_tokenId].lvl;
    }

    function pickShipsWhitelist(uint _numberOfShips, address _sender) external payable onlyRole(STARDUST_CONTRACTS_ROLE) {
        require(openForPresale == true, "Whitelist sale is not open");
        require(quantityMintedByWLs + _numberOfShips <= MAX_SUPPLY_PRESALE, "Presale supply is full");

        uint price = mintFeeAmountWL * _numberOfShips;
        require(msg.value >= price, "Not enough avax");

        for (uint i = 1; i <= _numberOfShips; i++) {
            quantityMintedByWLs += 1;
            _mint(_sender);
        }
        (bool sent,) = payable(royaltiesAddr).call{value: msg.value}("");
        require(sent, "Failed to pay royalties");
    }

    function pickShips(uint _numberOfShips) external payable {
        require(openForPublic == true, "Sale is not open");
        require(_numberOfShips <= 10, "Trying to buy too many ships");
        require(totalSupply() + _numberOfShips <= MAX_SUPPLY, "Ships supply is full");

        uint price = mintFeeAmount * _numberOfShips;
        require(msg.value >= price, "Not enough avax");

        for (uint i = 1; i <= _numberOfShips; i++) {
            _mint(msg.sender);
        }
        (bool sent,) = payable(royaltiesAddr).call{value: msg.value}("");
        require(sent, "Failed to pay royalties");
    }

    function openBox(uint _shipToGive, address _receiver) external onlyRole(STARDUST_CONTRACTS_ROLE) {
        require(totalSupply() + _shipToGive <= MAX_SUPPLY, "box is empty, sorry");

        for (uint i = 1; i <= _shipToGive; i++) {
            _mint(_receiver);
        }
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override(ERC721Upgradeable)
        returns (string memory)
    {
        require(_exists(_tokenId), "URI query for nonexistent token");
        return string(abi.encodePacked(string(abi.encodePacked(baseURI, StringsUpgradeable.toString(allShips[_tokenId].lvl))), StringsUpgradeable.toString(_tokenId), ".json"));
    }

    function setMintPrice(uint _publicPrice, uint _wlPrice) public onlyRole(DEFAULT_ADMIN_ROLE) {
        mintFeeAmount = _publicPrice;
        mintFeeAmountWL = _wlPrice;
    }

    function openPublic(bool _open) public onlyRole(DEFAULT_ADMIN_ROLE) {
        openForPublic = _open;
    }

    function openPresale(bool _open) public onlyRole(DEFAULT_ADMIN_ROLE) {
        openForPresale = _open;
    }

    function setBaseURI(string memory _newURI) public onlyRole(DEFAULT_ADMIN_ROLE) {
        baseURI = _newURI;
    }

    function setRoyaltyFees(uint _royaltyFees) public onlyRole(DEFAULT_ADMIN_ROLE) {
        royaltyFees = _royaltyFees;
    }

    function changeRoyaltyAddress(address _royaltiesAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        royaltiesAddr = _royaltiesAddress;
    }

    function setStarDustContractAddress(address _contractAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        _grantRole(STARDUST_CONTRACTS_ROLE, _contractAddress);
    }

    function royaltyInfo(uint256, uint256 _salePrice) external view returns (address receiver, uint256 royaltyAmount) {
        return (royaltiesAddr, _salePrice * royaltyFees / 100);
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721EnumerableUpgradeable)
    {
        // if (address(STARDUST_CONTRACT) != address(0) && address(from) != address(0)) {
        //     Stardust stardust = Stardust(STARDUST_CONTRACT);
        //     stardust.unstakeShipWithoutClaim(tokenId);
        // }
        super._beforeTokenTransfer(from, to, tokenId);
    }


    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721EnumerableUpgradeable, AccessControlUpgradeable)
        returns (bool)
    {
        if (interfaceId == _INTERFACE_ID_ERC2981) {
            return true;
        }
        return super.supportsInterface(interfaceId);
    }
}

