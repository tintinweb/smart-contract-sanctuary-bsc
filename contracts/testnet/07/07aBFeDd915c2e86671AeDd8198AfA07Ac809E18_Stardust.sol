pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat/console.sol";


contract Gems is Initializable, ERC20Upgradeable, AccessControlUpgradeable {
    bytes32 public constant STARDUST_CONTRACTS_ROLE = keccak256("STARDUST_CONTRACTS_ROLE");

    bool authorizeTransfers;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    mapping(address => bool) public whitelistTransfer;

    function initialize(
    ) initializer public {
        __ERC20_init("Gems", "GEMS");
        __AccessControl_init();
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        authorizeTransfers = false;
    }

    function burn(address _acc, uint _amount) external onlyRole(STARDUST_CONTRACTS_ROLE) {
        _burn(_acc, _amount);
    }

    function mint(address _acc, uint _amount) external onlyRole(STARDUST_CONTRACTS_ROLE) {
        _mint(_acc, _amount);
    }

    function airdrop(address[] calldata _addresses, uint _amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint i = 0; i < _addresses.length; i++) {
            _mint(_addresses[i], _amount);
        }
    }
     
    function transfer(address _recipient, uint256 _amount) public virtual override(ERC20Upgradeable) returns (bool) {
        require(authorizeTransfers, "No transfers allowed for the moment");
        return super.transfer(_recipient, _amount);
    }

    function transferFrom(
        address _sender,
        address _recipient,
        uint256 _amount
    ) public virtual override(ERC20Upgradeable) returns (bool) {
        require(authorizeTransfers, "No transfers allowed for the moment");
        return super.transferFrom(_sender, _recipient, _amount);
    }

    function setStarDustContractAddress(address _contractAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
       _grantRole(STARDUST_CONTRACTS_ROLE, _contractAddress);
    }
    
    function setSAuthorizeTransfer(bool _authorizeTransfers) public onlyRole(DEFAULT_ADMIN_ROLE) {
       authorizeTransfers = _authorizeTransfers;
    }
}

