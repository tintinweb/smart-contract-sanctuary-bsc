/**
 * @notice A Chainlink VRF consumer which uses randomness to mimic the rolling
 * of a 20 sided die
 * @dev This is only an example implementation and not necessarily suitable for mainnet.
 */

/**
 * Request testnet LINK and ETH here: https://faucets.chain.link/
 * Find information on LINK Token Contracts and get the latest ETH and LINK faucets here: https://docs.chain.link/docs/link-token-contracts/
 */

contract Balloting is VRFConsumerBase, ConfirmedOwner(msg.sender) {
    uint256 private constant ROLL_IN_PROGRESS = 42;
    bytes32 private s_keyHash;
    uint256 private s_fee;
    mapping(bytes32 => address) public s_rollers;
    mapping(address => uint256) public s_results;
    uint256 public random_results;
    uint256 public requestId;
    uint256[] particpent;
    uint8 counter;
    struct Winners{
        uint256 lotteryId;
        uint256 expiry;
        uint256[]  winners;
    }
    mapping(uint256 => Winners) public winnersList;


    event RandomStarted(bytes32 indexed requestId, address indexed roller);
    event RandomLanded(bytes32 indexed requestId, uint256 indexed result);
    event DrawResults(uint256  indexed id , uint256 timeStimped , uint256[] winners);
    /**
     * @notice Constructor inherits VRFConsumerBase
     *
     * @dev NETWORK: BSC_NET , KOVAN
     * @dev   Chainlink VRF Coordinator address: 0xa555fC018435bef5A13C6c6870a9d4C11DEC329C , || 0xdD3782915140c8f3b190B5D67eAc6dc5760C46E9
     * @dev   LINK token address:               0x84b9B910527Ad5C03A9Ca831909E21e236EA7b06 , || 0xa36085F69e2889c224210F603D836748e7dC0088
     * @dev   Key Hash:   0xcaf3c3727e033261d383b315559476f48034c13b18f8cafed4d871abe5049186 , || 0x6c3699283bda56ad74f6b855546325b68d482e983852a7a82979cc4807b641f4
     * @dev   Fee:        0.1 LINK (100000000000000000)
     *
     * @param vrfCoordinator address of the VRF Coordinator
     * @param link address of the LINK token
     * @param keyHash bytes32 representing the hash of the VRF job
     * @param fee uint256 fee to pay the VRF oracle
     */
    constructor(address vrfCoordinator, address link, bytes32 keyHash, uint256 fee)
        VRFConsumerBase(vrfCoordinator, link)
    {
        s_keyHash = keyHash;
        s_fee = fee;

    }

    /**
     * @notice Requests randomness
     * @dev Warning: if the VRF response is delayed, avoid calling requestRandomness repeatedly
     * as that would give miners/VRF operators latitude about which VRF response arrives first.
     * @dev You must review your implementation details with extreme care.
     *
     * @param roller address of the roller
     */
    function getRandomValue(address roller) public onlyOwner returns (bytes32 requestId) {
        if(counter >= 10)
        {require(LINK.balanceOf(address(this)) >= s_fee, "Not enough LINK to pay fee");
        // require(s_results[roller] == 0, "Already rolled");
        requestId = requestRandomness(s_keyHash, s_fee);
        s_rollers[requestId] = roller;
        s_results[roller] = ROLL_IN_PROGRESS;
        emit RandomStarted(requestId, roller);
        }else {
            return bytes32('0x');
        }

    }

    /**
     * @notice Callback function used by VRF Coordinator to return the random number
     * to this contract.
     * @dev Some action on the contract state should be taken here, like storing the result.
     * @dev WARNING: take care to avoid having multiple VRF requests in flight if their order of arrival would result
     * in contract states with different outcomes. Otherwise miners or the VRF operator would could take advantage
     * by controlling the order.
     * @dev The VRF Coordinator will only send this function verified responses, and the parent VRFConsumerBase
     * contract ensures that this method only receives randomness from the designated VRFCoordinator.
     *
     * @param requestId bytes32
     * @param randomness The random result returned by the oracle
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        uint256 d20Value = (randomness % 20) + 1;
        s_results[s_rollers[requestId]] = d20Value;
        random_results = randomness;
        requestId = requestId;
        emit RandomLanded(requestId, d20Value);
    }

 

    /**
     * @notice Withdraw LINK from this contract.
     * @dev this is an example only, and in a real contract withdrawals should
     * happen according to the established withdrawal pattern:
     * https://docs.soliditylang.org/en/v0.4.24/common-patterns.html#withdrawal-from-contracts
     * @param to the address to withdraw LINK to
     * @param value the amount of LINK to withdraw
     */
    function withdrawLINK(address to, uint256 value) public onlyOwner {
        require(LINK.transfer(to, value), "Not enough LINK");
    }

    /**
     * @notice Set the key hash for the oracle
     *
     * @param keyHash bytes32
     */
    function setKeyHash(bytes32 keyHash) public onlyOwner {
        s_keyHash = keyHash;
    }

    /**
     * @notice Get the current key hash
     *
     * @return bytes32
     */
    function keyHash() public view returns (bytes32) {
        return s_keyHash;
    }

    /**
     * @notice Set the oracle fee for requesting randomness
     *
     * @param fee uint256
     */
    function setFee(uint256 fee) public onlyOwner {
        s_fee = fee;
    }

    /**
     * @notice Get the current fee
     *
     * @return uint256
     */
    function fee() public view returns (uint256) {
        return s_fee;
    }

    
    function draw(uint256[]  memory totalPersons , uint256 totalAssets , uint256 id , uint256 expiry ) public {
        require(random_results != 0 , "Please Get Random Number first");
        particpent = totalPersons;
        uint256[] memory winnersArray= new uint256[](totalAssets);
        Winners storage newWinners = winnersList[id];
        newWinners.lotteryId = id;
        newWinners.expiry = expiry;
        unchecked {
                for (uint256 i ; i < totalAssets; i++) {
                    uint256 result = uint256(keccak256(abi.encode(random_results, i , particpent.length , totalAssets))) % uint256(particpent.length);
                    winnersArray[i] = particpent[result];
                    for (uint j =result; j<particpent.length-1; j++){
                        particpent[j] = particpent[j+1];
                    }
                    particpent.pop();
                }
            }
        newWinners.winners = winnersArray;
        counter++;
        emit DrawResults(id , expiry , newWinners.winners);
    }

   function getBalloting(uint256 _id) public view 
   returns (uint256 id , uint256 expiry , uint256[]  memory winners)
   {
    Winners storage newWinners = winnersList[_id];
   return (newWinners.lotteryId , newWinners.expiry , newWinners.winners );

   }

   function getWinners(uint256 _id) public view returns(uint256[] memory list){
    Winners storage newWinners = winnersList[_id];
    return (newWinners.winners);
   }

   function balanceOfLink() external view returns(uint256 balance){
       return LINK.balanceOf(address(this));
   } 

}

