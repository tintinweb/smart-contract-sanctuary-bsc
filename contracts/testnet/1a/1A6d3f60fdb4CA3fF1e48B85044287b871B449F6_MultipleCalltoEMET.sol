pragma solidity ^0.8.0;
import "./interface/router2.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";

interface stake {
    function numberOfDraws(uint, address) external view returns(uint, uint ,uint);
    function checkUserSlot_Box(uint , address) external view returns(uint[5] memory );
    function poolInfo_Pos(uint) external view returns(bool, uint256, uint256, uint256, uint256, uint256, uint256, uint256, uint256, address, address);
    function poolInfo_Share(uint) external view returns(bool, uint256, uint256, uint256, uint256, uint256, uint256,  uint256,  uint256,address, address);
    function poolInfo_Box(uint) external view returns( bool,  bool,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  address, uint);
    function userInfo_Share(uint, address)external view returns (uint256, uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256);
    function userInfo_Pos (uint, address)external view returns (uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256,  uint256);
    function userInfo_Box(uint, uint, address)external view returns (uint256,  uint256,  uint256,  uint256);
    function updataUserReward_Pos(uint, address)external view returns(uint);
    function updataLockReward_Pos(uint, address)external view returns(uint);
    function updataUserReward_Share(uint, address)external view returns(uint);
    function updataLockReward_Share(uint, address)external view returns(uint);
    function updataUserReward_Box (uint, uint, address)external view returns(uint);
}

contract MultipleCalltoEMET is Ownable  {
    // ICnnPair
    address public mining = 0x8A557c1138EDD68Bd7aD474ecdEbEB4cdA873DB8;
    address public USDT = 0xcb9D18c42a3e59283b8eedD1802CEB16BDAE01a2;
    uint public Acc = 1e18;
    function setMining(address addr_) public onlyOwner {
        mining = addr_;
    }

    function checkApy_Pos(address miningCon, uint pid_) public view returns(uint apy) {

        uint daliyOut_; 
        uint totalPower_;
        address LP_;
        (, totalPower_, daliyOut_, , , , , , , LP_, ) = stake(miningCon).poolInfo_Pos(pid_);
        uint total = ICnnPair(LP_).totalSupply();
        (uint re0, uint re1, ) = ICnnPair(LP_).getReserves();
        uint price;
        address t0 = ICnnPair(LP_).token0();
        address t1 = ICnnPair(LP_).token1();
        if (t0 == USDT) {
            price = re0 * Acc/ re1;
            apy = (daliyOut_ * price * 365 * total) / (totalPower_ * re0 * 2);
        } else if (t1 == USDT) {
            price = re1 * Acc/ re0;
            apy = (daliyOut_ * price * 365 * total) / (totalPower_ * re1 * 2);
        }
    }

    function checkApy_Share(address miningCon, uint pid_, uint amount_) public view returns(uint apy) {
        address LP_;
        uint tvl; 
        uint share;
        (, tvl, share, , , , , , , LP_, ) = stake(miningCon).poolInfo_Share(pid_);
        uint total = ICnnPair(LP_).totalSupply();
        (uint re0, uint re1, ) = ICnnPair(LP_).getReserves();
        uint price;
        address t0 = ICnnPair(LP_).token0();
        address t1 = ICnnPair(LP_).token1();
        uint daliyOut_ = amount_* Acc / share;

        if (t0 == USDT) {
            price = re0 * Acc / re1;
            apy = (daliyOut_ * price * 365 * total) / (tvl * re0 * 2);
        } else if (t1 == USDT) {
            price = re1 * Acc/ re0;
            apy = (daliyOut_ * price * 365 * total) / (tvl * re1 * 2);
        }
    }

    //---------------------------------------------------------------------------------------------------------------

    function checkBalance_Allowance(address lp, address user_) public view returns(uint256 ba, uint256 all){
        ba = ICnnPair(lp).balanceOf(user_);
        all = ICnnPair(lp).allowance(user_, address(mining));
    }

    function checkInfoTypePosShare (uint[] memory pid_, address user_)
    public view returns (address[] memory lpToken, uint[] memory depositTimes, 
    uint[7][] memory uintList, string[3][] memory symbels, address[2][] memory lpOneTwoAddr) {
        uint len = pid_.length;
        lpToken = new address[](len);
        depositTimes = new uint[](len);
        uintList =new uint[7][](len);
        symbels = new string[3][](len);
        lpOneTwoAddr = new address[2][](len);

        // uint x = 0;                                           
        for (uint i=0; i<len; i++) {
            if(pid_[i]>=10 && pid_[i]<100) {
                // (lpToken[i], depositTimes[i], amounts[i], toClaims[i], claimeds[i], uintList[i], symbels[i]) = checkPosInfo(pid_[i], user_);
                (lpToken[i], depositTimes[i], uintList[i], symbels[i]) = checkPosInfo(pid_[i], user_);
            } else if (pid_[i]>=100 ) {
                // (lpToken[i], depositTimes[i], amounts[i], toClaims[i], claimeds[i], uintList[i], symbels[i]) = checkShareInfo(pid_[i], user_);
                (lpToken[i], depositTimes[i], uintList[i], symbels[i]) = checkShareInfo(pid_[i], user_);
            } 
        }
        
        for (uint l; l<lpToken.length; l++) {
            if(lpToken[l] != address(0)){
                lpOneTwoAddr[l] = [ICnnPair(lpToken[l]).token0(), ICnnPair(lpToken[l]).token1()];
            } else {
                lpOneTwoAddr[l] = [address(0), address(0)];
            }


        }
    }

    function checkInfoTypeBox (uint[] memory pid_, address user_)
    public view returns (address[] memory lpToken, uint[] memory depositTimes, uint[7][] memory uintList, string[3][] memory symbels, address[2][] memory lpOneTwoAddr) {
        // User_NumberOfDraws storage number = numberOfDraws[pid_][msg.sender];
        uint len = pid_.length;
        uint listlen = pid_.length * 5;
        lpToken = new address[](listlen);
        depositTimes = new uint[](listlen);
        uintList =new uint[7][](listlen);
        symbels = new string[3][](listlen);
        lpOneTwoAddr = new address[2][](len);

        uint[5] memory _temp; 
        uint slot;
        uint _index;
        for (uint i=0; i<len; i++){
            if (i >0){
                _index = 5 * i;
            }
            _temp = stake(mining).checkUserSlot_Box(pid_[i] ,user_); 
            for(uint x=0; x<5; x++) {
                slot = _temp[x];
                (lpToken[_index + x], depositTimes[_index + x], uintList[_index + x], symbels[_index + x]) = checkBoxInfo(pid_[i], slot, user_);
                // (lpToken[index], depositTimes[index], amounts[index], toClaims[index], claimeds[index], uintList[index], symbels[index]) = checkBoxInfo(pid_, slot, user_);
            }
        }
        for (uint l; l<lpToken.length; l++) {
            if(lpToken[l] != address(0)){
                lpOneTwoAddr[l] = [ICnnPair(lpToken[l]).token0(), ICnnPair(lpToken[l]).token1()];
            } else {
                lpOneTwoAddr[l] = [address(0), address(0)];
            }
        }
    }

    function checkPosInfo(uint pid_, address user_) public view 
    returns(address _lpToken, uint _depositTimes, uint[7] memory uintList, string[3] memory _symbel) {

        _symbel = ["null", "null", "null"];
        address outputToken;
        (, , , , , , , , , _lpToken, outputToken) = stake(mining).poolInfo_Pos(pid_);
        if(_lpToken != address(0)){
            address _t0 = ICnnPair(_lpToken).token0();
            address _t1 = ICnnPair(_lpToken).token1();
            _symbel[0] = ERC20(_t0).symbol();
            _symbel[1] = ERC20(_t1).symbol();
            _symbel[2] = ERC20(outputToken).symbol();
            (uintList[0],uintList[1]) = checkBalance_Allowance(_lpToken, user_);
            uintList[2] = 0;
            uintList[3] = 0;
        }

        uint toClaim;
        uint lockReward;
        (uintList[4],  ,  toClaim,  uintList[5],  lockReward,  _depositTimes,  ,  ,  ) = stake(mining).userInfo_Pos(pid_, user_);
        // _depositTimes = user.depositTime;
        // uintList[4] = user.amount;
        // uintList[5] = user.claimed;
        uint temp1 =  stake(mining).updataUserReward_Pos(pid_, user_);
        uint temp2=0;
        if(lockReward > 0) {
            temp2 = stake(mining).updataLockReward_Pos(pid_, user_);
        }
        uintList[6] = toClaim + temp1 + temp2;
    }

    function checkShareInfo(uint pid_, address user_) public view 
    returns(address _lpToken, uint _depositTimes, uint[7] memory uintList, string[3] memory _symbel ) {
        address outputToken;
        uint toClaim;
        uint lockReward;
        _symbel = ["null", "null", "null"];
        (, , , , , , ,  ,  uintList[3], _lpToken, outputToken) = stake(mining).poolInfo_Share(pid_);
        (uintList[4], toClaim, uintList[5], lockReward, _depositTimes, uintList[2], , , ) = stake(mining).userInfo_Share(pid_, user_);
        // _lpToken = pool.lpToken;
        if(_lpToken != address(0)){
            address _t0 = ICnnPair(_lpToken).token0();
            address _t1 = ICnnPair(_lpToken).token1();
            _symbel[0] = ERC20(_t0).symbol();
            _symbel[1] = ERC20(_t1).symbol();
            _symbel[2] = ERC20(outputToken).symbol();
            (uintList[0],uintList[1]) = checkBalance_Allowance(_lpToken, user_);

        }

        uint temp1 = stake(mining).updataUserReward_Share(pid_, user_);
        uint temp2;
        if(lockReward != 0) {
            temp2 = stake(mining).updataLockReward_Share(pid_, user_);
        }
        uintList[6] = toClaim + temp1 + temp2;
    }


    function checkBoxInfo(uint pid_, uint slot_, address user_) public view 
    returns( address _lpToken, uint _depositTimes, uint[7] memory uintList, string[3] memory _symbel) {
        // PoolInfo_Box storage pool = poolInfo_Box[pid_];
        // UserInfo_Box storage user = userInfo_Box[pid_][slot_][user_];
        // User_NumberOfDraws storage number = numberOfDraws[pid_][user_];

        _symbel = ["null", "null", "null"];
        (uintList[4] ,_depositTimes ,uintList[2], ) = stake(mining).userInfo_Box(pid_, slot_,user_);
        (,  ,  ,  ,  ,  ,  ,  ,  uintList[3],  _lpToken,) = stake(mining).poolInfo_Box(pid_);
        // _lpToken = pool.lpToken;
        if(_lpToken != address(0)){
            address _t0 = ICnnPair(_lpToken).token0();
            address _t1 = ICnnPair(_lpToken).token1();
            _symbel[0] = ERC20(_t0).symbol();
            _symbel[1] = ERC20(_t1).symbol();
            _symbel[2] = "null";
            (uintList[0],uintList[1]) = checkBalance_Allowance(_lpToken, user_);
            // uintList[2] = user.rewrdTime;
            // uintList[3] = pool.updataTime;
        }

        (uintList[5], ,) = stake(mining).numberOfDraws(pid_, user_);
        uint temp1 =  stake(mining).updataUserReward_Box(pid_, slot_, user_);
        uintList[6] = temp1;
    }

}

