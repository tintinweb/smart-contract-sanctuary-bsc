        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}
contract Bidd is ERC20,Ownable{
    using SafeMath for *;
    
     address public backflow;//回流地址
     address public fund;//基金池地址 
     uint base = 1000;
     uint burnProportion = 40;//销毁比例
     uint backflowProportion = 20;//回流比例
      uint holdingProportion = 20;//持币比例
      uint fundProportion = 10;//基金比例
     uint public  fomoMaxLimit = 210000 * decimalVal;
      //1-8 level
       uint oneProportion = 20;
        uint twoProportion = 10;
         uint threeProportion = 5;
     uint public decimalVal = 1e18;
     address public  walletAddress;
    constructor (address addr_,address backflow_,address fund_,address walletAddress_)  ERC20("bidd", "BIDD") {
        _mint(addr_, 21000000*decimalVal); 
        
        backflow = backflow_;
        fund = fund_;
        walletAddress = walletAddress_;
    }
   
   mapping(address=>bool) public isFeeAddress;
   struct Player{
        address oneUser;
        address twoUser;
        address threeUser;
        address fourUser;
        address fiveUser;
        address sixUser;
        address sevenUser;
        address eightUser;
        bool isRegister;
    }
    mapping (address => Player) public _plyrs;
   //添加不收手续费地址
    function setFeeAddress(address addr) public onlyOwner {
        isFeeAddress[addr] = true;
    }
    function removeFeeAddress(address addr) public onlyOwner {
        isFeeAddress[addr] = false;
    }
     function setOneProportion(uint burn_) public onlyOwner {
        oneProportion = burn_;
     }
    function setTwoProportion(uint two_) public onlyOwner {
        twoProportion = two_;
    }
    function setThreeProportion(uint three_) public onlyOwner {
        threeProportion = three_;
    }
    function setMaxLimit(uint val) public onlyOwner {
        fomoMaxLimit = val;
    }
    
    function transfer(address to, uint value) public override returns (bool) {
       uint transferAmount = common(msg.sender,to,value);
         super.transfer(to, transferAmount);
         return true;
    }
    
    function transferFrom(address from, address to, uint value) public override returns (bool) {
         uint transferAmount = common(from,to,value);
        super._transfer(from, to, transferAmount);
        super._approve(from, msg.sender, _allowances[from][msg.sender].sub(value));
        return true;
    }
    function common(address addr,address to,uint value) internal returns (uint){
         uint transferAmount = value;
         if(!isFeeAddress[addr]){
         uint totalSupply = totalSupply();
          uint burnAmount = value.mul(burnProportion).div(base);
          //销毁数量 
          if(burnAmount>0 && totalSupply>fomoMaxLimit){
            _burn(msg.sender, burnAmount);
          }

           uint backflowAmount = value.mul(backflowProportion).div(base);
          if(backflowAmount>0){
              _transfer(addr,backflow, backflowAmount);
          }
          uint holdingAmount = value.mul(holdingProportion).div(base);
          if(holdingAmount>0){
              _transfer(addr,address(this), holdingAmount);
          }
           uint fundAmount = value.mul(fundProportion).div(base);
          if(fundAmount>0){
              _transfer(addr,fund, fundAmount);
          }

          inivt(to);
          uint256 inivt_num = value.mul(oneProportion.add(twoProportion).add(threeProportion.mul(6))).div(base);
           inivtTransfer(value,addr,inivt_num);
          transferAmount = value.sub(burnAmount.add(backflowAmount).add(holdingAmount).add(fundAmount).add(inivt_num)); //实际转账数
         }
        
        return transferAmount;
    }
    function inivt(address user) internal {
        address lastUser = msg.sender;
        address lastUserAdd = _plyrs[user].oneUser;
        if(lastUserAdd == address(0) && user != lastUser){
             _plyrs[user].isRegister = true;
             _plyrs[user].oneUser = lastUser;
             //邀请人是否有二级
          if(_plyrs[lastUser].oneUser != address(0)){
            _plyrs[user].twoUser = _plyrs[lastUser].oneUser;
             //邀请人是否有三级
             if(_plyrs[lastUser].twoUser != address(0)){
                 _plyrs[user].threeUser = _plyrs[lastUser].twoUser;
             }
             if(_plyrs[lastUser].threeUser != address(0)){
                 _plyrs[user].fourUser = _plyrs[lastUser].threeUser;
             }
             if(_plyrs[lastUser].fourUser != address(0)){
                 _plyrs[user].fiveUser = _plyrs[lastUser].fourUser;
             }
             if(_plyrs[lastUser].fiveUser != address(0)){
                 _plyrs[user].sixUser = _plyrs[lastUser].fiveUser;
             }
             if(_plyrs[lastUser].sixUser != address(0)){
                 _plyrs[user].sevenUser = _plyrs[lastUser].sixUser;
             }
             if(_plyrs[lastUser].sevenUser != address(0)){
                 _plyrs[user].eightUser = _plyrs[lastUser].sevenUser;
             }
          }
       }   
    }
    function inivtTransfer(uint value,address addr,uint inivt_num)  internal  {
        uint num;
         address oneUserAdd = _plyrs[addr].oneUser;
         if(oneUserAdd != address(0)){
            uint oneAmount = value.mul(oneProportion).div(base);
            num = num.add(oneAmount);
            _transfer(addr,oneUserAdd, oneAmount);
         }
          address twoUserAdd = _plyrs[addr].twoUser;
          if(twoUserAdd != address(0)){
            uint twoAmount = value.mul(twoProportion).div(base);
            num = num.add(twoAmount);
             _transfer(addr,twoUserAdd, twoAmount);
         }
           address threeUserAdd = _plyrs[addr].threeUser;
           if(threeUserAdd != address(0)){
            uint threeAmount = value.mul(threeProportion).div(base);
            num = num.add(threeAmount);
            _transfer(addr,threeUserAdd, threeAmount);
           }
            address fourUserAdd = _plyrs[addr].fourUser;
            if(fourUserAdd != address(0)){
            uint fourAmount = value.mul(threeProportion).div(base);
               num = num.add(fourAmount);
               _transfer(addr,fourUserAdd, fourAmount);
             }
             address fiveUserAdd = _plyrs[addr].fiveUser;
             if(fiveUserAdd != address(0)){
             uint fiveAmount = value.mul(threeProportion).div(base);
               num = num.add(fiveAmount);
                _transfer(addr,fiveUserAdd, fiveAmount);
             }
              address sixUserAdd = _plyrs[addr].sixUser;
               if(sixUserAdd != address(0)){
              uint sixAmount = value.mul(threeProportion).div(base);
                 num = num.add(sixAmount);
                 _transfer(addr,sixUserAdd, sixAmount);
              }
               uint256 values = value;
               address sevenUserAdd = _plyrs[addr].sevenUser;
                  if(sevenUserAdd != address(0)){
                    uint sevenAmount = values.mul(threeProportion).div(base);
                    num = num.add(sevenAmount);
                    _transfer(addr,sevenUserAdd, sevenAmount);
                  }
                   address eightUserAdd = _plyrs[addr].eightUser;
                  if(eightUserAdd != address(0)){
                    uint eightAmount = values.mul(threeProportion).div(base);
                    num = num.add(eightAmount);
                    _transfer(addr,eightUserAdd, eightAmount);
                  }
           if(inivt_num != num){
              num = inivt_num.sub(num);
             _transfer(addr,walletAddress, num);
           }
       
    }
    function _transferBurn(address from, uint amount) internal {
        require(from != address(0));
        super._burn(from, amount);
    }
    function burn(uint amount) public returns (bool) {
        super._burn(msg.sender, amount);
        return true;
    }
    struct Propors{
        address branchAddress;
        uint num;
    }
    Propors[] public proporsList;
    
    //分红
    function honus(address[] calldata addresses) public  onlyOwner{
        require(
      addresses.length <= 2000,
      "GAS Error: max airdrop limit is 2000 addresses"
    ); 
       uint256 SCCC = 0;
      for (uint256 i = 0; i < addresses.length; i++) {
          SCCC = SCCC + balanceOf(addresses[i]);
       }
       
      for (uint256 j = 0; j < addresses.length; j++) {
        uint  propor =  balanceOf(addresses[j]).mul(balanceOf(address(this))).div(SCCC);
           proporsList.push(Propors({branchAddress:addresses[j],num:propor}));
       }
       for (uint256 h = 0; h < proporsList.length; h++) {
          super._transfer(address(this),proporsList[h].branchAddress, proporsList[h].num);
       }
       
    } 
   

}