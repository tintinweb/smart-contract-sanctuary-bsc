pragma solidity ^0.8.13;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract ERC20Vesting is ERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    uint64 private _start;
    uint64 private _duration;

    mapping(address => uint256) internal _vestedBalances;
    mapping(address => uint256) internal _released;

    constructor(string memory name, string memory symbol) ERC20(name, symbol) {}

    /**
     * @dev Configures start and end date for vesting.
     */
    function configure(uint64 startDate, uint64 durationTime) external virtual onlyOwner {
        require(durationTime > 0, "Duration must be greater than 0");
        require(startDate > 0, "Start must be greater than 0");
        require(startDate >= block.timestamp, "Start must be greater than now");
        _start = startDate;
        _duration = durationTime;
    }

    /**
     * @dev Getter for the start timestamp.
     */
    function start() public view virtual returns (uint256) {
        return _start;
    }

    /**
     * @dev Getter for the vesting duration.
     */
    function duration() public view virtual returns (uint256) {
        return _duration;
    }

    /**
     * @dev Getter for the total amount of tokens that is vested for an account.
     */
    function vestedBalanceOf(address account) public view virtual returns (uint256) {
        return _vestedBalances[account];
    }

    /**
     * @dev Amount of token already released
     */
    function released(address account) public view virtual returns (uint256) {
        return _released[account];
    }

    /**
     * @dev Release the tokens that have already vested.
     *
     * Emits a {TokensReleased} event.
     */
    function release(address account) public virtual {
        uint256 releasable = vestedAmount(account, uint64(block.timestamp)) - released(account);
        _released[account] += releasable;
        emit TokensReleased(account, releasable);
        _vestedBalances[account] -= releasable;
    }

    /**
     * @dev Calculates the amount of tokens that has already vested. Default implementation is a linear vesting curve.
     */
    function vestedAmount(address account, uint64 timestamp) public view virtual returns (uint256) {
        return _vestingSchedule(vestedBalanceOf(account) + released(account), timestamp);
    }

    /**
     * @dev Virtual implementation of the vesting formula. This returns the amount vested, as a function of time, for
     * an asset given its total historical allocation.
     */
    function _vestingSchedule(uint256 totalAllocation, uint64 timestamp) internal view virtual returns (uint256) {
        if (timestamp < start()) {
            return 0;
        } else if (timestamp - start() < 604800) {
            // one week
            return 0;
        } else if (timestamp > start() + duration()) {
            return totalAllocation;
        } else {
            return (totalAllocation * (timestamp - start())) / duration();
        }
    }

    event TokensReleased(address indexed account, uint256 amount);
}

