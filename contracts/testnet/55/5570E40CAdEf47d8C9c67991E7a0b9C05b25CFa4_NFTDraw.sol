pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./interfaces/ICollectionWrapper.sol";
import "./interfaces/ILaunchpad721.sol";
import "./interfaces/INFTDraw.sol";
import "./LBlockPrice.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";
/**
 * @title A contact for picking an NFT winner.
 * @dev Constructor sets the operator, address of NFTLaunchpad721, CollectionWrapper and LBlockToken
 */

contract NFTDraw {
    using SafeMath for uint256;
    address public operator;
    address public collectionWrapperAddress;
    address public collectionAddress;
    address public admin;
    address public factory;
    address luckyBlockToken;
    address public nftWinner;
    uint256 winnerToken;

    struct Winner {
        string tokenURI;
        address nftWinner;
        uint256 amount;
        uint256 winnerDate;
    }

    LBlockPrice lblockPrice;

    Winner NFTwinner;

    Winner[] public winnersList;

    event PickWinner(
        address indexed winner,
        uint256 amount,
        uint256 winnerDate,
        uint256 winnerToken
    );

    /**
     * @param _collectionAddress . An address of NFTLaunchpad721 LuckyBlock nft collection.
     * @param _collectionWrapperAddress . An address of collectionWrapper contract.
     * @param _luckyBlockToken . An address of luckyblock token.
     */

    constructor(
        address _collectionAddress,
        address _collectionWrapperAddress,
        address _luckyBlockToken,
        address _lblockPrice
    ) {
        admin = msg.sender;
        operator = msg.sender;
        collectionWrapperAddress = _collectionWrapperAddress;
        luckyBlockToken = _luckyBlockToken;
        collectionAddress = _collectionAddress;
        lblockPrice = LBlockPrice(_lblockPrice);
    }

    /**
     * @dev Returns an array of winners.
     */

    function getWinnersList() external view returns (Winner[] memory) {
        return winnersList;
    }

    /**
     * @dev Sets the operator (lottery).
     * @dev Only admin or factory can do that.
     * @param _operator . Lottery address.
     */

    function setOperator(address _operator) external {
        require(
            msg.sender == admin || msg.sender == factory,
            "Not factory nor admin."
        );
        operator = _operator;
    }

    /**
     * @dev Sets the factory.
     * @dev Only admin can do that.
     * @param _factory . Factory address.
     */

    function setFactory(address _factory) external {
        require(msg.sender == admin, "Not admin.");
        factory = _factory;
    }

    /**
     * @dev Sets the factory.
     * @dev Only admin can do that.
     * @param _newAdmin . Admin address.
     */

    function setAdmin(address _newAdmin) external {
        require(msg.sender == admin, "Not admin.");
        admin = _newAdmin;
    }

    /**
     * @dev Gets the tokenURi of passed tokenId and returns it.
     * @param tokenId . TokenId of NFT.
     */

    function getTokenUri(uint256 tokenId)
        internal
        view
        returns (string memory)
    {
        return ILaunchpad721(collectionAddress).tokenURI(tokenId);
    }

    /**
     * @dev Sets the winnersList from old NFTDraw contract to this.
     * @param _previousNFTDrawAddress . previous NFTDraw address.
     */

    function setPreviousWinners(address _previousNFTDrawAddress) public {
        require(msg.sender == admin, "Not admin.");
        Winner[] memory _winnersList = NFTDraw(_previousNFTDrawAddress)
            .getWinnersList();
        for (uint256 i = 0; i < _winnersList.length; i++) {
            winnersList.push(
                Winner(
                    _winnersList[i].tokenURI,
                    _winnersList[i].nftWinner,
                    _winnersList[i].amount,
                    _winnersList[i].winnerDate
                )
            );
        }
    }

    /**
     * @dev Returns jackpot that nft winner will get.
     */

    function getTotalJackpot() external view returns (uint256) {
        uint256 currentBalance = ERC20(luckyBlockToken).balanceOf(
            address(this)
        );
        uint256 jackpotAmount = currentBalance.div(
            lblockPrice.getLBlockPrice()
        );
        return jackpotAmount;
    }

    /**
     * @dev Gets all sold NFTLaunchpad's tokens.
     * @dev Gets owner of winnerToken.
     * @dev Sends all LBlocks from the current contract to winner.
     * @param _randomNumber . Randomly generated number passed from Lottery Contract.
     * @dev amount variable. Amount of LuckyBlock that are needed to be sent to the nft winner
     * @dev nftWinner variable. Address of winner.
     * @dev soldTokensAmount variable. Amount of sold tokens.
     * @dev winnerToken . Token chosen by passed number.
     * @notice Picks a winner.
     */

    function pickWinner(uint256 _randomNumber) external {
        require(
            msg.sender == operator || msg.sender == admin,
            "Not operator nor admin."
        );
        uint256 soldTokensAmount = ICollectionWrapper(collectionWrapperAddress)
            .getTotalItemsInSet();
        uint256 amount = ERC20(luckyBlockToken).balanceOf(address(this));
        require(amount > 0, "Balance zero.");
        uint32 tokenIndex = uint32(_randomNumber.mod(soldTokensAmount));
        winnerToken = ICollectionWrapper(collectionWrapperAddress)
            .getTokenByIndex(tokenIndex);
        nftWinner = ILaunchpad721(collectionAddress).ownerOf(winnerToken);
        require(nftWinner != address(0), "Transfer to zero address.");
        NFTwinner.tokenURI = getTokenUri(winnerToken);
        NFTwinner.nftWinner = nftWinner;
        NFTwinner.amount = amount.div(10**9);
        NFTwinner.winnerDate = block.timestamp;
        winnersList.push(NFTwinner);
        ERC20(luckyBlockToken).transfer(nftWinner, amount);
        emit PickWinner(nftWinner, amount, block.timestamp, winnerToken);
    }
}

