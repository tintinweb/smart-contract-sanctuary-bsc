    address[] internal addresses;
    mapping(address => bool) internal seenAddresses;

    mapping(address => mapping(uint256 => uint256)) internal addressCardBalance;
    mapping(address => mapping(uint256 => uint256))
        internal addressLevelBalance;
    mapping(uint256 => uint256) internal lockedPerRank;

    bool public isLocked = false;

    uint256 internal iterations = 200;

    uint256 public level3Basis = 1000;
    uint256 public level4Basis = 4000;
    uint256 public level5Basis = 5000;

    uint256 public level3Reward = 0;
    uint256 public level4Reward = 0;
    uint256 public level5Reward = 0;

    uint256 public maxTokenAmount = 2000000000000000000000000000000;

    FManCard internal cardContract;

    modifier stakingUnlocked() {
        require(!isLocked, "Deposits and withdrawals are currently locked");
        _;
    }

    modifier onlyCard() {
        require(_msgSender() == cardAddress, "Only card contract may call this");
        _;
    }

    event Received(address, uint256);

    // /////////////////////////////////////////////
    // PUBLIC - OWNER ONLY
    // /////////////////////////////////////////////

    function setTokenAddress(address _address) external onlyOwner {
        tokenAddress = _address;
    }

    function setCardAddress(address _address) external onlyOwner {
        cardAddress = _address;

        cardContract = FManCard(_address);
    }

    function setBUSDAddress(address newBUSDAddress) external onlyOwner {
        busdAddress = newBUSDAddress;
    }

    function setRouterAddress(address newRouterAddress) external onlyOwner {
        routerAddress = newRouterAddress;
    }

    function setIterations(uint256 _iterations) external onlyOwner {
        iterations = _iterations;
    }

    function getAddressCount()
        external
        view
        onlyOwner
        returns (uint256 addressCount)
    {
        return addresses.length;
    }

    function setMaxTokenAmount(uint256 _amount) external onlyOwner {
        maxTokenAmount = _amount;
    }

    function getTotalSetRewards(uint256 _rewardSplitAmountUSD)
        external
        view
        onlyOwner
        returns (uint256 amount)
    {
        uint256 total = calculateTotalSetRewards() + _rewardSplitAmountUSD;
        return getFMANFromUSD(total);
    }

    function syncSeasonsCards(uint256[] memory _seasonSetRewards)
        external
        onlyOwner
    {
        FManCard card = FManCard(cardAddress);
        cardIds = card.getCardIds();

        uint256[] memory nftSeasonIds = sort(card.getSeasonIds());

        require(
            _seasonSetRewards.length == nftSeasonIds.length,
            "Season reward length must match contract seasons"
        );

        seasonIds = nftSeasonIds;

        for (uint256 i = 0; i < nftSeasonIds.length; i++) {
            seasonMap[nftSeasonIds[i]] = Season(
                _seasonSetRewards[i],
                card.getSeasonCards(nftSeasonIds[i])
            );
        }
    }

    function setRewardSplits(
        uint256 _level3Percent,
        uint256 _level4Percent,
        uint256 _level5Percent
    ) external onlyOwner {
        require(
            _level3Percent + _level4Percent + _level5Percent == 100,
            "Percent ratios must equal 100"
        );
        level3Basis = _level3Percent * 100;
        level4Basis = _level4Percent * 100;
        level5Basis = _level5Percent * 100;
    }

    function startDistribution(uint256 _rewardSplitAmountUSD)
        external
        onlyOwner
    {
        isLocked = true;

        uint256 setTotal = calculateTotalSetRewards();
        uint256 setTokenTotal = getFMANFromUSD(setTotal);
        uint256 total = (setTotal +
            _rewardSplitAmountUSD +
            _rewardSplitAmountUSD.mul(500).div(10000));
        uint256 tokenTotal = getFMANFromUSD(total);

        require(
            IERC20(tokenAddress).balanceOf(_msgSender()) >= tokenTotal,
            "Insufficient balance to start distribution"
        );

        IERC20(tokenAddress).transferFrom(
            _msgSender(),
            address(this),
            tokenTotal
        );

        // After transfer, calculate total wallet balance
        uint256 splitBalance = IERC20(tokenAddress)
            .balanceOf(address(this))
            .sub(setTokenTotal);

        // Figure out splits for levels
        if (lockedPerRank[3] > 0) {
            level3Reward = splitBalance.mul(level3Basis).div(10000).div(
                lockedPerRank[3]
            );

            splitBalance -= level3Reward;
        }

        if (lockedPerRank[4] > 0) {
            level4Reward = splitBalance.mul(level4Basis).div(10000).div(
                lockedPerRank[4]
            );

            splitBalance -= level4Reward;
        }
        if (lockedPerRank[5] > 0) {
            level5Reward = splitBalance.mul(level5Basis).div(10000).div(
                lockedPerRank[5]
            );
        }
    }

    function distribute(uint256 _startIndex) external onlyOwner nonReentrant {
        require(isLocked, "You must start distribution first");
        require(
            _startIndex < addresses.length,
            "Start index must be less than address count"
        );

        bool isLast = false;
        uint256 end = _startIndex + iterations;

        if (end > addresses.length) {
            isLast = true;
            end = addresses.length;
        }

        for (uint256 i = 0; i < end; i++) {
            address wallet = addresses[i];
            uint256 setAmount = calculateSetRewards(wallet);

            uint256 amount = 0;

            if (setAmount > 0) {
                amount += getFMANFromUSD(setAmount);
            }

            amount += level3Reward.mul(addressLevelBalance[wallet][3]);
            amount += level4Reward.mul(addressLevelBalance[wallet][4]);
            amount += level5Reward.mul(addressLevelBalance[wallet][5]);

            if (
                amount > 0 &&
                (IERC20(tokenAddress).balanceOf(wallet) + amount) <
                maxTokenAmount
            ) {
                IERC20(tokenAddress).transfer(wallet, amount);
            }
        }

        if (isLast) {
            isLocked = false;

            level3Reward = 0;
            level4Reward = 0;
            level5Reward = 0;
        }
    }

    function stakedrop(
        address _for,
        uint256[] memory _ids,
        uint256[] memory _quantities
    ) external onlyCard {
        require(_ids.length > 0, "Must supply at least 1 card id");
        require(
            _ids.length == _quantities.length,
            "Id and quantitie arrays must be equal length"
        );

        for (uint256 i = 0; i < _ids.length; i++) {
            logDeposit(_for, _ids[i], _quantities[i]);
        }
    }

    // /////////////////////////////////////////////
    // PUBLIC - ALL
    // /////////////////////////////////////////////

    receive() external payable {
        emit Received(msg.sender, msg.value);
    }

    function balanceOf(address _owner, uint256 _id)
        external
        view
        returns (uint256 balance)
    {
        return addressCardBalance[_owner][_id];
    }

    function deposit(
        uint256 _id,
        uint256 _quantity,
        bytes memory _data
    ) public stakingUnlocked {
        address sender = _msgSender();

        require(
            cardContract.balanceOf(sender, _id) > 0,
            "Owned balance must be greater than 0"
        );

        logDeposit(sender, _id, _quantity);

        cardContract.safeTransferFrom(
            sender,
            address(this),
            _id,
            _quantity,
            _data
        );
    }

    function batchDeposit(uint256[] memory _ids, uint256[] memory _quantities)
        public
        stakingUnlocked
    {
        require(_ids.length > 0, "Must supply at least 1 card id");
        require(
            _ids.length == _quantities.length,
            "Id and quantitie arrays must be equal length"
        );

        for (uint256 i = 0; i < _ids.length; i++) {
            deposit(_ids[i], _quantities[i], "");
        }
    }

    function depositAll() public stakingUnlocked {
        for (uint256 i = 0; i < cardIds.length; i++) {
            uint256 balance = cardContract.balanceOf(_msgSender(), cardIds[i]);

            if (balance > 0) {
                deposit(cardIds[i], balance, "");
            }
        }
    }

    function withdraw(
        uint256 _id,
        uint256 _quantity,
        bytes memory _data
    ) public stakingUnlocked {
        address sender = _msgSender();

        require(cardContract.isCardValid(_id), "Card is not valid");
        require(_quantity > 0, "Quantity must be greater than 0");
        require(
            _quantity <= addressCardBalance[sender][_id],
            "Cannot withdraw more than staked balance"
        );

        (, , , , uint256 level) = cardContract.getCard(_id);
        lockedPerRank[level] -= _quantity;

        addressCardBalance[sender][_id] -= _quantity;
        addressLevelBalance[sender][level] -= _quantity;

        cardContract.safeTransferFrom(
            address(this),
            sender,
            _id,
            _quantity,
            _data
        );
    }

    function batchWithdraw(uint256[] memory _ids, uint256[] memory _quantities)
        public
        stakingUnlocked
    {
        require(_ids.length > 0, "Must supply at least 1 card id");
        require(
            _ids.length == _quantities.length,
            "Id and quantitie arrays must be equal length"
        );

        for (uint256 i = 0; i < _ids.length; i++) {
            withdraw(_ids[i], _quantities[i], "");
        }
    }

    function withdrawAll() public stakingUnlocked {
        for (uint256 i = 0; i < cardIds.length; i++) {
            uint256 balance = addressCardBalance[_msgSender()][cardIds[i]];

            if (balance > 0) {
                withdraw(cardIds[i], balance, "");
            }
        }
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return
            bytes4(
                keccak256(
                    "onERC1155Received(address,address,uint256,uint256,bytes)"
                )
            );
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] memory,
        uint256[] memory,
        bytes calldata
    ) external pure override returns (bytes4) {
        return
            bytes4(
                keccak256(
                    "onERC1155BatchReceived(address,address,uint256[],uint256[],bytes)"
                )
            );
    }

    function supportsInterface(bytes4 interfaceId)
        external
        pure
        override
        returns (bool)
    {
        return
            interfaceId == 0x01ffc9a7 || // ERC165
            interfaceId == 0x4e2312e0; // ERC1155_ACCEPTED ^ ERC1155_BATCH_ACCEPTED;
    }

    // /////////////////////////////////////////////
    // INTERNAL
    // /////////////////////////////////////////////

    function logDeposit(
        address _for,
        uint256 _id,
        uint256 _quantity
    ) internal {
        require(cardContract.isCardValid(_id), "Card is not valid");
        require(_quantity > 0, "Quantity must be greater than 0");

        (, , , , uint256 level) = cardContract.getCard(_id);
        lockedPerRank[level] += _quantity;

        addressCardBalance[_for][_id] += _quantity;
        addressLevelBalance[_for][level] += _quantity;

        if (!seenAddresses[_for]) {
            seenAddresses[_for] = true;
            addresses.push(_for);
        }
    }

    function getFMANFromUSD(uint256 _usd)
        internal
        view
        returns (uint256 amount)
    {
        address[] memory path = new address[](2);
        path[0] = busdAddress;
        path[1] = tokenAddress;

        uint256[] memory amounts = IUniswapV2Router02(routerAddress)
            .getAmountsOut(1, path);
        return amounts[1] * _usd * 1 ether;
    }

    function calculateTotalSetRewards() internal view returns (uint256 amount) {
        uint256 total = 0;

        for (uint256 i = 0; i < addresses.length; i++) {
            total += calculateSetRewards(addresses[i]);
        }

        return total;
    }

    function calculateSetRewards(address _address)
        internal
        view
        returns (uint256 amount)
    {
        uint256 total = 0;

        for (uint256 s = 0; s < seasonIds.length; s++) {
            uint256 sets = getSetCount(_address, seasonIds[s]);

            if (sets > 0) {
                total += sets.mul(seasonMap[seasonIds[s]].setReward);
            }
        }

        return total;
    }

    function getSetCount(address _address, uint256 _seasonId)
        internal
        view
        returns (uint256 setCount)
    {
        require(
            cardContract.isSeasonValid(_seasonId),
            "Invalid season supplied"
        );
        Season memory season = seasonMap[_seasonId];

        // If someone has more sets than this, we should talk
        uint256 sets = 9999;

        // Finds the lowest balance of a given level in a season
        // ex: If we have 5 level 1s, but 0 level 2s, sets will be 0
        for (uint256 i = 0; i < season.cardIds.length; i++) {
            uint256 balance = addressCardBalance[_address][season.cardIds[i]];

            if (balance < sets) {
                sets = balance;
            }
        }

        return sets;
    }

    function sort(uint256[] memory data) internal returns (uint256[] memory) {
        quickSort(data, int256(0), int256(data.length - 1));

        return data;
    }

    function quickSort(
        uint256[] memory arr,
        int256 left,
        int256 right
    ) internal {
        int256 i = left;
        int256 j = right;
        if (i == j) return;
        uint256 pivot = arr[uint256(left + (right - left) / 2)];
        while (i <= j) {
            while (arr[uint256(i)] < pivot) i++;
            while (pivot < arr[uint256(j)]) j--;
            if (i <= j) {
                (arr[uint256(i)], arr[uint256(j)]) = (
                    arr[uint256(j)],
                    arr[uint256(i)]
                );
                i++;
                j--;
            }
        }
        if (left < j) quickSort(arr, left, j);
        if (i < right) quickSort(arr, i, right);
    }
}

