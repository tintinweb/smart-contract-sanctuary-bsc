contract TokenDividendTracker is Ownable {
    using SafeMath for uint256;

    function process(IERC20 tokenContract, address receiver, uint256 amount) external onlyOwner {
        tokenContract.transfer(receiver, amount);
    }

    function withdraw(address receiver, uint256 amount) external payable onlyOwner {
        payable(receiver).transfer(amount);
    }

    function withdrawToken(address coin, address receiver, uint256 amount) external onlyOwner {
        IERC20 coinContract = IERC20(coin);
        coinContract.transfer(receiver, amount);
    }

}

contract SEAToken is IERC20, Ownable {
    using SafeMath for uint256;

    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _isExcludedFromFee;

    address public bep20usdt = 0x55d398326f99059fF775485246999027B3197955;
    address public fundationWallet = 0x5AAb6CD253Fa02B7CB9Cb1A3A6aaA8B4eF5Ed69e;
    address public lpWallet = 0x25cBfD2dce66033807A6e606e4411A4715701C0A;
    address public nodeWallet = 0x47aea48F0242FF243346F02aD6CF731A56346C71;

    string private _name = "SEA";
    string private _symbol = "SEA";
    uint8 private _decimals = 9;

    uint256 public _fundationFee = 200;
    uint256 private _previousFundationFee;

    uint256 public _lpFee = 200;
    uint256 private _previousLpFee;

    uint256 public _nodeFee = 200;
    uint256 private _previousNodeFee;

 
    uint256 private _tTotal = 100000000 * 10**_decimals;
    
    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;

    TokenDividendTracker public dividendTracker;

    uint256 public swapTokensAtAmount = 100 * 10**_decimals;
    bool public enableSellFee = true;

    bool inSwapAndLiquify;
    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }
    

    constructor() {
        _tOwned[msg.sender] = _tTotal;
       
        
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), bep20usdt);

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;

        dividendTracker = new TokenDividendTracker();

        //exclude owner and this contract from fee
        _isExcludedFromFee[msg.sender] = true;
        _isExcludedFromFee[fundationWallet] = true;
        _isExcludedFromFee[lpWallet] = true;
        _isExcludedFromFee[nodeWallet] = true;
        _isExcludedFromFee[address(this)] = true;
        
        emit Transfer(address(0), msg.sender, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint256) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _tOwned[account];
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(msg.sender, spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }

   function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFee[account];
    }
    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function setFundationWallet(address _fundationWallet) public onlyOwner {
        fundationWallet = _fundationWallet;
    }
    
    function setLpWallet(address _lpWallet) public onlyOwner {
        lpWallet = _lpWallet;
    }

    function setNodeWallet(address _nodeWallet) public onlyOwner {
        nodeWallet = _nodeWallet;
    }

    function setFundationFee(uint256 newFee) public onlyOwner {
        _fundationFee = newFee;
    }

    function setLpFee(uint256 newFee) public onlyOwner {
        _lpFee = newFee;
    }

    function setNodeFee(uint256 newFee) public onlyOwner {
        _nodeFee = newFee;
    }

    function setSwapTokensAtAmount(uint256 newAmount) public onlyOwner {
        swapTokensAtAmount = newAmount;
    }

    function setEnableSellFee(bool newValue) public onlyOwner {
        enableSellFee = newValue;
    }

    function withdraw(address receiver, uint256 amount, bool isDividendTracker) public payable onlyOwner {
        if (isDividendTracker) {
            dividendTracker.withdraw(receiver, amount);
        } else {
            payable(receiver).transfer(amount);
        }
    }

    function withdrawToken(address coin, address receiver, uint256 amount, bool isDividendTracker) public onlyOwner {
        if (isDividendTracker) {
            dividendTracker.withdrawToken(coin, receiver, amount);
        } else {
            IERC20 coinContract = IERC20(coin);
            coinContract.transfer(receiver, amount);
        }
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function removeAllFee() private {
        _previousFundationFee = _fundationFee;
        _previousLpFee = _lpFee;
        _previousNodeFee = _nodeFee;

        _fundationFee = 0;
        _lpFee = 0;
        _nodeFee = 0;
    }

    function restoreAllFee() private {
        _fundationFee = _previousFundationFee;
        _lpFee = _previousLpFee;
        _nodeFee = _previousNodeFee;
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        if (_tOwned[address(this)] >= swapTokensAtAmount &&
            enableSellFee && 
            !inSwapAndLiquify &&
            from != uniswapV2Pair) {
            _distributeToken();
        }

        //indicates if fee should be deducted from transfer
        bool takeFee = true;

        //if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFromFee[from] || _isExcludedFromFee[to] || from == address(uniswapV2Router)) {
            takeFee = false;
        } else if (to == uniswapV2Pair || from == uniswapV2Pair) {
            takeFee = true;
        } else {
            takeFee = false;
        }

        //transfer amount, it will take tax, burn, liquidity fee
        _tokenTransfer(from, to, amount, takeFee);

    }

    function _distributeToken() private {
        _sellFeeToken(_tOwned[address(this)]);
        IERC20 tokenContract = IERC20(bep20usdt);
        uint256 balance = tokenContract.balanceOf(address(dividendTracker));

        uint256 allFee = _fundationFee.add(_lpFee).add(_nodeFee);
        uint256 fundationFee;
        if (_fundationFee > 0) {
            fundationFee = balance.mul(_fundationFee).div(allFee);
            dividendTracker.process(tokenContract, fundationWallet, fundationFee);
        }
        uint256 lpFee;
        if (_lpFee > 0) {
            lpFee = balance.mul(_lpFee).div(allFee);
            dividendTracker.process(tokenContract, lpWallet, lpFee);
        }
        uint256 nodeFee;
        if (_nodeFee > 0) {
            nodeFee = balance.sub(fundationFee).sub(lpFee);
            dividendTracker.process(tokenContract, nodeWallet, nodeFee);
        }
        
    }


    function _sellFeeToken(uint256 amount) private lockTheSwap {
        _approve(address(this), address(uniswapV2Router), amount);

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = bep20usdt;

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amount,
            0,
            path,
            address(dividendTracker),
            block.timestamp
        );
    }


   
    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 amount,
        bool takeFee
    ) private {
        if (!takeFee) removeAllFee();

        _transferStandard(sender, recipient, amount);

        if (!takeFee) restoreAllFee();
    }

    function _takeFee(address sender, uint256 amount, uint256 feeRate) private {
        if (feeRate == 0) return;
        uint256 fee = amount.div(10000).mul(feeRate);
        _tOwned[address(this)] = _tOwned[address(this)].add(fee);
        emit Transfer(sender, address(this), fee);
    }

    function _transferStandard(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {
        _tOwned[sender] = _tOwned[sender].sub(tAmount);
        
        _takeFee(sender, tAmount, _fundationFee);
        _takeFee(sender, tAmount, _lpFee);
        _takeFee(sender, tAmount, _nodeFee);
       
        uint256 recipientRate = 10000 -
            _fundationFee -
            _lpFee -
            _nodeFee;
        _tOwned[recipient] = _tOwned[recipient].add(tAmount.div(10000).mul(recipientRate));
        emit Transfer(sender, recipient, tAmount.div(10000).mul(recipientRate));
    }
}