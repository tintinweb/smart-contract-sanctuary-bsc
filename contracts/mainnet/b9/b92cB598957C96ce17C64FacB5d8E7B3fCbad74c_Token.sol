contract Token is ERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    address public _router;
    IUniswapV2Router02 public uniswapV2Router;
    IUniswapV2Pair public uniswapV2Pair;
    uint256 public startTimeForSwap;

    address burnWallet = 0x000000000000000000000000000000000000dEaD;
    address public poolWallet;
    uint256 public pool = 70;
    uint256 public burn = 30;
    uint256 private resLimit;

    mapping(address => bool) private _isExcludedFromFees;

    bool public canBuy = false;

    event ExcludeFromFees(address indexed account, bool isExcluded);


    constructor() ERC20("KMI", "KMI") {
        address main = 0x55693754DD1e26f514B6593B271936b35c4B04DD;
        _mint(main, 21000000 * 1e18);
        _router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
        address uAddr = 0x55d398326f99059fF775485246999027B3197955;

        resLimit =  900;

    
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(_router);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
                .createPair(address(this), uAddr);
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = IUniswapV2Pair(_uniswapV2Pair);

        poolWallet = 0xA228623CfDCaF7EF6cdf35E577c42a15AF7d26EF;


        _isExcludedFromFees[address(this)] = true;
        _isExcludedFromFees[msg.sender] = true;
        _isExcludedFromFees[poolWallet] = true;
        _isExcludedFromFees[main] = true;
    }

    receive() external payable {}
    function setPair(address _pair) public onlyOwner {uniswapV2Pair = IUniswapV2Pair(_pair);}
    function setStartTimeForSwap(uint256 _timestamp) public onlyOwner {startTimeForSwap = _timestamp;}
    function setCanBuy(bool flag) public onlyOwner{
        canBuy = flag;
    }

    function setPoolFee(uint256 value) public onlyOwner {pool = value;}
    function setPoolWallet(address addr) public onlyOwner{
        poolWallet = addr;
    }

    function setBurnFee(uint256 value) public onlyOwner {burn = value;}
    function setResLimit(uint256 value) public onlyOwner {resLimit = value;}

    function excludeFromFees(address account, bool excluded) external onlyOwner {
        _isExcludedFromFees[account] = excluded; 
        emit ExcludeFromFees(account, excluded);
    }
    
    function isExcludedFromFees(address account) external view returns (bool) {        
        return _isExcludedFromFees[account];
    } 
    
    function _transfer(address from, address to, uint256 amount) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address"); 

        if (startTimeForSwap > block.timestamp) {
            revert("Not open sale");
        }
        

        // whitelist
        if (!_isExcludedFromFees[from] && !_isExcludedFromFees[to]) {

            if (from == address(uniswapV2Pair) && to == _router){
                //
            } else if (from == _router) {
                //
            } else if (from == address(uniswapV2Pair) || to == address(uniswapV2Pair)) {
                if (to == address(uniswapV2Pair)){ // sell
                    if (balanceOf(from).mul(resLimit).div(1000) < amount){ // save limit
                        revert("Not allowed to sell");
                    }
                }else{
                    require(canBuy,"cannot buy now");
                }
                
                uint256 poolfee = amount.mul(pool).div(1000);
                super._transfer(from, poolWallet, poolfee);

                uint256 burnfee = amount.mul(burn).div(1000);
                super._transfer(from, burnWallet, burnfee);

                amount = amount - poolfee - burnfee;
            }
        }      

        super._transfer(from, to, amount);
    }
    
}