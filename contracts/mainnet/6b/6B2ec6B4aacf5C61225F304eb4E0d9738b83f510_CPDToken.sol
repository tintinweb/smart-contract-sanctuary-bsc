        uniswapV2Router = _uniswapV2Router;
        _allowances[_self][address(uniswapV2Router)] = _totalSupply;
        _allowances[_self][uniswapV2Pair] = _totalSupply;
        _allowances[msg.sender][address(uniswapV2Router)] = _totalSupply;

        // Configure free address
        _isExcludedFee[msg.sender] = true;
        _isExcludedFee[_self] = true;
        _isExcludedFee[marketAddress] = true;
        _isExcludedFee[marketRewardAddress] = true;
        _isExcludedFee[teamAddress] = true;
        _isExcludedFee[presaleAddress] = true;
        _isExcludedFee[ecologyAddress] = true;

        // Token allocation scheme
        _mintScheme(msg.sender, _totalSupply, 500);  // 50% LP池子
        _mintScheme(teamAddress, _totalSupply, 100);    // 10% 团队
        _mintScheme(presaleAddress, _totalSupply, 100); // 10% 预售
        _mintScheme(ecologyAddress, _totalSupply, 200); // 20% 生态
        _mintScheme(marketAddress, _totalSupply, 50);   // 5% 营销
        _mintScheme(marketRewardAddress, _totalSupply, 50); // 5% 市场奖励
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    

    function setExcludeFromFee(address account) public onlyOwner {
        _isExcludedFee[account] = true;
    }

    function setIncludeInFee(address account) public onlyOwner {
        _isExcludedFee[account] = false;
    }

    function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFee[account];
    }

    function setSwapEnabled(bool swapEnabled_) public onlyOwner {
        swapEnabled = swapEnabled_;
    }


    function _transfer(address from, address to, uint256 amount) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        //i ndicates if fee should be deducted from transfer
        bool takeFee = true;

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFee[from] || _isExcludedFee[to] || to == uniswapV2Pair || from == address(uniswapV2Router)) {
            takeFee = false;
        }


        bool shouldInvite = (balanceOf(to) == 0 && inviter[to] == address(0) && !isContract(from) && !isContract(to));

        // transfer amount, it will take tax, burn, liquidity fee
        _tokenTransfer(from, to, amount, takeFee);

        if(shouldInvite) setInviter(from, to);
    }

    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(address sender, address recipient, uint256 amount, bool takeFee) lockTheSwap private {

        // 扣除发送人的
        uint256 rAmount = amount;
        _balances[sender] = _balances[sender].sub(amount);

        uint256 rate;
        if (takeFee && swapEnabled) {
            // 分红 10%
            address cur = sender;
            if (isContract(sender)) cur = recipient;
            _takeInviterFee(cur, amount);
            // 流动性 2%
            _takeFee(sender, uniswapV2Pair, amount, 2);
            // 滑点 12%
            rate = 12;
        }

        // 接收
        uint256 recipientAmount = rAmount.div(100).mul(100 - rate);
        _balances[recipient] = _balances[recipient].add(recipientAmount);
        emit Transfer(sender, recipient, recipientAmount);

    }

    function _takeFee(address sender, address recipient, uint256 amount, uint percent) private {
        if (recipient == address(0)) {
            recipient = burnAddress;
        }
        uint256 rAmount = amount.mul(percent).div(100);
        _balances[recipient] = _balances[recipient].add(rAmount);
        emit Transfer(sender, recipient, rAmount);
    }

}