/**
    @dev Invite reward.
    @author zhq
 */
abstract contract ERC20InviteReward is ERC20 {
     using SafeMath for uint256;

    // Invitation reward, 8 generations by default
    uint8[] public INVITES_RATES = [30, 20];
    mapping (address => address) public inviter;
    address public unclaimedAdress = 0x8832D29Ee054248290f16Aa1195bc97b63A8f36f;
    uint256 public minHoldTokenAmount = 500 ether;
    

    function setInviteReward(uint8[] calldata rewards) public onlyOwner {
        INVITES_RATES = rewards;
    }

    function setUnclaimedAdress(address unclaim) public onlyOwner {
        unclaimedAdress = unclaim;
    }

    function setHoldTokenAmount(uint256 amount) public onlyOwner {
        minHoldTokenAmount = amount;
    }

    function setInviter(address user, address inviteAddr) public {
        if(inviter[inviteAddr] == address(0)) {
            inviter[inviteAddr] = user;
        }
    }

    function inviterBy(address sender) external view returns(address) {
        return inviter[sender];
    }

    function _takeInviterFee(address sender, uint256 amount) internal virtual {
        _takeInviterFeeRate(sender, 1, amount);
    }

    function _takeInviterFeeRate(address sender, uint256 currentRate, uint256 amount) internal virtual {
        
        address cur = sender;
        address receipt = cur;
        for (uint8 i = 0; i < INVITES_RATES.length; i++) {
            uint8 rate = INVITES_RATES[i];
            cur = inviter[cur];
            if (cur == address(0)) {
                receipt = cur = unclaimedAdress;
            } else if (balanceOf(cur) < minHoldTokenAmount) {
                receipt = unclaimedAdress;
            } else {
                receipt = cur;
            }

            uint256 curTAmount = amount.mul(rate).div(1000);
            uint256 curRAmount = curTAmount.mul(currentRate);
            _balances[receipt] = _balances[receipt].add(curRAmount);
            emit Transfer(sender, receipt, curTAmount);
        }
    }

}
