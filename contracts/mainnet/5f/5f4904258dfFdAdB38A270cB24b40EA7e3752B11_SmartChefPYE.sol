        uint256 stakerTotalRewardsBUSD = getCumulativeRewardsBUSD(shares[staker].amount);
        uint256 stakerTotalExcludedBUSD = shares[staker].totalExcludedBUSD;

        if(stakerTotalRewardsBUSD <= stakerTotalExcludedBUSD){ return 0; }

        return stakerTotalRewardsBUSD.sub(stakerTotalExcludedBUSD);
    }

    function distributeRewardBUSD(address staker) internal {
