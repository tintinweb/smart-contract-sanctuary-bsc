    function depositBUSDToStakingContract(uint256 _amountBUSD) external onlyToken {
        if (totalShares == 0) {unallocatedBUSDRewards = unallocatedBUSDRewards.add(_amountBUSD); return; } 
        
        if (unallocatedBUSDRewards > 0) {
            uint256 amount = _amountBUSD.add(unallocatedBUSDRewards);
            totalRewardsBUSD = totalRewardsBUSD.add(amount);
            rewardsPerShareBUSD = rewardsPerShareBUSD.add(rewardsPerShareAccuracyFactor.mul(amount).div(totalShares));
            unallocatedBUSDRewards = 0;
        } else {
            totalRewardsBUSD = totalRewardsBUSD.add(_amountBUSD);
            rewardsPerShareBUSD = rewardsPerShareBUSD.add(rewardsPerShareAccuracyFactor.mul(_amountBUSD).div(totalShares));
        }   
    }

    function depositBUSD(uint256 _amount) external onlyOwner {
        uint256 balanceBefore = IERC20(address(WBNB)).balanceOf(address(this));

        IERC20(WBNB).transferFrom(address(msg.sender), address(this), _amount);

        uint256 amount = IERC20(address(WBNB)).balanceOf(address(this)).sub(balanceBefore);

        totalRewardsWBNB = totalRewardsWBNB.add(amount);
        rewardsPerShareWBNB = rewardsPerShareWBNB.add(rewardsPerShareAccuracyFactor.mul(amount).div(totalShares));
        
    }

    function getCumulativeRewardsBUSD(uint256 share) internal view returns (uint256) {
