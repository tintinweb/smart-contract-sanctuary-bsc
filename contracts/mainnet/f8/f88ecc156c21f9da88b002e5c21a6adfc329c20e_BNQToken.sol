pragma solidity ^0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interface/IPancake.sol";
import "hardhat/console.sol";

interface IBEP20 is IERC20, IERC20Metadata {}

contract AiBox is OwnableUpgradeable {
	struct Meta {
		IBEP20 TGG;
		IBEP20 U;
		IPancakeRouter02 router;
		uint256 newDayTime;
		address tggPair;
		address fund;
		uint256 curTokenAmount;
	}

	struct User {
		uint256 lastClaimTime;
		uint256 stakeTime;
		uint256 level;
		uint256 staticRewarded;
		//
		uint256 referReward;
		uint256 referRewarded;
		uint256 referAmount;
		uint256 referValue;
		uint256 diffReward;
		uint256 diffRewarded;
		// uint256 totalAmount;
		uint256 totalClaimed;
		address inviter;
	}

	struct Node {
		uint256 level;
		uint256 price;
		uint256 dayRate;
		uint256 outRate;
	}
	struct TokenInfo {
		uint256 decimal;
		uint256 rate;
		IBEP20 token;
		address pair;
		string name;
	}

	struct Token {
		TokenInfo[2] info;
		bool status;
	}

	struct Difference {
		uint256 level;
		uint256 rate;
		uint256 value;
	}

	Difference[8] public diffInfos;

	mapping(uint256 => Token) tokenInfos;

	Meta public meta;
	mapping(address => User) public userInfos;
	Node[7] public nodeInfos;

	modifier checkTime() {
		uint256 temp = block.timestamp - (block.timestamp % 86400);
		if (temp != meta.newDayTime) {
			meta.newDayTime = temp;
		}
		_;
	}

	function initialize(address usdt_, address tgg_) public initializer {
		__Ownable_init_unchained();
		nodeInfos[1] = Node(1, 200 ether, 1 ether, 20);
		nodeInfos[2] = Node(1, 500 ether, 33e17, 25);
		nodeInfos[3] = Node(1, 1000 ether, 7e18, 30);
		nodeInfos[4] = Node(1, 3000 ether, 25e18, 32);
		nodeInfos[5] = Node(1, 5000 ether, 40e18, 35);
		nodeInfos[6] = Node(1, 10000 ether, 90e18, 40);
		meta.fund = _msgSender();
		meta.U = IBEP20(usdt_);
		meta.TGG = IBEP20(tgg_);
		meta.curTokenAmount = 1;

		IBEP20 tgg = IBEP20(tgg_);
		tokenInfos[0].info[0] = TokenInfo(tgg.decimals(), 100, tgg, address(0), tgg.symbol());
		tokenInfos[0].status = true;

		diffInfos[1] = Difference(1, 2, 10000 ether);
		diffInfos[2] = Difference(2, 3, 50000 ether);
		diffInfos[3] = Difference(3, 5, 100000 ether);
		diffInfos[4] = Difference(4, 6, 200000 ether);
		diffInfos[5] = Difference(5, 7, 500000 ether);
		diffInfos[6] = Difference(6, 8, 700000 ether);
		diffInfos[7] = Difference(7, 10, 1000000 ether);
	}

	function setRefLevel(address[] calldata accounts_, uint256[] calldata levels) public onlyOwner {
		for (uint256 i; i < accounts_.length; i++) {
			userInfos[accounts_[i]].referValue = diffInfos[levels[i]].value;
		}
	}

	function addToken(address[2] calldata tokens_, uint256[2] calldata rates_) public onlyOwner {
		uint256 tokenIndex = meta.curTokenAmount++;
		for (uint256 i; i < 2; i++) {
			address pair = address(0);
			// address pair = IPancakeFactory(meta.router.factory()).getPair(tokens_[i], address(meta.U));
			// require(pair != address(0) && meta.U.balanceOf(pair) != 0, "wrong pair");

			IBEP20 token = IBEP20(tokens_[i]);
			tokenInfos[tokenIndex].info[i] = TokenInfo(token.decimals(), rates_[i], token, pair, token.symbol());
			if (rates_[i] == 100) break;
		}
		tokenInfos[tokenIndex].status = true;
	}

	function setToken(
		uint256 tokenIndex_,
		bool status_,
		uint256[2] calldata rates_
	) public onlyOwner {
		tokenInfos[tokenIndex_].status = status_;
		if (rates_[0] != 0) {
			tokenInfos[tokenIndex_].info[0].rate = rates_[0];
			tokenInfos[tokenIndex_].info[1].rate = rates_[1];
		}
	}

	function setFund(address fund_) public onlyOwner {
		meta.fund = fund_;
	}

	function getTokenInfo(uint256 index_) public view returns (bool status, TokenInfo[2] memory tokenInfo) {
		Token memory tInfo = tokenInfos[index_];
		status = tInfo.status;
		tokenInfo = tInfo.info;
	}

	function viewUserInfo(address account_) public view returns (uint256[11] memory info, address inviter) {
		User memory uInfo = userInfos[account_];
		Node memory nInfo = nodeInfos[uInfo.level];

		(uint256 staticReward, uint256 refReward, uint256 diffReward) = coutingReward(account_);
		info[0] = uInfo.level; // 用户等级
		info[1] = nInfo.price - uInfo.totalClaimed; // 剩余额度

		info[2] = uInfo.staticRewarded; // 已领取打金收益
		info[3] = staticReward; // 带领取

		info[4] = uInfo.referRewarded; // 已领取推广收益1
		info[5] = refReward; // 待领取

		(info[6], ) = getReferLelve(account_); // 团队等级
		info[7] = uInfo.diffRewarded; // 已领取推广收益2
		info[8] = diffReward;

		info[9] = uInfo.referAmount;
		info[10] = uInfo.referValue;

		inviter = uInfo.inviter;
	}

	function viewAllTokenInfo()
		public
		view
		returns (
			uint256[] memory indexes,
			bool[] memory status,
			TokenInfo[2][] memory infos
		)
	{
		uint256 size = meta.curTokenAmount;
		infos = new TokenInfo[2][](size);
		status = new bool[](size);
		indexes = new uint256[](size);
		for (uint256 i; i < size; i++) {
			(status[i], infos[i]) = getTokenInfo(i);
			// (status[i], ) = getTokenInfo(i);
			indexes[i] = i;
		}
	}

	function getTokenPrice(IBEP20 token_, address pair_) public view returns (uint256 price, uint256 decimal) {
		decimal = token_.decimals();
		// uint256 balance2 = token_.balanceOf(pair_);
		// uint256 balance1 = meta.U.balanceOf(pair_);

		// TODO for test
		uint256 balance1 = 1 * 1e18;
		uint256 balance2 = 2 * 10**decimal;
		price = (balance1 * 10**decimal) / balance2;
	}

	function _coutingAmount(
		uint256 nodePrice_,
		IBEP20 token_,
		address pair_
	) public view returns (uint256) {
		(uint256 price, uint256 decimal) = getTokenPrice(token_, pair_);
		return (nodePrice_ * 10**decimal) / price;
	}

	function coutingInAmount(
		uint256 level_,
		address account_,
		uint256 tokenIndex_
	) public view returns (uint256[2] memory tokenAmounts, uint256[2] memory decimals) {
		uint256 needValue = nodeInfos[level_].price - nodeInfos[userInfos[account_].level].price;
		Token memory token = tokenInfos[tokenIndex_];
		require(token.status, "wrong token index");

		for (uint256 i; i < 2; i++) {
			TokenInfo memory tInfo = token.info[i];
			// if (address(tInfo.token) == address(0) || tInfo.pair == address(0)) {
			if (address(tInfo.token) == address(0)) {
				continue;
			}
			decimals[i] = tInfo.token.decimals();
			tokenAmounts[i] = _coutingAmount((needValue * tInfo.rate) / 100, tInfo.token, tInfo.pair);
		}
	}

	function getReferLelve(address account_) public view returns (uint256 level, uint256 rewardRate) {
		uint256 value = userInfos[account_].referValue;
		for (uint256 i = 8; i > 1; i--) {
			if (value >= diffInfos[i - 1].value) {
				level = i - 1;
				rewardRate = diffInfos[i - 1].rate;
				return (level, rewardRate);
			}
		}
	}

	function _processDifference(uint256 pirce_) private {
		address curr = _msgSender();
		uint256 lastRate;
		uint256 lastLevel;
		for (uint256 i; i < 10; i++) {
			address inviter = userInfos[curr].inviter;
			if (inviter == address(0) || lastLevel == 7) return;
			curr = inviter;

			userInfos[inviter].referValue += pirce_;
			(uint256 level, uint256 rewardRate) = getReferLelve(inviter);

			if (level < lastLevel || rewardRate == 0) {
				continue;
			}

			uint256 rate = rewardRate - lastRate;
			userInfos[inviter].diffReward += (rate * pirce_) / 100;

			lastLevel = level;
			lastRate = rewardRate;
		}
	}

	function stake(
		uint256 level_,
		uint256 tokenIndex_,
		address inviter_
	) public checkTime {
		require(level_ < 7, "wrong level");

		Token memory token = tokenInfos[tokenIndex_];
		User memory uInfo = userInfos[_msgSender()];
		// Node memory nInfo = nodeInfos[level_];
		require(token.status, "wrong token index");
		require(uInfo.level < level_, "need high level");

		userInfos[_msgSender()].level = level_;
		userInfos[_msgSender()].stakeTime = block.timestamp;

		if (uInfo.inviter == address(0) && inviter_ != address(0)) {
			// require(userInfos[inviter_].inviter != address(0) || inviter_ == address(this), "wrong invitor");
			userInfos[inviter_].referAmount++;
			userInfos[_msgSender()].inviter = inviter_;
			// emit Bond(msg.sende, invitor);
		}

		uint256 needValue = nodeInfos[level_].price - nodeInfos[uInfo.level].price;

		for (uint256 i; i < 2; i++) {
			TokenInfo memory tInfo = token.info[i];
			// if (address(tInfo.token) == address(0) || tInfo.pair == address(0)) {
			if (address(tInfo.token) == address(0)) {
				continue;
			}
			uint256 amount = _coutingAmount((needValue * tInfo.rate) / 100, tInfo.token, tInfo.pair);
			tInfo.token.transferFrom(_msgSender(), address(this), amount);
		}

		_processDifference(needValue);
	}

	function _processOut() private {
		User memory uInfo = userInfos[_msgSender()];
		if (uInfo.totalClaimed < nodeInfos[uInfo.level].price) {
			return;
		}
		delete userInfos[_msgSender()];
		// User storage uInfo = userInfos[_msgSender()];
	}

	function coutingReward(address account_)
		public
		view
		returns (
			uint256 staticReward,
			uint256 refReward,
			uint256 diffReward
		)
	{
		User storage uInfo = userInfos[account_];
		if (uInfo.lastClaimTime < meta.newDayTime) {
			staticReward = nodeInfos[uInfo.level].dayRate;
		}

		uint256 maxQuota = nodeInfos[uInfo.level].price;
		if (staticReward + uInfo.totalClaimed > maxQuota) {
			staticReward = maxQuota - staticReward;
		}

		refReward = uInfo.referReward - uInfo.referRewarded;
		diffReward = uInfo.diffReward - uInfo.diffRewarded;
	}

	function claimStaticReward() public checkTime {
		(uint256 staticReward, , ) = coutingReward(_msgSender());
		require(staticReward > 0, "out of limit");

		uint256 realReward = (staticReward * 85) / 100;

		// User storage uInfo = userInfos[_msgSender()];
		userInfos[_msgSender()].staticRewarded += realReward;
		userInfos[_msgSender()].totalClaimed += realReward;
		userInfos[_msgSender()].lastClaimTime = block.timestamp;

		// uint256 maxQuota = nodeInfos[uInfo.level].price;
		// if (uInfo.totalClaimed >= maxQuota) {
		// 	_processOut();
		// }

		_processRefer(_msgSender(), staticReward);
		meta.U.transfer(_msgSender(), realReward);

		_processOut();
	}

	function _processRefer(address account_, uint256 amount_) private {
		uint8[2] memory rate = [10, 20];
		address inviter = userInfos[account_].inviter;
		for (uint256 i; i < 2; i++) {
			uint256 reward = amount_ / rate[i];
			if (inviter != address(0) && inviter != address(this)) {
				userInfos[inviter].referReward += reward;
			} else {
				meta.U.transfer(meta.fund, reward);
			}
			inviter = userInfos[inviter].inviter;
		}
	}

	function claimRefReward() public checkTime {
		(, uint256 refReward, ) = coutingReward(_msgSender());
		require(refReward > 0, "not reward");

		uint256 realReward = (refReward * 85) / 100;

		// User storage uInfo = userInfos[_msgSender()];
		userInfos[_msgSender()].totalClaimed += realReward;
		userInfos[_msgSender()].referRewarded += realReward;

		_processRefer(_msgSender(), refReward);
		meta.U.transfer(_msgSender(), realReward);

		_processOut();
	}

	function claimDiffReward() public checkTime {
		(, , uint256 diffReward) = coutingReward(_msgSender());
		require(diffReward > 0, "not reward");

		// User storage uInfo = userInfos[_msgSender()];
		userInfos[_msgSender()].totalClaimed += diffReward;
		userInfos[_msgSender()].diffRewarded += diffReward;

		meta.U.transfer(_msgSender(), diffReward);

		_processOut();
	}

	function withdraw() public checkTime {
		User memory uInfo = userInfos[_msgSender()];
		Node memory nInfo = nodeInfos[uInfo.level];
		require(uInfo.level > 0, "not reward");
		require((block.timestamp - uInfo.stakeTime) > 240 days, "out of limit");
		require(uInfo.totalClaimed < nInfo.price, "error");
		uint256 diffValue = nInfo.price - uInfo.totalClaimed;
		(uint256 price, uint256 decimal) = getTokenPrice(meta.TGG, meta.tggPair);

		uint256 diffTgg = (diffValue * 10**decimal) / price;
		delete userInfos[_msgSender()];
		meta.TGG.transfer(_msgSender(), diffTgg);
	}
}

