import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract BovineVerseGamingMysteryBox is ERC721Enumerable, Ownable {
	using Strings for uint256;

	struct Minter {
		uint256 curSupply;
		uint256 maxSupply;
	}
	mapping(address => Minter) public minters;
	string public baseURI;
	address public launchpad;

	modifier onlyMinter() {
		require(minters[msg.sender].maxSupply > 0, "must call by  minter");
		_;
	}

	event MintTo(address indexed minter, address indexed to, uint256 indexed size);
	event Divest(address token, address payee, uint256 value);

	function getMaxLaunchpadSupply() public view returns (uint256) {
		return minters[launchpad].maxSupply;
	}

	function getLaunchpadSupply() public view returns (uint256) {
		return minters[launchpad].curSupply;
	}

	constructor(
		string memory name_,
		string memory symbol_,
		string memory baseURI_,
		address launchpad_,
		uint256 maxSupply_
	) ERC721(name_, symbol_) {
		baseURI = baseURI_;
		launchpad = launchpad_;
		minters[launchpad_].maxSupply = maxSupply_;
	}

	function setMiner(address[] calldata accounts_, uint256[] calldata amounts_) external onlyOwner {
		for (uint256 i; i < accounts_.length; i++) {
			minters[accounts_[i]].maxSupply += amounts_[i];
		}
	}

	function _baseURI() internal view virtual override returns (string memory) {
		return baseURI;
	}

	function setBaseURI(string memory _newURI) external onlyOwner {
		baseURI = _newURI;
	}

	function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
		require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token.");
		return string(abi.encodePacked(baseURI));
	}

	function burn(uint256[] calldata tokenIds_) external returns (bool) {
		for (uint256 i; i < tokenIds_.length; ++i) {
			uint256 tokenId_ = tokenIds_[i];
			require(_isApprovedOrOwner(_msgSender(), tokenId_), "ERC721: burn caller is not owner nor approved");

			_burn(tokenId_);
		}
		return true;
	}

	// you must impl this in your NFT contract
	// you NFT contract is responsible to maintain the tokenId
	// you may have another mint function hold by yourself, to skip the process after presale end
	// max size will be 10
	function mintTo(address to_, uint256 amount_) external onlyMinter {
		require(to_ != address(0), "can't mint to empty address");
		require(amount_ > 0, "size must greater than zero");

		Minter storage minter = minters[msg.sender];
		require(minter.curSupply + amount_ <= minter.maxSupply, "max supply reached");
		minter.curSupply += amount_;
		for (uint256 i = 1; i <= amount_; i++) {
			_safeMint(to_, ERC721Enumerable.totalSupply() + i);
		}
		emit MintTo(msg.sender, to_, amount_);
	}

	function divest(
		address token_,
		address payee_,
		uint256 value_
	) external onlyOwner {
		if (token_ == address(0)) {
			payable(payee_).transfer(value_);
			emit Divest(address(0), payee_, value_);
		} else {
			IERC20(token_).transfer(payee_, value_);
			emit Divest(address(token_), payee_, value_);
		}
	}
}