// File: contracts/4_Swap.sol

//SPDX-License-Identifier: Unlicense
pragma solidity ^0.8.4;






contract Swap is Ownable {
    using SafeMath for uint256;
    using SafeCast for int256;
    using SafeERC20 for IERC20;

    event SwapToken(address receiver, uint256 amount);
    event WithdrawWAD(address user);
    event SetWAD(address user, address wad);
    event TransferWADTo(address admin, address receiver, uint256 amount);
    event SetAdmin(address owner, address admin);
    event ClaimWAD(address user, uint256 amount);

    IERC20 public immutable wad;
    address public admin;

    modifier onlyAdmin() {
        require(msg.sender == admin, "only admin");
        _;
    }

    constructor(address _wad) {
        wad = IERC20(_wad);
    }

    // change admin of backend
    function setAdmin(address _admin) external onlyOwner {
        admin = _admin;

        emit SetAdmin(msg.sender, _admin);
    }

    // Swap wad to credit in game ,Ratio 1:1
    function swap(uint256 _amount) external returns (uint256 blockNumber) {
        require(
            wad.allowance(msg.sender, address(this)) >= _amount,
            "Allowance not enought."
        );

        wad.safeTransferFrom(msg.sender, address(this), _amount);
        emit SwapToken(msg.sender, _amount);

        return block.number;
    }

    function claim() external onlyOwner {
        uint256 wadBalance = address(this).balance;
        (bool sent, ) = payable(owner()).call{value: wadBalance}("");
        require(sent, "Failed to send Ether");

        emit ClaimWAD(msg.sender, wadBalance);
    }

    // call by admin when user request withdraw credit
    function transferWADTo(address _receiver, uint256 _amount)
        external
        onlyAdmin
    {
        require(wad.balanceOf(address(this)) >= _amount, "Balance not enought");
        wad.safeTransfer(_receiver, _amount);

        emit TransferWADTo(msg.sender, _receiver, _amount);
    }

    // Reserve gas for backend call transferWADTo
    function withdrawWAD() external payable {
        require(msg.value == 50000000 gwei, "Value is missmatch");
        emit WithdrawWAD(msg.sender);
    }
}