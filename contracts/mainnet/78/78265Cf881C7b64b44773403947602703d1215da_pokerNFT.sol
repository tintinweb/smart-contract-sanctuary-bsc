pragma solidity ^0.8.0;

    contract pokerNFT is ERC721, ERC721Enumerable, ERC721URIStorage, ERC721Burnable,Pausable{
        
        using Strings for uint256;
        using Strings for uint8;

        using Address for address;
        
        string private _baseURIextended;


        uint256 public currentMint;

        string public extension;

        uint64 randNonce = 0;

        mapping(uint=>mapping(uint => string)) private probabilityOfpoint;

        address public fragmentsAddress=0x38DFd748ED1a29AFa9511c71a7e454649d7B41f2;
        address public synthesisFeeAddress=0xA913f11b1bB4192b6B08825dCeeb440D225Bf3Ed;
        


        mapping(uint=>bool) public numberOfFragments;


        event Synthesis(address, uint256, uint256, uint256);

        constructor() ERC721("PEOPLE POKER", "PEOPLE POKER") {
            numberOfFragments[10]=true;
            numberOfFragments[50]=true;
            numberOfFragments[100]=true;
            numberOfFragments[300]=true;
            numberOfFragments[500]=true;
            _baseURIextended="https://freepeopledao.xyz/pokerNFT/";
            extension = ".png";
            probabilityOfpoint[0][0]="spade";
            probabilityOfpoint[0][1]="heart";
            probabilityOfpoint[0][2]="club";
            probabilityOfpoint[0][3]="dianmond";

            probabilityOfpoint[1][0]="3";
            probabilityOfpoint[1][1]="4";
            probabilityOfpoint[1][2]="5";

            probabilityOfpoint[2][0]="6";
            probabilityOfpoint[2][1]="7";
            probabilityOfpoint[2][2]="8";

            probabilityOfpoint[3][0]="9";
            probabilityOfpoint[3][1]="10";
            probabilityOfpoint[3][2]="J";

            probabilityOfpoint[4][0]="Q";
            probabilityOfpoint[4][1]="K";
            probabilityOfpoint[4][2]="A";

            probabilityOfpoint[5][0]="2";
            probabilityOfpoint[5][1]="RedJoker";
            probabilityOfpoint[5][2]="BlackJoker";
        }

        function createRandom() internal returns (uint8){
            randNonce++; 
            
            return uint8(
                (uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, randNonce))))
            );
        }

        function setBaseURI(string memory baseURI_) external onlyOwner() {
            _baseURIextended = baseURI_;
        }

        function setFragmentsAddress(address _addr) public onlyOwner{
            fragmentsAddress=_addr;
        }

        function setsynthesisFeeAddress(address _addr) public onlyOwner{
            synthesisFeeAddress=_addr;
        }


        function _baseURI() internal view virtual override returns (string memory) {
            return _baseURIextended;
        }
        

        function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {
            require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
            string memory baseURI = _baseURI();
            string memory metadata = string(abi.encodePacked(
                    '{"name": "',
                    'PEOPLE DAONFT #',
                    tokenId.toString(),
                    '", "description": "The pig that changed the world.", "image": "',
                    string(abi.encodePacked(baseURI,tokenTraits[tokenId].color,"/",tokenTraits[tokenId].point,extension)),
                    '", "attributes":',
                    compileAttributes(tokenId),
                    "}"
                ));

            return string(abi.encodePacked(
                    "data:application/json;base64,",
                    base64(bytes(metadata))
                ));
        }

        function compileAttributes(uint256 tokenId) internal view returns (string memory) {

            return string(abi.encodePacked(
                    '[',
                    '{"trait_type":"color","value":"',
                    tokenTraits[tokenId].color,
                    '"},{"trait_type":"point","value":"',
                    tokenTraits[tokenId].point,
                    '"},{"trait_type":"amountOfFragents","value":"',
                    tokenTraits[tokenId].amountOfFragents.toString(),
                    '"}]'
                ));
        }
        string internal constant TABLE = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/';

        function base64(bytes memory data) internal pure returns (string memory) {
            if (data.length == 0) return '';

            // load the table into memory
            string memory table = TABLE;

            // multiply by 4/3 rounded up
            uint256 encodedLen = 4 * ((data.length + 2) / 3);

            // add some extra buffer at the end required for the writing
            string memory result = new string(encodedLen + 32);

            assembly {
            // set the actual output length
                mstore(result, encodedLen)

            // prepare the lookup table
                let tablePtr := add(table, 1)

            // input ptr
                let dataPtr := data
                let endPtr := add(dataPtr, mload(data))

            // result ptr, jump over length
                let resultPtr := add(result, 32)

            // run over the input, 3 bytes at a time
                for {} lt(dataPtr, endPtr) {}
                {
                    dataPtr := add(dataPtr, 3)

                // read 3 bytes
                    let input := mload(dataPtr)

                // write 4 characters
                    mstore(resultPtr, shl(248, mload(add(tablePtr, and(shr(18, input), 0x3F)))))
                    resultPtr := add(resultPtr, 1)
                    mstore(resultPtr, shl(248, mload(add(tablePtr, and(shr(12, input), 0x3F)))))
                    resultPtr := add(resultPtr, 1)
                    mstore(resultPtr, shl(248, mload(add(tablePtr, and(shr(6, input), 0x3F)))))
                    resultPtr := add(resultPtr, 1)
                    mstore(resultPtr, shl(248, mload(add(tablePtr, and(input, 0x3F)))))
                    resultPtr := add(resultPtr, 1)
                }

            // padding with '='
                switch mod(mload(data), 3)
                case 1 {mstore(sub(resultPtr, 2), shl(240, 0x3d3d))}
                case 2 {mstore(sub(resultPtr, 1), shl(248, 0x3d))}
            }

            return result;
        }

        function setExtension(string memory _extension) public onlyOwner {
                extension = _extension;
        }

 
        function setPaused(bool _paused) external onlyOwner {
            if (_paused) _pause();
            else _unpause();
        }

        function synthesis(address _to,uint256 amount)external whenNotPaused payable returns(uint){
            require(msg.sender == tx.origin,"Address: The address cannot be a contract");
            require(numberOfFragments[amount],"Illegal number of ragments");
            require(msg.value>=0.03 ether,"Insufficient gas balance");
            (bool os, ) = payable(synthesisFeeAddress).call{ value: msg.value }("");
            require(os);

            IERC1155(fragmentsAddress).safeTransferFrom(
                                msg.sender,
                                address(0xdead),
                                1,
                                amount,
                                new bytes(0)
            );

            uint256 _tokenId = currentMint;
            _mint(_to, _tokenId);
            uint256 seed=createRandom();
            tokenTraits[_tokenId].amountOfFragents=amount;
            generate(_tokenId,seed);
            currentMint++;
            emit Synthesis(_to, _tokenId, block.number, block.timestamp);
            return _tokenId;
        }
        function generate(uint256 tokenId, uint256 seed) internal returns (NFTTraits memory t){
            t = selectTraits(uint16(seed & 0xFFFF),tokenId);
            tokenTraits[tokenId] = t;
            return t;
        }

        function selectTraits(uint16 seed,uint tokenId) internal view returns (NFTTraits memory t) {
            t=tokenTraits[tokenId];
            t.color=probabilityOfpoint[0][uint8(seed)%4];
            if (t.amountOfFragents==10){
                t.point=probabilityOfpoint[1][uint8(seed)%3];
            }
            if (t.amountOfFragents==50){
                t.point=probabilityOfpoint[2][uint8(seed)%3];
            }
            if (t.amountOfFragents==100){
                t.point=probabilityOfpoint[3][uint8(seed)%3];
            }
            if (t.amountOfFragents==300){
                t.point=probabilityOfpoint[4][uint8(seed)%3];
            }
            if (t.amountOfFragents==500){
                t.point=probabilityOfpoint[5][uint8(seed)%3];
                if (uint8(seed)%3==1||uint8(seed)%3==2){
                    t.color=probabilityOfpoint[5][uint8(seed)%3];
                }
            }
        }

        function _beforeTokenTransfer(address from, address to, uint256 tokenId)
            internal
            override(ERC721, ERC721Enumerable)
        {
            super._beforeTokenTransfer(from, to, tokenId);
        }
        function _burn(uint256 tokenId) 
            internal 
            override(ERC721, ERC721URIStorage)
        {
            super._burn(tokenId);
        }
        function supportsInterface(bytes4 interfaceId)
            public
            view
            override(ERC721, ERC721Enumerable)
            returns (bool)
        {
            return super.supportsInterface(interfaceId);
        }
}