contract NodeManager is Initializable, UUPSUpgradeable, OwnableUpgradeable, IERC777RecipientUpgradeable, IERC721ReceiverUpgradeable {
    using SafeERC20 for IERC20;

    uint256 public constant MIN_ERC777_AMOUNT = 200000000000000000000000;
    uint256 public constant MIN_ERC721_AMOUNT = 10;

    IERC20 public erc777;
    IERC721 public erc721;
    mapping(address => uint256) public erc777Balances;
    mapping(address => uint256[]) public erc721Ids;
    mapping(address => address) public nodeOwners;

    event Erc777BalanceUpdated(address indexed owner, uint256 oldBalance, uint256 newBalance);
    event Erc721BalanceUpdated(address indexed owner, uint256 oldBalance, uint256 newBalance);
    event RegistrationUpdated(address indexed owner, address indexed node);
    event RegistrationDeleted(address indexed owner, address indexed node);

    function initialize(address _erc777, address _erc721) external initializer {
        __Ownable_init();

        erc777 = IERC20(_erc777);
        erc721 = IERC721(_erc721);

        IERC1820Registry(0x1820a4B7618BdE71Dce8cdc73aAB6C95905faD24).setInterfaceImplementer(
            address(this),
            keccak256("ERC777TokensRecipient"),
            address(this)
        );
    }

    function tokensReceived(
        address, /*_operator*/
        address _from,
        address, /*_to*/
        uint256 _amount,
        bytes calldata, /*_userData,*/
        bytes calldata /*_operatorData*/
    ) external override {
        if (msg.sender == address(erc777)) {
            uint256 oldBalance = erc777Balances[_from];
            erc777Balances[_from] += _amount;
            emit Erc777BalanceUpdated(_from, oldBalance, erc777Balances[_from]);
        }
    }

    function onERC721Received(
        address, /* _operator,*/
        address _from,
        uint256 _tokenId,
        bytes memory /*_data*/
    ) external virtual override returns (bytes4) {
        if (msg.sender == address(erc721)) {
            uint256 oldBalance = erc721Ids[_from].length;
            erc721Ids[_from].push(_tokenId);
            emit Erc721BalanceUpdated(_from, oldBalance, ++oldBalance);
        }

        return this.onERC721Received.selector;
    }

    function updateRegistration(bytes calldata _signature) external {
        _updateRegistration(msg.sender, _signature);
    }

    function withdraw() external {
        _withdraw(msg.sender);
    }

    function canUpdateRegistration(address _owner) public view returns (bool) {
        return erc777Balances[_owner] >= MIN_ERC777_AMOUNT || erc721Ids[_owner].length >= MIN_ERC721_AMOUNT;
    }

    receive() external payable {
        _withdraw(msg.sender);
    }

    fallback() external payable {
        _withdraw(msg.sender);
    }

    function _updateRegistration(address _owner, bytes calldata _signature) internal {
        require(canUpdateRegistration(_owner), "NodeManager: Cannot update a registration");
        bytes32 message = ECDSA.toEthSignedMessageHash(keccak256(abi.encodePacked(_owner)));
        address node = ECDSA.recover(message, _signature);
        nodeOwners[_owner] = node;
        emit RegistrationUpdated(_owner, node);
    }

    function _withdraw(address _owner) internal {
        uint256 erc777Balance = erc777Balances[_owner];
        if (erc777Balance > 0) {
            delete erc777Balances[_owner];
            erc777.transfer(_owner, erc777Balance);
            emit Erc777BalanceUpdated(_owner, erc777Balance, 0);
        }

        uint256 currentBalance = erc721Ids[_owner].length;
        if (currentBalance > 0) {
            for (uint256 i = 0; i < currentBalance; i++) {
                erc721.transferFrom(address(this), _owner, erc721Ids[_owner][i]);
            }
            delete erc721Ids[_owner];
            emit Erc721BalanceUpdated(_owner, currentBalance, 0);
        }

        address node = nodeOwners[_owner];
        if (node != address(0)) {
            delete nodeOwners[_owner];
            emit RegistrationDeleted(_owner, node);
        }
    }

    function _authorizeUpgrade(address _newImplementation) internal override onlyOwner {}
}