// File: contracts/Vrakka/Vrk.sol


pragma solidity ^0.8.0;








contract Vrk is ERC20,Ownable{

    mapping(address => mapping(uint256=>uint256)) public checkpoints;
    mapping(uint256 => bool) public has_deposited;
    mapping(uint256 => address) public staking_accounts;
 
    mapping(address=>uint256) private tolerance;
    //check the string use by the user is not repeat
    mapping(string=>bool)public _names;

    VrakkaNFT private BASE_CONTRACT;

    //Smart Contract for the Initial Coin Offering
    Ico private ICO_CONTRACT;

    uint256 public HARD_CAP=500*1e14;

    constructor(
      VrakkaNFT vrakka_nft_contract,
      Ico ico_contract) ERC20("Vrakka", "VRK") {

      _mint(msg.sender, (HARD_CAP/100)*5);
      BASE_CONTRACT=vrakka_nft_contract;
      ICO_CONTRACT=ico_contract;
    }

    function decimals() public view virtual override returns (uint8) {
        return 8;
    }

    //////////////---Modifiers---------------------------------------------------------------------
    modifier onlyTokenOwner(uint256 _tokenId){
        address tokenOwner = IERC721(ICO_CONTRACT).ownerOf(_tokenId);
        require(tokenOwner == msg.sender,'only the owner of the ICO NFT');
        _;
    }
    ////////////////////////////////////////////////////////////////////////////////////////
    
    function deposit(uint256 tokenId) external{
        require (msg.sender == BASE_CONTRACT.ownerOf(tokenId), 'Sender must be owner');
        require (!has_deposited[tokenId], 'Sender already deposited');

        //Height of the staking block
        checkpoints[msg.sender][tokenId] = block.number;
        staking_accounts[tokenId]=msg.sender;
        
        BASE_CONTRACT.transferFrom(msg.sender, address(this), tokenId);
      
        has_deposited[tokenId]=true;
   }


   function claimSupply(uint256 tokenId)public onlyTokenOwner(tokenId){
      uint256 _supply=ICO_CONTRACT.checkSupply(tokenId);
      _mint(msg.sender,_supply);
      ICO_CONTRACT.burn(tokenId);
   }

    function withdraw(uint256 tokenId) external{
        require(has_deposited[tokenId], 'No tokens to withdraw');
        require(staking_accounts[tokenId]==msg.sender,'Only the Staker');
        collect(msg.sender,tokenId);
        BASE_CONTRACT.transferFrom(address(this), msg.sender, tokenId);
        
        has_deposited[tokenId]=false;
    }

    function privateMint(uint8 percent) external onlyOwner{
        require(totalSupply()<HARD_CAP);
        _mint(msg.sender, (HARD_CAP/100)*percent);
    }

    function collect(address beneficiary,uint256 tokenId) public{
        uint256 reward = calculateReward(beneficiary,tokenId);
        checkpoints[beneficiary][tokenId] = block.number;      
        _mint(msg.sender, reward);
    }

    function onStaking(uint256 tokenId)public view returns(bool){
        return has_deposited[tokenId];
    }

    function calculateReward(address beneficiary,uint256 tokenId) public view returns(uint256){
        if(!has_deposited[tokenId])
        {
            return 0;
        }
        uint classType=BASE_CONTRACT.getClass(tokenId);
        uint256 checkpoint = checkpoints[beneficiary][tokenId];
        uint256 weight;

        if(classType==1){
            weight=1;
        }else if(classType==2){
            weight=2;
        }else if(classType==3){
            weight=3;
        }else if(classType==4){
            weight=4;
        }

        return weight*(block.number-checkpoint)*1000;
    }   
}