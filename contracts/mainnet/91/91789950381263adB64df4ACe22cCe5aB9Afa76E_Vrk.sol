contract Ico is ERC721Burnable,Ownable{

    using Counters for Counters.Counter;

    ///////////////-----Variable-------------///////////
    Counters.Counter private _tokenIds;

    uint256 internal HARD_CAP=20000;

    /**@dev How much native token for 1 $VRK */
    uint256 internal RATE=10**14 wei;

    constructor() ERC721("ICO Token", "ICO") {

    }

    struct Entry {
        uint256 tokenId;
        uint256 supply;
    }
    
    mapping(uint256=>Entry) LISTA;

    modifier checkCap(){
        require(getTotal()<HARD_CAP,'Hard Cap achieved');
        _;
    }

    //Only the owner can call this function to mint the NFT.
    function ManualMinting(
        uint256 supply,
        address receiver
        ) external onlyOwner checkCap returns(uint256){

        //Increment the counter
        _tokenIds.increment();

        uint256 newItemId = _tokenIds.current();
        _mint(receiver, newItemId);


        Entry memory newCerti= Entry(
        _tokenIds.current(),//token id of initial coin offering NFT
        supply
        );

        LISTA[newItemId] = newCerti;

        return newItemId;

    }

    function checkPrice(uint256 quantity)external view returns(uint256){
        return RATE*quantity;
    }

    /**
    @dev Any user can call this function and mint their own supply tickets 
     */
    function publicMinting(uint256 quantity)
    external payable checkCap returns (uint256){
        
        uint256 finalPrice=quantity*RATE;
        require(msg.value>=finalPrice,'Need to reach the price per token');
        
        _tokenIds.increment();

        uint256 newItemId = _tokenIds.current();
        _mint(msg.sender, newItemId);

        Entry memory newCerti= Entry(
        newItemId,//token id of the ico exchange
        quantity
        );

        LISTA[newItemId] = newCerti;

        return newItemId;

    }

    // --------- Rate related functions------------------------------------
    function setRate(uint256 _howMuch)public onlyOwner{
        RATE=_howMuch;
    }

    function getRate()external view returns(uint256){
        return RATE;
    }

    function get(uint256 _index)public view returns(Entry memory){
          return LISTA[_index];
    }
  
    /**
    @dev
    This function is called by the Vrakka Smart contract to ask how many tokens
    will mint.
     */
    function checkSupply(uint256 tokenId)external view returns(uint256){
        uint256 ticketSupply=LISTA[tokenId].supply;
        return ticketSupply;
    }

    function getTotal()public view returns(uint256){
        return(_tokenIds.current());
    }

    function burn(uint256 tokenId)public override{
      address _owner=ownerOf(tokenId);
      require(_owner==tx.origin,'only owner can burn share');
      _burn(tokenId);
    }    
  
}
