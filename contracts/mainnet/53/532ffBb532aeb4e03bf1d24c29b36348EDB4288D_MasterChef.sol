    bool isExist;
    address creator;
    uint256 players;
    uint256 fundPerPlayer;
    uint256 maxPlayers;
    uint256 totalFunded;
  }
  
  // The Squid TOKEN!
  IBEP20 public squidToken;
  // Info of each room.
  mapping(string=>RoomInfo) public roomInfo;
  string[] public roomNames;

  mapping(string=>address[]) public user_funded_balance;
  address public treasury;

  modifier onlyTreasury() {
    require(_msgSender()==treasury, "Error: Caller is not treasury");
    _;
  }

