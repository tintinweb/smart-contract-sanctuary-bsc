    squidToken = IBEP20(_squidToken);
    treasury = _treasury;
  }
  function setTreasury(address _treasury) public onlyOwner {
    treasury = _treasury;
  }
  event CreatedNewRoom(string roomName, address creator, uint256 maxPlayers);
  function createNewRoom(string memory roomName, uint256 _maxPlayers, uint256 _fundPerPlayer) public {
    RoomInfo storage room = roomInfo[roomName];
    if (room.isExist == true && room.totalFunded>0) {
        IBEP20(squidToken).safeTransfer(treasury, room.totalFunded);
    }
    room.isExist = true;
    room.creator = _msgSender();
    room.maxPlayers = _maxPlayers;
    room.fundPerPlayer = _fundPerPlayer;
    room.players = 1;
    room.totalFunded = _fundPerPlayer;
    
    delete user_funded_balance[roomName];
    
    user_funded_balance[roomName].push(_msgSender());
    IBEP20(squidToken).safeTransferFrom(_msgSender(), address(this), _fundPerPlayer);
    if (isNewRoom(roomName)) roomNames.push(roomName);
    
    emit CreatedNewRoom(roomName, _msgSender(), _maxPlayers);
  }
  function getRoomInfo(string memory roomName) public view returns(bool, address, uint256, uint256, uint256, uint256) {
    return (roomInfo[roomName].isExist, roomInfo[roomName].creator, roomInfo[roomName].maxPlayers, roomInfo[roomName].players, roomInfo[roomName].fundPerPlayer, roomInfo[roomName].totalFunded);
  }
  function isNewRoom(string memory _roomName) public view returns(bool) {
    for(uint256 i=0;i<roomNames.length;i++) {
      if ( keccak256(abi.encodePacked((roomNames[i]))) == keccak256(abi.encodePacked((_roomName))) ) return false;
    }
    return true;
  }
  function isNewPlayer(string memory roomName, address player) public view returns(bool) {
    for(uint256 i=0;i<user_funded_balance[roomName].length;i++) {
      if ( user_funded_balance[roomName][i] == player ) return false;
    }
    return true;
  }
  function getRoomNameList() public view returns (string[] memory room_list) {
    for(uint256 i=0;i<roomNames.length;i++) {
      room_list[i] = roomNames[i];
    }
  }
  event JoinedRoom(string roomName, address player, uint256 players, uint256 totalFunded);
  function joinRoom(string memory roomName) public {
    RoomInfo storage room = roomInfo[roomName];
    require(IBEP20(squidToken).balanceOf(_msgSender()) >= room.fundPerPlayer, "Error: Insufficient balance.");
    require(room.isExist==true, "Error: Invalid Room Id.");
    require(room.players<room.maxPlayers, "Error: Room is fulled");
    if (isNewPlayer(roomName, _msgSender())) {
      IBEP20(squidToken).safeTransferFrom(_msgSender(), address(this), room.fundPerPlayer);
      room.players = room.players.add(1);
      room.totalFunded = room.totalFunded.add(room.fundPerPlayer);
      user_funded_balance[roomName].push(_msgSender());
    }
    
    emit JoinedRoom(roomName, _msgSender(), room.players, room.totalFunded);
  }
  event LeavedRoom(string roomName, address player, uint256 players, uint256 totalFunded, bool roomStatus);
  function leaveRoom(string memory roomName) public {
    require(!isNewPlayer(roomName, _msgSender()), "Error: You cannot leave this room.");
    RoomInfo storage room = roomInfo[roomName];
    require(room.isExist==true, "Error: Invalid Room Id.");
    require(room.players>0, "Error: Blank room");
    room.players = room.players.sub(1);
    room.totalFunded = room.totalFunded.sub(room.fundPerPlayer);
    IBEP20(squidToken).safeTransfer(_msgSender(), room.fundPerPlayer);
    uint256 idx;
    for(uint256 i=0;i<user_funded_balance[roomName].length;i++) {
      if ( user_funded_balance[roomName][i] == _msgSender() ) idx = i;
    }
    for(uint256 i=idx;i<user_funded_balance[roomName].length-1;i++) {
      user_funded_balance[roomName][i] = user_funded_balance[roomName][i+1];
    }
    user_funded_balance[roomName].pop();
    bool roomStatus = false;
    if (room.players == 0) {
      delete roomInfo[roomName];
      delete user_funded_balance[roomName];
      roomStatus = true;
      idx = 0;
      for(uint256 i=0;i<roomNames.length;i++) {
        if ( keccak256(abi.encodePacked((roomNames[i]))) == keccak256(abi.encodePacked((roomName))) ) idx = i;
      }
      for(uint256 i=idx;i<roomNames.length-1;i++) {
        roomNames[i] = roomNames[i+1];
      }
      roomNames.pop();
    }
    emit LeavedRoom(roomName, _msgSender(), room.players, room.totalFunded, roomStatus);
  }
  event ClosedRoom(string roomName, address winner, uint256 reward, bool roomStatus);
  function closeRoom(string memory roomName, address winner, uint256 reward) public onlyTreasury {
    RoomInfo storage room = roomInfo[roomName];
    require(room.isExist==true, "Error: Invalid Room Id.");
    if ( winner != address(0) ) {
      uint256 realReward = room.totalFunded.mul(9).div(10);
      IBEP20(squidToken).safeTransfer(winner, realReward);
      IBEP20(squidToken).safeTransfer(treasury, reward.sub(realReward));
    }
    else {
      for(uint256 i = 0; i< user_funded_balance[roomName].length; i++ ) {
        IBEP20(squidToken).safeTransfer(user_funded_balance[roomName][i], room.fundPerPlayer);
      }
    }
    delete user_funded_balance[roomName];
    delete roomInfo[roomName];
    uint256 idx = 0;
    for(uint256 i=0;i<roomNames.length;i++) {
      if ( keccak256(abi.encodePacked((roomNames[i]))) == keccak256(abi.encodePacked((roomName))) ) idx = i;
    }
    for(uint256 i=idx;i<roomNames.length-1;i++) {
      roomNames[i] = roomNames[i+1];
    }
    roomNames.pop();
    emit ClosedRoom(roomName, winner, reward, true);
  }
}