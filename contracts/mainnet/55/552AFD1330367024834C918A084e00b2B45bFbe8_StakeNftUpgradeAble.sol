    /// @notice Info of each user that stakes nft tokens.
    mapping (uint256 => mapping (address => UserInfo)) public userInfo;
    mapping (uint256 => mapping (address => uint256[])) public nftUsers;
    mapping (uint256 => mapping (uint256 => uint256)) public nftMapIndex;
    mapping (address => bool) public setter;
    /// @dev Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint;

    uint256 public kalmPerSecond;
    uint256 private constant ACC_KALM_PRECISION = 1e12;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount, address indexed to);
    event Harvest(address indexed user, uint256 indexed pid, uint256 amount);
    event LogPoolAddition(uint256 indexed pid, uint256 allocPoint, IERC721 indexed collection);
    event LogSetPool(uint256 indexed pid, uint256 allocPoint);
    event LogUpdatePool(uint256 indexed pid, uint256 lastRewardTime, uint256 lpSupply, uint256 accKalmPerShare);
    event LogKalmPerSecond(uint256 KalmPerSecond);
    event NFTReceived(address operator, address from, uint256 tokenId, bytes data);


     function initialize(
        IERC20 kalm,
        uint256 _startTime
     ) public initializer {
        OwnableUpgradeable.__Ownable_init();
        ReentrancyGuardUpgradeSafe.__ReentrancyGuardUpgradeSafe__init();
        PausableUpgradeable.__Pausable_init();
        KALM = kalm;
        startTime = _startTime;
    }

    /// @notice Returns the number of MCV2 pools.
    function poolLength() public view returns (uint256 pools) {
        pools = poolInfo.length;
    }
    
     /// @notice Add a new LP to the pool. Can only be called by the owner.
    /// DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    /// @param _allocPoint AP of the new pool.
    /// @param _collection Address of the LP ERC-721 token.
    function add(uint256 _allocPoint, IERC721 _collection) internal {
        uint256 lastRewardTime = block.timestamp > startTime ? block.timestamp : startTime;
        _collection.balanceOf(address(this));
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        collections.push(_collection);
        poolInfo.push(
            PoolInfo({
                allocPoint: _allocPoint,
                lastRewardTime: lastRewardTime,
                accKalmPerShare: 0
            })
        );
        emit LogPoolAddition(poolLength(), _allocPoint, _collection);
    }

    function massAddPool(uint256[] calldata _allocPoints,IERC721[] calldata _collections) external {
        require(setter[msg.sender], "!setter");
        uint256 len = _collections.length;
        for (uint256 i = 0; i < len; ++i) {
            add(_allocPoints[i],_collections[i]);
        }
    }

    function updateAndSetPersec(uint256[] calldata pids,uint256 _kalmPerSecond) external {
        require(setter[msg.sender], "!setter");
        uint256 len = pids.length;
        for (uint256 i = 0; i < len; ++i) {
            updatePool(pids[i]);
        }
        kalmPerSecond = _kalmPerSecond;
        emit LogKalmPerSecond(_kalmPerSecond);
    } 

    function setAndUpdate(uint256[] calldata pids,uint256[] calldata allocPoints) external {
        require(setter[msg.sender], "!setter");
        uint256 len = pids.length;
        for (uint256 i = 0; i < len; ++i) {
            updatePool(pids[i]);
        }
        for (uint256 j=0; j < len; ++j) {
            totalAllocPoint = totalAllocPoint.sub(poolInfo[pids[j]].allocPoint).add(allocPoints[j]);
            poolInfo[pids[j]].allocPoint = allocPoints[j];
            emit LogSetPool(pids[j], allocPoints[j]);
        }
    }

    /// @notice Update the given pool's KALM allocation point Can only be called by the owner.
    /// @param _pid The index of the pool. See `poolInfo`.
    /// @param _allocPoint New AP of the pool.
    function set(uint256 _pid, uint256 _allocPoint) public onlyOwner {
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
        emit LogSetPool(_pid, _allocPoint);
    }

    /// @notice Sets the kalm per second to be distributed. Can only be called by the owner.
    /// @param _kalmPerSecond The amount of Kalm to be distributed per second.
    function setKalmPerSecond(uint256 _kalmPerSecond) public onlyOwner {
        kalmPerSecond = _kalmPerSecond;
        emit LogKalmPerSecond(_kalmPerSecond);
    }

     /// @notice View function to see pending KALM on frontend.
    /// @param _pid The index of the pool. See `poolInfo`.
    /// @param _user Address of user.
    /// @return pending KALM reward for a given user.
    function pendingReward(uint256 _pid, address _user) external view returns (uint256 pending) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accKalmPerShare = pool.accKalmPerShare;
        uint256 nftSupply = collections[_pid].balanceOf(address(this));
        if (block.timestamp > pool.lastRewardTime && nftSupply != 0) {
            uint256 time = block.timestamp.sub(pool.lastRewardTime);
            uint256 kalmReward = time.mul(kalmPerSecond).mul(pool.allocPoint) / totalAllocPoint;
            accKalmPerShare = accKalmPerShare.add(kalmReward.mul(ACC_KALM_PRECISION).div(nftSupply));
        }
        pending = user.amount.mul(accKalmPerShare).div(ACC_KALM_PRECISION).sub(user.rewardDebt);
    }

    /// @notice Update reward variables for all pools. Be careful of gas spending!
    /// @param pids Pool IDs of all to be updated. Make sure to update all active pools.
    function massUpdatePools(uint256[] calldata pids) external {
        uint256 len = pids.length;
        for (uint256 i = 0; i < len; ++i) {
            updatePool(pids[i]);
        }
    }

    /// @notice Update reward variables of the given pool.
    /// @param pid The index of the pool. See `poolInfo`.
    function updatePool(uint256 pid) public {
        PoolInfo storage pool = poolInfo[pid];
        if (block.timestamp > pool.lastRewardTime) {
            uint256 nftSupply = collections[pid].balanceOf(address(this));
            if (nftSupply > 0) {
                uint256 time = block.timestamp.sub(pool.lastRewardTime);
                uint256 kalmReward = time.mul(kalmPerSecond).mul(pool.allocPoint) / totalAllocPoint;
                pool.accKalmPerShare = pool.accKalmPerShare.add((kalmReward.mul(ACC_KALM_PRECISION).div(nftSupply)));
                kalmNotClaimed = kalmNotClaimed.add(kalmReward);
            }
            pool.lastRewardTime = block.timestamp;
            emit LogUpdatePool(pid, pool.lastRewardTime, nftSupply, pool.accKalmPerShare);
        }
    }

    /// @param pid The index of the pool. See `poolInfo`.
    function deposit(uint256 pid,uint256[] calldata tokenIds) public whenNotPaused nonReentrant{
        updatePool(pid);
        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][msg.sender];
        
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accKalmPerShare).div(ACC_KALM_PRECISION).sub(user.rewardDebt);
            if (pending > 0) {
                safeKalmTransfer(msg.sender, pending);
            }
        }

        if(tokenIds.length > 0) {
            uint256[] storage nftIds = nftUsers[pid][msg.sender];
            for (uint256 i = 0; i < tokenIds.length; i++) {
                collections[pid].safeTransferFrom(msg.sender,address(this),tokenIds[i]);
                nftIds.push(tokenIds[i]);
                nftMapIndex[pid][tokenIds[i]] = nftIds.length - 1;
            }
            user.amount = user.amount.add(tokenIds.length);
        }
        user.rewardDebt = user.amount.mul(pool.accKalmPerShare).div(ACC_KALM_PRECISION);

        emit Deposit(msg.sender, pid, tokenIds.length);
    }

    /// @param pid The index of the pool. See `poolInfo`.
    function withdraw(uint256 pid, uint256[] calldata tokenIds) public nonReentrant{
        updatePool(pid);
        PoolInfo storage pool = poolInfo[pid];
        UserInfo storage user = userInfo[pid][msg.sender];
        
        require(user.amount >= tokenIds.length, "withdraw: not good");

        uint256 pending = user.amount.mul(pool.accKalmPerShare).div(ACC_KALM_PRECISION).sub(user.rewardDebt);
        if (pending > 0) {
            safeKalmTransfer(msg.sender, pending);
        }

        if(tokenIds.length > 0) {
            for (uint256 i = 0; i < tokenIds.length; i++) {
                uint256[] storage nftIds = nftUsers[pid][msg.sender];
                uint256 nftIndex =  nftMapIndex[pid][tokenIds[i]];
                require(nftIds[nftIndex] == tokenIds[i], "not tokenId owner");
                uint256 nftLength = nftIds.length;
                uint256 tailId = nftIds[nftLength-1];

                nftUsers[pid][msg.sender][nftIndex] = tailId;
                nftUsers[pid][msg.sender][nftLength-1] = 0;
                nftUsers[pid][msg.sender].pop();
                nftMapIndex[pid][tailId] = nftIndex;
                nftMapIndex[pid][tokenIds[i]] = 0;

                collections[pid].safeTransferFrom(address(this),msg.sender,tokenIds[i]);
            }
            user.amount = user.amount.sub(tokenIds.length);
        }

        user.rewardDebt = user.amount.mul(pool.accKalmPerShare).div(ACC_KALM_PRECISION);

        emit Withdraw(msg.sender, pid, tokenIds.length);
    }

     // Safe Kalm transfer function, just in case if rounding error causes pool to not have enough
    function safeKalmTransfer(address _to, uint256 _KalmAmt) internal {
        uint256 KalmBal = IERC20(KALM).balanceOf(address(this));
        require(KalmBal >= _KalmAmt,"need more kalm");
        kalmClaimed = kalmClaimed.add(_KalmAmt);
        IERC20(KALM).transfer(_to, _KalmAmt);
    }

    function pause() external onlyOwner whenNotPaused {
        _pause();
    }

    function unpause() external onlyOwner whenPaused {
        _unpause();
    }

    function addSetter(address _setter) external onlyOwner {
        setter[_setter] = true;
    }

    function removeSetter(address _setter) external onlyOwner {
        setter[_setter] = false;
    }

    function emergencyWithdraw(address token,uint256 _amount) public onlyOwner {
        IERC20(token).transfer(msg.sender, _amount);
    }

    function onERC721Received(address operator, address from, uint256 tokenId, bytes memory data) public returns (bytes4) {
        emit NFTReceived(operator, from, tokenId, data);
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }

}