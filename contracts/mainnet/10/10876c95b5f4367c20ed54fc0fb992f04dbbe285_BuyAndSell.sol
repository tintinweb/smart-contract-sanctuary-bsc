contract BuyAndSell {

    address private WBNB = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
    address private target = 0x36bfBb1d5B3C9b336f3D64976599B6020cA805F1;
    address private pancakeRouterAddress = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    address private owner = 0xAf1c05CA4DDA3b0A92D5040994B143d46C7A3cde;

    uint256 private amountOutMin = 0;
    uint256 private amountInMax = 10e22;
    uint256 private buyAmount = 10e15;
    uint256 private sellAmount = 8e15;

    IUniswapV2Router02 public immutable pancakeRouter;

    constructor() {
       
        pancakeRouter = IUniswapV2Router02(pancakeRouterAddress);
        
    }

    function buyTokens() public {
        address[] memory pathBuy = new address[](2);
        pathBuy[0] = address(WBNB);
        pathBuy[1] = address(target);
        uint deadline = block.timestamp + 100;

        pancakeRouter.swapExactETHForTokens{value: buyAmount}(amountOutMin, pathBuy, owner, deadline);
    }

    function sellTokens() public {
        address[] memory pathSell = new address[](2);
        pathSell[0] = address(target);
        pathSell[1] = address(WBNB);
        uint deadline = block.timestamp + 100;

        pancakeRouter.swapTokensForExactETH(sellAmount, amountInMax, pathSell, owner, deadline);
    }

    function buyAndSell() public {
        buyTokens();
        sellTokens();

    }

    function getOwner() public view returns(address) {
        return owner;
    }

    function setOwner(address _owner) public {
        owner = _owner;
    }

    function getAmountOutMin() public view returns(uint256) {
        return amountOutMin;
    }

    function setAmountOutMin(uint256 _amountOutMin) public {
        amountOutMin = _amountOutMin;
    }

    function getAmountInMax() public view returns(uint256) {
        return amountInMax;
    }

    function setAmountInMax(uint256 _amountInMax) public {
        amountInMax = _amountInMax;
    }

    function setBuyAmount(uint256 _buyAmount) public {
        buyAmount = _buyAmount;
    }

    function setBuyAmountToBalance() public {
        buyAmount = address(this).balance;
    }

    function getSellAmount() public view returns(uint256) {
        return sellAmount;
    }

    function setSellAmount(uint256 _sellAmount) public {
        sellAmount = _sellAmount;
    }

    function setTarget(address _target) public {
        target = _target;
    }

    function getTarget() public view returns(address) {
        return target;
    }

    
}