import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract RefererRegistry {
  using Counters for Counters.Counter;

  Counters.Counter private _refererIds;

  uint16 constant denominator = 10000;

  struct Referer {
    address payable referer;
    uint16 numerator;
  }
  // id => Referer
  mapping(uint256 => Referer) referers;

  /**
   * Event for new referer added
   * @param id referer id
   * @param referer referer address
   * @param numerator to calc how much should be paid
   */
  event NewReferer(uint256 indexed id, address indexed referer, uint16 numerator);

  /**
   * Event for register transfers to referer
   * @param id referer id
   * @param amount being paid
   */
  event TransferRefererFee(uint256 indexed id, uint256 amount);

  constructor() {}

  // -----------------------------------------
  // Internal interface (extensible)
  // -----------------------------------------

  /**
   * @dev Adds a new Referer.
   */
  function _addReferer(address payable referer, uint16 numerator) internal virtual returns(uint256) {
    require(referer != address(0), 'Should be a valid address');
    require(numerator > 0 && numerator <= 1000, 'Numerator should be between 1 and 1000 (10%)');

    _refererIds.increment();

    uint256 id = _refererIds.current();

    referers[id] = Referer(referer, numerator);

    emit NewReferer(id, referer, numerator);

    return id;
  }

  /**
   * Function to transfer referer fee if exists
   * @param id referer id
   * @param totalAmount referer address
   * @param token in which referer is paid
