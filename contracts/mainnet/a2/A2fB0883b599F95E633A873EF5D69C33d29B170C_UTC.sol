/**
 * @dev Crowdsales with Time lock and post delivery
 */
contract ICO is Crowdsale, AccessControl {
  using SafeMath for uint256;

  bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");
  bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");


 /**
   * @dev set how the price will chage during the ICO
   * @param _usdt Currency to pay for the token
   * @param _rate is the amount of token by wei - is mutiplied by 10.000 to see more decimals
   * @param _wallet Address where collected funds will be forwarded to
   * @param _token Address of the token being sold
   * @param _initTime init time of ICO
   * @param _endTime end time of ICO
   * @param _timePeriods Number of periods in seconds to pay investors (vesting time)
   * @param _hardcap Maximum % of total supply
   * @param _tokenPeriod is quantity of token wait till increase the rate
   * @param _percentage is the % to increase the rate on every tokenperiod format 0.1  = 10%
   */
  constructor(
    IERC20 _usdt,
    uint256 _rate,
    address payable _wallet,
    IUTC _token,
    uint256 _initTime,
    uint256 _endTime,
    uint32[] memory _timePeriods,
    uint256 _hardcap,
    uint128 _tokenPeriod,
    uint128 _percentage
  ) Crowdsale(
    _usdt,
    _wallet,
    _token,
    _initTime,
    _endTime,
    _timePeriods,
    _hardcap
  )  {

    priceSet.rate = _rate;
    priceSet.lastTokenRaise = 0;
    priceSet.tokenPeriod = _tokenPeriod;
    priceSet.percentage = _percentage;

    // setup this contract roles
    _setupRole(ADMIN_ROLE, msg.sender);
    _setupRole(MINTER_ROLE, msg.sender);

    // set new 'admin' role in order to manage other roles
    _setRoleAdmin(MINTER_ROLE, ADMIN_ROLE);
  }

  /**
   * @dev Withdraw tokens only after crowdsale ends.
   * @param _period to know if the investor has token to withdraw
   */
  function withdrawTokens(uint128 _period) public {
    require(isPeriodReach(_period), "Too early to claim this period");
    require(tokensPerPeriod[msg.sender] > 0 && tokensDelivered[msg.sender][_period] == false, "Nothing to be claimed");
    // set it as already delivered
    tokensDelivered[msg.sender][_period] = true;
    // token to deliver
    uint256 tokens = tokensPerPeriod[msg.sender];

    _deliverTokens(msg.sender, tokens);

    emit PeriodDelivered(
      msg.sender,
      _period,
      tokens
    );
  }

  /** Set Vesting periods, in seconds, fron the ICO start
   *  [0,3600,9000,43200,70000,10000]
   */
  function setVestingPeriods(uint32[] memory timePeriods)
    public
    onlyRole(MINTER_ROLE)
  {
    _setTimePeriods(timePeriods);
  }

  /**
   * @dev Checks whether the period is reached to withdraw the quota.
   * @return Whether the period has elapsed
   */
  function isPeriodReach(uint period) public view returns (bool) {
    return timePeriods[period] + closingTime <= block.timestamp;
  }

  /**
   * @dev Get periods opening time
   * @return list of opening times for each periods
   */
  function getPeriodsOpeningTime() public view returns (uint256[] memory) {
    uint256[] memory periods = new uint256[](timePeriods.length);

    for (uint256 i = 0; i < periods.length; i++) {
      periods[i] = timePeriods[i] + closingTime;
    }

    return periods;
  }

  /**
   * @dev Adds a new Referer.
   * @return referer id
   */
  function addReferer(
    address payable referer,
    uint16 numerator
  )
    external
    onlyRole(ADMIN_ROLE)
    returns(uint256)
  {
    return _addReferer(referer, numerator);
  }

  /**
   * @dev Return the price rate
   */
  function getRate() public view returns(uint256) {
    return priceSet.rate;
  }

  /**
   * @dev get tokens delivered per period
   */
  function getTokensDelivered() public view returns(bool[] memory) {
    return tokensDelivered[msg.sender];
  }

  /**
   * @dev get tokens to withdraw per period
   */
  function getTokensPerPeriod() public view returns(uint256) {
    return tokensPerPeriod[msg.sender];
  }

  /**
   * @dev retrieve locked ether to recipient
   */
  function retrieveLockedEther(address payable recipient, uint256 amount) external onlyRole(ADMIN_ROLE) {
    recipient.transfer(amount);
  }

  /**
   * @dev retrieve locked USDT tokens to recipient
   */
  function retrieveLockedUSDT(address payable recipient, uint256 amount) external onlyRole(ADMIN_ROLE) {
    usdt.transfer(recipient, amount);
  }
}

