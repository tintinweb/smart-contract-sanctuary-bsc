pragma solidity ^0.8.11;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./TimedCrowdsale.sol";
import "./RefererRegistry.sol";
import "./IUTC.sol";

/**
 * @title Crowdsale
 * @dev Crowdsale is a base contract for managing a token crowdsale,
 * allowing investors to purchase tokens with ether. This contract implements
 * such functionality in its most fundamental form and can be extended to provide additional
 * functionality and/or custom behavior.
 * The external interface represents the basic interface for purchasing tokens, and conform
 * the base architecture for crowdsales. They are *not* intended to be modified / overriden.
 * The internal interface conforms the extensible and modifiable surface of crowdsales. Override
 * the methods to add functionality. Consider using 'super' where appropiate to concatenate
 * behavior.
 */
contract Crowdsale is TimedCrowdsale, RefererRegistry {
  using SafeMath for uint256;

  // The token being sold
  IUTC public token;
  // The token used to buy
  IERC20 public usdt;

  // payment and periods
  mapping(address => bool[]) internal tokensDelivered;
  mapping(address => uint256) internal tokensPerPeriod;

  // Address where funds are collected
  address payable public wallet;

  // Amount of wei raised
  uint256 public usdtRaised;
  // Sold limit
  uint256 public hardcap;
  // Amount of tokens bought by investors
  uint256 public soldTokens;

  // Price, how the price will chage during the ICO
  // rate is the amount of token by wei
  // lastTokenRaise is the amount of minted token since the last raise
  // tokenPeriod is quantity of token wait till increase the rate
  // percentage is the % to increase the rate on every tokenPeriod
  struct PriceSet {
    uint256 rate;
    uint256 lastTokenRaise;
    uint128 tokenPeriod;
    uint128 percentage;
  }

  PriceSet public priceSet;

   /**
   * Event for token raise logging
   * @param rate is the amount of token by wei
   * @param lastTokenRaise the amount of minted token since the last raise
   */

  event NewRate(
    uint256 rate,
    uint256 lastTokenRaise
  );

  /** value in seconds
   *  period          time in seconds
   *    0               0 (ready to wihtdraw)
   *    1               43200  (1 month)
   */
  uint32[] internal timePeriods;

  /**
   * Event for token purchase logging
   * @param purchaser who paid for the tokens
   * @param beneficiary who got the tokens
   * @param value weis paid for purchase
   * @param amount amount of tokens purchased
   */
  event TokenPurchase(
    address indexed purchaser,
    address indexed beneficiary,
    uint256 value,
    uint256 amount
  );

  /**
   * Event for token devivered
   * @param beneficiary who got the tokens
   * @param period number
   * @param amount amount of tokens delivered
   */
  event PeriodDelivered(
    address indexed beneficiary,
    uint256 period,
    uint256 amount
  );

  /**
   * Event for sold tokens
   * @param soldTokens amount of tokens bought by investors
   */
  event SoldTokens(
    uint256 soldTokens
  );

  /**
   * @param _usdt Currency to pay for the token
   * @param _wallet Address where collected funds will be forwarded to
   * @param _token Address of the token being sold
   * @param  initTime init time of ICO
   * @param  endTime end time of ICO
   * @param _timePeriods Number of periods in seconds to pay investors (vesting time)
   * @param _hardcap Maximum % of total supply
   */
  constructor(
    IERC20 _usdt,
    address payable _wallet,
    IUTC _token,
    uint256 initTime,
    uint256 endTime,
    uint32[] memory _timePeriods,
    uint256 _hardcap
  )
    TimedCrowdsale(initTime, endTime)
  {
    require(_wallet != address(0));
    require(address(_token) != address(0));

    usdt = _usdt;
    wallet = _wallet;
    token = _token;
    timePeriods = _timePeriods;
    hardcap = _hardcap;
  }

  // -----------------------------------------
  // Crowdsale external interface
  // -----------------------------------------

  function _setNewRate(uint256 _rate) internal {
    priceSet.rate = _rate;
    priceSet.lastTokenRaise = soldTokens;
    emit NewRate(_rate, soldTokens);
  }

  /**
   * @dev Check if the Token period is reached and change it if necessary
   */
  function checkTokenPeriod() internal {
    if (priceSet.lastTokenRaise + priceSet.tokenPeriod <= soldTokens) {
      _setNewRate(priceSet.rate - (priceSet.rate *priceSet.percentage / denominator));
    }
  }

  /**
   * @dev low level token purchase ***DO NOT OVERRIDE***
   * @param _beneficiary Address performing the token purchase
   */
  function buyTokens(address _beneficiary, uint256 refererId, uint256 usdtAmount, uint256 tokens, uint16 slipperyNumerator) public payable {
    require(isOpen(), "ICO is not open");
    require(_beneficiary != address(0));
    require(usdtAmount > 0);
    require(slipperyNumerator <= 10000, "Slippery should be less than 100%");

    // calculate token amount to be created
    uint256 _tokens = _getTokenAmount(usdtAmount);

    require(_tokens >= tokens.mul(denominator - slipperyNumerator).div(denominator), "Slippery % was exceeded");

    soldTokens += _tokens;

    // limit amount of tokens sold up to hard cap
    require(soldTokens <= hardcap, 'Should not exceed hard cap');

    // pay to refereer if exists
    usdtAmount = _transferRefererFee(refererId, usdtAmount, usdt);

    // update state
    usdtRaised = usdtRaised.add(usdtAmount);

    // check rate change
    checkTokenPeriod();

    emit TokenPurchase(
      msg.sender,
      _beneficiary,
      usdtAmount,
      _tokens
    );

    emit SoldTokens(soldTokens);

    // collect ether in wallet
    _forwardFunds(usdtAmount);
    // set amount of tokens to withdraw each period
    _installmentsPurchase(_beneficiary, _tokens);
  }

  // -----------------------------------------
  // Internal interface (extensible)
  // -----------------------------------------

  /**
   * @dev Tokens are divided by the number of periods to withdraw.
   * @param _beneficiary Address performing the token purchase
   * @param _tokens Value in wei involved in the purchase
   * divided in vesting periods
   */
  function _installmentsPurchase(
    address _beneficiary,
    uint256 _tokens
  )
    internal
  {
    uint256 periodTokens = _tokens.div(timePeriods.length);

    if (tokensDelivered[_beneficiary].length == 0) {
      tokensDelivered[_beneficiary] = new bool[](timePeriods.length);
    }

    tokensPerPeriod[_beneficiary] += periodTokens;
  }

  /**
   * @dev Source of tokens. Override this method to modify the way in which the crowdsale ultimately gets and sends its tokens.
   * @param _beneficiary Address performing the token purchase
   * @param _tokenAmount Number of tokens to be emitted
   */
  function _deliverTokens(address _beneficiary, uint256 _tokenAmount) internal {
    token.mint(_beneficiary, _tokenAmount);
  }

  /**
   * @dev Override to extend the way in which ether is converted to tokens.
   * @param _usdtAmount Value in wei to be converted into tokens
   * @return Number of tokens that can be purchased with the specified _usdtAmount
   */
  function _getTokenAmount(uint256 _usdtAmount)
    internal view returns (uint256)
  {
    return _usdtAmount.mul(priceSet.rate).div(denominator);
  }

  /**
   * @dev Determines how ether is stored/forwarded on purchases.
   */
  function _forwardFunds(uint256 amount) internal {
    usdt.transferFrom(msg.sender, wallet, amount);
  }

  /**
   * @dev Expose time periods internally.
   */
  function _setTimePeriods(uint32[] memory _timePeriods) internal {
    timePeriods = _timePeriods;
  }
}

