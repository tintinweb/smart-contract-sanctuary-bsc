contract NFT is ERC721Enumerable, Ownable {
    mapping(uint256 => string) public _uris;
    mapping(uint256 => uint256) public _properties;
    mapping(address => bool) public _minter;
    mapping(address => bool) public _warriorCard;
    address public _cash;

    address public IDO_ADDRESS;

    uint256 public _maxSupply = 57000;
    uint256 public _baseId = 0;

    function batchMint(address to, uint256 num, uint256 property) external onlyOwner {
      require(_minter[msg.sender], "not minter");
      uint256 tokenId = totalSupply() + 1 + _baseId;
      for (uint256 i; i < num;) {
        _mint(to, tokenId);
        _properties[tokenId] = property;
        unchecked{
          ++tokenId;
          ++i;
        }
      }
      require(_maxSupply >= totalSupply(), "notQty");
    }

    function oneMint() external {
      require(_warriorCard[msg.sender], "Have no legal power");
      uint256 tokenId = totalSupply() + 1 + _baseId;
      _mint(msg.sender, tokenId);
      _properties[tokenId] = 1;
      require(_maxSupply >= totalSupply(), "notQty");
    }

    function setCardStatus(address account) external onlyOwner {
      _warriorCard[account] = true;
    }

    function setIDO_ADDRESS(address account) external onlyOwner {
      IDO_ADDRESS = account;
    }

    function IDOSetCardStatus(address account) external {
      require(msg.sender == IDO_ADDRESS, 'no permissions');
      _warriorCard[account] = true;
    }

    function getWarriorCardStatus(address account) public view returns(bool) {
      return _warriorCard[account];
    }

    function tokenURI(uint256 id) external view virtual override returns (string memory) {
      return _uris[_properties[id]];
    }

    function setTokenUri(uint256 property, string memory uri) external onlyFunder {
      _uris[property] = uri;
    }

    function setMinter(address minter, bool enable) external onlyOwner {
      _minter[minter] = enable;
    }

    function setMaxSupply(uint256 max) external onlyOwner {
        _maxSupply = max;
    }

    function setCash(address cash) external onlyFunder {
      _cash = cash;
    }

    function claimBalance() external onlyFunder{
      payable(_cash).transfer(address(this).balance);
    }

    function claimToken(address token, uint256 amount, address to) external onlyFunder{
      IERC20(token).transfer(to, amount);
    }

    modifier onlyFunder() {
        require(_owner == msg.sender || _cash == msg.sender, "!Funder");
        _;
    }

    constructor() ERC721("Warrior NFT", "Warrior NFT"){
        _minter[msg.sender] = true;
        _uris[1] = "http://heroimg.tmsaml.top/qs.json";
        _cash = msg.sender;
    }
}