contract Token is Context, IERC20, Ownable {
    using SafeMath for uint256;
    mapping (address => mapping (address => uint256)) private _allowances;
    mapping (address => bool) private _isExcludedFromFee;
    mapping (address => bool) private _isAutoPair;
    mapping(address => uint256) private _balances;
    
    uint256 private _tTotal = 13888 * 10**18;

    string private _name = "BIB Token";
    string private _symbol = "BIB";
    uint8  public _decimals = 18;

    address public devaAddress = address(0x4f97bfF1c9501BeeF0AC52C99352A4C713b3cA25);
    address public burnAddress = address(0x000000000000000000000000000000000000dEaD);

    address public _fistAddress = address(0xC9882dEF23bc42D53895b8361D0b1EDC7570Bc6A);
    address public _usdtAddress = address(0x55d398326f99059fF775485246999027B3197955);
    constructor (address ownerAddres) public {
        IPancakeRouter02 _uniswapV2Router = IPancakeRouter02(0x1B6C9c20693afDE803B27F8782156c0f892ABC2d);
        IPancakeRouter02 _uniswapV2Router2 = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _fistAddress);
        address uniswapV2Pair2 = IUniswapV2Factory(_uniswapV2Router2.factory()).createPair(address(this), _usdtAddress);
        _isAutoPair[uniswapV2Pair] = true;
        _isAutoPair[uniswapV2Pair2] = true;

        _isExcludedFromFee[ownerAddres] = true;
        _isExcludedFromFee[address(this)] = true;

         _balances[ownerAddres] = _balances[ownerAddres].add(_tTotal);
        emit Transfer(address(0),ownerAddres, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "ERC20: decreased allowance below zero"));
        return true;
    }

    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function exAutoPair(address account) public onlyOwner {
        _isAutoPair[account] = true;
    }

    function inAutoPair(address account) public onlyOwner {
        _isAutoPair[account] = false;
    }
 
    receive() external payable {}

    function isExcludedFromFee(address account) public view returns(bool) {
        return _isExcludedFromFee[account];
    }
    function isAutoPair(address account) public view returns(bool) {
        return _isAutoPair[account];
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,address to,uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        bool takeFee = true;
        if(!_isAutoPair[from] && !_isAutoPair[to]){
            takeFee = false;
        }
        if(_isExcludedFromFee[from] || _isExcludedFromFee[to]){
            takeFee = false;
        }

        _balances[from] = _balances[from].sub(amount, "ERC20: transfer amount exceeds balance");

        if(takeFee){
            uint256 devasfee = amount.mul(1).div(100);
            uint256 burnsfee = amount.mul(1).div(100);

            uint256 fees = devasfee;
            fees = fees.add(burnsfee);
            amount = amount.sub(fees);

            _balances[devaAddress] = _balances[devaAddress].add(devasfee);
            emit Transfer(from, devaAddress, devasfee);

            _balances[burnAddress] = _balances[burnAddress].add(burnsfee);
            emit Transfer(from, burnAddress, burnsfee);
        }

        _balances[to] = _balances[to].add(amount);
        emit Transfer(from, to, amount);
    }
}