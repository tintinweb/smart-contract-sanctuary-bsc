pragma solidity ^0.8.0;

import "./DividendPayingToken.sol";
import "./NFT.sol";
import "./VolumeWars.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/Ownable.sol";


contract LegendaryDividend is DividendPayingToken, Ownable {
    using EnumerableSet for EnumerableSet.UintSet;

    uint256 private dummyValue;
    uint256 public tokensPerNFT = 1000;
    uint16 public maxStakingSeasons;

    piggyNFT public rewardNFT;
    VolumeWars public volumeWars;

    mapping(uint256 => address) stakedNFTOwners;
    mapping(address => EnumerableSet.UintSet) stakedNFTsByOwner;
    mapping(uint16 => EnumerableSet.UintSet) stakedNFTsBySeason;

    struct CollectionNFT {
            uint256 id;
            uint8 num;
            uint16 set;
            bool staked;
    }

    event StakedLegendary(
        address indexed owner,
        uint256 tokenId
    );

    event UnstakedLegendary(
        address indexed owner,
        uint256 tokenId
    );

    event MaxStakingSeasonsChanged(address indexed owner, uint16 indexed newValue);
    event VolumeWarsAddressChanged(address indexed owner, address indexed newValue);
    event ExpiredLegendaryUnstaked(address indexed owner, uint256 indexed tokenId, uint256 indexed season);
    event ExpiredSeasonUnstaked(address indexed caller, uint256 indexed season, uint256 remaining, uint256 numDone);

    constructor(address rewardNFTAddr, address payable volumeWarsAddr, uint16 _maxSeasons) ERC20("Volume Wars Legendary Dividend Token", "VWLD") {
        rewardNFT = piggyNFT(rewardNFTAddr);
        volumeWars = VolumeWars(volumeWarsAddr);
        maxStakingSeasons = _maxSeasons;
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        require(false, "Transfer not allowed");
        require(recipient != recipient, "a");
        require(amount > 0, "a");
        dummyValue = 0;
        return false;
    }
    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        require(false, "Transfer not allowed");
        require(recipient != sender, "a");
        require(amount > 0, "a");
        dummyValue = 0;
        return false;
    }
    function approve(address spender, uint256 amount) public override returns (bool) {
        require(false, "Approval not allowed");
        require(spender != spender, "a");
        require(amount > 0, "a");
        dummyValue = 0;
        return false;
    }

    function setMaxStakingSeasons(uint16 _newMax) external onlyOwner {
        maxStakingSeasons = _newMax;
        emit MaxStakingSeasonsChanged(_msgSender(), _newMax);
    }

    function setVolumeWarsAddress(address payable _volumeWarsAddr) external onlyOwner {
        volumeWars = VolumeWars(_volumeWarsAddr);
        emit VolumeWarsAddressChanged(_msgSender(), _volumeWarsAddr);
    }

    function getMaxStakingSeasons() public view returns (uint256) {
        return maxStakingSeasons;
    }

    function currentStakingCutoffSeason() public view returns (uint16) {
        uint16 currentSeason = volumeWars.currentSeason();
        if (currentSeason > maxStakingSeasons) {
            return currentSeason - maxStakingSeasons;
        } else {
            return 0;
        }
    }
    function isLegendaryExpired(uint256 tokenId) public view returns (bool) {
        (uint16 set, uint8 num ) = rewardNFT.metadataOf(tokenId);
        require(set > 0, "Invalid set");
        require(num == 0, "Not legendary");
        uint16 cutoffSeason = currentStakingCutoffSeason();
        return set <= cutoffSeason;
    }

    function stakedNFTOwnerOf(uint256 tokenId) public view returns (address) {
        return stakedNFTOwners[tokenId];
    }

    function stakedNFTOfOwnerByIndex(address owner, uint256 index) public view returns (uint256) {
        return stakedNFTsByOwner[owner].at(index);
    }

    function dataOfTokenOfOwnerByIndex(address owner, uint256 index) public view returns (uint256, uint16, uint8) {
        uint256 tokenId = stakedNFTOfOwnerByIndex(owner, index);
        (uint16 set, uint8 num) = rewardNFT.metadataOf(tokenId);
        return (tokenId, set, num);
    }

    function stakedNFTCollectionOf(address owner) external view returns (uint256[][] memory) {
        uint256 nftBalance = stakedNFTBalanceOf(owner);
        uint256[][] memory nftCollection = new uint256[][](nftBalance);
        for (uint256 i = 0 ; i < nftBalance; i++) {
            (uint256 tokenId, uint16 tokenSet, uint8 tokenNumber) = dataOfTokenOfOwnerByIndex(owner, i);
            uint256[] memory tokenData = new uint256[](3);
            tokenData[0] = tokenId;
            tokenData[1] = uint256(tokenSet);
            tokenData[2] = uint256(tokenNumber);
            nftCollection[i] = tokenData;
        }
        return nftCollection;
    }

    function stakedNFTBalanceOf(address owner) public view returns (uint256) {
        return balanceOf(owner)/tokensPerNFT;
    }

    function stakeLegendary(uint256 tokenId) external {
        require(rewardNFT.ownerOf(tokenId) == _msgSender(), "Not Owner");
        (uint16 set, uint8 num) = rewardNFT.metadataOf(tokenId);
        require(set > 0, "Invalid set");
        require(num == 0, "NFT not Legendary");
        require(isLegendaryExpired(tokenId) == false, "Legendary is expired");
        withdrawDividend();
        rewardNFT.transferFrom(_msgSender(), address(this), tokenId);
        stakedNFTOwners[tokenId] = _msgSender();
        stakedNFTsByOwner[_msgSender()].add(tokenId);
        stakedNFTsBySeason[set].add(tokenId);
        _mint(_msgSender(), tokensPerNFT);
        emit StakedLegendary(_msgSender(), tokenId);
    }

    function _unstakeLegendary(uint256 tokenId, address legendaryOwner) internal {
        require(stakedNFTOwnerOf(tokenId) == legendaryOwner, "Not Original Owner");
        require(balanceOf(legendaryOwner) >= tokensPerNFT, "Not enough tokens to unstake");
        (uint16 set, ) = rewardNFT.metadataOf(tokenId);
        require(set > 0, "Invalid set");
        _burn(legendaryOwner, tokensPerNFT);
        stakedNFTsBySeason[set].remove(tokenId);
        stakedNFTsByOwner[legendaryOwner].remove(tokenId);
        delete stakedNFTOwners[tokenId];
        rewardNFT.transferFrom(address(this), legendaryOwner, tokenId);
        emit UnstakedLegendary(legendaryOwner, tokenId);
    }

    function unstakeLegendary(uint256 tokenId) external {
        withdrawDividend();
        _unstakeLegendary(tokenId, _msgSender());
    }

    function unstakeExpiredLegendary(uint256 tokenId) external {
        require(rewardNFT.ownerOf(tokenId) == address(this), "NFT is not staked");
        (uint16 set, ) = rewardNFT.metadataOf(tokenId);
        require(set > 0, "Invalid set");
        uint16 cutoffSeason = currentStakingCutoffSeason();
        require(set <= cutoffSeason, "Set is not expired");
        address legendaryOwner = stakedNFTOwnerOf(tokenId);
        require(legendaryOwner != address(0) , "Invalid owner");
        withdrawDividendOfOwner(legendaryOwner);
        _unstakeLegendary(tokenId, legendaryOwner);
        emit ExpiredLegendaryUnstaked(legendaryOwner, tokenId, set);
    }

    function unstakeExpiredSeason(uint256 maxToUnstake) external returns (uint256) {
        uint16 cutoffSeason = currentStakingCutoffSeason();
        // No season expired yet
        if (cutoffSeason == 0) {
            return 0;
        }

        uint256 nftAmount = stakedNFTsBySeason[cutoffSeason].length();

        if (nftAmount == 0) {
            // Nothing to unstake
            return 0;
        }
        if (maxToUnstake > 0) {
            if (nftAmount > maxToUnstake) {
                // Limit number of NFTs to unstake at once.
                nftAmount = maxToUnstake;
            }
        }
        for (uint256 i = 0 ; i < nftAmount; i++) {
            uint256 tokenId = stakedNFTsBySeason[cutoffSeason].at(0);
            address legendaryOwner = stakedNFTOwners[tokenId];
            withdrawDividendOfOwner(legendaryOwner);
            _unstakeLegendary(tokenId, legendaryOwner);
        }
        uint256 remaining = stakedNFTsBySeason[cutoffSeason].length();
        emit ExpiredSeasonUnstaked(_msgSender(), cutoffSeason, remaining, nftAmount);
        return remaining;
    }
}

