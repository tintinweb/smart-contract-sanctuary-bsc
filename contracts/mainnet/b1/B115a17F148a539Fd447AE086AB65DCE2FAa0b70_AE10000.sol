contract AE10000 is ERC20, Ownable {
    using SafeMath for uint256;
    using SafeMath for uint112;

    IUniswapV2Router02 public uniswapV2Router;

    mapping (address => bool) public uniswapV2Pair;
    mapping (address => bool) public isWalletLimitExempt;

    TokenDividendTracker public dividendTracker;

    uint256 public swapTokensAtAmount;

    uint256 public lpFeeSell = 0;
    uint256 public lpFeeBuy = 1;

    uint256 public marketFeeSell = 8;
    uint256 public marketFeeBuy = 24;

    uint256 public totalSellFee = 8;
    uint256 public totalBuyFee = 25;

    //test 0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684
    //main 0x55d398326f99059fF775485246999027B3197955
    address public USDT = 0x55d398326f99059fF775485246999027B3197955;

    uint256 public AmountLiquidityFee;
    uint256 public AmountLpRewardFee;
    uint256 launchTime;
    uint256 public minPeriod = 600;
    uint256 distributorGas = 200000;
    uint256 public _walletMax =3 * 10**18;
    uint256 public tranferMax = 50 * 10 **18;
    bool public tranferMaxOpen = true;
    address public marketWalletAddress;
    address private lpRewardReceiveAddress;
    address public deadWallet = 0x000000000000000000000000000000000000dEaD;
    address public pair;

    mapping (address => bool) private _isExcludedFromFees;
    mapping (address => bool) public isBlackAddress;
    mapping (address => bool) isDividendExempt;
    bool public openTrade = false;

    event ExcludeFromFees(address indexed account, bool isExcluded);
    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    constructor(
        string memory name_,
        string memory symbol_,
        uint256 totalSupply_,
        address marketWalletAddr_,
        address lpRewardReceiveAddress_
    ) payable ERC20(name_, symbol_) {
        uint256 totalSupply = totalSupply_ * (10**18);
        swapTokensAtAmount = totalSupply.mul(3).div(10**6);
        //0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
        //0x10ED43C718714eb63d5aA57B78B54704E256024E
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), USDT);
        dividendTracker = new TokenDividendTracker(lpRewardReceiveAddress_,_uniswapV2Pair,USDT);
        pair = _uniswapV2Pair;
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair[_uniswapV2Pair] = true;

        marketWalletAddress = marketWalletAddr_;
        lpRewardReceiveAddress = lpRewardReceiveAddress_;

        excludeFromFees(owner(), true);
        excludeFromFees(marketWalletAddress, true);
        excludeFromFees(lpRewardReceiveAddress, true);
        excludeFromFees(address(this), true);
        excludeFromFees(address(dividendTracker), true);

        isDividendExempt[address(this)] = true;
        isDividendExempt[address(0)] = true;
        isDividendExempt[address(dividendTracker)] = true;
        isDividendExempt[deadWallet] = true;

        isWalletLimitExempt[owner()] = true;
        isWalletLimitExempt[address(_uniswapV2Pair)] = true;
        isWalletLimitExempt[address(this)] = true;

        _cast(owner(), totalSupply);
    }

    function setUniswapV2Pair(address pairs, bool val) public onlyOwner {
        if(uniswapV2Pair[pairs] != val) uniswapV2Pair[pair] = val;
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        if(_isExcludedFromFees[account] != excluded){
            _isExcludedFromFees[account] = excluded;
            emit ExcludeFromFees(account, excluded);
        }
    }

    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }
        emit ExcludeMultipleAccountsFromFees(accounts, excluded);
    }

    function setMarketWalletAddress(address payable wallet) external onlyOwner{
        marketWalletAddress = wallet;
    }

    function setOpenTrade(bool newOpenTrade) external onlyOwner {
        openTrade = newOpenTrade;
        launchTime = block.number;
    }

    function setIsWalletLimitExempt(address holder, bool exempt) external onlyOwner {
        isWalletLimitExempt[holder] = exempt;
    }

    function setMinPeriod(uint256 number) public onlyOwner {
        minPeriod = number;
    }

    function updateDistributorGas(uint256 newValue) public onlyOwner {
        require(newValue >= 100000 && newValue <= 500000, "distributorGas must be between 200,000 and 500,000");
        require(newValue != distributorGas, "Cannot update distributorGas to same value");
        distributorGas = newValue;
    }

    function setBlackAddress(address user,bool flag) external onlyOwner {
        isBlackAddress[user] = flag;
    }

    function setSwapTokensAtAmount(uint256 amount) public onlyOwner {
        swapTokensAtAmount = amount;
    }

    function setBuyFee(uint256 lpFee_,uint256 marketFee_) public onlyOwner {
        lpFeeBuy = lpFee_;
        marketFeeBuy = marketFee_;
        totalBuyFee = lpFeeBuy.add(marketFeeBuy);
    }

    function setSellFee(uint256 lpFee_,uint256 marketFee_) public onlyOwner {
        lpFeeSell = lpFee_;
        marketFeeSell = marketFee_;
        totalSellFee = lpFeeSell.add(marketFeeSell);
    }

    function setWalletLimit(uint256 newLimit) external onlyOwner {
        _walletMax  = newLimit;
    }

    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }


    function setLpRewardReceiveAddress(address newAddress) public onlyOwner {
        lpRewardReceiveAddress = newAddress;
    }

    function setTranferMax(uint256 tranfer) public onlyOwner {
        tranferMax = tranfer;
    }

    function setTranferMaxOpen(bool flag) public onlyOwner {
        if(!flag){
            setBuyFee(3,3);
            setSellFee(3,3);
        }else{
            setBuyFee(1,24);
            setSellFee(0,8);
        }
        tranferMaxOpen = flag;
    }



    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!isBlackAddress[from], "Bot killed");

        if(amount == 0 || _isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            super._transfer(from, to, amount);
            return;
        }

        //buy
        if(uniswapV2Pair[from]){
            require(openTrade || from == owner(),"Trading not yet enabled.");
            if(tranferMaxOpen){
                require(amount < tranferMax, "Maximum transfer exceeded");
            }
           
            if(block.number < launchTime + 4)
             isBlackAddress[from] =true;
        }

        bool canSwap = balanceOf(address(this)) >= swapTokensAtAmount;
        if( canSwap &&!uniswapV2Pair[from]) {
            swapAndLiquify(balanceOf(address(this)));
        }
        amount = takeAllFee(from, to,amount);
        
        super._transfer(from, to, amount);
        if(!isDividendExempt[from] && !uniswapV2Pair[from] )   try dividendTracker.setShare(from) {} catch {}
        if(!isDividendExempt[to] && !uniswapV2Pair[to] ) try dividendTracker.setShare(to) {} catch {}      
        if(  
            from != owner() &&
            to != owner() &&
            from !=address(this) &&
            dividendTracker.LPRewardLastSendTime().add(minPeriod) <= block.timestamp
        ) {
            try dividendTracker.process(distributorGas) {} catch {}    
        }
    }


    function takeAllFee(address from,address to,uint256 amount) private returns(uint256 amountAfter) {
        amountAfter = amount;
        //buy or sell
        if(uniswapV2Pair[from]){
            uint256 totalBuyFeeAmount = amount.mul(totalBuyFee).div(100);
            amountAfter = amountAfter.sub(totalBuyFeeAmount);
            if(totalBuyFeeAmount > 0) super._transfer(from, address(this), totalBuyFeeAmount);
        }else if(uniswapV2Pair[to]){
            uint256 totalSellFeeAmount = amount.mul(totalSellFee).div(100);
            amountAfter = amountAfter.sub(totalSellFeeAmount);
            if(totalSellFeeAmount > 0) super._transfer(from, address(this), totalSellFeeAmount);
        }
    }

    function swapAndLiquify(uint256 tokens) private {
        uint256 totalFee = totalSellFee.add(totalBuyFee);
        uint256 totalLpFee = lpFeeBuy.add(lpFeeSell);
        uint256 totalMarketFee = marketFeeBuy.add(marketFeeSell);
        uint256 lpAmount = tokens.mul(totalLpFee).div(totalFee);
        uint256 marketAmount = tokens.mul(totalMarketFee).div(totalFee);

        swapTokensForUSDT(lpAmount, address(dividendTracker));
        swapTokensForUSDT(marketAmount, marketWalletAddress);

    }

    function swapTokensForUSDT(uint256 tokenAmount, address to) private {
        _approve(address(this), address(uniswapV2Router), tokenAmount);
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = USDT;
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            to,
            block.timestamp
        );
    }

    receive () payable external{}  

}