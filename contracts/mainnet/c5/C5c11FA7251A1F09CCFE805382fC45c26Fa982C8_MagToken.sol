    IERC20 public usdtToken;
    IERC20 public dorToken;


    function initialize(IERC20 _usdtToken, IERC20 _dorToken) external {
        require(address(usdtToken) == address(0), "has been initialized");
        usdtToken = _usdtToken;
        dorToken = _dorToken;

