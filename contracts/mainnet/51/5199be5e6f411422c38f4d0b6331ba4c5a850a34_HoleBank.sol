contract HoleBank is Ownable {
    using SafeMath for uint256;

    uint256 public _bhjTotal = 0;
    uint256 public _bhjDestroyMinlimit = 0;
    uint256 public _bhjTotalDestroy = 0;
    uint256 public _bhjDestroyFee = 94;
    uint256 public _tmDestroyFee = 94;
    uint256 public _usdtSwapFee = 50;
   
    address public _bhjAddress = 0xdEc17b486e0C280460E6c4e229BB881f630eF314;
    address public _usdtAddress = 0x55d398326f99059fF775485246999027B3197955;
    address private _destroyAddress =address(0x000000000000000000000000000000000000dEaD);
    
    address public uniswapV2Pair;

    IUniswapV2Router02 private uniswapV2Router;
  
    constructor() {
        _bhjTotal = IERC20(_bhjAddress).totalSupply();
        _bhjDestroyMinlimit = 70000 * 10 **IERC20(_bhjAddress).decimals();
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).getPair(_bhjAddress, _usdtAddress);
        uniswapV2Router = _uniswapV2Router;

        _owner = msg.sender;
    }

    function depositUsdtBhj(address from,uint256 u_amount,uint256 amount) public returns (bool) {
        require(IERC20(_bhjAddress).transferFrom(from,address(this),amount), "transferFrom failed.");
        if(_bhjDestroyMinlimit >= 50000 * 10 **IERC20(_bhjAddress).decimals()){
            uint256 amounts = amount.div(100).mul(_bhjDestroyFee);
            IERC20(_bhjAddress).transfer(_destroyAddress,amounts);
            _bhjTotalDestroy+=amounts;
        }
        return true;
    }

    //transfer batch
    function transferBatch(address contractAddr,address[] memory recipient,uint256[] memory amount) public onlyOwner returns (bool) {
        require(contractAddr==_bhjAddress || contractAddr==_usdtAddress, "ERC20:token is error.");
        for(uint256 i=0;i<recipient.length && i<amount.length;i++ ){
            IERC20(contractAddr).transfer(recipient[i],amount[i]);
        }
        return true;
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function claimBnb(uint256 amount) public onlyOwner {
        payable(_owner).transfer(amount);
    }

    function claimUsdt(uint256 amount) public onlyOwner {
        IERC20(_usdtAddress).transfer(_owner,amount);
    }

    function claimBhj(uint256 amount) public onlyOwner {
        IERC20(_bhjAddress).transfer(_owner,amount);
    }

    function changeRouter(address router) public onlyOwner {
        uniswapV2Pair = router;
    }

    //change bhj address
    function changBhjAddress(address bhjAddress) public onlyOwner {
        _bhjAddress = bhjAddress;
        _bhjTotal = IERC20(bhjAddress).totalSupply();
        _bhjDestroyMinlimit = 50000 * 10 **IERC20(bhjAddress).decimals();
    }

    //change bhj destroy min limit
    function changeBhjDestroyMinlimit(uint256 amount) public onlyOwner {
        _bhjDestroyMinlimit = amount;
    } 

    //change bhj destroy fee percentage
    function changeBhjDestroyFee(uint256 amount) public onlyOwner {
        _bhjDestroyFee = amount;
    } 

    //change 3m destroy fee percentage 
    function changeTmDestroyFee(uint256 amount) public onlyOwner {
        _tmDestroyFee = amount;
    } 

    //change usdt swap percentage 
    function changeUsdtSwapFee(uint256 amount) public onlyOwner {
        _usdtSwapFee = amount;
    } 

}