contract RadicalChessCharacters is ERC721, ERC721Enumerable, Pausable, Ownable, ERC721Burnable {
    using Counters for Counters.Counter;
    using Strings for uint256;
    using SafeMath for uint256;

    struct Character {
        uint256 tokenID;
        uint256 setID;
        uint256 typeID;
        string name;
    }

    Character[] private _characters;
    Counters.Counter private _tokenIdCounter;

    string public uri = "";

    address public paymentReceiver;

    bool public useWhitelist = false;

    uint256 public fixedPrice = 0.1 ether;
    uint256 public setIndexLimit = 2;
    uint256 public typeIndexLimit = 6;
    uint256 public maxEachCharacterSupply = 500;
    uint256 public baseMultiplier = 27;
    uint256 public addSliceOffset = 9;
    uint256 public addTargetBlock = 3;

    uint256 internal constant maskLast8Bits = uint256(0xff);
    uint256 internal constant maskFirst248Bits = type(uint256).max;

    mapping(address => bool) public whitelisted;
    mapping(address => uint256) public lastSetMinted;
    mapping(address => uint256) public lastTypeMinted;
    mapping(uint256 => mapping(uint256 => string)) public nameForCharacter;

    constructor(
        address paymentReceiverAddress
    ) ERC721("Radical Chess Characters", "RCC") {
        require(paymentReceiverAddress != address(0xdead), "Radical Chess Character: Payment receiver cannot be dead address.");
        require(paymentReceiverAddress != address(0), "Radical Chess Character: Payment receiver cannot be null address.");
        paymentReceiver = paymentReceiverAddress;

        nameForCharacter[1][1] = "Pawn";
        nameForCharacter[1][2] = "Knight";
        nameForCharacter[1][3] = "Bishop";
        nameForCharacter[1][4] = "Rook";
        nameForCharacter[1][5] = "Queen";
        nameForCharacter[1][6] = "King";

        nameForCharacter[2][1] = "Pawn";
        nameForCharacter[2][2] = "Knight";
        nameForCharacter[2][3] = "Bishop";
        nameForCharacter[2][4] = "Rook";
        nameForCharacter[2][5] = "Queen";
        nameForCharacter[2][6] = "King";

    }

    event UpdateWhitelist(address user, bool status);
    event MintNewCharacter(address _tokenOwner, uint256 _tokenID);

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function updateURI(string memory uriNew) external onlyOwner {
        uri = uriNew;
    }

    function updateFixedPrice(uint256 newPrice) external onlyOwner {
        require(fixedPrice != newPrice, "Update Fixed Price: This is the current value for fixed price.");
        fixedPrice = newPrice;
    }

    function updateSetIndexLimit(uint256 newLimit) external onlyOwner {
        require(setIndexLimit != newLimit, "Update Set Index Limit: This is the current value for set index limit.");
        require(setIndexLimit > newLimit, "Update Set Index Limit: New value should be higher than the current value for set index limit.");
        setIndexLimit = newLimit;
    }

    function updateTypeIndexLimit(uint256 newLimit) external onlyOwner {
        require(typeIndexLimit != newLimit, "Update Type Index Limit: This is the current value for type index limit.");
        require(typeIndexLimit != newLimit, "Update Type Index Limit: New value should be higher than the current value for type index limit.");
        typeIndexLimit = newLimit;
    }

    function updateCharacterName(uint256 idSet, uint256 idType, string memory newString) external onlyOwner {
        nameForCharacter[idSet][idType] = newString;
    }

    function updateMaxEachCharacterSupply(uint256 newMaxSupply) external onlyOwner {
        require(maxEachCharacterSupply != newMaxSupply, "Update Max Each Character Supply: This is the current value for max each character supply.");
        maxEachCharacterSupply = newMaxSupply;
    }

    function updateWhitelistState(bool status) external onlyOwner {
        require(useWhitelist != status, "Update Whitelist State: This is the current state for whitelisting.");
        useWhitelist = status;
    }

    function updateWhitelist(address[] memory whitelisting, bool status) external onlyOwner {
        require(useWhitelist, "Update Whitelist: Whitelisting is not enabled for this presale.");
        for (uint256 i = 0; i < whitelisting.length; i++) {
            require(whitelisted[whitelisting[i]] != status, "Update Whitelist: This is the current whitelist status for this address.");
            whitelisted[whitelisting[i]] = status;
            emit UpdateWhitelist(whitelisting[i], status);
        }
    }
    
    function prngUpdateSettings(uint256 multiplier, uint256 sliceOffset, uint256 targetBlock) external onlyOwner {
        require(baseMultiplier != multiplier, "PRNG Update Settings: This is the current value for base multiplier.");
        require(addSliceOffset != sliceOffset, "PRNG Update Settings: This is the current value for additional slice offset.");
        require(addTargetBlock != targetBlock, "PRNG Update Settings: This is the current value for additional target block.");
        baseMultiplier = multiplier;
        addSliceOffset = sliceOffset;
        addTargetBlock = targetBlock;
    }

    function _sliceNumber(uint256 n, uint256 base, uint256 index, uint256 offset) internal view returns (uint256) {
        uint256 theBase = base.mul(baseMultiplier);
        uint256 theOffset = offset.mul(addSliceOffset);
        (, uint256 theMask) = theBase.tryPow(index);

        uint256 mask = uint256((theMask) - 1) << theOffset;
        return uint256((n & mask) >> theOffset) / baseMultiplier;
    }

    function _randomNGenerator(uint256 param1, uint256 param2, uint256 targetBlock) internal view returns (uint256) {
        uint256 theTargetBlock = targetBlock.add(addTargetBlock);
        uint256 randomN = uint256(blockhash(theTargetBlock));
        
        if (randomN == 0) {
            theTargetBlock = (block.number & maskFirst248Bits) + (theTargetBlock & maskLast8Bits);
        
            if (theTargetBlock >= block.number) {
                theTargetBlock -= 256;
            }
            
            randomN = uint256(blockhash(theTargetBlock));
        }

        randomN = uint256(keccak256(abi.encodePacked(randomN, param1, param2, theTargetBlock)));

        return randomN;
    }

    function getCharacter(uint256 idToken) external view returns (Character memory) {
        require(idToken > 0, "Get Character: Character ID cannot be 0.");
        _requireMinted(idToken);
        return _characters[idToken.sub(1)];
    }

    function _createCharacter(uint256 idToken, uint256 idSet, uint256 idType) internal {
        
        string memory name = nameForCharacter[idSet.add(1)][idType.add(1)];

        Character memory characters = Character({
            tokenID: idToken,
            setID: idSet.add(1),
            typeID: idType.add(1),
            name: name
        });
        
        _characters.push(characters);
    }

    function mixer(address user) internal view returns (uint256 idSet, uint256 idType, uint256 update) {
        uint256 userAddress = uint256(uint160(user));
        uint256 random = _randomNGenerator(userAddress, block.timestamp, block.number);

        idSet = _sliceNumber(random, setIndexLimit, 1, setIndexLimit);
        idType = _sliceNumber(random, typeIndexLimit, 1, typeIndexLimit);
        update = _sliceNumber(random, setIndexLimit, 1, typeIndexLimit);
    }

    function prevention(uint256 idSet, uint256 idType, uint256 update) internal view returns (uint256 idSetNew, uint256 idTypeNew) {
        if (update.mod(2) == 0) {
            idSetNew = idSet.add(1);
            if (idSet > setIndexLimit) {
                idSetNew = idSetNew.sub(setIndexLimit);
            }
            idTypeNew = idType;
            return (idSetNew, idTypeNew);
        }

        if (update.mod(2) == 1) {
            idTypeNew = idType.add(1);
            if (idType > typeIndexLimit) {
                idTypeNew = idTypeNew.sub(typeIndexLimit);
            }
            idSetNew = idSet;
            return (idSetNew, idTypeNew);
        }
    }

    function batchMintCharacter(uint256 round) external payable whenNotPaused {
        require(msg.value == round.mul(fixedPrice), "Batch Mint Character: Please transfer the exact amount.");
        for (uint256 i = 0; i < round; i++) {
            _mintCharacter();
        }
        payable(paymentReceiver).transfer(msg.value);
    }

    function mintCharacter() external payable whenNotPaused {
        require(msg.value == fixedPrice, "Mint Character: Please transfer the exact amount.");
        _mintCharacter();
        payable(paymentReceiver).transfer(msg.value);
    }


    function _mintCharacter() internal {
        if (useWhitelist) {
            require(whitelisted[_msgSender()], "Mint Character: Your address is not in the whitelist.");
        }

        (uint256 idSet, uint256 idType, uint256 update) = mixer(_msgSender());

        if (idSet == lastSetMinted[_msgSender()] && idType == lastTypeMinted[_msgSender()]) {
            (idSet, idType) = prevention(idSet, idType, update);
        }

        lastSetMinted[_msgSender()] = idSet;
        lastTypeMinted[_msgSender()] = idType;
        _safeMint(idSet, idType, _msgSender());
    }

    function safeMint(uint256 idSet, uint256 idType, address to) external onlyOwner {
        _safeMint(idSet, idType, to);
    }

    function _safeMint(uint256 idSet, uint256 idType, address to) internal {
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();
        _createCharacter(tokenId, idSet, idType) ;
        _safeMint(to, tokenId);
        emit MintNewCharacter(to, tokenId);
    }

    function _baseURI() internal view override returns (string memory) {
        return uri;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        _requireMinted(tokenId);

        string memory baseURI = _baseURI();
        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString())) : "";
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId, uint256 batchSize)
        internal
        whenNotPaused
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId, batchSize);
    }

    // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

contract RadicalChessArenas is ERC721, ERC721Enumerable, Pausable, Ownable, ERC721Burnable {
    using Counters for Counters.Counter;
    using Strings for uint256;
    using SafeMath for uint256;

    struct Arena {
        uint256 tokenID;
        uint256 typeID;
        string name;
    }

    Arena[] private _arenas;
    Counters.Counter private _tokenIdCounter;

    string public uri = "";

    address public paymentReceiver;

    bool public useWhitelist = false;

    uint256 public fixedPrice = 0.2 ether;
    uint256 public typeIndexLimit = 3;
    uint256 public maxEachArenaSupply = 500;
    uint256 public baseMultiplier = 27;
    uint256 public addSliceOffset = 9;
    uint256 public addTargetBlock = 3;

    uint256 internal constant maskLast8Bits = uint256(0xff);
    uint256 internal constant maskFirst248Bits = type(uint256).max;

    mapping(address => bool) public whitelisted;
    mapping(address => uint256) public lastTypeMinted;
    mapping(uint256 => string) public nameForArena;

    constructor(
        address paymentReceiverAddress
    ) ERC721("Radical Chess Arenas", "RCA") {
        require(paymentReceiverAddress != address(0xdead), "Radical Chess Arena: Payment receiver cannot be dead address.");
        require(paymentReceiverAddress != address(0), "Radical Chess Arena: Payment receiver cannot be null address.");
        paymentReceiver = paymentReceiverAddress;

        nameForArena[1] = "Colliseum";
        nameForArena[2] = "Asian Temple";
        nameForArena[3] = "Egypt";

    }

    event UpdateWhitelist(address user, bool status);
    event MintNewArena(address _tokenOwner, uint256 _tokenID);

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function updateURI(string memory uriNew) external onlyOwner {
        uri = uriNew;
    }

    function updateFixedPrice(uint256 newPrice) external onlyOwner {
        require(fixedPrice != newPrice, "Update Fixed Price: This is the current value for fixed price.");
        fixedPrice = newPrice;
    }

    function updateTypeIndexLimit(uint256 newLimit) external onlyOwner {
        require(typeIndexLimit != newLimit, "Update Type Index Limit: This is the current value for type index limit.");
        require(typeIndexLimit != newLimit, "Update Type Index Limit: New value should be higher than the current value for type index limit.");
        typeIndexLimit = newLimit;
    }

    function updateArenaName(uint256 idType, string memory newString) external onlyOwner {
        nameForArena[idType] = newString;
    }

    function updateMaxEachArenaSupply(uint256 newMaxSupply) external onlyOwner {
        require(maxEachArenaSupply != newMaxSupply, "Update Max Each Arena Supply: This is the current value for max each arena supply.");
        maxEachArenaSupply = newMaxSupply;
    }

    function updateWhitelistState(bool status) external onlyOwner {
        require(useWhitelist != status, "Update Whitelist State: This is the current state for whitelisting.");
        useWhitelist = status;
    }

    function updateWhitelist(address[] memory whitelisting, bool status) external onlyOwner {
        require(useWhitelist, "Update Whitelist: Whitelisting is not enabled for this presale.");
        for (uint256 i = 0; i < whitelisting.length; i++) {
            require(whitelisted[whitelisting[i]] != status, "Update Whitelist: This is the current whitelist status for this address.");
            whitelisted[whitelisting[i]] = status;
            emit UpdateWhitelist(whitelisting[i], status);
        }
    }
    
    function prngUpdateSettings(uint256 multiplier, uint256 sliceOffset, uint256 targetBlock) external onlyOwner {
        require(baseMultiplier != multiplier, "PRNG Update Settings: This is the current value for base multiplier.");
        require(addSliceOffset != sliceOffset, "PRNG Update Settings: This is the current value for additional slice offset.");
        require(addTargetBlock != targetBlock, "PRNG Update Settings: This is the current value for additional target block.");
        baseMultiplier = multiplier;
        addSliceOffset = sliceOffset;
        addTargetBlock = targetBlock;
    }

    function _sliceNumber(uint256 n, uint256 base, uint256 index, uint256 offset) internal view returns (uint256) {
        uint256 theBase = base.mul(baseMultiplier);
        uint256 theOffset = offset.mul(addSliceOffset);
        (, uint256 theMask) = theBase.tryPow(index);

        uint256 mask = uint256((theMask) - 1) << theOffset;
        return uint256((n & mask) >> theOffset) / baseMultiplier;
    }

    function _randomNGenerator(uint256 param1, uint256 param2, uint256 targetBlock) internal view returns (uint256) {
        uint256 theTargetBlock = targetBlock.add(addTargetBlock);
        uint256 randomN = uint256(blockhash(theTargetBlock));
        
        if (randomN == 0) {
            theTargetBlock = (block.number & maskFirst248Bits) + (theTargetBlock & maskLast8Bits);
        
            if (theTargetBlock >= block.number) {
                theTargetBlock -= 256;
            }
            
            randomN = uint256(blockhash(theTargetBlock));
        }

        randomN = uint256(keccak256(abi.encodePacked(randomN, param1, param2, theTargetBlock)));

        return randomN;
    }

    function getArena(uint256 idToken) external view returns (Arena memory) {
        require(idToken > 0, "Get Arena: Arena ID cannot be 0.");
        _requireMinted(idToken);
        return _arenas[idToken.sub(1)];
    }

    function _createArena(uint256 idToken, uint256 idType) internal {
        
        string memory name = nameForArena[idType.add(1)];

        Arena memory arena = Arena({
            tokenID: idToken,
            typeID: idType.add(1),
            name: name
        });
        
        _arenas.push(arena);
    }

    function mixer(address user) internal view returns (uint256) {
        uint256 userAddress = uint256(uint160(user));
        uint256 random = _randomNGenerator(userAddress, block.timestamp, block.number);

        uint256 idType = _sliceNumber(random, typeIndexLimit, 1, typeIndexLimit);
        
        return idType;
    }

    function prevention(uint256 idType) internal view returns (uint256 idTypeNew) {
        idTypeNew = idType.add(1);
        if (idType > typeIndexLimit) {
            idTypeNew = idTypeNew.sub(typeIndexLimit);
        }
        return idTypeNew;
    }

    function batchMintArena(uint256 round) external payable whenNotPaused {
        require(msg.value == round.mul(fixedPrice), "Batch Mint Arena: Please transfer the exact amount.");
        for (uint256 i = 0; i < round; i++) {
            _mintArena(_msgSender());
        }
        payable(paymentReceiver).transfer(msg.value);
    }
    
