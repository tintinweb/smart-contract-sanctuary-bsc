        _mintArena(_msgSender());
        payable(paymentReceiver).transfer(msg.value);
    }

    function _mintArena(address sender) internal {        
        if (useWhitelist) {
            require(whitelisted[sender], "Mint Character: Your address is not in the whitelist.");
        }
        
        (uint256 idType) = mixer(sender);

        if (idType == lastTypeMinted[sender]) {
            idType = prevention(idType);
        }

        lastTypeMinted[sender] = idType;
        _safeMint(idType, sender);
    }

    function safeMint(uint256 idType, address to) external onlyOwner {
        _safeMint(idType, to);
    }

    function _safeMint(uint256 idType, address to) internal {
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();
        _createArena(tokenId, idType);
        _safeMint(to, tokenId);
        emit MintNewArena(to, tokenId);
