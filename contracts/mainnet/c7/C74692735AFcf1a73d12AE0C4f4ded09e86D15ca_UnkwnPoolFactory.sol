import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Wrapper.sol";
import "./interfaces/IPool.sol";
import "./interfaces/IMultiRewards.sol";
import "./interfaces/IUnkwnPool.sol";
import "./interfaces/IUnkwnPoolProxy.sol";
import "./interfaces/IConeLens.sol";
import "./interfaces/IUserProxy.sol";
import "./interfaces/IVoter.sol";
import "./ProxyImplementation.sol";

/**
 * @title UnkwnPoolFactory
 * @author Unknown
 * @dev Responsible for creating new oxPools
 * @dev For every cone pool there will be a corresponding oxPool
 * @dev A sync mechanism is utilized to automatically generate new oxPools when new cone pools are detected
 * @dev The sync mechanism utilizes EIP-1167 for very inexpensive contract deployments
 */
contract UnkwnPoolFactory is ProxyImplementation {
    /*******************************************************
     *                     Configuration
     *******************************************************/

    // Important addresses
    address public multiRewardsTemplateAddress;
    address public unkwnAddress;
    address public unkwnPoolImplementationSourceAddress;
    address public unkwnPoolProxyTemplateAddress;
    address public unConeAddress;
    address internal deprecated0; //clean, deprecated storage slot, kept-in to preserve storage structure
    address public unConeRewardsPoolAddress;
    address public rewardsDistributorAddress;
    address public coneLensAddress;
    address public tokensAllowlist;
    address internal deprecated1; //clean, deprecated storage slot, kept-in to preserve storage structure
    address public voterProxyAddress;
    address public vlUnkwnAddress;
    address internal unkwnPoolFactoryAddress;

    // oxPool syncing variables
    uint256 internal deprecatedSyncedPoolsLength; //dirty slot used for deprecated pools, do not use
    mapping(address => address) internal deprecatedUnkwnPoolByConePool; //dirty slot used for deprecated pools, do not use
    mapping(address => address) public conePoolByUnkwnPool;
    address[] internal deprecatedUnkwnPools; //dirty slot used for deprecated pools, do not use

    // Interfaces
    IConeLens coneLens;
    IVoter voter;

    // oxPool syncing variables V2
    mapping(address => address) public unkwnPoolByConePool;
    address[] public unkwnPools;
    uint256 public syncedPoolsLength;

    /**
     * @notice Initialize proxy storage
     */
    function initializeProxyStorage(
        address _coneLensAddress,
        address _unkwnPoolImplementationSourceAddress,
        address _unkwnPoolProxyTemplateAddress,
        address _multiRewardsTemplateAddress,
        address _voterProxyAddress,
        address _rewardsDistributorAddress,
        address _unkwnAddress,
        address _vlUnkwnAddress,
        address _unConeAddress,
        address _unConeRewardsPoolAddress,
        address _tokensAllowlist
    ) public checkProxyInitialized {
        multiRewardsTemplateAddress = _multiRewardsTemplateAddress;
        rewardsDistributorAddress = _rewardsDistributorAddress;
        unkwnAddress = _unkwnAddress;
        unkwnPoolImplementationSourceAddress = _unkwnPoolImplementationSourceAddress;
        unkwnPoolProxyTemplateAddress = _unkwnPoolProxyTemplateAddress;
        unConeAddress = _unConeAddress;
        unConeRewardsPoolAddress = _unConeRewardsPoolAddress;
        unkwnPoolFactoryAddress = address(this);
        tokensAllowlist = _tokensAllowlist;
        coneLensAddress = _coneLensAddress;
        coneLens = IConeLens(coneLensAddress);
        voter = IVoter(coneLens.voterAddress());
        voterProxyAddress = _voterProxyAddress;
        vlUnkwnAddress = _vlUnkwnAddress;
    }

    /*******************************************************
     *                   oxPool sync mechanism
     *******************************************************/

    /**
     * @notice Fetch unsynced oxPools length
     */
    function unsyncedPoolsLength() public view returns (uint256) {
        uint256 _poolsLength = poolsLength();
        uint256 _unsyncedPoolsLength = _poolsLength - syncedPoolsLength;
        return _unsyncedPoolsLength;
    }

    /**
     * @notice Fetch the number of Cone pools
     */
    function poolsLength() public view returns (uint256) {
        return coneLens.poolsLength();
    }

    /**
     * @notice Fetch the number of synced oxPools
     */
    function unkwnPoolsLength() public view returns (uint256) {
        return unkwnPools.length;
    }

    /**
     * @notice Sync all oxPools
     */
    function syncPools() external {
        syncPools(unsyncedPoolsLength());
    }

    /**
     * @notice Sync n number of oxPools
     * @param numberOfPoolsToSync Number of pools to sync
     */
    function syncPools(uint256 numberOfPoolsToSync) public {
        // Don't try to sync more pools than exist
        uint256 unsyncedPoolsLength = unsyncedPoolsLength();
        if (numberOfPoolsToSync > unsyncedPoolsLength) {
            numberOfPoolsToSync = unsyncedPoolsLength;
        }
        uint256 syncTargetLength = syncedPoolsLength + numberOfPoolsToSync;

        // For every unsynced Cone pool
        for (
            uint256 unsyncedPoolIdx = syncedPoolsLength;
            unsyncedPoolIdx < syncTargetLength;
            unsyncedPoolIdx++
        ) {
            // Create a new oxPool
            address conePoolAddress = voter.pools(unsyncedPoolIdx);
            string memory conePoolSymbol = IPool(conePoolAddress).symbol();
            string memory conePoolName = IPool(conePoolAddress).name();
            string memory unkwnPoolSymbol = string(
                abi.encodePacked("unkwn-", conePoolSymbol)
            );
            string memory unkwnPoolName = string(
                abi.encodePacked("Unkwn - ", conePoolName)
            );
            (address unkwnPoolAddress, ) = createPoolAndMultiRewards(
                conePoolAddress,
                unkwnPoolName,
                unkwnPoolSymbol
            );

            // Add new oxPool to oxPools[] and increment synced length
            unkwnPools.push(unkwnPoolAddress);
            unkwnPoolByConePool[conePoolAddress] = unkwnPoolAddress;
            conePoolByUnkwnPool[unkwnPoolAddress] = conePoolAddress;
            syncedPoolsLength++;
        }
    }

    /**
     * @notice Create oxPool and multirewards contracts
     */
    function createPoolAndMultiRewards(
        address conePoolAddress,
        string memory unkwnPoolName,
        string memory unkwnPoolSymbol
    ) internal returns (address unkwnPoolAddress, address stakingAddress) {
        unkwnPoolAddress = _cloneWithTemplateAddress(
            unkwnPoolProxyTemplateAddress
        );
        IUnkwnPoolProxy(unkwnPoolAddress).initialize(
            unkwnPoolImplementationSourceAddress
        );

        stakingAddress = _cloneWithTemplateAddress(multiRewardsTemplateAddress);
        address bribeAddress = coneLens.bribeAddresByPoolAddress(
            conePoolAddress
        );

        // Initialize multirewards
        IMultiRewards(stakingAddress).initialize(
            rewardsDistributorAddress,
            unkwnPoolAddress
        );

        // Initialize oxPool
        IUnkwnPool(unkwnPoolAddress).initialize(
            unkwnPoolFactoryAddress,
            conePoolAddress,
            stakingAddress,
            unkwnPoolName,
            unkwnPoolSymbol,
            bribeAddress,
            tokensAllowlist
        );
    }

    /*******************************************************
     *                    Helper utilities
     *******************************************************/

    /**
     * @notice Clones using EIP-1167 template
     */
    function _cloneWithTemplateAddress(address templateAddress)
        internal
        returns (address poolAddress)
    {
        bytes20 _templateAddress = bytes20(templateAddress);
        assembly {
            let clone := mload(0x40)
            mstore(
                clone,
                0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000
            )
            mstore(add(clone, 0x14), _templateAddress)
            mstore(
                add(clone, 0x28),
                0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000
            )
            poolAddress := create(0, clone, 0x37)
        }
    }

    /**
     * @notice Determine whether or not a pool is an oxPool
     */
    function isUnkwnPool(address unkwnPoolAddress) public view returns (bool) {
        return
            unkwnPoolByConePool[conePoolByUnkwnPool[unkwnPoolAddress]] ==
            unkwnPoolAddress;
    }

    /**
     * @notice Determine whether or not a pool is or was an oxPool
     */

    function isUnkwnPoolOrLegacyUnkwnPool(address unkwnPoolAddress)
        external
        view
        returns (bool _isUnkwnPoolOrLegacyPool)
    {
        _isUnkwnPoolOrLegacyPool = isUnkwnPool(unkwnPoolAddress);
        if (!_isUnkwnPoolOrLegacyPool) {
            _isUnkwnPoolOrLegacyPool =
                conePoolByUnkwnPool[unkwnPoolAddress] != address(0);
        }
    }
}

