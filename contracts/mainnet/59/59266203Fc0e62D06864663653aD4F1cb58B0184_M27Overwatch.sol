    }

    modifier nonReentrant() {
        _nonReentrantBefore();
        _;
        _nonReentrantAfter();
    }

    function _nonReentrantBefore() private {
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");
        _status = _ENTERED;
    }

    function _nonReentrantAfter() private {
        _status = _NOT_ENTERED;
    }
}

interface IERC20 {
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
    function totalSupply() external view returns (uint256);
    function balanceOf(address account) external view returns (uint256);
    function transfer(address to, uint256 amount) external returns (bool);
    function allowance(address owner, address spender) external view returns (uint256);
    function approve(address spender, uint256 amount) external returns (bool);
    function transferFrom(
        address from,
        address to,
        uint256 amount
    ) external returns (bool);
}

contract M27Overwatch is ERC721Enumerable, Base, Ownable, ReentrancyGuard {
    using Strings for uint256;

    address private _collectionAddress;
    uint256 internal maxSupply = 7777;
    uint256 internal successSupply = 0;
    uint256 internal startTime;
    uint256 internal endTime;
    uint256 internal heroNums = 8;

    string internal baseURI = "ipfs://QmfSzVK8UVGTX5iWLzFR9tEFUF1UfjtiqjN7xigXSciuSZ/";
    string internal blindBoxURI = "ipfs://QmPvoz8bqbfuYa8Uvd8zXFWJTys7wiKi6S8yx8ivYSabuz/vip.json";
    string internal baseExtension = ".json";

    struct NftInfo {
        uint256 tokenId;
        uint256 level;
        uint256 id;
        uint256 status;
    }

    IERC20 payToken;
    uint256 payAmount;
    uint256[] internal probability;
    mapping(uint256 => NftInfo) internal _nftList;

    event OpenBlindBoxEvent(string[] uri);

    constructor() ERC721("M27 Overwatch", "M27") {
        _collectionAddress = address(0xC4357D73AD8d84D2BBA89aF701B9B3d134377d7C);
        payToken = IERC20(address(0x55d398326f99059fF775485246999027B3197955));
        payAmount = 77 * 1e18;
        _master = msg.sender;
        _thisAddress = address(this);
        probability = [38.01 * 1e18, 28.06 * 1e18, 18.72 * 1e18, 9.36 * 1e18, 4.68 * 1e18, 1.17 * 1e18];
        startTime = 1664467200;
        endTime   = 1759161600;
    }

    function setPayAmount(uint256 amount) public onlyOwner {
        payAmount = amount;
    }

    function setProbability(uint256[] memory _probability) public {
        probability = _probability;
    }

    function mintMeta(uint256 tokenQuantity) public nonReentrant payable {
        uint256 amount = tokenQuantity * payAmount;
        require(block.timestamp >= startTime && block.timestamp <= endTime, "Sales have not started");
        require(
            totalSupply() + tokenQuantity <= maxSupply,
            "Sale would exceed max supply"
        );

        require(
            tokenQuantity <= 10,
            "Buy up to 10 blind boxes at a time"
        );

        require(
            payToken.balanceOf(_msgSender()) >= amount,
            "Insufficient available balance"
        );

        _mintMeta(tokenQuantity);
        payToken.transferFrom(_msgSender(), _thisAddress, amount);
        payToken.approve(_collectionAddress, amount);
        payToken.transfer(_collectionAddress, amount);
        successSupply = successSupply + tokenQuantity;
    }

    function getMaxSupply() view public returns(uint256) {
        return maxSupply;
    }

    function getSuccessSupply() view public returns(uint256) {
        return successSupply;
    }

    function getMintMetaAmount(uint256 tokenQuantity) view public returns(uint256) {
        return tokenQuantity * payAmount;
    }

    function getStartTime() view public returns(uint256) {
        return startTime;
    }

    function getEndTime() view public returns(uint256) {
        return endTime;
    }

    function _mintMeta(uint256 tokenQuantity) internal {
        string[] memory uris = new string[](10);
        for (uint256 i = 0; i < tokenQuantity; i++) {
            uint256 mintIndex = totalSupply();
            if (totalSupply() < maxSupply) {
                _safeMint(_msgSender(), mintIndex);

                _nftList[mintIndex].tokenId = mintIndex;
                uris[i] = openBlindBox(mintIndex);
            }
        }
        emit OpenBlindBoxEvent(uris);
    }

    function openBlindBox(uint256 tokenId) private returns(string memory) {
        uint256 level = getRandLevel();
        uint256 id = rand(heroNums, totalSupply());
        if (id <= 0) {
            id = 1;
        }
        _nftList[tokenId].level = level;
        _nftList[tokenId].id = id;
        _nftList[tokenId].status = 1;
        return tokenURI(tokenId);
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");

        _transfer(from, to, tokenId);
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _safeTransfer(from, to, tokenId, _data);
    }
    

    function getMyNtfList() public view returns(NftInfo[] memory) {
        return _getNftList(_msgSender());
    }

    function getNftListFromUser(address users) public view returns(NftInfo[] memory) {
        return _getNftList(users);
    }

    function _getNftList(address users) internal view returns(NftInfo[] memory) {
        uint256 balance = balanceOf(users);
        NftInfo[] memory items = new NftInfo[](balance);
        uint256 itemCount = 0;
        for (uint256 i = balance; i > 0; i--) {
            uint256 tokenId = tokenOfOwnerByIndex(users, i - 1);
            items[itemCount] = _nftList[tokenId];
            itemCount++;
        }

        return items;
    }

    function getRandLevel() internal returns(uint256) {
        uint256 max = 100 * 1e18;
        uint256 rands;
        uint256 level;
        uint256 length = probability.length;
        uint256 start;
        uint256 end;
        rands = rand(max, totalSupply());
        for(uint256 i = 0; i < length; i++) {
            start = probability[i];
            if (i == 0) {
                end = length;
            }
            else {
                end = probability[i - 1];
            }

            if (i == length - 1) {
                start = 0;
            }

            if (rands >= start && rands <= end) {
                level = i + 1;
                break;
            }
        }
        if (level <= 0) {
            level = 1;
        }

        return level;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        if(_nftList[tokenId].status == 0) {
            return blindBoxURI;
        }

        return string(abi.encodePacked(_baseURI(), _nftList[tokenId].level.toString(), "-", _nftList[tokenId].id.toString(), baseExtension));
    }

    function getTokenLevel(uint256 tokenId) public view returns (uint256) {
        return _nftList[tokenId].level;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setHeroNums(uint256 _heroNums) public onlyOwner {
        heroNums = _heroNums;
    }

    function setMaxSupply(uint256 _newMaxSupply) public onlyOwner {
        maxSupply = _newMaxSupply;
    }

    function setStartTime(uint256 _startTime) public onlyOwner {
        startTime = _startTime;
    }

    function setEndTime(uint256 _endTime) public onlyOwner {
        endTime = _endTime;
    }

    function setBlindBoxURI(string memory _blindBoxURI) public onlyOwner {
        blindBoxURI = _blindBoxURI;
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }
}