import "./Interface/IUniswapV2Router02.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract MoonPadToken is Context, IERC20, IERC20Metadata, Ownable {
    using SafeMath for uint256;
    event SwapAndLiquify(
        uint256 tokenToETH,
        uint256 ethAmount,
        uint256 tokenToLiquidity
    );

    mapping(address => uint256) private _balances;

    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) public _isExcludedFromFee;

    uint256 private _totalSupply;
    uint256 private _minAmountToSwap;

    string private _name;
    string private _symbol;

    uint256 public buyLiquidity;
    uint256 public buyBurn;
    uint256 public buyVipDividend;
    uint256 public buyBuyBackWallet;

    uint256 public sellLiquidity;
    uint256 public sellBurn;
    uint256 public sellVipDividend;
    uint256 public sellBuyBackWallet; 
    uint256 public sellTeamWallet;

    uint256 public toLiquidityBalance;
    uint256 public toBuyBackBalance;
    uint256 public toTeamBalance;
    uint256 public toVipDividendBalance;

    bool private inSwapAndLiquify;

    address private _burnWalletAddress =
        0x000000000000000000000000000000000000dEaD;

    address payable private _buyBackWalletAddress;
    address payable private _dividendWalletAddress;
    address payable public teamWallet;

    address public uniswapV2Pair;
    IUniswapV2Router02 public uniswapV2Router;

    constructor(
        string memory name_,
        string memory symbol_,
        IUniswapV2Router02 uniswapAddress_,
        address payable buyBackWalletAddress_,
        address payable dividendWalletAddress_,
        address payable teamWalletAddress_,
        uint256 totalSupply_,
        uint256 minAmountToSwap
    ) {
        _name = name_;
        _symbol = symbol_;
        _buyBackWalletAddress = buyBackWalletAddress_;
        _dividendWalletAddress = dividendWalletAddress_;
        teamWallet = teamWalletAddress_;

        _minAmountToSwap = minAmountToSwap;

        uniswapV2Router = uniswapAddress_;
        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(
                address(this),
                uniswapV2Router.WETH()
            );

        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[_burnWalletAddress] = true;
        _isExcludedFromFee[buyBackWalletAddress_] = true;
        _isExcludedFromFee[dividendWalletAddress_] = true;
        _isExcludedFromFee[teamWalletAddress_] = true;

        _mint(msg.sender, totalSupply_);
    }

    receive() external payable {}

    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    /**
     * @dev Returns the name of the token.
     */
    function name() public view virtual override returns (string memory) {
        return _name;
    }

    /**
     * @dev Returns the symbol of the token, usually a shorter version of the
     * name.
     */
    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    /**
     * @dev Returns the number of decimals used to get its user representation.
     * For example, if `decimals` equals `2`, a balance of `505` tokens should
     * be displayed to a user as `5.05` (`505 / 10 ** 2`).
     *
     * Tokens usually opt for a value of 18, imitating the relationship between
     * Ether and Wei. This is the value {ERC20} uses, unless this function is
     * overridden;
     *
