    }

    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function _mint(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: mint to the zero address");

        _beforeTokenTransfer(address(0), account, amount);

        _totalSupply += amount;
        _balances[account] += amount;
        emit Transfer(address(0), account, amount);

        _afterTokenTransfer(address(0), account, amount);
    }

    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: burn from the zero address");

        _beforeTokenTransfer(account, address(0), amount);

        uint256 accountBalance = _balances[account];
        require(accountBalance >= amount, "ERC20: burn amount exceeds balance");
        unchecked {
            _balances[account] = accountBalance - amount;
        }
        _totalSupply -= amount;

        emit Transfer(account, address(0), amount);

        _afterTokenTransfer(account, address(0), amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual returns (uint256) {
        //indicates if fee should be deducted from transfer
        bool takeFee;
        bool buyFee;
        uint256 amountAfterTax = amount;

        if (to == address(uniswapV2Pair) || from == address(uniswapV2Pair)) {
            takeFee = true;

            if (to == address(uniswapV2Pair)) buyFee = false;
            if (from == address(uniswapV2Pair)) buyFee = true;
            if (_isExcludedFromFee[from] || _isExcludedFromFee[to]) {
                takeFee = false;
            }

            if (takeFee == true && buyFee == false) {
                toLiquidityBalance += amount.mul(sellLiquidity).div(10000);
                uint256 burnFee = takeTax(from, amount, sellBurn);
                toVipDividendBalance += amount.mul(sellVipDividend).div(10000);
                toBuyBackBalance += amount.mul(sellBuyBackWallet).div(10000);
                toTeamBalance += amount.mul(sellTeamWallet).div(10000);

                _transfer(
                    from,
                    address(this),
                    toVipDividendBalance + toBuyBackBalance + toTeamBalance
                );

                amountAfterTax =
                    amount -
                    toLiquidityBalance -
                    burnFee -
                    toVipDividendBalance -
                    toBuyBackBalance -
                    toTeamBalance;
            }
            if (takeFee == true && buyFee == true) {
                toLiquidityBalance += amount.mul(buyLiquidity).div(10000);
                uint256 burnFee = takeTax(from, amount, buyBurn);
                toVipDividendBalance += amount.mul(buyVipDividend).div(10000);
                toBuyBackBalance += amount.mul(buyBuyBackWallet).div(10000);

                _transfer(
                    from,
                    address(this),
                    toVipDividendBalance + toBuyBackBalance
                );

                amountAfterTax =
                    amount -
                    toLiquidityBalance -
                    burnFee -
                    toVipDividendBalance -
                    toBuyBackBalance;
            }
        }

        bool overMinTokenBalance = balanceOf(address(this)) >= _minAmountToSwap;
        if (
            overMinTokenBalance &&
            !inSwapAndLiquify &&
            from != uniswapV2Pair &&
            from != address(uniswapV2Router)
        ) {
            swapAndLiquify(balanceOf(address(this)));
        }

        return amountAfterTax;
    }

    function takeTax(
        address sender,
        uint256 _amount,
        uint256 _tax
    ) internal returns (uint256) {
        uint256 burnFee = _amount.mul(_tax).div(10000);
        _transfer(sender, _burnWalletAddress, burnFee);
        return burnFee;
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}

    function swapAndLiquify(uint256 contractTokenBalance) private lockTheSwap {
        uint256 sumOfOtherTokens = toBuyBackBalance +
            toTeamBalance +
            toVipDividendBalance;
        uint256 fromLiquidityFee = (contractTokenBalance * toLiquidityBalance) /
            (sumOfOtherTokens + toLiquidityBalance);

        uint256 OtherTokens = contractTokenBalance.sub(fromLiquidityFee);

        uint256 half = fromLiquidityFee.div(2);
        uint256 otherHalf = fromLiquidityFee.sub(half);

        uint256 initialBalance = address(this).balance;
        uint256 amountToSwap = half.add(OtherTokens);
        swapTokensForEth(amountToSwap);
        uint256 newBalance = address(this).balance.sub(initialBalance);

        uint256 liquidityPart = (newBalance * half) / amountToSwap;
        uint256 otherTokensPart = newBalance - liquidityPart;
        uint256 buyBackPart = (otherTokensPart * toBuyBackBalance) /
            sumOfOtherTokens;
        uint256 teamPart = (otherTokensPart * toTeamBalance) / sumOfOtherTokens;
        uint256 vipDividendPart = otherTokensPart - buyBackPart - teamPart;

        _buyBackWalletAddress.transfer(buyBackPart);
        teamWallet.transfer(teamPart);
        _dividendWalletAddress.transfer(vipDividendPart);

        addLiquidity(otherHalf, liquidityPart);

        toBuyBackBalance = 0;
        toTeamBalance = 0;
        toVipDividendBalance = 0;
        toLiquidityBalance = balanceOf(address(this));
        emit SwapAndLiquify(half.add(OtherTokens), liquidityPart, otherHalf);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);
        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    // buy fees setters
    function setBuyLiquidityFee(uint256 _liquidityFee) public onlyOwner {
        require(_liquidityFee <= 10000,"incorrect fee basis points");
        buyLiquidity = _liquidityFee;
    }

    function setBuyBurnFee(uint256 _burnFee) public onlyOwner {
        require(_burnFee <= 10000, "incorrect fee basis points");
        buyBurn = _burnFee;
    }

    function setBuyVipDividendFee(uint256 _vipDividendFee) public onlyOwner {
        require(_vipDividendFee <= 10000, "incorrect fee basis points");
        buyVipDividend = _vipDividendFee;
    }

    function setBuyBuyBackFee(uint256 _buyBackFee) public onlyOwner {
        require(_buyBackFee <= 10000, "incorrect fee basis points");
        buyBuyBackWallet = _buyBackFee;
    }

    // sell fees setters
    function setSellLiquidityFee(uint256 _liquidityFee) public onlyOwner {
        require(_liquidityFee <= 10000,"incorrect fee basis points");
        sellLiquidity = _liquidityFee;
    }

    function setSellBurnFee(uint256 _burnFee) public onlyOwner {
        require(_burnFee <= 10000, "incorrect fee basis points");
        sellBurn = _burnFee;
    }

    function setSellVipDividendFee(uint256 _vipDividendFee) public onlyOwner {
        require(_vipDividendFee <= 10000, "incorrect fee basis points");
        sellVipDividend = _vipDividendFee;
    }

    function setSellBuyBackFee(uint256 _buyBackFee) public onlyOwner {
        require(_buyBackFee <= 10000, "incorrect fee basis points");
        sellBuyBackWallet = _buyBackFee;
    }

    function setSellTeamFee(uint256 _teamFee) public onlyOwner {
        require(_teamFee <= 10000, "incorrect fee basis points");
        sellTeamWallet = _teamFee;
    }
}

