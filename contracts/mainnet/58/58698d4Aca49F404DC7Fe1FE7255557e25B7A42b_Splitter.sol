contract Splitter is Ownable {
    using SafeMath for uint256;

    receive() external payable {}

    address payable wallet1;
    address payable wallet2;
    address payable wallet3;

    uint256 w1Percentage = 30;
    uint256 w2Percentage = 20;
    uint256 w3Percentage = 10;

    constructor(
        address payable address1,
        address payable address2,
        address payable address3
    ) {
        wallet1 = address1;
        wallet2 = address2;
        wallet3 = address3;
    }

    function setupPercentage(
        uint256 w1,
        uint256 w2,
        uint256 w3
    ) public onlyOwner {
        require(w1.add(w2).add(w3) == 60, "Splitter: Must be 60 or 6% based on VCG Tax");
        w1Percentage = w1;
        w2Percentage = w2;
        w3Percentage = w3;
    }

    function setupWallet(
        address payable w1,
        address payable w2,
        address payable w3
    ) public onlyOwner {
        wallet1 = w1;
        wallet2 = w2;
        wallet3 = w3;
    }

    function withdraw() public onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "Splitter: Insufficent balance");
        _withdraw(wallet1, ((balance * w1Percentage) / 100));
        _withdraw(wallet2, ((balance * w2Percentage) / 100));
        _withdraw(wallet3, ((balance * w3Percentage) / 100));
    }

    function _withdraw(address _address, uint256 _amount) private {
        (bool success, ) = _address.call{value: _amount}("");
        require(success, "Splitter: Failed to widthdraw Ether");
    }
}