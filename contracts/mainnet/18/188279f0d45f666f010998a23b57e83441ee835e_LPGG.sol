        _isExcludedFromFee[address(this)] = true;
        
        _whitelist[owner()] = true;
        _whitelist[address(this)] = true;
       _whitelist[uniswapV2Pair] = true;
        _whitelist[address(uniswapV2Router)] = true;
 
        
