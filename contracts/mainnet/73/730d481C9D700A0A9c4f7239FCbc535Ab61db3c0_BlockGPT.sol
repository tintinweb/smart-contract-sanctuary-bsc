  mapping(address => TRADING) private trading;

  constructor(address _mint_to, address _win_contract) BEP20("BlockGPT", "GPT") {
      require(_win_contract > address(0), "Can't set Win Contract to zero address");

      max_supply = 20000000000 * 10 ** decimals();
      win_contract = _win_contract;

      // init whitelist
      whitelist[_msgSender()] = true;
      whitelist[_mint_to] = true;
      whitelist[address(buyback_manual_addr)] = true;
      whitelist[address(gpt_dao_addr)] = true;
      whitelist[dead_addr] = true;
      whitelist[address(0)] = true;
      whitelist[_win_contract] = true;

      is_me_less_than_USDT = address(this) < usdt_addr;

      super._mint(_mint_to, 300000 * 10 ** decimals());
      trading[_mint_to].total_income += 300000 * 10 ** decimals();
      
      pancake_router = IUniswapV2Router02(pancake_router_addr);
      pancake_pair = IUniswapV2Pair(IUniswapV2Factory(pancake_router.factory()).createPair(usdt_addr, address(this)));
  }

  fallback() external virtual payable {}

  receive() external virtual payable {
      payable(_msgSender()).transfer(msg.value);
  }

  modifier onlyWinContract()
  {
      require(_msgSender() == win_contract, "Only allow win contract to call");
      _;
  }

  function availiableSelling(address addr, uint want_to_sell) public view returns(uint allow_to_sell)
  {
      uint single_limited = 1000 * 10 ** decimals();     
      uint quarter = trading[addr].total_income / 4;
      uint max_quantity =  quarter > trading[addr].sold ? quarter - trading[addr].sold : 0;
      max_quantity = max_quantity > single_limited ? single_limited : max_quantity;
      if (max_quantity > 0) {
          uint level = getSellingLevel(want_to_sell);
          if (level == 1)
          { 
              allow_to_sell = max_quantity;
          }
          else if (level == 2){
              allow_to_sell = trading[addr].total_income / 100;
          }
          else {
              allow_to_sell = trading[addr].total_income / 10000;
          }

          if (allow_to_sell > max_quantity)
          {
              allow_to_sell = max_quantity;
          }
      }
  }

  function checkSelling(address from, uint amount) public view returns(bool allow)
  {
      require(block.timestamp - trading[from].last_sold_timestamp >=  90000, "Only one sale is allowed within 25 hours");      
      uint allow_quantity = availiableSelling(from, amount);
      require(allow_quantity > 0 && amount <= allow_quantity, "Exceed the max selling amount limited");
      allow = true;
  }

  function appendToWhitelist(address addr) external onlyOwner
  {
      whitelist[addr] = true;
  }

  function removeFromWhitelist(address addr) external onlyOwner
  {
      whitelist[addr] = false;
  }

  function mint(address account, uint256 amount) external onlyWinContract
  {
      require(amount + totalSupply() <= max_supply, "Exceed the max supply");
      super._mint(account, amount);
      trading[account].total_income += amount;
  }

  function liquidityReserves() public view returns (uint reserve_USDT, uint reserve_me)
  {
      (reserve_USDT, reserve_me, ) = pancake_pair.getReserves();
      if (is_me_less_than_USDT)
      {
          (reserve_me, reserve_USDT) = (reserve_USDT, reserve_me);
      }
  }

  function getSellingLevel(uint amountIn) public view returns (uint level)
  {
      (uint reserve_USDT, uint reserve_me) = liquidityReserves();

      uint amount_USDT = pancake_router.getAmountOut(amountIn, reserve_me, reserve_USDT);
      if (amountIn <= amount_USDT * 2)
      {
          level = 1;
      }
      else if (amountIn < amount_USDT * 100 / 15)
      {
          level = 2;
      }
      else {
          level = 3;
      }
  }

  function _transfer(
         address from,
         address to,
         uint256 amount
      ) internal override nonReentrant
   {
      require(from != address(0), "ERC20: transfer from the zero address");
      require(to != address(0), "ERC20: transfer to the zero address");

      if (whitelist[from] || whitelist[to] || from == address(pancake_pair))
      {
          super._transfer(from, to, amount);
          if (from == address(pancake_pair))
          {
              trading[to].total_income += amount;
          }
      }
      else if (to == address(pancake_pair))
      {
          require(checkSelling(from, amount), "Not eligible to sell");
              
          trading[from].last_sold_timestamp = block.timestamp;
          trading[from].sold += amount;
          uint burning_amount = amount * 3 / 100;
          super._burn(from, burning_amount);
          super._transfer(from, to, amount - burning_amount);
      }
      else
      {
          uint burning_amount = amount * 30 / 100;
          super._burn(from, burning_amount);
          super._transfer(from, to, amount - burning_amount);
      }
   }
}