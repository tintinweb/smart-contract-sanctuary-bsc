        usdt.transferFrom(owner, collectScienceAddress, _sciencePool);
    }

    function burnToken(uint256 usdtAmount, IERC20 token) internal {
        address[] memory path = new address[](2);
        path[0] = collectUsdt;
        path[1] = address(token);
        uint256 _initTokenBalance = token.balanceOf(address(this));
        IPancakeRouter(pancakeRouter)
            .swapExactTokensForTokensSupportingFeeOnTransferTokens(
                usdtAmount,
                0,
                path,
                address(this),
                block.timestamp
            );
        uint256 _afterTokenBalance = token.balanceOf(address(this));
        uint256 _burnAmount = _afterTokenBalance - _initTokenBalance;
        if (_burnAmount > 0) token.burn(_burnAmount);
    }
}