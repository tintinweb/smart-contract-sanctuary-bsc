pragma solidity ^0.6.12;
pragma experimental ABIEncoderV2;









// import "@openzeppelin/contracts/utils/structs/EnumerableMap.sol";


contract VipPandaCommunityLottery is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    IPancakeRouter01 public immutable uniswapV2Router;


    // PoolInfo
    struct PoolInfo {
        IBEP20 token1;
        IBEP20 token2;
        IBEP20 awardToken;
        uint8[] bingoNumber; // 3 digit
        uint256 luckyPrize;
        uint256 surprisePrize;
        uint256 happyPrize;
        uint256 fee;
        uint poolType;
        uint256 minTradeAmount;
        // uint tradeType;
    }

    struct Transaction {
        uint poolId;
        uint blockNumber;
        uint timeStamp;
        string hash;
        address from;
        address to;
        string methodId;
        string lotteryResult;
    }

    // trade pool
    PoolInfo[] public poolInfo;
    // trasaction orders
    Transaction[] public transactions;
    mapping(string => bool) public transactionExistence;
    modifier nonDuplicated(string memory hash) {
        require(transactionExistence[hash] == false, "nonDuplicated: duplicated");
        _;
    }


    // using Address for address;
    mapping (address => mapping (address => uint256)) private _allowances;

    // pancakeRoulette 0x10ED43C718714eb63d5aA57B78B54704E256024E
    constructor (address pancakeRouterAddress) public {
        IPancakeRouter01 _uniswapV2Router = IPancakeRouter01(pancakeRouterAddress);
        uniswapV2Router = _uniswapV2Router;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    function pools() external view returns (PoolInfo[] memory) {
        return poolInfo;
    }

    modifier PoolValidation(uint256 _pid) {
     require (_pid < poolInfo.length , "Pool does not exist.") ;
     _;
    }

    function transactionsLength() external view returns (uint256) {
        return transactions.length;
    }

    function withdrawFund(IBEP20 token, uint256 tokenAmount) public onlyOwner{
        token.approve(address(this), tokenAmount);
        token.safeTransferFrom(address(this), address(msg.sender), tokenAmount);
    }

    function lotterySwap(uint _pid, uint256 tokenAmount) public {
        PoolInfo storage pool = poolInfo[_pid];
        
        // pool info
        // Token1
        // Token2
        // Name
        // AwardToken
        // BingoNumber
        // LuckyPrize
        // SurprisePrize
        // HappyPrize
        // Fee

        // generate the uniswap pair path of token -> weth

        // buy
        require(tokenAmount >= pool.minTradeAmount, "trade amount too less");

        IBEP20 token1 = pool.token1;
        token1.safeTransferFrom(address(msg.sender), address(this), tokenAmount);
        // token1.approve(address(msg.sender),address(uniswapV2Router),tokenAmount);
        token1.approve(address(uniswapV2Router), tokenAmount);
        uint256 tFee = tokenAmount.mul(pool.fee).div(100);
        uint256 tTransferAmount = tokenAmount.sub(tFee);

        address[] memory path = new address[](2);
        path[0] = address(pool.token1);
        path[1] = address(pool.token2);
        uniswapV2Router.swapExactTokensForTokens(
            tTransferAmount,
            0, // accept any amount of ETH
            path,
            address(msg.sender),
            block.timestamp
        );
    }

    function add(IBEP20 _token1, IBEP20 _token2, uint _poolType, IBEP20 _awardToken, uint8[] memory _bingoNumber, uint256 _luckyPrize, uint256 _surprisePrize, uint256 _happyPrize,uint256 _fee, uint256 _minTradeAmount) public onlyOwner {
        require(_bingoNumber.length == 3, "bingo number error");

        poolInfo.push(PoolInfo({
            token1 : _token1,
            token2 : _token2,
            awardToken : _awardToken,
            bingoNumber : _bingoNumber,
            luckyPrize : _luckyPrize,
            surprisePrize : _surprisePrize,
            happyPrize : _happyPrize,
            fee : _fee,
            poolType: _poolType,
            minTradeAmount: _minTradeAmount
        }));

        _awardToken.approve(address(this), ~uint256(0));
    }


    function set(uint256 _pid, IBEP20 _token1, IBEP20 _token2, uint _poolType, IBEP20 _awardToken, uint8[] memory _bingoNumber, uint256 _luckyPrize, uint256 _surprisePrize, uint256 _happyPrize,uint256 _fee, uint256 _minTradeAmount) public onlyOwner PoolValidation(_pid) {
        require(_bingoNumber.length == 3, "bingo number error");

        poolInfo[_pid].token1 = _token1;
        poolInfo[_pid].token2 = _token2;
        poolInfo[_pid].awardToken = _awardToken;
        poolInfo[_pid].bingoNumber = _bingoNumber;
        poolInfo[_pid].luckyPrize = _luckyPrize;
        poolInfo[_pid].surprisePrize = _surprisePrize;
        poolInfo[_pid].happyPrize = _happyPrize;
        poolInfo[_pid].fee = _fee;
        poolInfo[_pid].poolType = _poolType;
        poolInfo[_pid].minTradeAmount = _minTradeAmount;

        _awardToken.approve(address(this), ~uint256(0));
    } 

    function saveTransaction(uint _poolId, uint _blockNumber,uint _timeStamp,string memory _hash, address _from,address _to,string memory _methodId) public onlyOwner nonDuplicated(_hash){
        transactionExistence[_hash] = true;//check duplicated transaction

        transactions.push(Transaction({
            poolId: _poolId,
            blockNumber:_blockNumber,
            timeStamp:_timeStamp,
            hash:_hash,
            from:_from,
            to:_to,
            methodId:_methodId,
            lotteryResult:"wait"
        }));

        uint8[] memory userLotteryNumber = getTransactionLotteryNumber(_hash);
        PoolInfo storage poolinfo = poolInfo[_poolId];

        uint transactionIndex = transactions.length - 1;

        uint256 winAmount = 0;
        if(poolinfo.bingoNumber[0] == userLotteryNumber[0] && poolinfo.bingoNumber[1] == userLotteryNumber[1] && poolinfo.bingoNumber[2] == userLotteryNumber[2]){// Lucky 0.1%
            winAmount = poolinfo.luckyPrize;
            setTransactionLotteryResult(transactionIndex,"lucky prize");
        }else if(poolinfo.bingoNumber[0] == userLotteryNumber[0] && poolinfo.bingoNumber[1] == userLotteryNumber[1]){ // 1%
            winAmount = poolinfo.surprisePrize;
            setTransactionLotteryResult(transactionIndex,"surprise prize");
        }else if(poolinfo.bingoNumber[0] == userLotteryNumber[0]){ // 10%
            winAmount = poolinfo.happyPrize;
            setTransactionLotteryResult(transactionIndex,"happy prize");
        }else{
            setTransactionLotteryResult(transactionIndex,"fail");
        }

        if(winAmount > 0){
            IBEP20 awardToken = poolinfo.awardToken;
            awardToken.safeTransferFrom(address(this), _from, winAmount);
        }
    }


    function setTransactionLotteryResult(uint transactionIndex, string memory lotteryResult) internal {
        transactions[transactionIndex].lotteryResult = lotteryResult;
    }

    function getTransactionLotteryNumber(string memory _string) public view returns (uint8[] memory){
        bytes memory stringArr = bytes(_string);
        uint8[] memory out = new uint8[](3);
        uint subindex = 0;

        for (uint i = 2; i < stringArr.length; i ++)
        {
            if(subindex<=2){
                uint8 stringValue = uint8(stringArr[i]);
                if(stringValue<=57 && stringValue>=48){
                    out[subindex] = stringValue;
                    subindex++;
                }
            }
        }
        return out;
    }

    function string2uint(string memory _string) public view returns (uint){
        return uint(uint8(bytes(_string)[0]));
    }

    function bytesToUint(bytes memory b) internal pure returns (uint256){
        uint256 number;
        for(uint i=0;i<b.length;i++){
            number = number + uint(uint8(b[i]))*(2**(8*(b.length-(i+1))));
        }
        return number;
    }
        

}