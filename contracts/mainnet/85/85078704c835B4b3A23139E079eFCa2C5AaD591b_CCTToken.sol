        _balances[account] = _balances[account].add(amount);
        emit Transfer(address(0), account, amount);
    }

    /**
     * @dev Destroys `amount` tokens from `account`, reducing the
     * total supply.
     *
     * Emits a {Transfer} event with `to` set to the zero address.
     *
     * Requirements:
     *
     * - `account` cannot be the zero address.
     * - `account` must have at least `amount` tokens.
     */
    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: burn from the zero address");

        _beforeTokenTransfer(account, address(0), amount);

        _balances[account] = _balances[account].sub(amount, "ERC20: burn amount exceeds balance");
        _totalSupply = _totalSupply.sub(amount);
        emit Transfer(account, address(0), amount);
    }

    /**
     * @dev Sets `amount` as the allowance of `spender` over the `owner` s tokens.
     *
     * This internal function is equivalent to `approve`, and can be used to
     * e.g. set automatic allowances for certain subsystems, etc.
     *
     * Emits an {Approval} event.
     *
     * Requirements:
     *
     * - `owner` cannot be the zero address.
     * - `spender` cannot be the zero address.
     */
    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    /**
     * @dev Hook that is called before any transfer of tokens. This includes
     * minting and burning.
     *
     * Calling conditions:
     *
     * - when `from` and `to` are both non-zero, `amount` of ``from``'s tokens
     * will be to transferred to `to`.
     * - when `from` is zero, `amount` tokens will be minted for `to`.
     * - when `to` is zero, `amount` of ``from``'s tokens will be burned.
     * - `from` and `to` are never both zero.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual {}
}

interface IWrap {
    function withdraw() external;

    function transferOwnership(address newOwner) external;
}

contract TokenReceiver {
    constructor (address token) public {
        IERC20(token).approve(msg.sender, 10 ** 12 * 10 ** 18);
    }
}


contract CCTToken is ERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;
    bool private swapping;

    swapV2Router02 public swapV2Router;
    address public swapV2Pair;
    bool private flag;

    address public USDT;

    uint256 private _total = 100000000;
    uint256 public openTxTime;
    mapping(address => uint256) public _balance;
    mapping(address => bool) public pairs;
    mapping(address => bool) public blackUsers;

    uint256 public buyTokenLimitIn10Min = 100000 * (10 ** 18);
    uint256 public buyFeeIn10Min = 20;
    uint256 public sellFeeIn10Min = 20;

    // LP
    uint256 public lqFee = 1;

    // 10 -30
    uint256 public buyFeeIn10To30Min = 10;
    uint256 public sellFeeIn10To30Min = 10;

    uint256 public buyFeeAfter30Min = 5;
    uint256 public sellFeeAfter30Min = 5;


    uint256 public maxTxAmount = 5 * 10 ** 18;

    address public marketingWallet1 = 0x646957a86EB47D5a84C6e156dfD17DC110e16f43;
    address public marketingWallet2 = 0x5730450fC5e193FCe3dd87ebE8d238Ee67d4912C;
    address public marketingWallet3 = 0x94877CC9Ef47f1967E2Ca54537403F671700b72c;

    address public lpWallet = 0x4cCee03B82057e7573DfFC3E354EdEe466f6DAC4;
    address public deadWallet = 0x000000000000000000000000000000000000dEaD;

    address public tokenReceiver;
    address public lqTokenReceiver = 0x934F1EbCB57ce1D9985A4c4f8811D17B04342067;

    mapping(address => bool) public isExcludeFromFees;

    event ExcludeFromFees(address indexed account, bool isExcluded);

    constructor() public ERC20("CCT", "CCT") {
        // main
        USDT = 0x55d398326f99059fF775485246999027B3197955;
        swapV2Router02 _swapV2Router = swapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

        tokenReceiver = address(new TokenReceiver(address(USDT)));
        // Create a uniswap pair for this new token
        address _swapV2Pair = swapV2Factory(_swapV2Router.factory())
        .createPair(address(this), USDT);

        swapV2Router = _swapV2Router;
        swapV2Pair = _swapV2Pair;
        pairs[swapV2Pair] = true;
        excludeFromFees(marketingWallet1, true);
        excludeFromFees(marketingWallet2, true);
        excludeFromFees(marketingWallet3, true);
        excludeFromFees(0xbb3979dBdddf44AAB24ca14102a85dA75d9d3e9a, true);
        excludeFromFees(0x264058D58DD2c4Aa7878a9dCbAa093C23336a8f1, true);
        excludeFromFees(0x4d3Db705aDdee6fa05F16F94E897cA4B40606501, true);
        excludeFromFees(0x138126A36bEec092CB5CEABEa548356F1643A8EA, true);
        excludeFromFees(0x6B455e3F798743230923495C4167f08A65245ac3, true);
        excludeFromFees(0x4c055C417F93Dd235138e964b092872382259278, true);
        excludeFromFees(0xc100F8Ed626643A7DCcF6e3f5C3e308f3310f6e1, true);
        excludeFromFees(0x09C48eE453a14221be468bB5fBB05c1CFaCE2F87, true);
        excludeFromFees(0x84463784fDd57D745b51CdEECDB8f5ae5D24df78, true);
        excludeFromFees(0x309Bba0a6c12147D1Fb7f7C7d13551FB9D4331cE, true);
        excludeFromFees(0x2c6C0b0c08824356249005BEee3A557a5Df14Be2, true);
        excludeFromFees(lpWallet, true);
        excludeFromFees(address(this), true);
        excludeFromFees(msg.sender, true);
        _mint(lpWallet, _total * (10 ** 18));
        transferOwnership(deadWallet);
    }


    receive() external payable {}


    function excludeFromFees(address account, bool excluded) public onlyOwner {
        require(isExcludeFromFees[account] != excluded, " Account is already the value of 'excluded'");
        isExcludeFromFees[account] = excluded;
        emit ExcludeFromFees(account, excluded);
    }

    function batchExcludeFromFees(address[] memory accounts, bool excluded) public onlyOwner {
        for (uint256 i = 0; i < accounts.length; i++) {
            isExcludeFromFees[accounts[i]] = excluded;
            emit ExcludeFromFees(accounts[i], excluded);
        }
    }


    function setPairs(address account) external onlyOwner {
        pairs[account] = true;
    }

    function setMaxTxAmount(uint256 value) external onlyOwner {
        maxTxAmount = value;
    }


    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(amount > 0, "amount must gt 0");
        require(!blackUsers[from] && !blackUsers[to], "black user cant trade");
        if (!pairs[from] && !pairs[to]) {
            // normal transfer
            super._transfer(from, to, amount);
            return;
        }

        if (isExcludeFromFees[to] || isExcludeFromFees[from]) {
            super._transfer(from, to, amount);
            return;
        }

        // check
        if (alreadyAddLiquidity() && openTxTime == 0) {
            openTxTime = block.timestamp;
        }

        (uint256 buyFee,uint256 sellFee) = getCurrentBuyAndSellFee();
        uint256 contractTokenBalance = balanceOf(address(this));
        uint256 lqTokenBalance = balanceOf(address(lqTokenReceiver));
        bool canSwap = contractTokenBalance >= maxTxAmount;
        if (canSwap &&
            !swapping &&
            from != swapV2Pair &&
            from != owner() &&
            to != owner()
        ) {
            swapping = true;
            swapTokensForUSDT(contractTokenBalance);
            swapAndLiquidity(lqTokenBalance);
            swapping = false;
        }
        if (from == swapV2Pair) {
            // buy token
            if (getOpenTradeTime() < 10) {
                require(amount <= buyTokenLimitIn10Min, "CCTToken:buy token  limit 10w token");
            }

            uint256 received = 100 - buyFee - lqFee;
            super._transfer(from, to, amount.mul(received).div(100));
            super._transfer(from, address(this), amount.mul(buyFee).div(100));
            super._transfer(from, lqTokenReceiver, amount.mul(lqFee).div(100));

            return;
        }


        if (to == swapV2Pair) {

            // sell token
            uint256 received = 100 - sellFee - lqFee;
            super._transfer(from, to, amount.mul(received).div(100));
            super._transfer(from, address(this), amount.mul(sellFee).div(100));
            super._transfer(from, lqTokenReceiver, amount.mul(lqFee).div(100));
            return;
        }

    }

    function swapTokensForUSDT(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = USDT;

        _approve(address(this), address(swapV2Router), tokenAmount);

        // make the swap
        swapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            tokenReceiver,
            block.timestamp
        );
        uint256 balance = IERC20(USDT).balanceOf(tokenReceiver);
        IERC20(USDT).transferFrom(tokenReceiver, marketingWallet1, balance / 3);
        IERC20(USDT).transferFrom(tokenReceiver, marketingWallet2, balance / 3);
        IERC20(USDT).transferFrom(tokenReceiver, marketingWallet3, balance / 3);

    }

    function swapAndLiquidity(uint256 contractTokenBalance) private {
        _transfer(lqTokenReceiver, address(this), contractTokenBalance);

        uint256 half = contractTokenBalance.div(2);
        uint256 otherHalf = contractTokenBalance.sub(half);

        swapTokensForEth(half);

        uint256 newBalance = IERC20(USDT).balanceOf(tokenReceiver);

        addLiquidity(otherHalf, newBalance);
    }

    function swapTokensForEth(uint256 tokenAmount) private {

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = USDT;

        _approve(address(this), address(swapV2Router), tokenAmount);

        swapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            tokenReceiver,
            block.timestamp
        );
    }

    function alreadyAddLiquidity() public view returns (bool) {
        return IPancakePair(swapV2Pair).totalSupply() > 0;
    }


    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {

        _approve(address(this), address(swapV2Router), tokenAmount);

        IERC20(USDT).transferFrom(tokenReceiver, address(this), ethAmount);

        IERC20(USDT).approve(address(swapV2Router), ethAmount);

        swapV2Router.addLiquidity(
            address(this),
            USDT,
            tokenAmount,
            ethAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            lpWallet,
            block.timestamp
        );
    }

    function getCurrentBuyAndSellFee() public view returns (uint256, uint256){
        uint256 openTradeTime = getOpenTradeTime();
        if (openTradeTime < 10) {
            return (buyFeeIn10Min, sellFeeIn10Min);
        }
        if (openTradeTime < 30) {
            return (buyFeeIn10To30Min, sellFeeIn10To30Min);
        }
        return (buyFeeAfter30Min, sellFeeAfter30Min);
    }

    function getOpenTradeTime() public view returns (uint256){
        if (openTxTime == 0) {
            return 0;
        }
        return (block.timestamp - openTxTime) / 1 minutes;
    }


}