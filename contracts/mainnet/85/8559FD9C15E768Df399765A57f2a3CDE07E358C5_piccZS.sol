contract piccZS is IERC20 , Ownable {
    using SafeMath for uint256;

    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping (address => bool) isDividendExempt;
  


    string private _name = "piccZS";
    string private _symbol = "piccZS";
    uint8 private _decimals = 18;


    uint256 private _tTotal = 200000000 * 10**18;

    uint256 public maxprice = 0;

    uint256 public lastTime = 0;

    bool public swapOpen = false;
    uint256 public rateM  =  10000;
    uint256 public rateZ  =  10000;
    uint256 public Maxhold  =  1000;

 
    IUniswapV2Router02 public immutable uniswapV2Router;

    address public immutable uniswapV2Pair;
 
    address public CommunityNodesAddress = 0x04eE0F0B4A159280218ceAB6B85F8b909cf1777C;
    address public FoundationAddress = 0x4df582e3eAA676962d00A71B31DD96eDC4a320E4;
    address public czAddress = 0x4df582e3eAA676962d00A71B31DD96eDC4a320E4;
 
 
    mapping(address => address) public inviter;

    uint256 private _tFeeTotal;

 

  
     mapping(address => bool) private _isExcludedFromFee;

 
    
    constructor() {
        _tOwned[msg.sender] = _tTotal;
       
        
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x10ED43C718714eb63d5aA57B78B54704E256024E
         );

        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), address(
                // 0x55d398326f99059fF775485246999027B3197955
                0x9C974B0cA62E9bbB82b116F29Cc666EdE1eb8C0a
 
                ));


        uniswapV2Router = _uniswapV2Router;

        _isExcludedFromFee[msg.sender] = true;
        _isExcludedFromFee[address(this)] = true;

        isDividendExempt[address(this)] = true;
        isDividendExempt[address(0)] = true;
         emit Transfer(address(0), msg.sender, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint256) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _tOwned[account];
    }

    function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFee[account];
    }
    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: burn from the zero address");
        _tOwned[account] = _tOwned[account].sub(amount);
        _tOwned[address(0)] = _tOwned[address(0)].add(amount);
        emit Transfer(account, address(0), amount);
    }

    function burn(uint256 amount) public virtual {
        _burn(msg.sender, amount);
    }
    
    function burnFrom(address account, uint256 amount) public virtual {
        uint256 currentAllowance = allowance(account, msg.sender);
        require(currentAllowance >= amount, "ERC20: burn amount exceeds allowance");
        unchecked {
            _approve(account, msg.sender, currentAllowance.sub(amount));
        }
        _burn(account, amount);
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

  function setczAddress(address Address) public onlyOwner {
        czAddress = Address;
    }


    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(msg.sender, spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }
    function setSwapOpen() public onlyOwner {
        swapOpen = !swapOpen;
    }

    function setMaxHold(uint256 amount) public onlyOwner {
        Maxhold = amount;
    }
    function setmaxprice() public onlyOwner {
        maxprice = price();
    }


    //  function setrate(uint256 rate) public onlyOwner {
    //     Maxhold = rate;
    // }
 

    receive() external payable {}



    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

      
        uint256  toblance =  _tOwned[to].add(amount);
        uint256 PairBalance = this.balanceOf(address(uniswapV2Pair));


          bool pd =  _isExcludedFromFee[to]
            || from == address(uniswapV2Router) || to == address(uniswapV2Pair)|| to == address(1) ;

        if (from == uniswapV2Pair) {


            if (!_isExcludedFromFee[to]){

                if(czAddress != to){
                    require(swapOpen , "Suspend buying");

                }
            }
        }

        if (!pd ) {
    
 
            require(toblance <= PairBalance.div(Maxhold), "Holding exceeds the upper limit");

        }


 
        // if (block.timestamp.sub(lastTime)>=86400){
        //     maxprice = todayMaxPrice;
        //     todayMaxPrice = 0;
        // }else {
        //     if (price()>todayMaxPrice){
        //         todayMaxPrice = price();
        //     }
        // }
     
       

         _transferStandard(from, to, amount);
 
    }
 
 

   
    
  
  

    function _transferStandard(
        address sender,
        address recipient,
        uint256 tAmount 
    ) private {


    uint256  commission = 0;
 
     if (_isExcludedFromFee[sender] || _isExcludedFromFee[recipient]
      || sender == address(uniswapV2Router) ) {
    
        }else if(recipient == address(uniswapV2Pair)){
            uint256  Piccprice = price();
 

            if (Piccprice < maxprice.div(1000).mul(800)){
                commission = 3000;
                commission = commission.mul(rateZ).div(rateM);
            } else if (Piccprice>=maxprice.div(1000).mul(950)){
                commission = 200;
            }else {
                commission = maxprice.sub(Piccprice).mul(1000).div(maxprice);
                commission = commission.mul(rateZ).div(rateM);
            }
         
        }else if( sender == address(uniswapV2Pair)){
            commission = 200;
        }
  
    _tOwned[sender] = _tOwned[sender].sub(tAmount);
    _takeLPFee(sender,tAmount,commission); 

 
     
    uint256 recipientRate = 10000 -
            commission;


        _tOwned[recipient] = _tOwned[recipient].add(
            tAmount.div(10000).mul(recipientRate)
        );

      uint256  Amount = tAmount.div(10000).mul(recipientRate);

        emit Transfer(sender, recipient, Amount);
    }

    function _takeLPFee(address sender,uint256 tAmount,uint256 _Fee) private {
        if (_Fee == 0) return;

        uint256 _FeeAmount = tAmount.div(10000).mul(_Fee);
        _tOwned[address(1)] = _tOwned[address(1)].add(_FeeAmount.div(2));
        emit Transfer(sender, address(1), _FeeAmount.div(2));
        
 
        _tOwned[address(CommunityNodesAddress)] = _tOwned[address(CommunityNodesAddress)].add(_FeeAmount.mul(3).div(10));
        emit Transfer(sender, address(CommunityNodesAddress), _FeeAmount.mul(3).div(10));


        _tOwned[address(FoundationAddress)] = _tOwned[address(FoundationAddress)].add(_FeeAmount.mul(2).div(10));
        emit Transfer(sender, address(FoundationAddress), _FeeAmount.mul(2).div(10));
            
    }
 
  function price() public view returns(uint256)   {
        // IERC20 USDT = IERC20(0x55d398326f99059fF775485246999027B3197955);
        IERC20 USDT = IERC20(0x9C974B0cA62E9bbB82b116F29Cc666EdE1eb8C0a);
      

        uint256 PICCBalance = this.balanceOf(address(uniswapV2Pair));
        uint256 USDTBalance = USDT.balanceOf(address(uniswapV2Pair));
        if(USDTBalance == 0){
            return  0;
        }else{
            
            return  USDTBalance.mul(10000000).div(PICCBalance);
        }
    }

}