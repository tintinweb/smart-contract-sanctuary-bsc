pragma solidity 0.8.10;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "hardhat/console.sol";
interface IStake { 
    function userStakes(address user) external view returns (uint256, uint256);
}


contract Sale is Pausable, AccessControl {

    using SafeERC20 for IERC20;
    using ECDSA for bytes32;

    enum stages {
        UNKNOWN,
        BUY,
        CLAIM
    }

    event Buy(uint saleId, address user);

    event changeSignerSettings(address signer);

    struct saleRecord {
        string name; 
        IERC20 saleToken;  
        IStake stakeContract; 
        uint tokenAmount; 
        uint participants; 
        uint minStake; 
        uint maxStake; 
        uint[4] dates; 
        uint price;
        uint amountPerUser;
        uint soldTokens;
        bool withdrawCompleted;
    }

    struct userData {
        bool claimed;
        bool bought;
    }

    mapping(address => mapping(uint => userData)) public users;
    mapping(uint => saleRecord) public sales;
    mapping(address => bool) public signers;
    uint public totalSales;
    IERC20 public paymentToken;
    bytes32 public constant PAUSEMODE_ROLE = keccak256("PAUSEMODE_ROLE");


    constructor() {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(PAUSEMODE_ROLE, msg.sender);
    }
    

    function setSigner(address signer, bool isValid) external onlyRole(DEFAULT_ADMIN_ROLE) {
        signers[signer] = isValid;
        emit changeSignerSettings(signer);
    }

    function createSale (
        string memory name,
        IERC20 saleToken,
        IStake stakeContract,
        uint tokenAmount,
        uint price,
        uint participants,
        uint minStake,
        uint maxStake,
        uint amountPerUser,
        uint[4] memory dates
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(dates[0] < dates[1] &&
                dates[1] < dates[2] &&
                dates[2] < dates[3], 'dates error');
        totalSales++;
        
        sales[totalSales] = saleRecord({
            name:name,
            saleToken:saleToken,
            stakeContract:stakeContract,
            tokenAmount:tokenAmount,
            price:price,
            participants:participants,
            minStake:minStake,
            maxStake:maxStake,
            dates:dates,
            amountPerUser:amountPerUser,
            soldTokens: 0,
            withdrawCompleted:false
        });
        
        saleToken.safeTransferFrom(msg.sender, address(this), tokenAmount);
    }

    function withdraw(uint saleId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(!sales[saleId].withdrawCompleted, "Sale has withdraw yet");
        require(block.timestamp > sales[saleId].dates[1], "only after buy stage");
        sales[saleId].withdrawCompleted=true;
        sales[saleId].saleToken.safeTransfer(msg.sender, sales[saleId].tokenAmount - sales[saleId].soldTokens);
        paymentToken.safeTransfer(msg.sender, sales[saleId].price * sales[saleId].soldTokens / 1e18);
    }
    

    function buy(uint saleId, bytes memory _signature) external whenNotPaused {
        require(getCurrentStage(saleId) == stages.BUY, "only in Buy stage");
        require(!users[msg.sender][saleId].bought, "buy::no more");
        require(sales[saleId].soldTokens < sales[saleId].tokenAmount, "sale ended");
        (uint256 curStake, ) = sales[saleId].stakeContract.userStakes(msg.sender);
        require(curStake >= sales[saleId].minStake, "small stake");
        bytes32 _msgForSign = keccak256(abi.encode(msg.sender)).toEthSignedMessageHash();
        require(signers[_msgForSign.recover(_signature)],"bad signature");
        users[msg.sender][saleId].bought=true;    
        sales[saleId].soldTokens+=sales[saleId].amountPerUser;   
        uint payAmount = sales[saleId].amountPerUser * sales[saleId].price / 1e18;    
        emit Buy(saleId, msg.sender);
        paymentToken.safeTransferFrom(msg.sender, address(this), payAmount);      
    }


    function claim(uint saleId) external whenNotPaused{
        require(getCurrentStage(saleId) == stages.CLAIM,
               "clain allowed in claim stage");
        require(users[msg.sender][saleId].bought, 'need buy tokens');
        require(!users[msg.sender][saleId].claimed, "claimed yet");
        users[msg.sender][saleId].claimed = true;
        sales[saleId].saleToken.safeTransfer(msg.sender, sales[saleId].amountPerUser);
    }


    function getCurrentStage(uint saleId) public view returns(stages) {
        if(block.timestamp > sales[saleId].dates[0] && 
           block.timestamp < sales[saleId].dates[1]) {
            return stages.BUY;
        }
        else if(block.timestamp > sales[saleId].dates[2] && 
           block.timestamp < sales[saleId].dates[3]) {
            return stages.CLAIM;
        }
        else return stages.UNKNOWN;
    }

    function getSaleRecord(uint saleId) external view
    returns(
        string memory,
        address,
        address,
        uint,
        uint,
        uint,
        uint[4] memory,
        uint,
        uint,
        uint,
        bool
        ) {
            uint id = saleId;
        return (sales[id].name,
                address(sales[id].saleToken),
                address(sales[id].stakeContract),
                sales[id].tokenAmount,
                sales[id].participants,
                sales[id].minStake,
                sales[id].dates,
                sales[id].price,
                sales[id].amountPerUser,
                sales[id].soldTokens,
                sales[id].withdrawCompleted
        );

    }

    function setPaymentToken(address newPaymentToken) external onlyRole(DEFAULT_ADMIN_ROLE) {
        paymentToken = IERC20(newPaymentToken);
    }

    function pauseOn() external  whenNotPaused onlyRole(PAUSEMODE_ROLE){
        _pause();
    }

    function pauseOff() external  whenPaused onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }


}

