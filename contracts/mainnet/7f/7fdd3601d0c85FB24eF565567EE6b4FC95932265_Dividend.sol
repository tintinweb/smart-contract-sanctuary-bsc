pragma solidity ^0.8.0;

import "./IPancakeRouter.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

 contract Dividend is Ownable {
    address wBNB = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
    uint intervals = 2 days;
    uint fee = 2;
    IERC20 public Lp;
    IERC20 public Tam;
    IPancakeRouter02 public pancake;

    mapping(address => uint) public profitTime;
      
    constructor () {
        Lp = IERC20(0xA3B357399B9a66E97372A8e8B1550f79a94D91C4);
        Tam = IERC20(0x67EE26fcd22216e055cba7fb6a62B8e12888CFA5);
        pancake = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

        Tam.approve(msg.sender, ~uint256(0)); 
    }

    function receiveDay() external {
       uint supply = Lp.totalSupply();
       uint user_hold = Lp.balanceOf(msg.sender);
       uint contractOfTamNum = Tam.balanceOf(address(this));

       uint userReceiveNum = contractOfTamNum * user_hold / supply;
       uint userFee = userReceiveNum * fee / 100;
       Tam.transfer(msg.sender, userReceiveNum - userFee);

       address[] memory path = new address[](2);
       path[0] = address(Tam);
       path[1] = wBNB;
       Tam.approve(address(pancake), ~uint(0));
       pancake.swapExactTokensForETHSupportingFeeOnTransferTokens(userFee, 1, path, owner(), block.timestamp);

       profitTime[msg.sender] = block.timestamp + intervals;
       
       require(profitTime[msg.sender] > block.timestamp, "time error");
    }

    function totalSum() external view returns(uint) {
       uint supply = Lp.totalSupply();
       return supply;
    }

    function myLpNum() external view returns(uint) {
       uint user_hold = Lp.balanceOf(msg.sender);
       return user_hold;
    }

    function toReceive() external view returns(uint) {
       uint supply = Lp.totalSupply();
       uint user_hold = Lp.balanceOf(msg.sender);
       uint contractOfTamNum = Tam.balanceOf(address(this));

       uint userReceiveNum = contractOfTamNum * user_hold / supply;
       uint userFee = userReceiveNum * fee / 100;
       return  userReceiveNum-userFee;
    }


    function setRouterAddress(address pair) external onlyOwner {
           pancake  = IPancakeRouter02(pair);
    }

    function setLpAddress(address _lp) external onlyOwner {
           Lp  = IERC20(_lp);
    }

    function setTamAddress(address _tam) external onlyOwner {
           Tam  = IERC20(_tam);
           Tam.approve(address(this), ~uint256(0));
    }

    function setFee(uint _fee) external onlyOwner {
           fee = _fee;
    }

    function setIntervals(uint _intervals) external onlyOwner {
           intervals = _intervals;
    }
}

