        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(uniswapRouterAddress);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), usdtAddress);
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        
        isPair[_uniswapV2Pair] = true;

        _totalSupply += initNumber*10**decimals();
        unchecked {
            // Overflow not possible: balance + amount is at most totalSupply + amount, which is checked above.
            _balances[msg.sender] += initNumber*10**decimals();
        }
        emit Transfer(address(0), msg.sender, initNumber*10**decimals());
        
    }

