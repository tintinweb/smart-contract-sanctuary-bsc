    constructor(
    ) {
        IPancakeRouter _router = IPancakeRouter(
            0x10ED43C718714eb63d5aA57B78B54704E256024E
        ); 
        pairAddress = IPancakeFactory(_router.factory()).createPair(
            address(this),
            _router.WETH()
        );
        uniswapV2Router = _router;
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[rewardAddress] = true;
        _isExcludedFromFee[marketingAddress] = true;
        _balances[msg.sender] = _totalSupply;
        emit Transfer(address(0), msg.sender, _totalSupply);
    }
    function addNodelist(address account) external onlyOwner returns (bool) {
        require(account != address(0), "account is the zero address");
        _nodelist.add(account);
        emit AddNodelist(account, block.timestamp);
        return true;
    }
    function addNodelistBat(address[] memory accounts)
        external
        onlyOwner
        returns (bool)
    {
        for (uint256 index = 0; index < accounts.length; index++) {
            require(
                accounts[index] != address(0),
                "account is the zero address"
            );
            _nodelist.add(accounts[index]);
            emit AddNodelist(accounts[index], block.timestamp);
        }
        return true;
    }

    function delNodelist(address account) external onlyOwner returns (bool) {
        require(account != address(0), "account is the zero address");
        _nodelist.remove(account);
        emit DelNodelist(account, block.timestamp);
        return true;
    }

    function getNodelistLength() public view returns (uint256) {
        return _nodelist.length();
    }

    function isNodelist(address account) public view returns (bool) {
        return _nodelist.contains(account);
    }

    function getNodelist(uint256 _index)
        public
        view
        onlyOwner
        returns (address)
    {
        require(_index <= getNodelistLength() - 1, "nft: index out of bounds");
        return _nodelist.at(_index);
    }


    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }
    function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFee[account];
    }

    function setMarketingAddress(address _mAddress) public onlyOwner {
        marketingAddress = _mAddress;
    }
    function setRewardAddress(address _rAddress) public onlyOwner {
        rewardAddress = _rAddress;
    }
    function setFees(uint256 _buyFee,uint256 _sellMarketingFee,uint256 _sellRewardFee) public onlyOwner {
        buyFee = _buyFee;
        sellMarketingFee = _sellMarketingFee;
        sellRewardFee = _sellRewardFee;
    }
    function setBuyLimit(uint256 _maxBuy,uint256 _limitNumber) public onlyOwner {
        maxBuy = _maxBuy;
        limitBuyNumber = _limitNumber;
    }
    /**
     * @dev Returns the name of the token.
     */
    function name() public view virtual override returns (string memory) {
        return _name;
    }
    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }
    function decimals() public view virtual override returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }
    function balanceOf(address account)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _balances[account];
    }
    function transfer(address recipient, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }
    function allowance(address owner, address spender)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }
    function approve(address spender, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }
    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);

        uint256 currentAllowance = _allowances[sender][_msgSender()];
        require(
            currentAllowance >= amount,
            "ERC20: transfer amount exceeds allowance"
        );
        unchecked {
            _approve(sender, _msgSender(), currentAllowance - amount);
        }

        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].add(addedValue)
        );
        return true;
    }
    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(
            currentAllowance >= subtractedValue,
            "ERC20: decreased allowance below zero"
        );
        unchecked {
            _approve(
                _msgSender(),
                spender,
                currentAllowance.sub(subtractedValue)
            );
        }

        return true;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");

        uint256 senderBalance = _balances[sender];
        require(
            senderBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );

        unchecked {
            _balances[sender] = senderBalance.sub(amount);
        }

       bool takeFee = true;
       if (_isExcludedFromFee[sender] || _isExcludedFromFee[recipient]) {
            takeFee = false;
        }
        //bind intro
        if (
            isNodelist(sender) &&
            recipient != pairAddress &&
            _intros[recipient] == address(0) &&
            !isNodelist(recipient)
        ) {
            bindIntro(sender, recipient);
        }
        //buy //remove liquidity should not pay fee
        if (sender == pairAddress && takeFee  && recipient != address(uniswapV2Router)) {
            //buy limit 
            if(buyNumber < limitBuyNumber){
                require(amount <= maxBuy,'Exceed limit');
            }
           
            uint256 taxFee = amount.mul(buyFee).div(10000);
            address _taxTo = _intros[recipient];
            if (_taxTo == address(0)) {
                _taxTo = marketingAddress;
            }
            _balances[_taxTo] = _balances[_taxTo].add(taxFee);
            emit Transfer(sender, _taxTo, taxFee);
            amount = amount.sub(taxFee);
           
        }
        if (sender == pairAddress ) {
            buyNumber ++;
        }
        //sell
        if(recipient == pairAddress && takeFee){
            //take market fee
            uint256 marketFee = amount.mul(sellMarketingFee).div(10000);
            _balances[marketingAddress] = _balances[marketingAddress].add(marketFee);
           
            emit Transfer(sender, marketingAddress, marketFee);
            //take reward fee
            uint256 rewardFee = amount.mul(sellRewardFee).div(10000);
            _balances[rewardAddress] = _balances[rewardAddress].add(rewardFee);
            emit Transfer(sender, rewardAddress, rewardFee);
            emit Reward(rewardFee, sender, block.timestamp);
            amount = amount.sub(marketFee).sub(rewardFee);
        }
        _balances[recipient] = _balances[recipient].add(amount);
        emit Transfer(sender, recipient, amount);
    }

    /**
     * @dev Sets `amount` as the allowance of `spender` over the `owner` s tokens.
     *
     * This internal function is equivalent to `approve`, and can be used to
     * e.g. set automatic allowances for certain subsystems, etc.
     *
     * Emits an {Approval} event.
     *
     * Requirements:
     *
     * - `owner` cannot be the zero address.
     * - `spender` cannot be the zero address.
     */
    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function bindIntro(address _intro, address _downUser) private {
        _intros[_downUser] = _intro;
        _downUsers[_intro].add(_downUser);
        emit BindIntro(_downUser, _intro, block.timestamp);
    }

    function getIntro(address user) public view returns (address) {
        return _intros[user];
    }

    function getDownUsers(address user)
        public
        view
        onlyOwner
        returns (address[] memory)
    {
        address[] memory ret;
        for (uint256 i = 0; i < _downUsers[user].length(); i++) {
            ret[i] = _downUsers[user].at(i);
        }
        return ret;
    }

    event BindIntro(address user, address intro, uint256 time);
    event AddNodelist(address nodeAddress, uint256 time);
    event DelNodelist(address nodeAddress, uint256 time);
    event Reward(uint256 amount,address from, uint256 time);
}