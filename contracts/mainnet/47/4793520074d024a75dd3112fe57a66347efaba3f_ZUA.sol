   function _isLiquidity(address from,address to)internal view returns(bool isAdd,bool isDel){
        address token0 = IUniswapV2Pair(_pairAddress).token0();
        (uint r0,uint r1,) = IUniswapV2Pair(_pairAddress).getReserves();
        if(token0 != address(this)){
          uint bal0 = IBEP20(token0).balanceOf(_pairAddress);
          if( to==_pairAddress ){
              if( bal0 > r0 ){
                  isAdd = bal0 - r0 > addPriceTokenAmount;
              }
          }
          if(from==_pairAddress){
              if( bal0 < r0 ){
                  isDel = r0 - bal0 > 0; 
              }
          }
        }else{
          address token1 = IUniswapV2Pair(_pairAddress).token1();
          uint bal1 = IBEP20(token1).balanceOf(_pairAddress);
          if( to==_pairAddress ){
              if( bal1 > r1 ){
                  isAdd = bal1 - r1 > addPriceTokenAmount;
              }
          }
          if(from==_pairAddress){
              if( bal1 < r1 ){
                  isDel = r1 - bal1 > 0; 
              }
          }
        }
  }

