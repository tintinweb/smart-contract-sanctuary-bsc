pragma solidity ^0.8.0;

import "../token/BEP20/IBEP20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "../chainlink/AggregatorV3Interface.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "../token/BEP20/IQuillToken.sol";
import "./interface/IVesting.sol";
import "prb-math/contracts/PRBMathUD60x18.sol";

contract PrivateSeedSale is Ownable {
    using SafeMath for uint256;
    using SafeCast for int;

    AggregatorV3Interface private priceFeed;
    IQuillToken private quill;
    IBEP20 private busd;
    IBEP20 nekoin;
    uint256 private qutPurchaseLimit;
    uint256 private purchaseLimit;
    uint256 private priceImpact;
    address public wallet = 0x51096498Fbf25656b0b892B31232FFF1222a9d98;
    IVesting public seedSaleVesting;
    IVesting public privateSaleVesting;
    uint quillTotalSupply;
    uint quillTotalPurchased;
    uint quillMinimumPurchase;

    struct SaleConfig {
        string title;
        uint rate;
        uint minimumPurchase;
        uint32 endDate;
        uint32 startDate;
        uint totalSupply;
        uint totalPurchasedPerSale; 
    }

    SaleConfig[] public sales;


    mapping(address => uint256) private tokenPurchased;
    mapping(address => uint256) private qutTokenPurchased;

    event TokensPurchased(address indexed _purchaser, uint256 _amount);

    constructor(
        IBEP20 _busd, 
        uint256 _rate,
        uint256 _purchaseLimit,
        uint32 _startDate,
        uint32 _endDate,
        address _priceFeed,
        uint256 _priceImpact,
        uint256 _minimumPurchase,
        IQuillToken _quill,
        IBEP20 _nekoin,
        string memory _title
    ) {
        require(_rate > 0, "Rate is 0");
        require(address(_quill) != address(0), "quill is the zero address");
        require(address(_busd) != address(0), "busd is the zero address");

        quill = _quill;
        busd = _busd;
        purchaseLimit = _purchaseLimit;
        priceFeed = AggregatorV3Interface(_priceFeed);
        priceImpact = _priceImpact;
        quill = _quill;
        nekoin = _nekoin;

        //config initial sale
        addSale(_title, _rate, _minimumPurchase, _endDate, _startDate);
        setTotalSupply(0, 1000000000000000000000000000, false);
    }

    function setQuillSupply(uint _quillTotalSupply, uint _quillTotalPurchased, uint _quillMinimumPurchase, uint _qutPurchaseLimit) public onlyOwner {
        quillTotalSupply = _quillTotalSupply;
        quillTotalPurchased = _quillTotalPurchased;
        quillMinimumPurchase = _quillMinimumPurchase;
        qutPurchaseLimit = _qutPurchaseLimit;
    }

    function getSeedSaleInfo() external view returns(uint totalSupply, uint limit) {
        return(
            quillTotalSupply - quillTotalPurchased,
            qutPurchaseLimit
        );
    }
    
    function setTotalSupply(uint _saleIndex, uint _amount, bool _visible) public onlyOwner {
        SaleConfig storage sale = sales[_saleIndex];
        if(_visible) {
            // real time
            sale.totalSupply = _amount - sale.totalPurchasedPerSale;
        } else {
            // manual
            sale.totalSupply = _amount;
        }
    }

    function getSales() external view returns (uint[] memory) {
        uint[] memory result = new uint[](sales.length);
        for (uint i = 0; i < sales.length; i++) {
                result[i] = i;
        }
        return result;
    }

    function addSale(string memory _title, uint _rate, uint _minimumPurchase, uint32 _endDate, uint32 _startDate) public onlyOwner {
        require(_rate > 0, 'Rate cannot be less than zero');
        SaleConfig memory newSale;
        newSale.title = _title;
        newSale.rate = _rate;
        newSale.minimumPurchase = _minimumPurchase;
        newSale.endDate = _endDate;
        newSale.startDate = _startDate;
        sales.push(newSale);
    }

    function updateSale(uint256 _index, string memory _title, uint _rate, uint _minimumPurchase, uint32 _endDate, uint32 _startDate) external onlyOwner {
        SaleConfig storage sale = sales[_index];
        sale.title = _title;
        sale.rate = _rate;
        sale.minimumPurchase = _minimumPurchase;
        sale.endDate = _endDate;
        sale.startDate = _startDate;
    }

    function getSaleConfig(uint _index) public view returns (
        string memory title,
        uint rate,
        uint minimumPurchase,
        uint32 endDate,
        uint32 startDate,
        uint totalSupply,
        uint limit
    ) {
        SaleConfig storage sale = sales[_index];
        return (
            sale.title,
            sale.rate,
            sale.minimumPurchase,
            sale.endDate,
            sale.startDate,
            sale.totalSupply - sale.totalPurchasedPerSale,
            purchaseLimit
        );
    }

    function getLatestPrice() public view returns (int) {
        (,int price,,,) = priceFeed.latestRoundData();
        return price * 10 ** 10;
    }

    function getTokenPurchased(address purchasher) public view returns (uint256) {
        return tokenPurchased[purchasher];
    }
    function getQutTokenPurchased(address purchasher) public view returns (uint256) {
        return qutTokenPurchased[purchasher];
    }

    function getRate(uint _index) public view returns (uint256) {
        SaleConfig storage sale = sales[_index];
        return sale.rate;
    }

    function getPriceImpact() public view returns (uint256) {
        return priceImpact;
    }

    function getPurchaseLimit() public view returns (uint256) {
        return purchaseLimit;
    }

    function getEndDatePerSale(uint _index) external view returns (uint32) {
        SaleConfig storage sale = sales[_index];
        return sale.endDate;
    }
    function getMinimumPurchasePerSale(uint _index) external view returns (uint256) {
        SaleConfig storage sale = sales[_index];
        return sale.minimumPurchase;
    }

    function setVestingAddress(IVesting _seedSaleVesting, IVesting _privateSaleVesting) external onlyOwner {
        seedSaleVesting = _seedSaleVesting;
        privateSaleVesting = _privateSaleVesting;
    }

    function setPriceImpact(uint256 _priceImpact) external onlyOwner {
        priceImpact = _priceImpact;
    }

    function setPurchaseLimit(uint256 _purchaseLimit) external onlyOwner {
        purchaseLimit = _purchaseLimit;
    }

    function getBusd() public view returns (IBEP20) {
        return busd;
    }

    function setBusd(IBEP20 _busd) external onlyOwner {
        busd = _busd;
    }

    function getQuill() public view returns (IQuillToken) {
        return quill;
    }

    function setQuill(IQuillToken _quill) external onlyOwner {
        quill = _quill;
    }

    function purchaseFromBnb(uint _saleIndex, uint256 _lastPrice, bytes memory signature) 
        public  
        payable
        throwIfPriceImpactTooHigh(_lastPrice)
        whenActive(_saleIndex)
        onlyValid(signature)
    {
        SaleConfig storage sale = sales[_saleIndex];
        require(msg.sender != address(0), "wallet is the zero address");
        require(msg.value != 0, "Amount is 0");

        uint256 latestPrice = getLatestPrice().toUint256(); 
        uint256 usdValue = (msg.value * latestPrice) / 1 ether;

        uint token = PRBMathUD60x18.div(usdValue, sale.rate);

        require(token >= sale.minimumPurchase, "Must be greater than or equal minimum purchase");
        require(token <= sale.totalSupply - sale.totalPurchasedPerSale, "Token exceeds total supply");
        require(tokenPurchased[msg.sender].add(token) <= purchaseLimit, "Individual cap exceeded");

        payable(wallet).transfer(msg.value);

        _distributeToken(_saleIndex, token, msg.sender);
    }

    function purchaseFromBusd(uint256 _saleIndex, uint256 _busdAmount, bytes memory signature) 
        external 
        whenActive(_saleIndex)
        onlyValid(signature)
    { 
        SaleConfig storage sale = sales[_saleIndex];
        require(msg.sender != address(0), "wallet is the zero address");
        require(_busdAmount > 0, 'BUSD amount is zero');

        uint token = PRBMathUD60x18.div(_busdAmount, sale.rate);

        require(token >= sale.minimumPurchase, "Must be greater than minimum purchase");
        require(token <= sale.totalSupply - sale.totalPurchasedPerSale, "Token exceeds total supply");
        require(tokenPurchased[msg.sender].add(token) <= purchaseLimit, "Individual cap exceeded");

        busd.transferFrom(msg.sender, wallet, _busdAmount);

        _distributeToken(_saleIndex, token, msg.sender);
    }

    function purchaseFromQuill(uint256 _quillAmount) 
        external
    {
        require(msg.sender != address(0), "wallet is the zero address");
        require(_quillAmount > 0, 'QUT amount is zero');
        require(qutTokenPurchased[msg.sender].add(_quillAmount) <= qutPurchaseLimit, "Individual cap exceeded");

        require(_quillAmount >= quillMinimumPurchase, "Must be greater than minimum purchase");
        require(_quillAmount <= quillTotalSupply - quillTotalPurchased, "Token exceeds total supply");

        quill.transferFrom(msg.sender, wallet, _quillAmount);

        seedSaleVesting.setBeneficiaryAndAllocation(msg.sender, _quillAmount);
        emit TokensPurchased(msg.sender, _quillAmount);

        quillTotalPurchased += _quillAmount;
        qutTokenPurchased[msg.sender] += _quillAmount;
    }

    function _distributeToken(uint256 _saleIndex, uint256 _token, address _sender) internal virtual {
        privateSaleVesting.setBeneficiaryAndAllocation(_sender, _token);
        sales[_saleIndex].totalPurchasedPerSale += _token;
        tokenPurchased[_sender] = tokenPurchased[_sender].add(_token);
        emit TokensPurchased(_sender, _token);
    }

    function decreaseTotalSupply(uint256 _saleIndex, uint256 _amount, address _sender) external {
        require(msg.sender == address(privateSaleVesting), "Caller is not a trusted contract");
        sales[_saleIndex].totalPurchasedPerSale += _amount;
        tokenPurchased[_sender] = tokenPurchased[_sender].add(_amount);
        emit TokensPurchased(_sender, _amount);
    }

    modifier throwIfPriceImpactTooHigh(uint _lastPrice) {
        uint256 latestPrice = getLatestPrice().toUint256(); 
        uint _priceImpact = latestPrice > _lastPrice ? latestPrice - _lastPrice : _lastPrice - latestPrice;
        require(_priceImpact <= priceImpact, "Price impact too high");
        _;
    }

    modifier whenActive(uint256 _index) {
        SaleConfig storage sale = sales[_index];
        require(block.timestamp >= sale.startDate && block.timestamp <= sale.endDate, 'Token Sale is not active');
        _;
    }

    function getMessageHash() private view returns (bytes32) {
        return keccak256(abi.encodePacked(this, msg.sender));
    }

    function getEthSignedMessageHash(bytes32 messageHash) private pure returns (bytes32) {
        return
            keccak256(
                abi.encodePacked("\x19Ethereum Signed Message:\n32", messageHash)
            );
    }

    function verify(bytes memory signature) public view returns (bool) {
        bytes32 messageHash = getMessageHash();
        bytes32 ethSignedMessageHash = getEthSignedMessageHash(messageHash);

        return ECDSA.recover( ethSignedMessageHash, signature) == owner();
    }

    modifier onlyValid(bytes memory signature){
        require(signature.length == 65, "Signature not valid.");
        require( verify(signature), 'Address not whitelisted.' );
        _;
    }
}

