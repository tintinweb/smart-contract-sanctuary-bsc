        svgPartTwo = '</tspan></text></svg>';
    }

    function safeMint(address to, string memory uri) public onlyOwner {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, uri);
    }

    // The following functions are overrides required by Solidity.
    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    //end overrides
    //allows payments to be sent to customizeable address

    function setpaymentReceiver(address payable _paymentReceiver)
        external
        onlyOwner
        returns (address)
    {
        if (paymentReceiver != address(0)) {
            revert();
        }
        paymentReceiver = _paymentReceiver;
        return (paymentReceiver);
    }

    //mappings
    mapping(uint256 => string) public names;
    mapping(string => string) public records;
    mapping(string => string) public linkedWallet;
    mapping(string => string) public webLink;
    mapping(string => string) public text;
    mapping(string => address) public domains;
    mapping(string => bool) isRegistered;
    mapping(bytes1 => string) _baseBytesString;
    mapping(string => bool) testStr;
    


    // pricing presets
    function setpriceTier1(uint256 newTier1) external onlyOwner {
        emit priceTier1Change(priceTier1);
        priceTier1 = newTier1;
    }

    function setpriceTier2(uint256 newTier2) external onlyOwner {
        emit priceTier2Change(priceTier1);
        priceTier2 = newTier2;
    }

    function setpriceTier(uint256 newTier3) external onlyOwner {
        emit priceTier3Change(priceTier1);
        priceTier1 = newTier3;
    }

    event priceTier1Change(uint256 priceTier1);
    event priceTier2Change(uint256 priceTier2);
    event priceTier3Change(uint256 priceTier3);

    // pricing function
    function price(string calldata preName) public view returns (uint256) {
        uint256 len = StringUtils.strlen(preName);
        require(len > 0);
        if (len <= 2) {
            return priceTier1; 
        } else if (len <= 4) {
            return priceTier2; 
        } else {
            return priceTier3; 
        }
    }

    // Thanks to mirg on Stack Exchange.
     function testString(string calldata str) internal pure returns (bool) {
        bytes memory b = bytes(str);
        if (b.length > 13) return false;

        for (uint256 i; i < b.length; i++) {
            bytes1 char = b[i];

            if (
                !(char >= 0x30 && char <= 0x39) && //9-0 hex
                !(char >= 0x61 && char <= 0x7A) && //a-z hex
                !(char >= 0x21 && char <= 0x21) && //!-! hex
                !(char >= 0x23 && char <= 0x26) && //#-& hex
                !(char == 0x2D) && //- hex
                !(char == 0x3D) && //= hex
                !(char == 0x5F)  //= hex
               
            ) return false;
        }

        return true;
    } 

    function setNameTakenMessage(string memory _inputMessage) external onlyOwner {
        nameTakenMessage = _inputMessage;
    }

    function setNameInvalidMessage(string memory _inputTooLongMessage) external onlyOwner {
        nameInvalidMessage = _inputTooLongMessage;
    }

     function getTimestamp() internal view returns (uint256){
        return block.timestamp;
    }

    function setSvgPartOne(string calldata svgOneSub) external onlyOwner {
        svgPartOne = svgOneSub;
    }

    function setSvgPartTwo(string calldata svgTwoSub) external onlyOwner {
        svgPartTwo = svgTwoSub;
    }

    function register(string calldata preName)
        public
        payable
    {
        require(
            valid(preName),
            nameInvalidMessage
        );
        require(
            testString(preName),
            nameInvalidMessage
        );
        require(
            !isRegistered[preName],
            nameTakenMessage
        );
        
        isRegistered[preName] = true; //proceed
        uint256 mintPrice = price(preName);
        //check payment amount
        require(msg.value >= mintPrice, "Not enough paid");
        //joins name of domain into full domain name
        string memory name = string(abi.encodePacked(preName, ".", tld));
        string memory finalSvg = string(abi.encodePacked(svgPartOne, name, svgPartTwo));
        uint256 newRecordId = _tokenIdCounter.current();
        uint256 length = StringUtils.strlen(name);
        string memory strLen = StringsUpgradeable.toString(length);



        //this creates the JSON metadata of the NFT.
     string memory json = Base64Upgradeable.encode(
      abi.encodePacked(
        '{"name": "',
        name,
        '", "description": ".ape domains, fully immutable.", "image": "data:image/svg+xml;base64,',
        Base64Upgradeable.encode(bytes(finalSvg)),
        '","length":"',
        strLen,
        '"}'
      )
    );
 

        string memory finalTokenUri = string(
            abi.encodePacked("data:application/json;base64,", json)
        );

        require(domains[name] == address(0));
        
        domains[name] = msg.sender;
        records[name] = name;

        webLink[name] = name;
        linkedWallet[name] = name;
        text[name] = name;
        _safeMint(msg.sender, newRecordId);
        _setTokenURI(newRecordId, finalTokenUri);
        names[newRecordId] = name;
        _tokenIdCounter.increment();
    }

    //record declarations



    function setRecord(string calldata name, string calldata record) external {
        if (msg.sender != domains[name]) revert Unauthorized();
         records[name] = record;
    }

    function valid(string calldata preName) public pure returns (bool) {
        return
            StringUtils.strlen(preName) >= 1 &&
            StringUtils.strlen(preName) <= 10;
    }

    function getAddress(string calldata name) external view returns (address) {
        return domains[name];
    }

    error Unauthorized();
    error textTooLong();
    error textTooShort();


    function setLinkedWallet(string calldata name, string calldata _linkedWalletInput) external {
        if (msg.sender != domains[name]) revert Unauthorized();
        linkedWallet[name] = _linkedWalletInput;
    }

    function setWebLink(string calldata name, string calldata _webLinkInput) external {
        if (msg.sender != domains[name]) revert Unauthorized();
        webLink[name] = _webLinkInput;
    }

    function setText(string calldata name, string calldata _textInput) external {
        if (msg.sender != domains[name]) revert Unauthorized();
        else if (StringUtils.strlen(_textInput) <= 0) revert textTooShort();
        else if (StringUtils.strlen(_textInput) >= 145) revert textTooLong(); 
        text[name] = _textInput;
    }

    function getText(string calldata name) external view returns (string memory) {
        return text[name];
    }

    function getTotalRegistered() external view returns (uint256) {
        return _tokenIdCounter.current();
    }

    function getLinkedWallet(string calldata name)
        external
        view
        returns (string memory)
    {
        return linkedWallet[name];
    }

    function getWebLink(string calldata name)
        external
        view
        returns (string memory)
    {
        return webLink[name];
    }

    function getTLD()
        external
        view
        returns (string memory)
    {
        return tld;
    }

    function withdraw() external payable onlyOwner {
        uint256 amount = address(this).balance;

        (bool success, ) = msg.sender.call{value: amount}("");
        require(success, "Failed to withdraw funds");
    }
}

