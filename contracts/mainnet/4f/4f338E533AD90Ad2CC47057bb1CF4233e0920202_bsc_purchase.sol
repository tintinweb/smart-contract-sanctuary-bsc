// SPDX-License-Identifier: MIT
pragma solidity >=0.7.0 <0.9.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";

import "./IErc1155Asset.sol";
import "./Proof.sol";

contract bsc_purchase is ERC1155Holder,ERC721Holder{
    
    address public admin;
    address public usdt;
    address public usdt_cet_pair;
    address public  reward;
    address public feeTo;
    uint256 public  price_reward;


    event purchaseEvent(address account,uint256 amount,uint256 price,uint256 time);
    constructor(address usdt_,address reward_){
        admin = msg.sender;
        usdt = usdt_;
        reward = reward_;
    }
    

    modifier timeLimit(uint256 time){
        require( block.timestamp <= time + 300,"error timeStamp");
        _;
    }

  

    modifier owner(address account) {
        require(admin == account,"cyberpop: FORBIDDEN");
        _;
    }

    function setPrice(uint256 amount) external  owner(msg.sender){
        price_reward = amount;
    }


    function purchase(uint256 amount,uint256 price_cet,uint256 timestamp,uint256 chainId,bytes memory signature) external  timeLimit(timestamp) returns(bool) {    
        require(Proof.checkPermissions(admin,signature,amount,price_cet,msg.sender,timestamp,chainId)==true,"You don't get the proof right");
        (bool flag1,uint256 amount_usdt) = SafeMath.tryMul( price_reward , price_cet);
        IERC20(usdt).transferFrom(msg.sender,feeTo, amount_usdt);
        IErc1155Asset(reward).mint(msg.sender,1,amount,"0x");
        emit purchaseEvent(msg.sender,amount,amount_usdt,block.timestamp);
        return  true;
    }

    function setFeeTo(address _feeTo) external  owner(msg.sender){
        require(msg.sender == admin, 'cyberpop: FORBIDDEN');
        feeTo = _feeTo;
    }

    function setFeeToSetter(address _admin) external  owner(msg.sender){
        require(msg.sender == admin, 'cyberpop: FORBIDDEN');
        admin = _admin;
    }

}

