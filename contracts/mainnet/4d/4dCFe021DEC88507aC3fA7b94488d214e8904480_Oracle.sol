    event NewMaxPriceSub(uint oldValue, uint newValue);
    event NewMaxPriceAddPercent(uint oldValue, uint newValue);
    event NewMaxPriceSubPercent(uint oldValue, uint newValue);

    function addMockToken(address _mockToken, address _realToken) external onlyOwner {
        mockTokens[_mockToken] = _realToken;
    }

    function delMockToken(address _mockToken) external onlyOwner {
        delete mockTokens[_mockToken];
    }

    function addPath(address _token, address[] memory _path) external onlyOwner {
        paths[_token] = _path;
    }

    function delPath(address _token) external onlyOwner {
        delete paths[_token];
    }

    function getPath(address _token) external view returns(address[] memory) {
        return paths[_token];
    }

    function currentPrice(address _token) external view returns (uint) {
        if (internalCurrentPrice[_token] != 0) {
            return internalCurrentPrice[_token];
        }
        if (mockTokens[_token] != address(0)) {
            _token = mockTokens[_token];
        }
        return internalCurrentPrice[_token];
    }

    function initialize(address[] memory _tokens, uint[] memory _initPrices, IRouter _router, address _baseToken, address _owner) external initializer {
        require(_tokens.length == _initPrices.length, "illegal length");
        for (uint i = 0; i < _tokens.length; i ++) {
            internalCurrentPrice[_tokens[i]] = _initPrices[i];
            tokens.push(_tokens[i]);
        }
        router = _router;
        baseToken = _baseToken;

        SafeOwnable.__SafeOwnable_init(_owner);
        ReentrancyGuardUpgradeable.__ReentrancyGuard_init();
    }

    function addToken(address _token) external onlyOwner {
        for (uint i = 0; i < tokens.length; i ++) {
            require(tokens[i] != _token, "already exist");
        }
        tokens.push(_token);
    }

    function delToken(address _token) external onlyOwner {
        uint idx = tokens.length;
        for (uint i = 0; i < tokens.length; i ++) {
            if (tokens[i] == _token) {
                idx = i;
                break;
            }
        }
        require(idx < tokens.length, "not exist");
        if (idx != tokens.length - 1) {
            tokens[idx] = tokens[tokens.length - 1];
        }
        tokens.pop();
    }

    function tokensLength() external view returns (uint) {
        return tokens.length;
    }

    function setCurrentPrice(address _token, uint _currentPrice) external onlyOwner {
        emit NewPrice(_token, internalCurrentPrice[_token], _currentPrice);
        internalCurrentPrice[_token] = _currentPrice;
    }

    function setMaxPriceAdd(uint _maxPriceAdd) external onlyOwner {
        emit NewMaxPriceAdd(maxPriceAdd, _maxPriceAdd);
        maxPriceAdd = _maxPriceAdd;
    }

    function setMaxPriceSub(uint _maxPriceSub) external onlyOwner {
        emit NewMaxPriceSub(maxPriceSub, _maxPriceSub);
        maxPriceSub = _maxPriceSub;
    }

    function setMaxPriceAddPercent(uint _maxPriceAddPercent) external onlyOwner {
        emit NewMaxPriceAddPercent(maxPriceAddPercent, _maxPriceAddPercent);
        maxPriceAddPercent = _maxPriceAddPercent;
    }

    function setMaxPriceSubPercent(uint _maxPriceSubPercent) external onlyOwner {
        emit NewMaxPriceSubPercent(maxPriceSubPercent, _maxPriceSubPercent);
        maxPriceSubPercent = _maxPriceSubPercent;
    }

    function updatePrice() external {
        for (uint i = 0; i < tokens.length; i ++) {
            address token = tokens[i];
            address[] memory path = paths[token];
            if (path.length == 0) {
                continue;
            }

            uint[] memory amounts = router.getAmountsOut(1e18, path);
            uint swapPrice = amounts[1];

            if (swapPrice > internalCurrentPrice[token]) {
                uint maxCanChange = MathUpgradeable.min(internalCurrentPrice[token] + maxPriceAdd, internalCurrentPrice[token] + internalCurrentPrice[token] * maxPriceAddPercent / PERCENT_BASE);
                if (swapPrice > maxCanChange) {
                    swapPrice = maxCanChange;
                }
                emit NewPrice(token, internalCurrentPrice[token], swapPrice);
                internalCurrentPrice[token] = swapPrice;
            } else if (swapPrice < internalCurrentPrice[token]) {
                uint cond1 = internalCurrentPrice[token] > maxPriceSub ? internalCurrentPrice[token] - maxPriceSub : 0;
                uint cond2 = internalCurrentPrice[token] > internalCurrentPrice[token] * maxPriceSubPercent / PERCENT_BASE ? internalCurrentPrice[token] - internalCurrentPrice[token] * maxPriceSubPercent / PERCENT_BASE : 0;
                uint maxCanChange = MathUpgradeable.max(cond1, cond2);
                if (swapPrice < maxCanChange) {
                    swapPrice = maxCanChange;
                }
                emit NewPrice(token, internalCurrentPrice[token], swapPrice);
                internalCurrentPrice[token] = swapPrice;
            }
        }
    }
}