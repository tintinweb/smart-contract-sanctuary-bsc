    address public secretSigner = 0xC025B50f3c31E4FcCDd573cB6b2152A89c27c7c0;

    event WithdrawReward(address indexed user, uint amount,uint _burnAmount, uint withdrawRewardId);

    constructor (address _aToken,address MagUsdt) {
        aToken = _aToken;
        MAG_USDT = MagUsdt;
    }

    function setSecretSigner(address _account) external onlyOwner {
        secretSigner = _account;
    }

    function setAToken(address _account) external onlyOwner {
        aToken = _account;
    }

    function setMagUsdt(address _account) external onlyOwner {
        MAG_USDT = _account;
    }

    function setMarketAddress(address _account) external onlyOwner {
        MARKET_ADDRESS = _account;
    }

    function swapExactBNBForTokens(uint _amount) external onlyOwner {
        address[] memory _path = new address[](2);
        _path[0] = WETH_ADDRESS;
        _path[1] = USDT_ADDRESS;
        uint[] memory Uamounts = PancakeLibrary.getAmountsOut(FACTORY, _amount, _path);
        uint UamountOutMin = Uamounts[Uamounts.length - 1];
        IPancakeRouter02(ROUTER).swapExactETHForTokens{value: _amount}(UamountOutMin, _path, address(this), block.timestamp);
    }

    function swapExactTokensForBNB(uint _amount) external onlyOwner {
        address[] memory _path = new address[](2);
        _path[0] = USDT_ADDRESS;
        _path[1] = WETH_ADDRESS;
        uint[] memory Uamounts = PancakeLibrary.getAmountsOut(FACTORY, _amount, _path);
        uint UamountOutMin = Uamounts[Uamounts.length - 1];
        IPancakeRouter02(ROUTER).swapExactTokensForETHSupportingFeeOnTransferTokens(_amount, UamountOutMin, _path, address(this), block.timestamp);
    }

    function getBlockTimestamp() external view returns(uint){
        return block.timestamp;
    }

    function extractExtra(address _constantAddress, address _account,uint _amount) external onlyOwner {
        if(_constantAddress == WETH_ADDRESS){
            payable(_account).transfer(_amount);
        }else{
            IERC20(_constantAddress).safeTransfer(_account, _amount);
        }
        
    }

    function getUamounts(uint _amount,address _to) internal{
        address[] memory _path = new address[](2);
        _path[0] = WETH_ADDRESS;
        _path[1] = USDT_ADDRESS;
        uint[] memory Uamounts = PancakeLibrary.getAmountsIn(FACTORY, _amount, _path);
        uint UamountOutMin = Uamounts[0];
        IPancakeRouter02(ROUTER).swapETHForExactTokens{value: UamountOutMin}(_amount, _path, _to, block.timestamp);
    }


    function getUburnAmounts(uint _amount) internal {
        address[] memory _path = new address[](2);
        _path[0] = MAG_USDT;
        _path[1] = aToken;
        uint[] memory Uamounts = PancakeLibrary.getAmountsOut(FACTORY, _amount, _path);
        uint UburnAmountMin = Uamounts[Uamounts.length - 1];
        IPancakeRouter02(ROUTER).swapExactTokensForTokensSupportingFeeOnTransferTokens(_amount, UburnAmountMin, _path, BURN_ADDRESS, block.timestamp);
    }


    
    function withdrawReward(uint _withdrawRewardId, address _to, uint _amount,uint _burnAmount,uint withdrawTimestamp, uint8 _v, bytes32 _r, bytes32 _s) external {
        require(!withdrawRewardIdOf[_withdrawRewardId], "withdrawReward: invalid withdrawRewardId");
        require(address(0) != _to, "withdrawReward: address(0) != _to");
        require(0 < _amount, "withdrawReward: 0 < _amount");
        require(address(0) != secretSigner, "withdrawReward: address(0) != secretSigner");
        uint failureTimestamp=block.timestamp - withdrawTimestamp;
        require(failureTimestamp < 1000, "withdrawReward: have expired withdrawTimestamp");
        bytes32 msgHash = keccak256(abi.encodePacked(_withdrawRewardId, _to, _amount,_burnAmount,withdrawTimestamp));
        require(ecrecover(msgHash, _v, _r, _s) == secretSigner, "withdrawReward: incorrect signer");
        getUamounts(_amount,_to);
        getUamounts(_burnAmount,MARKET_ADDRESS);
        getUburnAmounts(_burnAmount);
        
        withdrawRewardOf[_to] += _amount;
        totalWithdrawReward += _amount;
       
        withdrawRewardAdrOf[_withdrawRewardId] = msg.sender;
        withdrawRewardIdOf[_withdrawRewardId] = true;
        emit WithdrawReward(_to,_amount,_burnAmount, _withdrawRewardId);

    }

    receive() external payable {}
    fallback() external {}

}