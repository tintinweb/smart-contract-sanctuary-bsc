contract BuyAndBurn is Ownable, ReentrancyGuard {
    address public presaleToken;
    IUniswapV2Router02 public uniswapV2Router;

    constructor(address initialToken, address initialRouter) {
        presaleToken = initialToken;
        uniswapV2Router = IUniswapV2Router02(initialRouter);
    }

    // allow receiving ETH from uniswapV2Router after swapping
    receive() external payable {
        _swapEthForTokensAndBurn(msg.value);
    }

    fallback() external payable {
        _swapEthForTokensAndBurn(msg.value);
    }

    function setRouter(address newRouter) onlyOwner external {
        uniswapV2Router = IUniswapV2Router02(newRouter);
    }

    function setToken(address newToken) onlyOwner external {
        presaleToken = newToken;
    }

    function swapEthForTokensAndBurn() external payable nonReentrant {
        _swapEthForTokensAndBurn(msg.value);
    }

    function _swapEthForTokensAndBurn(uint256 amount) private nonReentrant {
        require(presaleToken != address(0), "TOKEN_NOT_SET");
        require(address(uniswapV2Router) != address(0), "ROUTER_NOT_SET");

        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = presaleToken;
        path[1] = uniswapV2Router.WETH();

        // make the swap
        uniswapV2Router.swapExactETHForTokens{value: amount}(
            0,
            path,
            0x000000000000000000000000000000000000dEaD,
            block.timestamp
        );
    }
}