contract CR is ERC20Burnable {

    //PancakeRouter 0x10ED43C718714eb63d5aA57B78B54704E256024E

    mapping(address => bool) private admin;
    uint256 public _swapRate_sell_to_burn = 3;
    uint256 public _swapRate_sell_to_NFT = 6;
    uint256 public _swapRate_slippage = 150;
    uint256 public _swapRate_sell_to_splitter = 6;

    address public _swapRate_sell_to_NFT_address;
    address private _swapRate_sell_to_splitter_address;
    address public _CR_LP_USDT_address;

    address private _CristianoRonaldo_address;

    mapping(address => bool) private _isSwapInclude;
    mapping(address => bool) private _isSwap_LP_Include;
    mapping(address => bool) private _special_list;


    uint256 private _startTime;


    address public _pancake_Router = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    //address public _pancake_Router = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3;

    address private _USDT_address = address(0x55d398326f99059fF775485246999027B3197955);
    //address private _USDT_address = address(0x803a3e2bfC656d971bFA0dbF4353BaBD9D4D6d5e);

    IPancakeRouter02 public _pancake_V2Router02;
    IPancakePair public _pancake_V2Pair;

    uint256 private _minSwapNumber = 10 * 10 ** 18;
    uint256 private _remainSwapNumber;

    mapping(address => bool) private _is_Contract;

    uint256 private _compound_Interest_rate = 21;

    mapping(address => uint256) private _compound_Interest_timestamp;


    uint256 private _one_day_second = 24 * 60 * 60;


    uint256[10] private _CR_referral_rate = [40, 30, 20, 5, 20, 5, 5, 10, 5, 10];

    mapping(address => uint256) private _compound_received;


    address private _IFriendsStorage_address;

    uint256 private _can_burn_swap_number;

    uint256 private _min_compound_interest_number;
    uint256 public _end_IDO_time;
    uint256 public _end_compound_interest_time;
    constructor(
        string memory name,
        string memory symbol
    ) ERC20(name, symbol) {
        admin[_msgSender()] = true;
        _special_list[_msgSender()] = true;
        _startTime = block.timestamp + 356 days;

        _pancake_V2Router02 = IPancakeRouter02(_pancake_Router);
        _CR_LP_USDT_address = IPancakeFactory(_pancake_V2Router02.factory()).createPair(
            address(this),
            _USDT_address
        );
        _isSwapInclude[_pancake_Router] = true;
        _isSwap_LP_Include[_CR_LP_USDT_address] = true;
        _special_list[_msgSender()] = true;
        _pancake_V2Pair = IPancakePair(_CR_LP_USDT_address);
        _min_compound_interest_number = 20000 * 1 * 10 ** 18;
        _end_IDO_time = block.timestamp + 20 days;
        _end_compound_interest_time = block.timestamp + 118 days;
    }

    function join_mint(address to, uint256 amount) external {
        require(isContract(_msgSender()), "Only contract can mint");
        require(_is_Contract[_msgSender()], "Only Cristiano Ronaldo can mint");
        if (block.timestamp < _end_IDO_time) {
            if (_compound_Interest_timestamp[to] > 0) {
                _calculate_Compound_Interest(to);
            } else {
                _compound_Interest_timestamp[to] = block.timestamp;
            }
            _mint(to, amount);
        }
    }

    function can_burn_ss(address to, uint256 amount) external {
        require(isContract(_msgSender()), "Only contract can burn");
        require(_is_Contract[_msgSender()], "Only Cristiano Ronaldo can burn");
        _burn(to, amount);
    }


    function _transfer(address from, address to, uint256 amount) internal override {

        if (_special_list[from] || _special_list[to]) {
            super._transfer(from, to, amount);
            return;
        } else if (_isSwap_LP_Include[from] && !_isSwapInclude[to]) {
            require(_startTime < block.timestamp, "Swap is not started");
            //buy
            uint256 amount_slippage = amount * _swapRate_slippage / 1000;
            distribute_CR(from, to, amount_slippage);
            super._transfer(from, to, amount - amount_slippage);
            if (_compound_Interest_timestamp[to] > 0) {
                _calculate_Compound_Interest(to);
            } else {
                _compound_Interest_timestamp[to] = block.timestamp;
            }
            return;
        } else if (_isSwap_LP_Include[to] && !_isSwapInclude[from]) {
            require(_startTime < block.timestamp, "Swap is not started");
            _sell(amount, from, to);
            return;
        }

        else {
            _calculate_Compound_Interest(to);
            _sell(amount, from, to);
            return;
        }
    }

    function _sell(uint amount, address from, address to) internal {
        uint256 amount_burn = amount * _swapRate_sell_to_burn / 100;
        uint256 amount_NFT = amount * _swapRate_sell_to_NFT / 100;
        uint256 amount_splitter = amount * _swapRate_sell_to_splitter / 100;
        _calculate_Compound_Interest(from);
        super._burn(from, amount_burn);
        super._transfer(from, _swapRate_sell_to_NFT_address, amount_NFT);
        super._transfer(from, _swapRate_sell_to_splitter_address, amount_splitter);
        super._transfer(from, to, amount - amount_burn - amount_NFT - amount_splitter);
    }

    function calculate_compound() external {
        _calculate_Compound_Interest(_msgSender());
    }


    function _calculate_Compound_Interest(address _user) internal {
        uint256 i = _C_C_I(_user);
        _compound_Interest_timestamp[_user] = block.timestamp;
        if (i > 0) {
            super._mint(_user, i);
            _compound_received[_user] += i;
        }

    }

    function _C_C_I(address _user) public view returns (uint256) {
        uint256 t = _compound_Interest_timestamp[_user];
        uint256 b = this.balanceOf(_user);
        if (b > _min_compound_interest_number && _end_compound_interest_time > block.timestamp) {

            if (block.timestamp - t > _one_day_second) {
                uint256 s = block.timestamp - t;
                uint256 times = s / _one_day_second;
                uint256 l = b * (1000 + _compound_Interest_rate) ^ times;
                uint256 r = l / 1000 ^ times;
                uint256 remainder = block.timestamp % _one_day_second;
                uint256 interest = r - b + b * _compound_Interest_rate * remainder / _one_day_second / 1000;
                return interest;

            }
            else
            {
                uint256 remainder = block.timestamp - t;
                return b * _compound_Interest_rate * remainder / _one_day_second / 1000;
            }
        } else
        {
            return 0;
        }
    }

    function distribute_CR(address from, address to, uint256 amount) internal {
        uint256 part = amount / 15;
        uint256 c;
        for (uint256 i = 0; i < 10; i++) {
            uint256 _amount_CR;
            if (i == 9) {
                _amount_CR = amount - c;
            } else {
                _amount_CR = part * _CR_referral_rate[i] / 10;
                c += _amount_CR;
            }
            address re = IFriendsStorage(_IFriendsStorage_address).getFriendByLevel(to, i);
            if (re != address(0)) {
                super._transfer(from, re, _amount_CR);
            } else
            {
                super._burn(from, _amount_CR);
            }
        }
    }

    function isContract(address account) internal view returns (bool) {
        return account.code.length > 0;
    }


    function set_swapRate_sell_to_NFT_address(address _address) external {
        require(admin[_msgSender()], "Only admin can set swapRate_to_NFT_address");
        _swapRate_sell_to_NFT_address = _address;
    }

    function set_swapRate_sell_to_splitter_address(address _address) external {
        require(admin[_msgSender()], "Only admin can set NFT_to_splitter_address");
        _swapRate_sell_to_splitter_address = _address;
    }

    function set_isSwapIncludes(address _address) external {
        require(admin[_msgSender()], "Only admin can set isSwapIncludes");
        _isSwapInclude[_address] = true;
    }

    function set_isSwap_LP_Includes(address _address, bool flag) external {
        require(admin[_msgSender()], "Only admin can set isSwap_LP_Includes");
        _isSwap_LP_Include[_address] = flag;
    }

    function set_special_list(address _address, bool flag) external {
        require(admin[_msgSender()], "Only admin can set special_list");
        _special_list[_address] = flag;
    }

    function set_startTime(uint256 _time) external {
        require(admin[_msgSender()], "Only admin can set startTime");
        _startTime = _time;
    }

    function set_CR_LP_USDT_address(address _address) external {
        require(admin[_msgSender()], "Only admin can set YBC_LP_USDT_address");
        _CR_LP_USDT_address = _address;
    }

    function set_CristianoRonaldo_address(address _address, bool flag) external {
        require(admin[_msgSender()], "Only admin can set YBC_LP_USDT_address");
        _CristianoRonaldo_address = _address;
        _is_Contract[_CristianoRonaldo_address] = flag;
    }

    function set_admin(address _address, bool flag) external {
        require(admin[_msgSender()], "Only admin can set admin");
        admin[_address] = flag;
    }

    function set_is_Contract(address _address, bool flag) external {
        require(admin[_msgSender()], "Only admin can set is_Contract");
        _is_Contract[_address] = flag;
    }

    function get_compound_Interest_timestamp(address _address) public view returns (uint256) {
        return _compound_Interest_timestamp[_address];
    }

    function update_CR_referral_rate(uint256 _rate, uint256 k) external {
        require(admin[_msgSender()], "Only admin can update CR_referral_rate");
        _CR_referral_rate[k] = _rate;
    }

    function set_IFriendsStorage_address(address _address) external {
        require(admin[_msgSender()], "Only admin can set IFriendsStorage_address");
        _IFriendsStorage_address = _address;
    }

    function get_compound_received(address _address) public view returns (uint256) {
        return _compound_received[_address];
    }

    function set_min_compound_interest_number(uint256 _number) external {
        require(admin[_msgSender()], "Only admin can set min_compound_interest_number");
        _min_compound_interest_number = _number;
    }

    function set_end_IDO_time(uint256 _time) external {
        require(admin[_msgSender()], "Only admin can set end_IDO_time");
        _end_IDO_time = _time;
    }
    
    function set_end_compound_interest_time(uint256 _time) external {
        require(admin[_msgSender()], "Only admin can set end_compound_interest_time");
        _end_compound_interest_time = _time;
    }
}