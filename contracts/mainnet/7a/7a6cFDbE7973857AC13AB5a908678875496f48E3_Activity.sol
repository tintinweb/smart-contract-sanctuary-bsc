        baseURI = uri;
    }

    /*========== view ===============*/

    function getSlotDetail(uint256 slot_)
        public
        view
        returns (SlotDetail memory)
    {
        return _slotDetails[slot_];
    }
    function _baseURI() internal view virtual override  returns (string memory) {
        return baseURI;
    }
   
    /**
     * @dev Generate the value of slot by utilizing keccak256 algorithm to calculate the hash
     * value of multi properties.
     */
    function _getSlot(
        string memory slotName_,
        uint8 vestingType_
    ) internal pure virtual returns (uint256 slot_) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        keccak256(bytes(slotName_)),
                        vestingType_
                    )
                )
            );
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC3525,AccessControl) returns (bool)
    {
        return
            interfaceId == type(IERC3525).interfaceId ||
            interfaceId == type(IERC3525Metadata).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function getAddressNFTs(address addr) external view returns(uint256[] memory infos) {
        uint256 total =  balanceOf(addr);
        infos = new uint256[](total);
        if (total > 0) {
            for (uint256 i=0; i < total; i++) {
                uint256  tokenID = tokenOfOwnerByIndex(addr, i);
                infos[i] = tokenID;
            }
        }
    }

    function exists(uint256 tokenId) external view returns(bool){
        return _exists(tokenId);
    }
}

// File: contracts/Activity.sol



pragma solidity ^0.8.0;





contract Activity is AccessControl{
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIdCounter;
    bytes32 public root;
    AtomNFT public nft;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    address private _feeAddress = 0x7D0b249e5Fe411c0bFB3D9b6a2e3e414c8322375;
    address private _erc20;
    mapping(uint256=>SoltAction) private _soltInfo;
    mapping(uint256=>bool) private _soltInfoAction;
    mapping(address=>uint256) public mintNum;
    SoltDetail public soltDetail;
    uint256 public maxMin=5;
    uint256 public wMaxMin=20;
    uint256 public startTime;
    uint256 public totalFrgs;
    uint256 public baseNum = 10**18;
    struct SoltAction {
        uint256 price;
        uint256 number;
        uint256 shellNum;
    }

    struct SoltDetail{
        string name;
        string description;
        uint8 vestingType;
        uint256 price;
        uint256 topPrice;
        uint256 cycleTime;
        uint256 upBase;
    }
  

    constructor() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        soltDetail = SoltDetail("Fragment","Mining machine fragment",1,baseNum,100*baseNum,10,1000);
    }

    event SetFeeAddress(address indexed sener,address oldFeeAddress,address newFeeAddress,uint256 createTime);
    event AddSoltSellInfo(address indexed sener,uint256 solt,SoltAction slotAction,uint256 createTime);
    event EditSoltSell(address indexed sener,uint256 indexed solt,string ftion,SoltAction slotAction,uint256 createTime);
    event SetTokenAddress(address indexed sener,address oldTokenAddress,address newTokenAddress,uint256 createTime);
    event SetNftAddress(address indexed sener,address oldTokenAddress,address newTokenAddress,uint256 createTime);
    event SetMerkleRoot(address indexed sender,bytes32 root,bytes32 oldRoot);
    /* =================================== Mutable Functions START ================================ */
    function setFeeAddress(address _feeAddr) public onlyRole(MINTER_ROLE){
        require(_feeAddress!=_feeAddr,"Invalid operation");
        emit SetFeeAddress(_msgSender(),_feeAddress,_feeAddr,block.timestamp);
        _feeAddress = _feeAddr;
    }

    function addSoltSellInfo(uint256 solt,SoltAction calldata soltAtion) public onlyRole(MINTER_ROLE){
        require(!_soltInfoAction[solt],"solt already exists");
        _soltInfo[solt] = soltAtion;
        _soltInfoAction[solt] = true;
        emit AddSoltSellInfo(_msgSender(),solt,soltAtion,block.timestamp);
    }

    function editSoltActionPrice(uint256 solt,uint256 price) public onlyRole(MINTER_ROLE){
        require(_soltInfoAction[solt],"solt doesn't exist");
        _soltInfo[solt].price = price;
        emit EditSoltSell(_msgSender(),solt,"editPrice",_soltInfo[solt],block.timestamp);
    }

    function editSoltActionNumber(uint256 solt,uint256 number) public onlyRole(MINTER_ROLE){
        require(_soltInfoAction[solt],"solt doesn't exist");
        require(_soltInfo[solt].shellNum<=number,"Cannot be less than the quantity sold");
        _soltInfo[solt].number = number;
        emit EditSoltSell(_msgSender(),solt,"editNumber",_soltInfo[solt],block.timestamp);
    }

    function setErc20Token(address token) public onlyRole(MINTER_ROLE){
        require(token!=address(0),"erc20 token is not zero address");
        emit SetTokenAddress(_msgSender(),_erc20,token,block.timestamp);
        _erc20 = token;
    }

    function setNFT(address _nft) public onlyRole(MINTER_ROLE){
        require(_nft!=address(0),"nft  is not zero address");
        emit SetNftAddress(_msgSender(),address(nft),_nft,block.timestamp);
        nft = AtomNFT(_nft);
    }


    function setMerkleRoot(bytes32 _root) external onlyRole(MINTER_ROLE) {
        require(_root!=root,"There is no change");
        emit SetMerkleRoot(_msgSender(),_root,root);
        root = _root;
    }

    function mint(uint256 number,bytes32[] calldata proofs) public{
        require(_erc20!=address(0),"No receive address is set");
        require(number>0,"must >0");
        require(startTime<=block.timestamp,"not time start");
        uint256 remaining = getRmNumber();
        require(remaining>=number,"Exceed the upper limit of this round");
        bytes32 _leaf = keccak256(abi.encodePacked(_msgSender()));
        bool _verify = MerkleProof.verify(proofs, root, _leaf);
        if (_verify){
            require(mintNum[_msgSender()]+number<=wMaxMin,"A maximum of 20 castings per address");
        }else{
            require(mintNum[_msgSender()]+number<=maxMin,"A maximum of 5 castings per address");
        }
        
        if(remaining-number==0){
            startTime = block.timestamp+soltDetail.cycleTime;
        }
        mintNum[_msgSender()] = mintNum[_msgSender()]+number;
        totalFrgs+=number;
        IERC20(_erc20).safeTransferFrom(_msgSender(),_feeAddress,number*getPrice());
        uint256 tokenId = IERC20(_erc20).totalSupply()+1;
        nft.mint(soltDetail.name,soltDetail.description,tokenId,_msgSender(),soltDetail.vestingType,number);
    }

    /* ====================================== View Functions  ================================ */
     function getPrice() public view returns(uint256 price){
         price =  soltDetail.price+totalFrgs/soltDetail.upBase*baseNum;
         if(price>soltDetail.topPrice){
             price = soltDetail.topPrice;
         }
     }

     function getRmNumber() public view returns(uint256 remaining){
        remaining = soltDetail.upBase;
        if(totalFrgs!=0){
            remaining = soltDetail.upBase - (totalFrgs - totalFrgs/soltDetail.upBase*soltDetail.upBase);
            if(totalFrgs==totalFrgs/soltDetail.upBase*soltDetail.upBase){
                remaining = soltDetail.upBase;
            }
        }
    }
}