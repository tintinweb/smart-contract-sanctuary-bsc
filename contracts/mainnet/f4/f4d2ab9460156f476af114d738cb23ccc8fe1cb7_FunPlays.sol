    modifier onlyFarmField() {
        require(_msgSender() == farmField, "funplay: Caller is not the farm field");
        _;
    }

    modifier onlyCombatField() {
        require(_msgSender() == combatField, "funplay: Caller is not the combat field");
        _;
    }
    
    constructor() ERC20("FunPlays", "FPL") {}    
    
    function setTransferFeeRate(uint256 _sellFeeRate, uint256 _buyFeeRate) public onlyOwner returns (bool) {
        require(_sellFeeRate + _buyFeeRate <= 100, "funplay: Fee can not set above 10%");
        sellFeeRate = _sellFeeRate;
        buyFeeRate = _buyFeeRate;
        return true;
    }

    function setFeeCap(uint256 _amount) public onlyOwner returns (bool) {
        require(_amount <= maxSupply.div(10**18).div(1000).mul(20), "funplay: Cannot fee more than 2% of the supply at once!");
        tokenForMaintenance = _amount.mul(10**18);
        return true;
    }

    function setCombatField(address _combatField) public onlyOwner returns (bool) {
        require(_combatField != combatField, "funplay: This address is current combat field");
        combatField = _combatField;
        return true;
    }

    function setFarmField(address _farmField) public onlyOwner returns (bool) {
        require(_farmField != farmField, "funplay: This address is current farm field");
        farmField = _farmField;
        return true;
    }

    function updateOperationWallet(address _operationWallet) public onlyOwner returns (bool) {
        require(_operationWallet != operationWallet, "funplay: This address is current operation address");
        require(_operationWallet != address(0), "funplay: 0x is not accepted here");
        operationWallet = _operationWallet;
        return true;
    }

    function farmReward(address recipient, uint256 _amount) external onlyFarmField {
        require(capFarm > farmRewarded, "funplay: Over cap farm reward");
        require(recipient != address(0), "funplay: 0x is not accepted here");
        require(_amount > 0, "funplay: Can not reward 0 token");

        uint256 prevRewarded = farmRewarded;
        farmRewarded = farmRewarded.add(_amount);
        if (farmRewarded <= capFarm) {
            _mint(recipient, _amount);
        }
        else {
            uint256 availableReward = capFarm.sub(prevRewarded);
            _mint(recipient, availableReward);
            farmRewarded = capFarm;
        }
    }

    function combatReward(address recipient, uint256 _amount) external onlyCombatField {
        require(capCombat > combatRewarded, "funplay: Over cap combat reward");
        require(recipient != address(0), "funplay: 0x is not accepted here");
        require(_amount > 0, "funplay: Can not reward 0 token");

        uint256 prevRewarded = combatRewarded;
        combatRewarded = combatRewarded.add(_amount);
        if (combatRewarded <= capCombat) {
            _mint(recipient, _amount);
        }
        else {
            uint256 availableReward = capCombat.sub(prevRewarded);
            _mint(recipient, availableReward);
            combatRewarded = capCombat;
        }
    }
    
    function rescueERC20(address _token_address) public onlyOwner returns (bool) {
        IERC20 _token = IERC20(_token_address);
        _token.transfer(_msgSender(), _token.balanceOf(address(this)));
        return true;
    }

    function rescueETH() public onlyOwner returns (bool) {
        payable(_msgSender()).transfer(address(this).balance);
        return true;
    }
}

/**
 * @notice implement ERC20
 */
contract FunPlays is FunPlaysERC20 {
    using SafeMath for uint256;

    mapping(address => bool) public _feeExempt;

    IUniswapV2Router02 public immutable uniswapV2Router;
    address public uniswapV2Pair;
    uint256 public liquidTime = 0;
    uint256 public deployTime = 0;

    bool inSwap = false;

    modifier swaping() {
        inSwap = true;
        _;
        inSwap = false;
    }

    constructor() {
        uint256 _mintAmount = maxSupply.sub(capFarm).sub(capCombat);
        _mint(msg.sender, (_mintAmount));

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);

        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;

        _feeExempt[address(uniswapV2Router)] = true;
        _feeExempt[msg.sender] = true;
        _feeExempt[operationWallet] = true;

        deployTime = block.timestamp;
    }


    function _transfer(address from, address to, uint256 amount) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(balanceOf(from) >= amount, "ERC20: transfer amount exceeds balance");

        if ((from == uniswapV2Pair || to == uniswapV2Pair) && !inSwap) {
            if (liquidTime == 0) {
                require(_feeExempt[from], "funplay: Only liquidity setter can do this transaction");
                liquidTime = (block.timestamp).add(10);
            }

            if (from != uniswapV2Pair) {
                if (balanceOf(address(this)) >= tokenForMaintenance) {
                    _ethSwap(tokenForMaintenance);
                }
            }

            uint256 transferAmount = takeFee(from, to, amount);
            super._transfer(from, to, transferAmount);
        } 
        else {
            super._transfer(from, to, amount);
        }
    }

    function takeFee(address from, address to, uint256 amount) internal returns (uint256) {
        uint256 transferAmount;
        if (_feeExempt[from] || _feeExempt[to]) {
            transferAmount = amount;
        }
        else {
            uint256 transferFeeRate = to == uniswapV2Pair
                ? sellFeeRate
                : (from == uniswapV2Pair ? buyFeeRate : 0);

            if (block.timestamp < liquidTime.add(130)) {
                transferFeeRate = 500;
            } else {
                require(liquidTime > deployTime, "funplay: Anti bot front run");
            }

            uint256 fee = amount.mul(transferFeeRate).div(1000);
            transferAmount = amount.sub(fee);
            super._transfer(from, address(this), (fee));
        }

        return transferAmount;
    }
    
    function setFeeExempt(address _address) external onlyOwner {
        require(!_feeExempt[_address], "funplay: Fee exempted");
        _feeExempt[_address] = true;
    }
    
    function _ethSwap(uint256 amount) private swaping {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), amount);

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amount,
            0,
            path,
            operationWallet,
            (block.timestamp).add(180)
        );
    }

    receive() external payable {}
}