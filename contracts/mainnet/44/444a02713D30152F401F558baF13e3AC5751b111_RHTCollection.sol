contract RHTCollection is ERC1155URIStorage, Ownable {

    constructor(IBEP20 _busd) ERC1155("")
    {busd = _busd;}
    /*
    * -------------------------------------------
    * |               Address                   |
    * -------------------------------------------
    */
    IBEP20 public busd;
    address public OwnerContract;
    /*
    * -------------------------------------------
    * |               Numbers                   |
    * -------------------------------------------
    */
    uint256 private decimals = 10**18;
    uint256 private limitClassic = 2;
    uint256 private limitPremium = 3;
    uint256 private valueClassic = 40 * decimals ;
    uint256 private valuePremium = 500 * decimals;
    /*
    * -------------------------------------------
    * |               Mapping                   |
    * -------------------------------------------
    */
    mapping (uint256 => NFTs) public getNFT;
    mapping (address => Counters) public tokenID;
    mapping (address => QuantityMint) public mintersID;
    /*
    * -------------------------------------------
    * |               Structs                   |
    * -------------------------------------------
    */
    struct NFTs {
        // Id da NFT
        uint256 idNFT;
        // Raridade da NFT
        uint256 typeRarity;
        // Comprador da NFT
        address payable ownerNFT;
        // Armazena Booleano
        bool isUser;
    }
    struct Counters {
        uint256 counterID;
    }
    struct QuantityMint {
        uint256 quantityClassic;
        uint256 quantityPremium;
    }
    /*
    * -------------------------------------------
    * |               Events                    |
    * -------------------------------------------
    */
    event MintNFT(address indexed from, address indexed to, uint256 indexed id, uint256 amount);
    event TransferNFT(address indexed from, address indexed to, uint256 indexed id);
    event UpdateAddress(address indexed newWallet);
    /*
    * -------------------------------------------
    * |  Private/InternalExternal/Public/View   |
    * -------------------------------------------
    */
    // Gera ID NFT
    function increment() private returns(uint256){
        Counters storage count = tokenID[address(this)];
        count.counterID += 1;
        return count.counterID ;
    }
    // Verifica ultima ID
    function getIDNFT() private view returns(uint256) {
        Counters storage count = tokenID[address(this)];
        return count.counterID;
    }
    // Contador Classico
    function countClassicMint() internal returns(uint256) {
        QuantityMint storage count = mintersID[address(this)];
        require(count.quantityClassic < limitClassic, "RHT: can't mint new nfts");
        count.quantityClassic += 1;
        uint256 counter = count.quantityClassic;
        return counter;
    }
    // Contador Premium
    function countPremiumMint() internal returns(uint256) {
        QuantityMint storage count = mintersID[address(this)];
        require(count.quantityPremium < limitPremium, "RHT: can't mint new nfts");
        count.quantityPremium += 1;
        uint256 counter = count.quantityPremium;
        return counter;
    }
    // Verifica qual NFT foi escolhida
    function generateRarity(uint256 newID, uint256 amount) private {
        require(amount == valueClassic || amount == valuePremium, "RHT: You need to set the price being charged");
        NFTs storage nft = getNFT[newID];
        if(amount == valueClassic) {
                countClassicMint();
                nft.typeRarity = 1;
                IBEP20(busd).transferFrom(_msgSender(), OwnerContract, amount);
        }
        else if (amount == valuePremium) {
                countPremiumMint();
                nft.typeRarity = 2;
                IBEP20(busd).transferFrom(_msgSender(), OwnerContract, amount);
        }
    }
    // Visualiza NFT
    function fetchMyItems() public view returns(NFTs[] memory) {
        uint256 totalItemCount = getIDNFT();
        uint256 itemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < totalItemCount; i++) {
            if (getNFT[i + 1].ownerNFT == _msgSender()) {
            itemCount += 1;
            }
        }

        NFTs[] memory items = new NFTs[](itemCount);
        for (uint256 i = 0; i < totalItemCount; i++) {
            if (getNFT[i + 1].ownerNFT == _msgSender()) {
            uint256 currentId = i + 1;
            NFTs storage currentItem = getNFT[currentId];
            items[currentIndex] = currentItem;
            currentIndex += 1;
            }
        }
        return items;
    }
    /*
    * -------------------------------------------
    * |         External/Public                 |
    * -------------------------------------------
    */
    // Mint uma nova NFT
    function createNFT(string memory tokenURI, uint256 amount) external returns(uint256 getID) {
        uint256 newID = increment();
        // Storage NFT
        NFTs storage nft = getNFT[newID];
        // Armazena ID
        nft.idNFT = newID;
        // Armazena Owner NFT
        nft.ownerNFT = payable(_msgSender());
        // Verifica se é True
        nft.isUser = true;
        // Armazena o Preço NFT
        generateRarity(newID, amount);
        // Gera os Metadados
        _setURI(newID, tokenURI);
        // Mint NFT
        _mint(_msgSender(), newID, 1, "");
        // Emite Evento
        emit MintNFT(address(0), _msgSender(), newID, amount);
        // Retorna ID
        return newID;
    }
    // Transferir NFT
    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public override {
        require(
            from == _msgSender() || isApprovedForAll(from, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        NFTs storage nft = getNFT[id];
        require(nft.ownerNFT == from, "RHT: Need Owner NFT");
        nft.ownerNFT = payable(to);
        //Transfere de forma Segura a NFT para o contrato de NFTs Marketplace
        _safeTransferFrom(from, to, id, amount, data);
        // Emite um evento de Transferencia
        emit TransferNFT(from, to, id);
    }
    // Atualiza URI de uma NFT
    function updateURI(uint256 id, string memory tokenURI) external onlyOwner {
        _setURI(id, tokenURI);
    }
    // Atualiza o Limite de Mint
    function updateLimitMint(uint256 _limitClassic, uint256 _limitPremium) external onlyOwner {
        limitClassic = _limitClassic;
        limitPremium = _limitPremium;
    }
    // Atualiza o Preço de Venda
    function updatePriceNFT(uint256 _valueClassic, uint256 _valuePremium) external onlyOwner {
        valueClassic = _valueClassic * decimals;
        valuePremium = _valuePremium * decimals;
    }
    // Atualiza Token e Wallet Marketing
    function updateTokenWallet(IBEP20 _token, address _wallet) external onlyOwner {
        busd = _token;
        OwnerContract = _wallet;
        // Emite um Evento
        emit UpdateAddress(OwnerContract);
    }
    // Remove BUSD Manualmente do Contrato
    function removeBUSD(uint256 amount) external onlyOwner {
        IBEP20(busd).transfer(OwnerContract, amount);
    }
}