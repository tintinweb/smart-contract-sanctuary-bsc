contract MSPToken is IERC20, Ownable {
    using SafeMath for uint256;
    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowances;

    address public lpReward;
    address public poolBack;
    address public eFund;
    address public blackHole;
    address public nftReward;

    IERC20 public usdt;

    string private _name = "MSP";
    string private _symbol = "MSP";
    uint8 private _decimals = 8;
    uint256 private _tTotal = 13000 * 10 ** _decimals;

    uint256[6] priceStage = [0, 2000 * 10 ** 18,4000 * 10 ** 18, 6000 * 10 ** 18, 8000 * 10 ** 18, 10000 * 10 ** 18];
    uint256[6] buyFees = [600, 500, 400, 300, 200, 100];
    uint256[6] sellFees = [1500, 1200, 900, 600, 300, 200];

    uint256 public buyFee = buyFees[0];
    uint256 public sellFee = sellFees[0];

    uint256 public lpRewardFee = 5000;
    uint256 public poolBackFee = 1000;
    uint256 public eFundFee = 2000;
    uint256 public blackHoleFee = 1000;
    uint256 public nftRewardFee = 1000;

    uint256 public offset = 0 * 3600;

    uint256 public INTERVAL = 24 * 60 * 60;
    uint256 public _protectionT;
    uint256 public todayInitPrice;

    uint256 public feeDenominator = 10000;

    uint256 public blackAmountLimit = 6500 * 10 ** _decimals;


    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;

    bool inSwapAndLiquify;

    constructor(
        address _router, IERC20 _usdt, 
        address _lpRewardAddress,
        address _poolBackAddress,
        address _eFundAddress,
        address _blackHoleAddress,
        address _nftRewardAddress,
        address _recvAddress
        ) {

        usdt = _usdt;

        lpReward = _lpRewardAddress;
        poolBack = _poolBackAddress;
        eFund = _eFundAddress;
        blackHole = _blackHoleAddress;
        nftReward = _nftRewardAddress;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(_router);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), address(_usdt));

        uniswapV2Router = _uniswapV2Router;


        _tOwned[_recvAddress] = _tTotal;
        emit Transfer(address(0),_recvAddress, _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint256) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _tOwned[account];
    }

    function transfer(address recipient, uint256 amount)
    public
    override
    returns (bool)
    {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
    public
    view
    override
    returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
    public
    override
    returns (bool)
    {
        _approve(msg.sender, spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            msg.sender,
            _allowances[sender][msg.sender].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
    public
    virtual
    returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
    public
    virtual
    returns (bool)
    {
        _approve(
            msg.sender,
            spender,
            _allowances[msg.sender][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }




    function rescueToken(
        address token,
        address recipient,
        uint256 amount
    ) public onlyOwner {
        IERC20(token).transfer(recipient, amount);
    }


    // private function
    function _resetProtection() private {
        if (block.timestamp.sub(_protectionT) >= INTERVAL) {
            uint256 current = getCurrentPrice();
            if (current >0 ){
            uint256 today = block.timestamp - (block.timestamp + offset) % 1 days;
            _protectionT = today;
            todayInitPrice = current;
            }
        }
    }
    

    function _mint(address account, uint256 amount) private {
        require(account != address(0), "BEP20: mint to the zero address");
        _tTotal = _tTotal.add(amount);
        _tOwned[account] = _tOwned[account].add(amount);
        emit Transfer(address(0), account, amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        _resetProtection();
        _tokenTransfer(from, to, amount);
    }


    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 amount
    ) private {
        _tOwned[sender] = _tOwned[sender].sub(amount);

        uint256 currentPrice = getCurrentPrice();
        for (uint256 i = priceStage.length - 1; i >= 0; i--) {
            if (currentPrice > priceStage[i]) {
                buyFee = SafeMath.min(buyFee, buyFees[i]);
                sellFee = SafeMath.min(sellFee, sellFees[i]);
                break;
            }
            if (i == 0) {
                break;
            }
        }

        uint256 taxFee = 0;
        uint256 sellProFee = getSellProtectFee(currentPrice);

        if (sender == uniswapV2Pair) {
            // buy
            taxFee = buyFee;
        } else if (recipient == uniswapV2Pair) {
            // sell
            taxFee = sellFee.add(sellProFee);

        } 

        uint256 taxFeeAmount = amount.mul(taxFee).div(feeDenominator);

        if (taxFeeAmount > 0){
            // lpReward
            _tOwned[lpReward] = _tOwned[lpReward].add(taxFeeAmount.mul(lpRewardFee).div(feeDenominator));
            emit Transfer(sender, lpReward, taxFeeAmount.mul(lpRewardFee).div(feeDenominator));

            // poolBack
            _tOwned[poolBack] = _tOwned[poolBack].add(taxFeeAmount.mul(poolBackFee).div(feeDenominator));
            emit Transfer(sender, poolBack, taxFeeAmount.mul(poolBackFee).div(feeDenominator));

            // eFund
            _tOwned[eFund] = _tOwned[eFund].add(taxFeeAmount.mul(eFundFee).div(feeDenominator));
            emit Transfer(sender, eFund, taxFeeAmount.mul(eFundFee).div(feeDenominator));

            // blackHole
            if (_tOwned[blackHole] < blackAmountLimit){
                _tOwned[blackHole] = _tOwned[blackHole].add(taxFeeAmount.mul(blackHoleFee).div(feeDenominator));
                emit Transfer(sender, blackHole, taxFeeAmount.mul(blackHoleFee).div(feeDenominator));
            }else{
                _tOwned[poolBack] = _tOwned[poolBack].add(taxFeeAmount.mul(blackHoleFee).div(feeDenominator));
                emit Transfer(sender, poolBack, taxFeeAmount.mul(blackHoleFee).div(feeDenominator));
            }

            // nftReward
            _tOwned[nftReward] = _tOwned[nftReward].add(taxFeeAmount.mul(nftRewardFee).div(feeDenominator));
            emit Transfer(sender, nftReward, taxFeeAmount.mul(nftRewardFee).div(feeDenominator));
        }
        
        uint256 recipientAmount = amount.sub(taxFeeAmount);
        _tOwned[recipient] = _tOwned[recipient].add(recipientAmount);
        emit Transfer(sender, recipient, recipientAmount);
    }

    function getSellProtectFee(uint256 currentPrice) view public returns (uint256) {
        if(currentPrice >= todayInitPrice || todayInitPrice == 0 || currentPrice == 0){
            return 0;
        }else{
            uint256 priceGap = todayInitPrice.sub(currentPrice);
            return priceGap.mul(feeDenominator).div(todayInitPrice).div(1000).mul(1000);
        }
    }

    function getCurrentPrice() view  public returns (uint256){
        if(_tOwned[uniswapV2Pair] == 0){
            return 0;
        }
        uint256 current = IERC20(usdt).balanceOf(uniswapV2Pair).mul(10 ** 8).div(_tOwned[uniswapV2Pair]);
        return current;
    }
}