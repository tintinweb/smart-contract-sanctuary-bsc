contract TokenBack {
    address token;

    constructor(address tokenAddr) {
        token = tokenAddr;
    }

    function take() external {
        uint256 balance = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(msg.sender, balance);
        selfdestruct(payable(msg.sender));
    }
}

