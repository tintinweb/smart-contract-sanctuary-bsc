// File: contracts/vip/CocosMappingVip.sol

pragma solidity ^0.8.0;




contract CocosMappingVip is ICocosVipBook, Ownable {
    bytes32 public merkleRoot;

    event ResetVipBook (bytes32 merkleRoot);

    // Flag of initialize data
    bool private initialized;

    function initialize(address _owner, bytes32 _merkleRoot) external {
        require(!initialized, "initialize: Already initialized!");
        _transferOwnership(_owner);
        merkleRoot = _merkleRoot; 
    }

    function hasRegistered(address account, uint256 accountId, bytes32[] memory merkleProof) external view override
        returns (bool registered) {

        bytes32 node = keccak256(abi.encodePacked(accountId, account));
        registered = MerkleProof.verify(merkleProof, merkleRoot, node);
    }

    function resetVipBook(bytes32 _merkleRoot) external onlyOwner {
        merkleRoot = _merkleRoot;
        emit ResetVipBook(merkleRoot);
    }
}